Dialogue: 0,0:00:26.91,0:00:30.54,Opening,,0000,0000,0000,,Po čom naozaj túžiš?
Dialogue: 0,0:00:32.00,0:00:35.09,Opening,,0000,0000,0000,,Preber si to
Dialogue: 0,0:00:36.63,0:00:40.09,Opening,,0000,0000,0000,,Malá revolúcia
Dialogue: 0,0:00:41.14,0:00:44.51,Opening,,0000,0000,0000,,Ak to chceš vzdať
Dialogue: 0,0:00:45.10,0:00:48.64,Opening,,0000,0000,0000,,Na to je vždy príliš skoro
Dialogue: 0,0:00:49.73,0:00:51.15,Opening,,0000,0000,0000,,Skús to znova!
Dialogue: 0,0:00:51.44,0:00:54.23,Opening,,0000,0000,0000,,Kým vládzeš
Dialogue: 0,0:00:54.36,0:00:55.65,Opening,,0000,0000,0000,,Skús to znova!
Dialogue: 0,0:00:55.99,0:00:58.15,Opening,,0000,0000,0000,,A znova!
Dialogue: 0,0:00:58.24,0:00:59.99,Opening,,0000,0000,0000,,Chcem spraviť viac!
Dialogue: 0,0:01:00.03,0:01:00.70,Opening,,0000,0000,0000,,Chci viac!
Dialogue: 0,0:01:00.70,0:01:02.20,Opening,,0000,0000,0000,,Chceš spraviť viac!
Dialogue: 0,0:01:02.25,0:01:02.95,Opening,,0000,0000,0000,,Chci viac!
Dialogue: 0,0:01:03.00,0:01:07.20,Opening,,0000,0000,0000,,My chceme, chceme viac
Dialogue: 0,0:01:08.63,0:01:12.38,Opening,,0000,0000,0000,,Keď vyslovíš tajné zaklínadlo
Dialogue: 0,0:01:13.17,0:01:15.92,Opening,,0000,0000,0000,,Odvahu získaš na čokoľvek
Dialogue: 0,0:01:17.05,0:01:19.13,Opening,,0000,0000,0000,,Zázraky sa dejú
Dialogue: 0,0:01:19.43,0:01:21.43,Opening,,0000,0000,0000,,Tým, čo na ne veria
Dialogue: 0,0:01:22.31,0:01:26.68,Opening,,0000,0000,0000,,S vášňou v srdci všetko sa dá
Dialogue: 0,0:01:33.82,0:01:36.53,Opening,,0000,0000,0000,,Chcem spraviť viac!
Dialogue: 0,0:01:36.57,0:01:37.15,Opening,,0000,0000,0000,,Chcem viac!
Dialogue: 0,0:01:37.16,0:01:38.66,Opening,,0000,0000,0000,,Chceš spraviť viac!
Dialogue: 0,0:01:38.70,0:01:39.37,Opening,,0000,0000,0000,,Chci viac!
Dialogue: 0,0:01:39.41,0:01:42.79,Opening,,0000,0000,0000,,My chceme, chceme viac
Dialogue: 0,0:01:42.83,0:01:43.95,Opening,,0000,0000,0000,,Tak sprav, sprav viac
Dialogue: 0,0:01:43.99,0:01:45.70,Opening,,0000,0000,0000,,Chcem spraviť viac!
Dialogue: 0,0:01:45.70,0:01:46.28,Opening,,0000,0000,0000,,Chci viac!
Dialogue: 0,0:01:46.37,0:01:47.87,Opening,,0000,0000,0000,,Chceš spraviť viac!
Dialogue: 0,0:01:47.87,0:01:48.54,Opening,,0000,0000,0000,,Chci viac!
Dialogue: 0,0:01:48.62,0:01:52.87,Opening,,0000,0000,0000,,My chceme, chceme viac
Dialogue: 0,0:01:53.04,0:01:55.21,Opening,,0000,0000,0000,,Ešte viac!
Dialogue: 0,0:01:55.38,0:01:57.30,Opening,,0000,0000,0000,,Chceš spraviť viac!
Dialogue: 0,0:01:57.75,0:02:01.01,Opening,,0000,0000,0000,,Chceš, chceš spraviť viac
Dialogue: 0,0:02:02.26,0:02:03.76,Opening,,0000,0000,0000,,Ja chcem spraviť viac
Dialogue: 0,0:02:03.88,0:02:06.43,Opening,,0000,0000,0000,,Ja chcem spraviť viac
Dialogue: 0,0:02:07.02,0:02:10.14,Opening,,0000,0000,0000,,My chceme, chceme viac
Dialogue: 0,0:02:10.23,0:02:12.26,Opening,,0000,0000,0000,,Ja chcem spraviť viac!
Dialogue: 0,0:02:12.31,0:02:15.61,Opening,,0000,0000,0000,,Ja chcem viac!
Dialogue: 0,0:02:16.23,0:02:19.36,Opening,,0000,0000,0000,,My chceme, chceme viac
