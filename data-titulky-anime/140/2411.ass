Dialogue: 0,0:00:00.00,0:00:05.00,Default,,0000,0000,0000,,
Dialogue: 0,0:00:22.33,0:00:25.67,Opening,,0000,0000,0000,,Pozri sa do svojho srdca
Dialogue: 0,0:00:26.33,0:00:29.96,Opening,,0000,0000,0000,,Po čom naozaj túžiš?
Dialogue: 0,0:00:31.42,0:00:34.51,Opening,,0000,0000,0000,,Preber si to
Dialogue: 0,0:00:36.05,0:00:39.51,Opening,,0000,0000,0000,,Malá revolúcia
Dialogue: 0,0:00:40.56,0:00:43.93,Opening,,0000,0000,0000,,Ak to chceš vzdať
Dialogue: 0,0:00:44.52,0:00:48.06,Opening,,0000,0000,0000,,Na to je vždy príliš skoro
Dialogue: 0,0:00:49.15,0:00:50.57,Opening,,0000,0000,0000,,Skús to znova!
Dialogue: 0,0:00:50.90,0:00:53.65,Opening,,0000,0000,0000,,Kým vládzeš
Dialogue: 0,0:00:53.78,0:00:55.07,Opening,,0000,0000,0000,,Skús to znova!
Dialogue: 0,0:00:55.44,0:00:57.57,Opening,,0000,0000,0000,,A znova!
Dialogue: 0,0:00:57.66,0:00:59.45,Opening,,0000,0000,0000,,Chcem spraviť viac!
Dialogue: 0,0:00:59.49,0:01:00.08,Opening,,0000,0000,0000,,Chci viac!
Dialogue: 0,0:01:00.12,0:01:01.66,Opening,,0000,0000,0000,,Chceš spraviť viac!
Dialogue: 0,0:01:01.70,0:01:02.37,Opening,,0000,0000,0000,,Chci viac!
Dialogue: 0,0:01:02.45,0:01:06.62,Opening,,0000,0000,0000,,My chceme, chceme viac
Dialogue: 0,0:01:08.08,0:01:11.80,Opening,,0000,0000,0000,,Keď vyslovíš tajné zaklínadlo
Dialogue: 0,0:01:12.59,0:01:15.34,Opening,,0000,0000,0000,,Odvahu získaš na čokoľvek
Dialogue: 0,0:01:16.51,0:01:18.55,Opening,,0000,0000,0000,,Zázraky sa dejú
Dialogue: 0,0:01:18.89,0:01:20.85,Opening,,0000,0000,0000,,Tým, čo na ne veria
Dialogue: 0,0:01:21.76,0:01:26.10,Opening,,0000,0000,0000,,S vášňou v srdci všetko sa dá
Dialogue: 0,0:01:33.27,0:01:35.95,Opening,,0000,0000,0000,,Chcem spraviť viac!
Dialogue: 0,0:01:35.99,0:01:36.53,Opening,,0000,0000,0000,,Chcem viac!
Dialogue: 0,0:01:36.61,0:01:38.07,Opening,,0000,0000,0000,,Chceš spraviť viac!
Dialogue: 0,0:01:38.11,0:01:38.78,Opening,,0000,0000,0000,,Chci viac!
Dialogue: 0,0:01:38.82,0:01:42.20,Opening,,0000,0000,0000,,My chceme, chceme viac
Dialogue: 0,0:01:42.24,0:01:43.37,Opening,,0000,0000,0000,,Tak sprav, sprav viac
Dialogue: 0,0:01:43.41,0:01:45.12,Opening,,0000,0000,0000,,Chcem spraviť viac!
Dialogue: 0,0:01:45.12,0:01:45.70,Opening,,0000,0000,0000,,Chci viac!
Dialogue: 0,0:01:45.75,0:01:47.29,Opening,,0000,0000,0000,,Chceš spraviť viac!
Dialogue: 0,0:01:47.29,0:01:47.96,Opening,,0000,0000,0000,,Chci viac!
Dialogue: 0,0:01:48.00,0:01:52.29,Opening,,0000,0000,0000,,My chceme, chceme viac
Dialogue: 0,0:01:52.46,0:01:54.63,Opening,,0000,0000,0000,,Ešte viac!
Dialogue: 0,0:01:54.80,0:01:56.67,Opening,,0000,0000,0000,,Chceš spraviť viac!
Dialogue: 0,0:01:57.17,0:02:00.43,Opening,,0000,0000,0000,,Chceš, chceš spraviť viac
Dialogue: 0,0:02:01.68,0:02:03.18,Opening,,0000,0000,0000,,Ja chcem spraviť viac
Dialogue: 0,0:02:03.31,0:02:05.85,Opening,,0000,0000,0000,,Ja chcem spraviť viac
Dialogue: 0,0:02:06.44,0:02:09.56,Opening,,0000,0000,0000,,My chceme, chceme viac
Dialogue: 0,0:02:09.65,0:02:11.68,Opening,,0000,0000,0000,,Ja chcem spraviť viac!
Dialogue: 0,0:02:11.73,0:02:15.03,Opening,,0000,0000,0000,,Ja chcem viac!
Dialogue: 0,0:02:15.65,0:02:18.78,Opening,,0000,0000,0000,,My chceme, chceme viac
