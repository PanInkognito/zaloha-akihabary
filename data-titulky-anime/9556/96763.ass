Dialogue: 0,0:14:02.11,0:14:06.83,Default,,0,0,0,,Chamber je UI, ktoré pomáha pilotom pri boji a dozerá na životné funkcie.
Dialogue: 0,0:14:07.26,0:14:08.61,Default,,0,0,0,,Wow...
Dialogue: 0,0:14:08.99,0:14:10.08,Default,,0,0,0,,Neuveriteľné!
Dialogue: 0,0:14:10.08,0:14:11.93,Default,,0,0,0,,To je neuveriteľné!
Dialogue: 0,0:14:11.93,0:14:14.90,Default,,0,0,0,,Takže vo vnútri skutočne nikto nie je! Je to len stroj!
Dialogue: 0,0:14:14.90,0:14:18.75,Default,,0,0,0,,Znovu opakujem: Povedz nám o súčasnom stave Zeme.
Dialogue: 0,0:14:18.75,0:14:20.96,Default,,0,0,0,,To je neuveriteľné!
Dialogue: 0,0:14:25.68,0:14:28.06,Default,,0,0,0,,Hovoria, že predtým bola Zem ľadová planéta.
Dialogue: 0,0:14:28.57,0:14:31.39,Default,,0,0,0,,Potom sa ľad roztopil a Zem sa zmenila na vodný svet.
Dialogue: 0,0:14:31.89,0:14:35.68,Default,,0,0,0,,Pravdepodobne dakedy dávno boli aj miesta zvané "pevnina",
Dialogue: 0,0:14:35.68,0:14:37.35,Default,,0,0,0,,ale neviem, čo si pod tým predstaviť.
Dialogue: 0,0:14:37.89,0:14:41.81,Default,,0,0,0,,Teraz žijeme takto a spájame naše lode do flotily.
Dialogue: 0,0:14:42.97,0:14:45.53,Default,,0,0,0,,Na dne oceána sú stále staré lode a zásoby
Dialogue: 0,0:14:45.89,0:14:48.00,Default,,0,0,0,,a tieto sa snažíme vyťahovať.
Dialogue: 0,0:14:48.00,0:14:52.99,Default,,0,0,0,,Gargantia - tak sa volá táto flotila - nie je výnimkou.
Dialogue: 0,0:14:52.99,0:14:54.50,Default,,0,0,0,,Gargantia?
Dialogue: 0,0:14:54.50,0:14:56.26,Default,,0,0,0,,Áno, Gargantia.
Dialogue: 0,0:14:56.26,0:14:57.73,Default,,0,0,0,,Sú tu aj iné?
Dialogue: 0,0:14:57.73,0:14:58.70,Default,,0,0,0,,Huh?
Dialogue: 0,0:14:58.70,0:15:01.62,Default,,0,0,0,,Oh, áno sú aj iné flotily.
Dialogue: 0,0:15:01.62,0:15:03.72,Default,,0,0,0,,Sú rôznej veľkosti.
Dialogue: 0,0:15:04.27,0:15:06.96,Default,,0,0,0,,Všetko, čo musíš urobiť, je spojiť niekoľko lodí.
Dialogue: 0,0:15:07.68,0:15:10.43,Default,,0,0,0,,Ale sú tu aj iní, ako napríklad piráti.
Dialogue: 0,0:15:10.43,0:15:12.49,Default,,0,0,0,,{\p1\an7\fscx33\fscy23\pos(640,660)}m 184 20 l 172 20 l 172 -120 l 196 -120 l 196 20 l 184 20 m 196 20 l 220 20 l 220 -120 l 196 -120 l 196 -180 l 172 -180 l 172 -120 l 148 -120 l 148 -54 l 172 -54 l 172 80 l 196 80 l 196 20 m 92 20 l 80 20 l 80 -120 l 104 -120 l 104 20 l 92 20 m 104 20 l 128 20 l 128 -120 l 104 -120 l 104 -180 l 80 -180 l 80 -21 l 56 -21 l 56 20 l 80 20 l 80 80 l 104 80 l 104 20 m 0 20 l -12 20 l -12 -120 l 12 -120 l 12 20 l 0 20 m 12 20 l 36 20 l 36 -84 l 12 -84 l 12 -180 l -12 -180 l -12 -120 l -36 -120 l -36 20 l -12 20 l -12 80 l 12 80 l 12 20 m -184 20 l -196 20 l -196 -120 l -172 -120 l -172 20 l -184 20 m -172 20 l -148 20 l -148 -20 l -172 -20 l -172 -180 l -196 -180 l -196 -120 l -220 -120 l -220 20 l -196 20 l -196 80 l -172 80 l -172 20 m -92 20 l -104 20 l -104 -120 l -80 -120 l -80 20 l -92 20 m -80 -84 l -56 -84 l -56 -120 l -80 -120 l -80 -180 l -104 -180 l -104 -120 l -128 -120 l -128 20 l -104 20 l -104 80 l -80 80 l -80 20
Dialogue: 0,0:15:12.49,0:15:13.89,Default,,0,0,0,,Sú títo "piráti" nepriatelia?
Dialogue: 0,0:15:13.89,0:15:15.66,Default,,0,0,0,,Hej, tak nejako.
Dialogue: 0,0:15:17.70,0:15:19.98,Default,,0,0,0,,Tým sa netráp.
Dialogue: 0,0:15:24.51,0:15:27.53,Default,,0,0,0,,Toto je galaktický prúd. Niektorí to volajú "galaktické more".
Dialogue: 0,0:15:28.36,0:15:29.86,Default,,0,0,0,,Je naozaj pekné, keď ho takto vidíš.
Dialogue: 0,0:15:33.86,0:15:34.70,Default,,0,0,0,,To je pekné.
Dialogue: 0,0:15:35.49,0:15:38.12,Default,,0,0,0,,Za chvíľu vstúpime do nového galaktického prúdu.
Dialogue: 0,0:15:38.12,0:15:39.77,Default,,0,0,0,,Definuj "galaktický prúd".
Dialogue: 0,0:15:40.36,0:15:42.78,Default,,0,0,0,,Žiariace prúdy na mori.
Dialogue: 0,0:15:42.78,0:15:45.75,Default,,0,0,0,,Môžeme žiť len vďaka nim,
Dialogue: 0,0:15:46.11,0:15:49.34,Default,,0,0,0,,takže sa vždy plavíme a hľadáme ďalšie,
Dialogue: 0,0:15:49.81,0:15:52.93,Default,,0,0,0,,ale oni nevydržia večne.
Dialogue: 0,0:15:53.43,0:15:57.77,Default,,0,0,0,,Pravdepodobne budeme musieť nájsť ďalší prúd, do ktorého vstúpime.
Dialogue: 0,0:16:00.40,0:16:03.00,Default,,0,0,0,,More svieti, lebo sú v ňom svetlušky.
Dialogue: 0,0:16:03.54,0:16:06.83,Default,,0,0,0,,Tieto malé tvory zhromažďujú elektrinu.
Dialogue: 0,0:16:06.83,0:16:10.32,Default,,0,0,0,,A my našu elektrinu získame z nich.
Dialogue: 0,0:16:23.49,0:16:24.42,Default,,0,0,0,,To je nádherné!
Dialogue: 0,0:16:24.97,0:16:27.24,Default,,0,0,0,,{\p1\an7\fscx33\fscy23\pos(640,660)}m 276 20 l 264 20 l 264 -120 l 288 -120 l 288 20 l 276 20 m 288 20 l 312 20 l 312 -120 l 288 -120 l 288 -180 l 264 -180 l 264 -60 l 240 -60 l 240 20 l 264 20 l 264 80 l 288 80 l 288 20 m 368 20 l 356 20 l 356 -120 l 380 -120 l 380 20 l 368 20 m 380 20 l 404 20 l 404 -120 l 380 -120 l 380 -180 l 356 -180 l 356 -120 l 332 -120 l 332 0 l 356 0 l 356 80 l 380 80 l 380 20 m -92 20 l -104 20 l -104 -120 l -80 -120 l -80 20 l -92 20 m -80 20 l -56 20 l -56 -120 l -80 -120 l -80 -180 l -104 -180 l -104 -56 l -128 -56 l -128 20 l -104 20 l -104 80 l -80 80 l -80 20 m -12 80 l -12 80 l -12 -120 l 12 -120 l 12 0 l 12 0 m 12 0 l 36 0 l 36 -120 l 12 -120 l 12 -180 l -12 -180 l -12 -120 l -36 -120 l -36 80 l -12 80 l -12 80 l -12 80 l 12 0 m 184 20 l 172 20 l 172 -120 l 196 -120 l 196 20 l 184 20 m 196 20 l 220 20 l 220 -82 l 196 -82 l 196 -180 l 172 -180 l 172 -120 l 148 -120 l 148 20 l 172 20 l 172 80 l 196 80 l 196 20 m -368 20 l -380 20 l -380 -120 l -356 -120 l -356 20 l -368 20 m -356 20 l -332 20 l -332 -38 l -356 -38 l -356 -180 l -380 -180 l -380 -120 l -404 -120 l -404 20 l -380 20 l -380 80 l -356 80 l -356 20 m 92 20 l 80 20 l 80 -120 l 104 -120 l 104 20 l 92 20 m 104 -50 l 128 -50 l 128 -120 l 104 -120 l 104 -180 l 80 -180 l 80 -120 l 56 -120 l 56 20 l 80 20 l 80 80 l 104 80 l 104 20 m -278 20 l -290 20 l -290 -120 l -266 -120 l -266 20 l -278 20 m -266 20 l -242 20 l -242 -120 l -266 -120 l -266 -180 l -290 -180 l -290 -120 l -314 -120 l -314 -64 l -290 -64 l -290 80 l -266 80 l -266 20 m -186 20 l -198 20 l -198 -120 l -174 -120 l -174 20 l -186 20 m -174 80 l -150 80 l -150 -120 l -174 -120 l -174 -180 l -198 -180 l -198 -120 l -222 -120 l -222 20 l -198 20 l -198 20 l -174 20 l -174 20
Dialogue: 0,0:16:27.24,0:16:28.44,Default,,0,0,0,,{\p1\an7\fscx33\fscy23\pos(640,660)}m 0 20 l -12 20 l -12 -120 l 12 -120 l 12 20 l 0 20 m 12 20 l 36 20 l 36 -120 l 12 -120 l 12 -180 l -12 -180 l -12 -120 l -36 -120 l -36 -34 l -12 -34 l -12 80 l 12 80 l 12 20 m -92 20 l -104 20 l -104 -120 l -80 -120 l -80 20 l -92 20 m -80 20 l -56 20 l -56 -120 l -80 -120 l -80 -180 l -104 -180 l -104 -102 l -128 -102 l -128 20 l -104 20 l -104 80 l -80 80 l -80 20 m 184 20 l 172 20 l 172 -120 l 196 -120 l 196 20 l 184 20 m 196 20 l 220 20 l 220 -20 l 196 -20 l 196 -180 l 172 -180 l 172 -120 l 148 -120 l 148 20 l 172 20 l 172 80 l 196 80 l 196 20 m -184 20 l -196 20 l -196 -120 l -172 -120 l -172 20 l -184 20 m -172 20 l -148 20 l -148 -8 l -172 -8 l -172 -180 l -196 -180 l -196 -120 l -220 -120 l -220 20 l -196 20 l -196 80 l -172 80 l -172 20 m 92 20 l 80 20 l 80 -120 l 104 -120 l 104 20 l 92 20 m 104 -8 l 128 -8 l 128 -120 l 104 -120 l 104 -180 l 80 -180 l 80 -120 l 56 -120 l 56 20 l 80 20 l 80 80 l 104 80 l 104 20
Dialogue: 0,0:16:28.44,0:16:29.59,Default,,0,0,0,,Čo?
Dialogue: 0,0:16:30.23,0:16:32.05,Default,,0,0,0,,Zachytili ste ich súradnice?
Dialogue: 0,0:16:32.54,0:16:35.23,Default,,0,0,0,,Kvôli atmosférickému rušeniu, nedostatku informácií
Dialogue: 0,0:16:35.23,0:16:37.60,Default,,0,0,0,,a obmedzenej optickej analýze
Dialogue: 0,0:16:37.60,0:16:41.27,Default,,0,0,0,,je potrebné dlhodobé pozorovanie na úspešné zistenie našej aktuálnej polohy.
Dialogue: 0,0:16:50.38,0:16:52.53,Default,,0,0,0,,Hovorím vám, je to pravda!
Dialogue: 0,0:16:52.53,0:16:53.49,Default,,0,0,0,,Teda myslím.
Dialogue: 0,0:16:54.24,0:16:56.94,Default,,0,0,0,,Ty sa ani trocha {\i1}nesnažíš{\i0} aby to znelo realisticky.
Dialogue: 0,0:16:57.46,0:16:59.76,Default,,0,0,0,,Je len malá možnosť, aby to bol hovoriaci stroj—
Dialogue: 0,0:16:59.76,0:17:00.41,Default,,0,0,0,,Ale je to pravda!
Dialogue: 0,0:17:00.90,0:17:04.37,Default,,0,0,0,,Ale aj tak, viesť vojnu proti vesmírnym stvoreniam?
Dialogue: 0,0:17:04.37,0:17:06.06,Default,,0,0,0,,To je úplná sprostosť!
Dialogue: 0,0:17:06.61,0:17:11.24,Default,,0,0,0,,Čo by sme mali urobiť je rozobrať tú vec a presvedčiť sa, či to vie skutočne hovoriť!
Dialogue: 0,0:17:11.24,0:17:12.59,Default,,0,0,0,,To decko môžeme pokojne prehodiť cez palubu.
Dialogue: 0,0:17:12.97,0:17:13.90,Default,,0,0,0,,To myslíš vážne?
Dialogue: 0,0:17:13.90,0:17:16.25,Default,,0,0,0,,Oni nemajú kam ísť! Potrebujú pomoc!
Dialogue: 0,0:17:16.25,0:17:18.68,Default,,0,0,0,,Mali by ste si vypočuť, čo vám chce Ledo povedať!
Dialogue: 0,0:17:19.13,0:17:23.20,Default,,0,0,0,,Prílišný kontakt s abnormalitami nie je dobrý pre nikoho.
Dialogue: 0,0:17:23.71,0:17:26.18,Default,,0,0,0,,To platí pre obe, more aj vesmír.
Dialogue: 0,0:17:32.62,0:17:33.44,Default,,0,0,0,,Miesto identifikované.
Dialogue: 0,0:17:34.35,0:17:37.16,Default,,0,0,0,,Toto je presne to miesto, kde sme našli to Yunboro.
Dialogue: 0,0:17:37.16,0:17:39.28,Default,,0,0,0,,Dobre! Nenechajte kameň na kameni!
Dialogue: 0,0:17:51.07,0:17:55.49,Default,,0,0,0,,Zásoby sú tu dostatočné a miestny obyvatelia predstavujú len minimálnu hrozbu.
Dialogue: 0,0:17:55.49,0:17:57.30,Default,,0,0,0,,Sčasti súhlasím.
Dialogue: 0,0:17:58.01,0:18:00.01,Default,,0,0,0,,Ak sa nemôžeme vrátiť na bojisko sami,
Dialogue: 0,0:18:01.14,0:18:04.29,Default,,0,0,0,,musíme počkať, kým naši spojenci zachytia náš signál.
Dialogue: 0,0:18:04.81,0:18:07.63,Default,,0,0,0,,Vytvoriť si tu dobré vzťahy je asi najlepšia možnosť.
Dialogue: 0,0:18:07.63,0:18:08.44,Default,,0,0,0,,Súhlasím.
Dialogue: 0,0:18:10.84,0:18:16.86,Default,,0,0,0,,Slová ako "bojisko" a "spojenci" sú pre ľudí na tejto planéte úplne cudzie.
Dialogue: 0,0:18:17.81,0:18:20.82,Default,,0,0,0,,Náš súboj na život a na smrť proti Hideauze ich vôbec nezaujíma.
Dialogue: 0,0:18:26.99,0:18:28.71,Default,,0,0,0,,Čo to bolo? Čo sa deje?
Dialogue: 0,0:18:29.60,0:18:31.44,Default,,0,0,0,,Čo sa stalo?
Dialogue: 0,0:18:31.44,0:18:33.71,Default,,0,0,0,,Piráti útočia na zberačov!
Dialogue: 0,0:18:33.71,0:18:34.92,Default,,0,0,0,,Bellowsina loď!
Dialogue: 0,0:18:35.85,0:18:37.08,Default,,0,0,0,,Bellowsini ľudia...
Dialogue: 0,0:18:45.73,0:18:46.60,Default,,0,0,0,,Strieľajte!
Dialogue: 0,0:18:46.60,0:18:48.54,Default,,0,0,0,,Tí hnusní bastardi útočia.
Dialogue: 0,0:18:49.08,0:18:49.85,Default,,0,0,0,,Kapitán stráže!
Dialogue: 0,0:18:50.25,0:18:52.02,Default,,0,0,0,,Nemáme šancu! Je ich príliš veľa!
Dialogue: 0,0:18:54.92,0:18:57.15,Default,,0,0,0,,Zberači, ostaňte pod vodou!
Dialogue: 0,0:18:57.72,0:19:00.03,Default,,0,0,0,,Zaznamenal som niekoľko motorov.
Dialogue: 0,0:19:00.03,0:19:01.01,Default,,0,0,0,,Potvrdzujem!
Dialogue: 0,0:19:01.01,0:19:02.07,Default,,0,0,0,,Ledo!
Dialogue: 0,0:19:05.68,0:19:06.54,Default,,0,0,0,,Amy?
Dialogue: 0,0:19:23.73,0:19:24.81,Default,,0,0,0,,Majú aj letcov?
Dialogue: 0,0:19:32.81,0:19:35.47,Default,,0,0,0,,Majú deväť lodí a niekoľko malých člnov.
Dialogue: 0,0:19:35.47,0:19:37.32,Default,,0,0,0,,Útočia aj s letcami.
Dialogue: 0,0:19:37.32,0:19:38.28,Default,,0,0,0,,Kde sú posily?
Dialogue: 0,0:19:38.70,0:19:39.99,Default,,0,0,0,,Pravdepodobne sa im nepodarí prísť včas.
Dialogue: 0,0:19:40.34,0:19:41.20,Default,,0,0,0,,Nepriateľský útok?
Dialogue: 0,0:19:41.78,0:19:43.88,Default,,0,0,0,,Nie ste vy dvaja silní?
Dialogue: 0,0:19:43.88,0:19:45.88,Default,,0,0,0,,Nie je to super silné Yunboro?
Dialogue: 0,0:19:45.88,0:19:47.41,Default,,0,0,0,,Pomôž nám! Prosím!
Dialogue: 0,0:19:47.97,0:19:49.37,Default,,0,0,0,,Požaduje pomoc.
Dialogue: 0,0:19:49.98,0:19:52.58,Default,,0,0,0,,Urobíme to. Môžeme to neskôr pri vyjednávaní využiť.
Dialogue: 0,0:19:57.00,0:19:58.00,Default,,0,0,0,,Aktivuj vznášanie.
Dialogue: 0,0:20:09.04,0:20:11.06,Default,,0,0,0,,To zvláštne Yunboro sa hýbe!
Dialogue: 0,0:20:11.06,0:20:12.44,Default,,0,0,0,,Čože?!
Dialogue: 0,0:20:21.32,0:20:22.82,Default,,0,0,0,,Čo plánuje urobiť?
Dialogue: 0,0:20:23.72,0:20:24.53,Default,,0,0,0,,Štart.
Dialogue: 0,0:20:39.72,0:20:42.26,Default,,0,0,0,,Takže ty si Bellows z Gargantie?
Dialogue: 0,0:20:42.59,0:20:45.47,Default,,0,0,0,,Zoberte si, čo chcete a zmiznite!
Dialogue: 0,0:20:46.06,0:20:48.96,Default,,0,0,0,,Prečo sa zdržuješ s touto sprostou flotilou?
Dialogue: 0,0:20:48.96,0:20:50.18,Default,,0,0,0,,Poď s nami.
Dialogue: 0,0:20:50.18,0:20:52.98,Default,,0,0,0,,Lady Rackage má o tebe vysokú mienku.
Dialogue: 0,0:20:54.16,0:20:56.02,Default,,0,0,0,,O tom si nechaj zdať, blbec!
Dialogue: 0,0:20:58.89,0:20:59.69,Default,,0,0,0,,Čo si myslíš, že robíš?!
Dialogue: 0,0:21:00.08,0:21:02.28,Default,,0,0,0,,Bude lepšie, ak sa nás nebudeš snažiť nahnevať.
Dialogue: 0,0:21:02.67,0:21:03.77,Default,,0,0,0,,Preklínam ťa!
Dialogue: 0,0:21:08.13,0:21:11.08,Default,,0,0,0,,Priateľská a nepriateľská identifikácia skončená.
Dialogue: 0,0:21:11.70,0:21:14.12,Default,,0,0,0,,Požadujem povolenie na identifikáciu menších cieľov.
Dialogue: 0,0:21:14.12,0:21:14.79,Default,,0,0,0,,Urob to.
Dialogue: 0,0:21:24.53,0:21:25.84,Default,,0,0,0,,Identifikácia kompletná.
Dialogue: 0,0:21:25.84,0:21:27.59,Default,,0,0,0,,Deflekčný lúč pripravený.
Dialogue: 0,0:21:29.77,0:21:30.68,Default,,0,0,0,,Eliminuj ich.
Dialogue: 0,0:21:40.82,0:21:41.98,Default,,0,0,0,,Čo sa deje?!{pfffhahahaha}
Dialogue: 0,0:21:43.99,0:21:45.31,Default,,0,0,0,,Čo sa tam deje?
Dialogue: 0,0:22:02.59,0:22:03.88,Default,,0,0,0,,Čo, dopekla...
Dialogue: 0,0:22:07.28,0:22:09.31,Default,,0,0,0,,Čo to bolo?
