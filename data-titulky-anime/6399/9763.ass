Dialogue: 0,0:00:06.15,0:00:10.11,Default,,0000,0000,0000,,Pohledný válečník Zarbon prošel hrozivou proměnou.
Dialogue: 0,0:00:10.82,0:00:15.99,Default,,0000,0000,0000,,Vegeta byl překonán jeho ohromnou silou.
Dialogue: 0,0:00:18.87,0:00:23.21,Default,,0000,0000,0000,,Mezitím Kuririn zoufale spěchá k Velkému stařešinovi, nicméně...
Dialogue: 0,0:00:24.25,0:00:28.30,Default,,0000,0000,0000,,D-Dragon Bally nikomu nenechám!
Dialogue: 0,0:00:31.75,0:00:34.50,Op cz,,0000,0000,0000,,Výbuch! Výbuch! Jaké štěstí.
Dialogue: 0,0:00:34.72,0:00:37.63,Op cz,,0000,0000,0000,,Výbuch! Výbuch! To je ráj.
Dialogue: 0,0:00:38.05,0:00:45.05,Op cz,,0000,0000,0000,,Genki Dama řítí se vzduchem. Jo! Jo! Jdeme na to!
Dialogue: 0,0:00:46.56,0:00:52.36,Op cz,,0000,0000,0000,,Cítím nebezpečnou energii.
Dialogue: 0,0:00:52.47,0:00:57.80,Op cz,,0000,0000,0000,,Pokusím se jí postavit - jen se dívej.
Dialogue: 0,0:00:58.32,0:01:04.28,Op cz,,0000,0000,0000,,Ochráním tě i za cenu vlastní smrti.
Dialogue: 0,0:01:04.41,0:01:10.12,Op cz,,0000,0000,0000,,Použiji svou moc - sílu lásky.
Dialogue: 0,0:01:10.67,0:01:16.04,Op cz,,0000,0000,0000,,Všechny problémy překonám vytrvalostí a bojovým duchem.
Dialogue: 0,0:01:16.46,0:01:23.47,Op cz,,0000,0000,0000,,Jestli mě ti zloduši naštvou, tak je vyřídím.
Dialogue: 0,0:01:25.13,0:01:27.77,Op cz,,0000,0000,0000,,Výbuch! Výbuch! Jaké štěstí.
Dialogue: 0,0:01:28.11,0:01:30.93,Op cz,,0000,0000,0000,,Výbuch! Výbuch! To je naše šance!
Dialogue: 0,0:01:31.31,0:01:36.52,Op cz,,0000,0000,0000,,Nebeská jízda napříč vzduchem, to je ráj.
Dialogue: 0,0:01:36.98,0:01:39.68,Op cz,,0000,0000,0000,,Výbuch! Výbuch! Jaké štěstí.
Dialogue: 0,0:01:39.86,0:01:42.82,Op cz,,0000,0000,0000,,Výbuch! Výbuch! Stejný zítřek.
Dialogue: 0,0:01:43.20,0:01:50.00,Op cz,,0000,0000,0000,,Tvá duše již zná odpověď.
Dialogue: 0,0:01:50.37,0:01:53.44,Op cz,,0000,0000,0000,,Duše draka!
