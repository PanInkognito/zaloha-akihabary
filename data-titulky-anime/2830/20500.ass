Dialogue: 0,0:00:08.00,0:00:12.00,AKT-TEAM,,0000,0000,0000,,{\pos(203,57)\fad(500,500)}www.anikiteam.org
Dialogue: 0,0:00:10.00,0:00:14.00,x3,,0000,0000,0000,,{\fad(500,500)\pos(505,44)}Preložil
Dialogue: 0,0:00:12.00,0:00:16.00,Saphirro,,0000,0000,0000,,{\fad(500,500)\pos(555,75)}Saphirro
Dialogue: 0,0:00:01.64,0:00:04.68,Aquarion,,0000,0000,0000,,Svätá kniha, rok 0011.
Dialogue: 0,0:00:04.68,0:00:07.48,Aquarion,,0000,0000,0000,,Zem, ktorá bola uložená do záhuby veľkou katastrofou, je pod útokom
Dialogue: 0,0:00:07.48,0:00:10.52,Aquarion,,0000,0000,0000,,Tieňových anjelov z Atlandie
Dialogue: 0,0:00:10.52,0:00:14.22,Aquarion,,0000,0000,0000,,ktorí sa prebudili z 12.000 rokov spánku.
Dialogue: 0,0:00:14.22,0:00:16.53,Aquarion,,0000,0000,0000,,K protiútoku k tejto invázii,
Dialogue: 0,0:00:16.53,0:00:20.03,Aquarion,,0000,0000,0000,,na ochranu ľudí a sveta...
Dialogue: 0,0:00:20.03,0:00:28.94,Aquarion,,0000,0000,0000,,mládež spojená srdcami, telami a dušou začína vznešenú vojnu!
Dialogue: 0,0:00:29.97,0:00:32.41,Aquarion,,0000,0000,0000,,Aquarion!
Dialogue: 0,0:00:34.51,0:00:35.94,Aquarion,,0000,0000,0000,,Čo je to za silu?!
Dialogue: 0,0:00:35.94,0:00:38.28,Aquarion,,0000,0000,0000,,Je to... skvelé!
Dialogue: 0,0:01:23.73,0:01:25.28,Aquarion,,0000,0000,0000,,Majster Toma...
Dialogue: 0,0:01:26.63,0:01:29.16,Aquarion,,0000,0000,0000,,Ľudia bez krídel...
Dialogue: 0,0:01:29.16,0:01:31.07,Aquarion,,0000,0000,0000,,Majster Toma...
Dialogue: 0,0:01:31.07,0:01:33.03,Aquarion,,0000,0000,0000,,Si to ty, Otoha?
Dialogue: 0,0:01:33.03,0:01:35.24,Aquarion,,0000,0000,0000,,Ako dlho som spal?
Dialogue: 0,0:01:35.24,0:01:38.51,Aquarion,,0000,0000,0000,,12.000 rokov.
Dialogue: 0,0:01:38.51,0:01:41.84,Aquarion,,0000,0000,0000,,12.000 rokov, čo?
Dialogue: 0,0:01:41.84,0:01:44.41,Aquarion,,0000,0000,0000,,A aká je situácia v meste?
Dialogue: 0,0:01:44.41,0:01:49.02,Aquarion,,0000,0000,0000,,Všetko preto, že bláznivý bez krídlí človek zjedol ovocie poznania...
Dialogue: 0,0:01:49.02,0:01:52.19,Aquarion,,0000,0000,0000,,Je to svet moc tvrdý na sledovanie...
Dialogue: 0,0:01:52.19,0:01:54.52,Aquarion,,0000,0000,0000,,Títo ľudia bez krídel...
Dialogue: 0,0:01:54.52,0:01:58.33,Aquarion,,0000,0000,0000,,Takže naďalej pošpiňujú náš sen?
Dialogue: 0,0:01:58.33,0:02:00.85,Aquarion,,0000,0000,0000,,Majster Toma... Musím vám povedať...
Dialogue: 0,0:02:02.53,0:02:08.77,Aquarion,,0000,0000,0000,,Jeden, ktorý nás zradil... jeden, ktorý zradil vás... Slnečné Krídlo...
Dialogue: 0,0:02:10.77,0:02:14.74,Aquarion,,0000,0000,0000,,Bol oživený?
Dialogue: 0,0:02:14.74,0:02:19.11,Aquarion,,0000,0000,0000,,Snaží sa snami znova bojovať.
Dialogue: 0,0:02:20.55,0:02:23.29,Aquarion,,0000,0000,0000,,Slnečné Krídlo...
Dialogue: 0,0:02:27.72,0:02:34.33,Aquarion,,0000,0000,0000,,Dovol mi spýtať sa ťa ešte raz... ako sa voláš? Koľko máš rokov?
Dialogue: 0,0:02:34.33,0:02:36.30,Aquarion,,0000,0000,0000,,Máš rodinu?
Dialogue: 0,0:02:39.13,0:02:43.51,Aquarion,,0000,0000,0000,,No tak, sme tu zaneprázdnení. Daj mi pár odpovedí.
Dialogue: 0,0:02:43.51,0:02:51.71,Aquarion,,0000,0000,0000,,Tak ešte raz, ako sa voláš? Koľko máš rokov? Máš rodinu?
Dialogue: 0,0:02:56.12,0:03:00.38,Aquarion,,0000,0000,0000,,Je mi z tohto zle a tiež unavená! Si hluchý?!
Dialogue: 0,0:03:04.16,0:03:06.13,Aquarion,,0000,0000,0000,,Hej ty!
Dialogue: 0,0:03:14.94,0:03:15.84,Aquarion,,0000,0000,0000,,Nehýb sa!
Dialogue: 0,0:03:15.84,0:03:18.43,Aquarion,,0000,0000,0000,,Čo?! Z-Zástupca veliteľa...
Dialogue: 0,0:03:45.80,0:03:47.57,Aquarion,,0000,0000,0000,,Kto do pekla si-
Dialogue: 0,0:03:47.57,0:03:48.94,Aquarion,,0000,0000,0000,,Dieťa...
Dialogue: 0,0:03:48.94,0:03:55.04,Aquarion,,0000,0000,0000,,Keď sa niekoho pýtaš na meno, tak sa najprv predstav.
Dialogue: 0,0:03:56.04,0:03:58.81,Aquarion,,0000,0000,0000,,Dieťa, ako sa voláš?
Dialogue: 0,0:04:02.28,0:04:04.05,Aquarion,,0000,0000,0000,,Apollo.
Dialogue: 0,0:04:04.05,0:04:07.08,Aquarion,,0000,0000,0000,,Apollo? To je pekné meno.
Dialogue: 0,0:04:09.06,0:04:11.99,Aquarion,,0000,0000,0000,,Povedz brat, ako si sa cítil?
Dialogue: 0,0:04:11.99,0:04:13.73,Aquarion,,0000,0000,0000,,Tvoje 1. zjednotenie?
Dialogue: 0,0:04:13.73,0:04:18.00,Aquarion,,0000,0000,0000,,Cítil som zmysel nového života...
Dialogue: 0,0:04:18.00,0:04:21.34,Aquarion,,0000,0000,0000,,Ako keby som bol obklopený tisícami ruží...
Dialogue: 0,0:04:21.34,0:04:25.07,Aquarion,,0000,0000,0000,,spolu s tým, koho miluješ.
Dialogue: 0,0:04:25.07,0:04:26.64,Aquarion,,0000,0000,0000,,Je to... skvelé!
Dialogue: 0,0:04:26.64,0:04:29.48,Aquarion,,0000,0000,0000,,Ach jaj...
Dialogue: 0,0:04:29.48,0:04:31.55,Aquarion,,0000,0000,0000,,Oslepujúce svetlo...
Dialogue: 0,0:04:31.55,0:04:35.35,Aquarion,,0000,0000,0000,,Nepredstaviteľné, ako niečo tohto sveta... svetlo z raja.
Dialogue: 0,0:04:35.35,0:04:41.76,Aquarion,,0000,0000,0000,,Moje 1. zjednotenie... to som cítil 1. krát s Glenom a Reikou.
Dialogue: 0,0:04:41.76,0:04:44.53,Aquarion,,0000,0000,0000,,Čo? S Reikou?
Dialogue: 0,0:04:44.53,0:04:49.56,Aquarion,,0000,0000,0000,,To je konečné svetlo, ktoré sme my, rodina Alisia, videli.
Dialogue: 0,0:04:49.56,0:04:54.84,Aquarion,,0000,0000,0000,,Možno je to svetlo stvorenia, ktoré nám dá konečný mier.
Dialogue: 0,0:04:54.84,0:04:57.37,Aquarion,,0000,0000,0000,,Avšak... 2. krát...
Dialogue: 0,0:04:57.37,0:05:01.48,Aquarion,,0000,0000,0000,,S týmto chlapcom, ktorý môže byť reinkarnáciou Slnečného Krídla...
Dialogue: 0,0:05:01.48,0:05:03.41,Aquarion,,0000,0000,0000,,Moje zjednotenie s ním bolo...
Dialogue: 0,0:05:06.38,0:05:07.87,Aquarion,,0000,0000,0000,,desivé.
Dialogue: 0,0:05:09.42,0:05:13.22,Aquarion,,0000,0000,0000,,Tento typ zjednotenia je divoký a agresívny.
Dialogue: 0,0:05:13.22,0:05:16.16,Aquarion,,0000,0000,0000,,Nie je to pravé zjednotenie, ktoré je nádherné!
Dialogue: 0,0:05:16.16,0:05:22.03,Aquarion,,0000,0000,0000,,Máš pravdu! Chalan ako on... nemôže byť... vôbec Slnečné Krídlo!
Dialogue: 0,0:05:22.03,0:05:24.13,Aquarion,,0000,0000,0000,,Si krídlo!
Dialogue: 0,0:05:24.13,0:05:28.77,Aquarion,,0000,0000,0000,,Silvia, nesmieš hovoriť tak ľahkovážne.
Dialogue: 0,0:05:28.77,0:05:30.44,Aquarion,,0000,0000,0000,,Ale...
Dialogue: 0,0:05:30.44,0:05:31.80,Aquarion,,0000,0000,0000,,To nesmieš hovoriť!
Dialogue: 0,0:05:39.38,0:05:43.28,Aquarion,,0000,0000,0000,,Sakra! Pustite ma von!
Dialogue: 0,0:05:45.55,0:05:51.25,Aquarion,,0000,0000,0000,,Sakra! Kto môže veriť, že ten divoký chlapec, môže byť Slnečne Krídlo?
Dialogue: 0,0:05:53.09,0:05:57.63,Aquarion,,0000,0000,0000,,Slnečné Krídlo... hrdina, ktorý zachránil ľudstvo pred 12.000 rokmi
Dialogue: 0,0:05:57.63,0:06:00.80,Aquarion,,0000,0000,0000,,v boji proti Atlandii.
Dialogue: 0,0:06:00.80,0:06:02.84,Aquarion,,0000,0000,0000,,Kto vie?
Dialogue: 0,0:06:02.84,0:06:06.17,Aquarion,,0000,0000,0000,,Čo? Minútku!
Dialogue: 0,0:06:06.17,0:06:10.80,Aquarion,,0000,0000,0000,,Kto dovolil niekomu takému, o ktorom nevieme nič, do Aquariona?!
Dialogue: 0,0:06:12.15,0:06:14.78,Aquarion,,0000,0000,0000,,Aquarion si ho vybral.
Dialogue: 0,0:06:14.78,0:06:15.75,Aquarion,,0000,0000,0000,,Čo?
Dialogue: 0,0:06:15.75,0:06:22.32,Aquarion,,0000,0000,0000,,Ako keď si kôň vyberie jazdca, tak si ho Aquarion vybral.
Dialogue: 0,0:06:22.32,0:06:25.95,Aquarion,,0000,0000,0000,,Aquarion... si ho vybral?
Dialogue: 0,0:06:28.86,0:06:30.73,Aquarion,,0000,0000,0000,,Umieram od hladu...
Dialogue: 0,0:06:36.91,0:06:37.87,Aquarion,,0000,0000,0000,,Tu!
Dialogue: 0,0:06:37.87,0:06:39.27,Aquarion,,0000,0000,0000,,Čo?
Dialogue: 0,0:06:40.44,0:06:42.48,Aquarion,,0000,0000,0000,,Jedz, dieťa!
Dialogue: 0,0:06:42.48,0:06:46.38,Aquarion,,0000,0000,0000,,Jedz, kým môžeš. To je trik na prežitie.
Dialogue: 0,0:06:49.52,0:06:50.64,Aquarion,,0000,0000,0000,,Baron...
Dialogue: 0,0:07:02.06,0:07:05.16,Aquarion,,0000,0000,0000,,Neboj sa, slaboch.
Dialogue: 0,0:07:11.84,0:07:13.64,Aquarion,,0000,0000,0000,,Si len beštia.
Dialogue: 0,0:07:13.64,0:07:14.84,Aquarion,,0000,0000,0000,,Čo?
Dialogue: 0,0:07:14.84,0:07:16.88,Aquarion,,0000,0000,0000,,Veliteľ Fudo povedal...
Dialogue: 0,0:07:16.88,0:07:21.42,Aquarion,,0000,0000,0000,,že hlboko v tebe, má útočisko beštia tmy.
Dialogue: 0,0:07:21.42,0:07:22.91,Aquarion,,0000,0000,0000,,Beštia tmy?
Dialogue: 0,0:07:25.85,0:07:28.45,Aquarion,,0000,0000,0000,,Nie vážne, si presne ako beštia.
Dialogue: 0,0:07:31.29,0:07:33.66,Aquarion,,0000,0000,0000,,Tvoje oči...
Dialogue: 0,0:07:33.66,0:07:38.33,Aquarion,,0000,0000,0000,,Vidím... s očami v srdci.
Dialogue: 0,0:07:38.33,0:07:41.80,Aquarion,,0000,0000,0000,,Tak som sa prišla pozrieť sama...
Dialogue: 0,0:07:41.80,0:07:45.64,Aquarion,,0000,0000,0000,,Či si Slnečné Krídlo alebo beštia tmy.
Dialogue: 0,0:07:47.61,0:07:49.63,Aquarion,,0000,0000,0000,,Tak, čo si?
Dialogue: 0,0:07:54.32,0:07:58.05,Aquarion,,0000,0000,0000,,Bože! Aj keď sú to traja najlepší používatelia elementov,
Dialogue: 0,0:07:58.05,0:08:00.86,Aquarion,,0000,0000,0000,,ak takto skončia v skutočnom boji...
Dialogue: 0,0:08:00.86,0:08:06.69,Aquarion,,0000,0000,0000,,Keby som sa zjednotil, tak by to takto neskončilo.
Dialogue: 0,0:08:06.69,0:08:11.36,Aquarion,,0000,0000,0000,,Nepreháňaj to. Je to nepekné.
Dialogue: 0,0:08:12.97,0:08:14.60,Aquarion,,0000,0000,0000,,Preháňať, čo?
Dialogue: 0,0:08:14.60,0:08:18.13,Aquarion,,0000,0000,0000,,Povedal by som, že je to výraz pre niekoho, kto si myslí, že je "princ"!
Dialogue: 0,0:08:48.04,0:08:49.90,Aquarion,,0000,0000,0000,,Reika?
Dialogue: 0,0:08:49.90,0:08:51.57,Aquarion,,0000,0000,0000,,Ospravedlňujem sa.
Dialogue: 0,0:08:51.57,0:08:55.77,Aquarion,,0000,0000,0000,,Ja... Ja som nebola schopná riadiť hlavu Aquariona správne...
Dialogue: 0,0:08:57.15,0:09:00.08,Aquarion,,0000,0000,0000,,Tak mesto... a Glen...
Dialogue: 0,0:09:01.85,0:09:03.75,Aquarion,,0000,0000,0000,,Nie je to iba tvoja chyba.
Dialogue: 0,0:09:03.75,0:09:07.22,Aquarion,,0000,0000,0000,,Ak by som ťa správne nasledoval...
Dialogue: 0,0:09:07.22,0:09:09.32,Aquarion,,0000,0000,0000,,Sirius...
Dialogue: 0,0:09:09.32,0:09:11.39,Aquarion,,0000,0000,0000,,Preč od neho!
Dialogue: 0,0:09:11.39,0:09:12.23,Aquarion,,0000,0000,0000,,Silvia?
Dialogue: 0,0:09:12.23,0:09:16.23,Aquarion,,0000,0000,0000,,Nosíš smolu! Nepribližuj sa k môjmu bratovi!
Dialogue: 0,0:09:16.23,0:09:19.60,Aquarion,,0000,0000,0000,,Hocikto, kto sa s tebou stretne, stretne ho nešťastie!
Dialogue: 0,0:09:19.60,0:09:20.20,Aquarion,,0000,0000,0000,,To je...
Dialogue: 0,0:09:20.20,0:09:21.24,Aquarion,,0000,0000,0000,,Silvia!
Dialogue: 0,0:09:21.24,0:09:23.47,Aquarion,,0000,0000,0000,,Ale...
Dialogue: 0,0:09:23.47,0:09:27.64,Aquarion,,0000,0000,0000,,Ja... Ja...
Dialogue: 0,0:09:25.94,0:09:28.77,Aquarion,,0000,0000,0000,,{\an8}"Prívesok ochrany"
Dialogue: 0,0:09:29.44,0:09:31.05,Aquarion,,0000,0000,0000,,Čo?!
Dialogue: 0,0:09:31.05,0:09:34.25,Aquarion,,0000,0000,0000,,Počkajte na odpaľovacej rampe. Počkajte na odpaľovacej rampe.
Dialogue: 0,0:09:34.25,0:09:38.59,Aquarion,,0000,0000,0000,,Všetky systémy bežia. Všetci piloti na odpaľovaciu rampu.
Dialogue: 0,0:09:38.59,0:09:39.55,Aquarion,,0000,0000,0000,,Pripravený.
Dialogue: 0,0:09:39.55,0:09:41.45,Aquarion,,0000,0000,0000,,Pripravte všetky Vectory.
Dialogue: 0,0:09:47.63,0:09:48.63,Aquarion,,0000,0000,0000,,Pripraviť pre vzlet.
Dialogue: 0,0:09:48.63,0:09:51.37,Aquarion,,0000,0000,0000,,Kontrola Vectora hotová.
Dialogue: 0,0:09:51.37,0:09:53.86,Aquarion,,0000,0000,0000,,Vzlietnutie za 10 sekúnd.
Dialogue: 0,0:10:02.81,0:10:04.80,Aquarion,,0000,0000,0000,,Vector Sol, vzlietavam!
Dialogue: 0,0:10:06.28,0:10:07.94,Aquarion,,0000,0000,0000,,Vector Mars, vzlietam!
Dialogue: 0,0:10:09.52,0:10:11.11,Aquarion,,0000,0000,0000,,Vector Luna, vzlietam!
Dialogue: 0,0:10:51.46,0:10:54.16,Aquarion,,0000,0000,0000,,Zberač berie ľudí...
Dialogue: 0,0:10:54.16,0:10:55.53,Aquarion,,0000,0000,0000,,Také nemilosrdné...
Dialogue: 0,0:10:55.53,0:10:57.73,Aquarion,,0000,0000,0000,,Pomstím ťa, Glen...
Dialogue: 0,0:11:01.54,0:11:05.94,Aquarion,,0000,0000,0000,,Vector Sol tímu... Formácia S-L-M.
Dialogue: 0,0:11:05.94,0:11:08.60,Aquarion,,0000,0000,0000,,Kódové meno: "Forma obra"
Dialogue: 0,0:11:10.61,0:11:12.41,Aquarion,,0000,0000,0000,,Viac dimenzionálna kvantová úroveň narástla.
Dialogue: 0,0:11:12.41,0:11:15.45,Aquarion,,0000,0000,0000,,Rezonácia áur na zjednocovacej úrovni.
Dialogue: 0,0:11:15.45,0:11:16.65,Aquarion,,0000,0000,0000,,Nenshin!
Dialogue: 0,0:11:16.65,0:11:17.65,Aquarion,,0000,0000,0000,,Gattai!
Dialogue: 0,0:11:17.65,0:11:19.42,Aquarion,,0000,0000,0000,,Ideš, Aquarion!
Dialogue: 0,0:11:28.90,0:11:30.63,Aquarion,,0000,0000,0000,,Čo to je?
Dialogue: 0,0:11:30.63,0:11:31.86,Aquarion,,0000,0000,0000,,Oni sú...
Dialogue: 0,0:11:35.10,0:11:36.09,Aquarion,,0000,0000,0000,,Ach!
Dialogue: 0,0:12:03.36,0:12:04.85,Aquarion,,0000,0000,0000,,Volá ma...
Dialogue: 0,0:12:06.80,0:12:08.29,Aquarion,,0000,0000,0000,,Také rozkošné.
Dialogue: 0,0:12:11.34,0:12:13.54,Aquarion,,0000,0000,0000,,Je zmena v duševnej úrovni Reiky.
Dialogue: 0,0:12:13.54,0:12:16.87,Aquarion,,0000,0000,0000,,Úroveň strachu v Aquarionovi je veľmi vysoká!
Dialogue: 0,0:12:18.85,0:12:21.58,Aquarion,,0000,0000,0000,,Ach nie! Vracia sa jej to!
Dialogue: 0,0:12:27.69,0:12:28.82,Aquarion,,0000,0000,0000,,Zjednotenie zrušené!
Dialogue: 0,0:12:28.82,0:12:31.13,Aquarion,,0000,0000,0000,,Ukazovateľ zjednotenia spadol na kritickú úroveň.
Dialogue: 0,0:12:31.13,0:12:32.49,Aquarion,,0000,0000,0000,,Vector Sol... Reika!
Dialogue: 0,0:12:32.49,0:12:35.03,Aquarion,,0000,0000,0000,,Jej úroveň energie klesá... Situácia je nebezpečná!
Dialogue: 0,0:12:35.03,0:12:38.73,Aquarion,,0000,0000,0000,,Veliteľ! Pustite ma!
Dialogue: 0,0:12:38.73,0:12:40.87,Aquarion,,0000,0000,0000,,Potrebujeme výmenu... ja a Reika!
Dialogue: 0,0:12:40.87,0:12:42.57,Aquarion,,0000,0000,0000,,Výmenu?
Dialogue: 0,0:12:42.57,0:12:45.67,Aquarion,,0000,0000,0000,,Ale to môže byť extrémne nebezpečné!
Dialogue: 0,0:12:45.67,0:12:48.54,Aquarion,,0000,0000,0000,,Ak zomriem, budete plakať?
Dialogue: 0,0:12:48.54,0:12:50.01,Aquarion,,0000,0000,0000,,Energická sieť, je nebezpečné nízka.
Dialogue: 0,0:12:50.01,0:12:52.58,Aquarion,,0000,0000,0000,,Energická sieť, je nebezpečné nízka. Obávam sa o Reiku!
Dialogue: 0,0:12:52.58,0:12:54.52,Aquarion,,0000,0000,0000,,Si nudná, Chloe!
Dialogue: 0,0:12:54.52,0:12:59.95,Aquarion,,0000,0000,0000,,Seriane! Ako môžeš takto bojovať?!
Dialogue: 0,0:13:06.49,0:13:10.73,Aquarion,,0000,0000,0000,,Čo sa deje? Zabudla si na hrdosť bojovníka, Seriane?!
Dialogue: 0,0:13:13.63,0:13:16.17,Aquarion,,0000,0000,0000,,Apollonius...
Dialogue: 0,0:13:16.17,0:13:19.11,Aquarion,,0000,0000,0000,,Opovažuješ sa ma poučovať?!
Dialogue: 0,0:13:19.11,0:13:21.04,Aquarion,,0000,0000,0000,,Čo som to povedala?!
Dialogue: 0,0:13:21.04,0:13:23.53,Aquarion,,0000,0000,0000,,Niekto volá Silviu z jej minulého života...
Dialogue: 0,0:13:25.75,0:13:29.92,Aquarion,,0000,0000,0000,,Tak sa ku mne pripoj a ukáž, že vieš bojovať, Seriane!
Dialogue: 0,0:13:29.92,0:13:35.26,Aquarion,,0000,0000,0000,,Prenos človeka? Pripája sa do minulého života?!
Dialogue: 0,0:13:35.26,0:13:37.96,Aquarion,,0000,0000,0000,,Vystúp, pôjdem.
Dialogue: 0,0:13:37.96,0:13:40.13,Aquarion,,0000,0000,0000,,Hej, si tu?
Dialogue: 0,0:13:40.13,0:13:42.12,Aquarion,,0000,0000,0000,,Tento raz neodídeš len s popáleninami...
Dialogue: 0,0:13:46.47,0:13:48.00,Aquarion,,0000,0000,0000,,Hmm...
Dialogue: 0,0:13:48.00,0:13:49.70,Aquarion,,0000,0000,0000,,Hej, čo robíš?!
Dialogue: 0,0:13:49.70,0:13:51.21,Aquarion,,0000,0000,0000,,Rýchlo začnite!
Dialogue: 0,0:13:51.21,0:13:53.44,Aquarion,,0000,0000,0000,,Niekto, odtiahnite ho!
Dialogue: 0,0:13:53.44,0:13:54.93,Aquarion,,0000,0000,0000,,Pripraviť na výmenu!
Dialogue: 0,0:13:56.88,0:14:01.02,Aquarion,,0000,0000,0000,,Teraz počúvaj! Čakaj na okamžitú zmenu teleportácie!
Dialogue: 0,0:14:01.02,0:14:06.02,Aquarion,,0000,0000,0000,,Ale veliteľ... Stále o ňom nevieme veľa!
Dialogue: 0,0:14:06.02,0:14:08.89,Aquarion,,0000,0000,0000,,Preto ho chceme otestovať.
Dialogue: 0,0:14:20.10,0:14:27.71,Aquarion,,0000,0000,0000,,Odpočítavanie do teleportu... 5... 4... 3... 2... 1
Dialogue: 0,0:14:27.71,0:14:28.70,Aquarion,,0000,0000,0000,,Teraz...
Dialogue: 0,0:14:56.87,0:14:58.57,Aquarion,,0000,0000,0000,,Ideme, kamaráti!
Dialogue: 0,0:14:58.57,0:15:00.21,Aquarion,,0000,0000,0000,,Kto si myslí, že je?!
Dialogue: 0,0:15:00.21,0:15:03.24,Aquarion,,0000,0000,0000,,Myslíš si, že si Apollonius?
Dialogue: 0,0:15:03.24,0:15:04.55,Aquarion,,0000,0000,0000,,Nemožné!
Dialogue: 0,0:15:04.55,0:15:06.88,Aquarion,,0000,0000,0000,,Sousei Gattai! Ideš!
Dialogue: 0,0:15:11.69,0:15:14.12,Aquarion,,0000,0000,0000,,Také silné puto!
Dialogue: 0,0:15:14.12,0:15:16.22,Aquarion,,0000,0000,0000,,Viac dimenzionálna kvantová úroveň narástla.
Dialogue: 0,0:15:16.22,0:15:17.85,Aquarion,,0000,0000,0000,,Ukazovateľ zjednotenia narastá.
Dialogue: 0,0:15:33.98,0:15:35.44,Aquarion,,0000,0000,0000,,Ach... funguje to!
Dialogue: 0,0:15:38.71,0:15:42.08,Aquarion,,0000,0000,0000,,Aquarion!
Dialogue: 0,0:15:47.66,0:15:49.35,Aquarion,,0000,0000,0000,,Nechceme malé ryby!
Dialogue: 0,0:16:01.20,0:16:02.77,Aquarion,,0000,0000,0000,,Svetlo?
Dialogue: 0,0:16:02.77,0:16:08.31,Aquarion,,0000,0000,0000,,Je to svetlo, na ktoré sme čakali 12.000 rokov?
Dialogue: 0,0:16:08.31,0:16:10.85,Aquarion,,0000,0000,0000,,Majster Toma...
Dialogue: 0,0:16:10.85,0:16:13.71,Aquarion,,0000,0000,0000,,Otoha... Prenosová pesnička...
Dialogue: 0,0:16:16.22,0:16:19.78,Aquarion,,0000,0000,0000,,Prenosová pesnička duše... Otoha.
Dialogue: 0,0:17:01.90,0:17:04.90,Aquarion,,0000,0000,0000,,Prestal... ten zvuk.
Dialogue: 0,0:17:04.90,0:17:06.33,Aquarion,,0000,0000,0000,,O čom to hovoríš?
Dialogue: 0,0:17:06.33,0:17:07.32,Aquarion,,0000,0000,0000,,Čo?
Dialogue: 0,0:17:32.43,0:17:34.10,Aquarion,,0000,0000,0000,,Oživil sa!
Dialogue: 0,0:17:34.10,0:17:36.43,Aquarion,,0000,0000,0000,,A je silnejší!
Dialogue: 0,0:17:36.43,0:17:39.37,Aquarion,,0000,0000,0000,,Mýtické oživenie, čo?
Dialogue: 0,0:17:39.37,0:17:43.07,Aquarion,,0000,0000,0000,,Viac dimenzionálna kvantová úroveň vojaka Cherubína, sa zväčšila 7 krát!
Dialogue: 0,0:17:43.07,0:17:45.31,Aquarion,,0000,0000,0000,,7 krát?! Určite je to chyba!
Dialogue: 0,0:17:45.31,0:17:47.01,Aquarion,,0000,0000,0000,,To nie je chyba!
Dialogue: 0,0:17:47.01,0:17:49.50,Aquarion,,0000,0000,0000,,No tak, niekto! Nechajte to na mne!
Dialogue: 0,0:17:52.95,0:17:54.28,Aquarion,,0000,0000,0000,,Telo je moc ťažké!
Dialogue: 0,0:17:54.28,0:17:56.72,Aquarion,,0000,0000,0000,,Kto ovláda nohy! Tvoje reakcie sú moc pomalé!
Dialogue: 0,0:17:56.72,0:17:58.31,Aquarion,,0000,0000,0000,,Č-Čo?!
Dialogue: 0,0:18:00.62,0:18:01.78,Aquarion,,0000,0000,0000,,Prichádza! Čo?!
Dialogue: 0,0:18:06.23,0:18:07.53,Aquarion,,0000,0000,0000,,Čo robíš?!
Dialogue: 0,0:18:07.53,0:18:09.73,Aquarion,,0000,0000,0000,,Seriane, máš ochraňovať zadnú stranu!
Dialogue: 0,0:18:09.73,0:18:10.99,Aquarion,,0000,0000,0000,,Čo?!
Dialogue: 0,0:18:12.77,0:18:17.07,Aquarion,,0000,0000,0000,,Toto nie je zjednotenie! Nie sme vôbec v synchronizácii!
Dialogue: 0,0:18:24.78,0:18:25.75,Aquarion,,0000,0000,0000,,Čo?!
Dialogue: 0,0:18:34.59,0:18:37.11,Aquarion,,0000,0000,0000,,Nie! Budeme tam vcucnutí!
Dialogue: 0,0:18:40.26,0:18:41.85,Aquarion,,0000,0000,0000,,Smiešne!
Dialogue: 0,0:19:02.18,0:19:07.14,Aquarion,,0000,0000,0000,,Príliš slabý... Si príliš slabý, Krídlo.
Dialogue: 0,0:19:12.36,0:19:13.16,Aquarion,,0000,0000,0000,,Kto to je?
Dialogue: 0,0:19:13.16,0:19:16.10,Aquarion,,0000,0000,0000,,Ide to z vojaka Cherubína?
Dialogue: 0,0:19:16.10,0:19:19.40,Aquarion,,0000,0000,0000,,Kedy si tak zoslabol?
Dialogue: 0,0:19:19.40,0:19:24.00,Aquarion,,0000,0000,0000,,Čo sa stalo tomu nádhernému, silnému Krídlu?
Dialogue: 0,0:19:24.00,0:19:26.77,Aquarion,,0000,0000,0000,,Ten hlas... to nemôže byť...
Dialogue: 0,0:19:26.77,0:19:27.97,Aquarion,,0000,0000,0000,,Toma!
Dialogue: 0,0:19:32.81,0:19:33.87,Aquarion,,0000,0000,0000,,Toma?
Dialogue: 0,0:20:02.94,0:20:05.81,Aquarion,,0000,0000,0000,,Je tak zima... zima.
Dialogue: 0,0:20:05.81,0:20:11.12,Aquarion,,0000,0000,0000,,Č-Čo je to za pocit... ako svetlo v hĺbke temnoty v mojej duši?
Dialogue: 0,0:20:30.17,0:20:32.14,Aquarion,,0000,0000,0000,,Úroveň aury klesla!
Dialogue: 0,0:20:32.14,0:20:34.47,Aquarion,,0000,0000,0000,,Ich srdcia požiera temnota!
Dialogue: 0,0:20:35.81,0:20:37.65,Aquarion,,0000,0000,0000,,Sú na hrane!
Dialogue: 0,0:20:37.65,0:20:41.71,Aquarion,,0000,0000,0000,,Chlacova duša padla do hĺbok vedomia!
Dialogue: 0,0:20:43.72,0:20:44.98,Aquarion,,0000,0000,0000,,Krídlo...
Dialogue: 0,0:20:46.62,0:20:48.82,Aquarion,,0000,0000,0000,,Žiara.
Dialogue: 0,0:20:48.82,0:20:52.19,Aquarion,,0000,0000,0000,,Žiara tvojho svetla.
Dialogue: 0,0:20:52.19,0:20:54.40,Aquarion,,0000,0000,0000,,Svetlo?
Dialogue: 0,0:20:54.40,0:20:57.93,Aquarion,,0000,0000,0000,,Svetlo... ktoré nás zradilo... a zničilo svet...
Dialogue: 0,0:20:57.93,0:20:59.70,Aquarion,,0000,0000,0000,,konečné svetlo!
Dialogue: 0,0:20:59.70,0:21:02.17,Aquarion,,0000,0000,0000,,Čo?! Prebuď sa,
Dialogue: 0,0:21:02.17,0:21:03.90,Aquarion,,0000,0000,0000,,Slnečné Krídlo...
Dialogue: 0,0:21:03.90,0:21:08.64,Aquarion,,0000,0000,0000,,Jediný muž... ktorého som miloval!
Dialogue: 0,0:21:20.32,0:21:21.79,Aquarion,,0000,0000,0000,,Temnota...
Dialogue: 0,0:21:34.80,0:21:37.79,Aquarion,,0000,0000,0000,,Ach... toto je!
Dialogue: 0,0:21:46.28,0:21:48.88,Aquarion,,0000,0000,0000,,Svetlo... krídel beštie.
Dialogue: 0,0:21:48.88,0:21:51.42,Aquarion,,0000,0000,0000,,Svetlo sa zrodilo z temnoty...
Dialogue: 0,0:21:51.42,0:21:54.22,Aquarion,,0000,0000,0000,,Zrodenie sveta, z ktorého vznikol vesmír!
Dialogue: 0,0:22:01.83,0:22:03.16,Aquarion,,0000,0000,0000,,Svetlo...
Dialogue: 0,0:22:05.20,0:22:08.69,Aquarion,,0000,0000,0000,,Svetlo, ktoré som miloval.
Dialogue: 0,0:23:58.45,0:24:01.15,Aquarion,,0000,0000,0000,,Čo? Nemôžem zobraziť náhľad!
Dialogue: 0,0:24:01.15,0:24:05.15,Aquarion,,0000,0000,0000,,Nič také! Všetko, čo si spravil, je úžasné, brat!
Dialogue: 0,0:24:05.15,0:24:06.25,Aquarion,,0000,0000,0000,,Vážne?
Dialogue: 0,0:24:06.25,0:24:09.62,Aquarion,,0000,0000,0000,,Nabudúce v Aquarion: "Škola elementov"
Dialogue: 0,0:24:09.50,0:24:12.42,Aquarion,,0000,0000,0000,,{\fad500\pos(452,360)}"Škola elementov"
Dialogue: 0,0:24:09.62,0:24:12.42,Aquarion,,0000,0000,0000,,Znova sa zjednotím s bratom nabudúce!
Dialogue: 0,0:24:12.42,0:24:14.42,Aquarion,,0000,0000,0000,,
