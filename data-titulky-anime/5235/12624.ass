Dialogue: 0,0:04:09.89,0:04:15.58,Hanguel,,0000,0000,0000,,{\k50}O{\k34}ko{\k32}shi{\k31}te {\k34}ku{\k55}re{\k22}na{\k56}kya {\k31}ne{\k40}mu{\k22}ri{\k37}hi{\k39}me {\k86}no
Dialogue: 0,0:04:15.61,0:04:20.93,Hanguel,,0000,0000,0000,,{\k25}ma{\k26}ma {\k89}de {\k35}zu{\k12}t{\k108}to {\k35}ko{\k14}ma{\k15}ra{\k46}se{\k11}ru {\k39}ka{\k44}ra {\k33}ne
Dialogue: 0,0:04:21.74,0:04:27.56,Hanguel,,0000,0000,0000,,{\k61}Yo{\k24}n{\k19}da{\k29}ra {\k45}go {\k37}byo{\k39}u {\k49}de {\k29}ka{\k43}ke{\k21}tsu{\k39}ke {\k30}na{\k117}kya
Dialogue: 0,0:04:27.58,0:04:33.08,Hanguel,,0000,0000,0000,,{\k17}a{\k24}na{\k84}ta {\k38}na{\k22}n{\k88}te {\k32}ho{\k22}u{\k15}ri{\k36}da{\k26}su {\k25}ka{\k50}mo {\k71}yo
