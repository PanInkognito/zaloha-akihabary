Comment: 0,0:00:00.00,0:00:00.00,Romaji,,0,0,0,code once,function burst(top,left,bottom,right,radius,blur) center=math.floor((right-left)/2)+left middle=math.floor((bottom-top)/2)+top x=math.random(left,right) y=math.random(top,bottom) dx=x-center dy=y-middle dx=(radius+math.random(blur))*dx/math.abs(dx) dy=(radius+math.random(blur))*dy/math.abs(dy) return string.format("\\move(%d,%d,%d,%d)",x,y,x+dx,y+dy) end
Comment: 0,0:00:00.00,0:00:00.00,Romaji,,0,0,0,code once,function text_burst(top,left,height,width,rows,dist_factor,scale) squareside=math.ceil(height/rows) columns=math.ceil(width/squareside) maxloop(rows*columns) iteration=math.ceil(j/(rows*columns)) xclip=(math.floor((j%(rows*columns))/rows))*squareside+left yclip=((j%(rows*columns))%rows)*squareside+top tag=string.format("\\clip(%d,%d,%d,%d)",xclip,yclip,xclip+squareside,yclip+squareside) c_x=left+math.floor(width/2-squareside/2) c_y=top+math.floor(height/2-squareside/2) delta_x=xclip-c_x delta_y=yclip-c_y blur_x=math.random(-10,10) blur_y=math.random(-10,10) delta_x=math.floor(dist_factor*(delta_x+blur_x)) delta_y=math.floor(dist_factor*(delta_y+blur_y)) tag=tag..string.format("\\move(%d,%d,%d,%d)",left,top,left+delta_x,top+delta_y) tag=tag..string.format("\\t(\\clip(%d,%d,%d,%d)\\fscx%d\\fscy%d)",xclip+delta_x,yclip+delta_y,xclip+delta_x+math.floor(squareside*scale/100),yclip+delta_y+math.floor(squareside*scale/100),scale,scale) return tag end
