Dialogue: 0,0:23:11.85,0:23:20.93,RMLyrics2,,0,0,0,,{\blur2}akai nami o kette
Dialogue: 0,0:23:21.44,0:23:25.54,RMLyrics2,,0,0,0,,{\blur2}machine ga sakebu
Dialogue: 0,0:23:26.15,0:23:34.88,RMLyrics2,,0,0,0,,{\blur2}kurutta asa no hikari ni mo nita
Dialogue: 0,0:23:35.63,0:23:39.83,RMLyrics2,,0,0,0,,{\blur2}Walther P-38
Dialogue: 0,0:23:40.45,0:23:44.90,RMLyrics2,,0,0,0,,{\blur2}kono te no naka ni
Dialogue: 0,0:23:45.28,0:23:48.87,RMLyrics2,,0,0,0,,{\blur2}dakareta-mono wa
Dialogue: 0,0:23:49.38,0:23:53.90,RMLyrics2,,0,0,0,,{\blur2}subete kieyuku
Dialogue: 0,0:23:54.60,0:23:58.42,RMLyrics2,,0,0,0,,{\blur2}sadame na no sa
Dialogue: 0,0:23:58.98,0:24:03.59,RMLyrics2,,0,0,0,,{\blur2}Lupin sansei
