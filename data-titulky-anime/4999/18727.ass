Dialogue: 0,0:01:33.69,0:01:35.74,Carino,,0000,0000,0000,,Nepoznám ťa, špicatá hlava.
Dialogue: 0,0:01:36.63,0:01:38.68,Carino,,0000,0000,0000,,Iba môj drahý brat je môj partner.
Dialogue: 0,0:01:39.21,0:01:41.60,Carino,,0000,0000,0000,,Q-feuille! On ťa klame!
Dialogue: 0,0:01:42.98,0:01:44.11,Carino,,0000,0000,0000,,Klame...?
Dialogue: 0,0:01:49.86,0:01:51.89,Carino,,0000,0000,0000,,Ty si klamárka.
Dialogue: 0,0:01:53.52,0:01:54.67,Carino,,0000,0000,0000,,Už nebudeš klamať nikoho!
Dialogue: 0,0:01:59.80,0:02:00.26,Carino,,0000,0000,0000,,Čo to?
Dialogue: 0,0:02:02.91,0:02:03.94,Carino,,0000,0000,0000,,Warp.
Dialogue: 0,0:02:24.15,0:02:26.15,Carino,,0000,0000,0000,,Poplach.
Dialogue: 0,0:02:26.86,0:02:28.47,Carino,,0000,0000,0000,,Poplach.
Dialogue: 0,0:02:37.48,0:02:38.21,Carino,,0000,0000,0000,,Warp?
Dialogue: 0,0:02:38.89,0:02:39.77,Carino,,0000,0000,0000,,Kde?
Dialogue: 0,0:02:40.97,0:02:42.08,Carino,,0000,0000,0000,,Jedno miesto.
Dialogue: 0,0:02:43.20,0:02:45.49,Carino,,0000,0000,0000,,Miesto, kde boli moje sny zmrazené.
Dialogue: 0,0:02:46.84,0:02:50.39,Carino,,0000,0000,0000,,Áno... Je to miesto, kde ty a ja zrealizujeme náš dlho stratený sen.
Dialogue: 0,0:02:52.67,0:02:53.73,Carino,,0000,0000,0000,,Drahý brat?
Dialogue: 0,0:02:55.13,0:02:57.34,Carino,,0000,0000,0000,,Priestor zmrazeného času.
Dialogue: 0,0:02:59.42,0:03:02.80,Carino,,0000,0000,0000,,{\an8}Oživenie spomienok
Dialogue: 0,0:03:04.39,0:03:07.38,Carino,,0000,0000,0000,,Q-feuille, chytíme to dievča.
Dialogue: 0,0:03:07.38,0:03:09.66,Carino,,0000,0000,0000,,Áno, ale...
Dialogue: 0,0:03:09.66,0:03:10.93,Carino,,0000,0000,0000,,Toto malé dieťa stojí v ceste...
Dialogue: 0,0:03:11.55,0:03:12.35,Carino,,0000,0000,0000,,Toto?
Dialogue: 0,0:03:13.13,0:03:14.38,Carino,,0000,0000,0000,,Q-feuille!
Dialogue: 0,0:03:14.38,0:03:14.99,Carino,,0000,0000,0000,,Q-feuille!
Dialogue: 0,0:03:15.41,0:03:16.84,Carino,,0000,0000,0000,,Prišli sme si po teba!
Dialogue: 0,0:03:17.63,0:03:19.78,Carino,,0000,0000,0000,,Trixie, Troisienne...
Dialogue: 0,0:03:20.26,0:03:23.27,Carino,,0000,0000,0000,,Mi Nourose, a ostatný... Spmeň si na nich!
Dialogue: 0,0:03:23.27,0:03:26.01,Carino,,0000,0000,0000,,Trixie, Troisienne...?
Dialogue: 0,0:03:26.01,0:03:26.88,Carino,,0000,0000,0000,,Sprav to.
Dialogue: 0,0:03:26.88,0:03:28.27,Carino,,0000,0000,0000,,Nestrachuj sa tým dievčatkom.
Dialogue: 0,0:03:29.19,0:03:31.19,Carino,,0000,0000,0000,,Zbavím sa jej ak bude treba.
Dialogue: 0,0:03:31.70,0:03:32.31,Carino,,0000,0000,0000,,Čo?
Dialogue: 0,0:03:32.93,0:03:33.95,Carino,,0000,0000,0000,,A-Áno.
Dialogue: 0,0:03:34.74,0:03:35.19,Carino,,0000,0000,0000,,Dia!
Dialogue: 0,0:03:38.16,0:03:39.61,Carino,,0000,0000,0000,,Prepáč...
Dialogue: 0,0:03:42.32,0:03:42.71,Carino,,0000,0000,0000,,Čo to?
Dialogue: 0,0:03:44.83,0:03:45.39,Carino,,0000,0000,0000,,TAMA!
Dialogue: 0,0:03:53.47,0:03:54.65,Carino,,0000,0000,0000,,Si v poriadku, Dia?
Dialogue: 0,0:03:55.25,0:03:56.06,Carino,,0000,0000,0000,,Dia...
Dialogue: 0,0:03:56.06,0:03:57.89,Carino,,0000,0000,0000,,Som v poriadku.
Dialogue: 0,0:03:57.89,0:03:59.11,Carino,,0000,0000,0000,,Uf. Uf.
Dialogue: 0,0:03:59.95,0:04:00.98,Carino,,0000,0000,0000,,Som rada.
Dialogue: 0,0:04:02.23,0:04:04.40,Carino,,0000,0000,0000,,Obytná časť,
Dialogue: 0,0:04:04.40,0:04:07.03,Carino,,0000,0000,0000,,bola uzavretá špeciálnym vychyľovacím poľom.
Dialogue: 0,0:04:07.43,0:04:09.80,Carino,,0000,0000,0000,,Neprejdeš a ani tvojou teleportáciou.
Dialogue: 0,0:04:10.29,0:04:11.15,Carino,,0000,0000,0000,,Nie počkať...
Dialogue: 0,0:04:12.55,0:04:13.93,Carino,,0000,0000,0000,,Ospravedlňte nás.
Dialogue: 0,0:04:14.48,0:04:16.94,Carino,,0000,0000,0000,,Trvalo nám obnoviť ochranný perimete.
Dialogue: 0,0:04:17.37,0:04:18.43,Carino,,0000,0000,0000,,Erde-Meteor,
Dialogue: 0,0:04:18.85,0:04:21.24,Carino,,0000,0000,0000,,prejde do cieľovej oblasti ako planéta.
Dialogue: 0,0:04:22.09,0:04:24.00,Carino,,0000,0000,0000,,To je k ničomu, Liese-Lotte.
Dialogue: 0,0:04:24.00,0:04:27.20,Carino,,0000,0000,0000,,Jasné! Zastavím vás oboch!
Dialogue: 0,0:04:27.91,0:04:30.15,Carino,,0000,0000,0000,,Q-feuille! Spomínaj!
Dialogue: 0,0:04:30.15,0:04:31.08,Carino,,0000,0000,0000,,Ja som...
Dialogue: 0,0:04:31.08,0:04:32.21,Carino,,0000,0000,0000,,Zmlkni! Zmlkni!
Dialogue: 0,0:04:32.63,0:04:33.96,Carino,,0000,0000,0000,,Špicatá hlava prestaň!
Dialogue: 0,0:04:34.69,0:04:35.74,Carino,,0000,0000,0000,,Q-feuille...!
Dialogue: 0,0:04:39.66,0:04:41.05,Carino,,0000,0000,0000,,Ascoo!
Dialogue: 0,0:04:41.72,0:04:42.63,Carino,,0000,0000,0000,,To je ono.
Dialogue: 0,0:04:43.65,0:04:44.63,Carino,,0000,0000,0000,,Drahý brat.
Dialogue: 0,0:04:48.15,0:04:49.36,Carino,,0000,0000,0000,,Liese-Lotte.
Dialogue: 0,0:04:50.13,0:04:51.71,Carino,,0000,0000,0000,,Moja malá sestra.
Dialogue: 0,0:04:53.67,0:04:54.55,Carino,,0000,0000,0000,,Drahý brat.
Dialogue: 0,0:05:14.23,0:05:17.17,Carino,,0000,0000,0000,,Nie!
Dialogue: 0,0:05:22.14,0:05:23.87,Carino,,0000,0000,0000,,To malé dievča robý to isté mojím silám...!
Dialogue: 0,0:05:27.74,0:05:30.23,Carino,,0000,0000,0000,,Vidím... Zachránila si ma, Dia.
Dialogue: 0,0:05:33.28,0:05:34.27,Carino,,0000,0000,0000,,Dia.
Dialogue: 0,0:05:46.61,0:05:47.44,Carino,,0000,0000,0000,,Znova?
Dialogue: 0,0:05:47.44,0:05:48.48,Carino,,0000,0000,0000,,Super.
Dialogue: 0,0:05:49.33,0:05:50.05,Carino,,0000,0000,0000,,Čož-?!
Dialogue: 0,0:05:51.48,0:05:52.44,Carino,,0000,0000,0000,,Pani Q-feuille bola...
Dialogue: 0,0:05:53.09,0:05:56.05,Carino,,0000,0000,0000,,Môže sa pohybovať iba v poli.
Dialogue: 0,0:05:56.05,0:05:59.27,Carino,,0000,0000,0000,,To znamená, že je niekde v obytnej oblasti.
Dialogue: 0,0:06:00.60,0:06:03.51,Carino,,0000,0000,0000,,Pôjdeme na malý lov líšky?
Dialogue: 0,0:06:11.84,0:06:13.02,Carino,,0000,0000,0000,,Tak poď, špicatá hlava!
Dialogue: 0,0:06:13.73,0:06:14.80,Carino,,0000,0000,0000,,Ty...!
Dialogue: 0,0:06:17.47,0:06:18.09,Carino,,0000,0000,0000,,Q-feuille...
Dialogue: 0,0:06:21.13,0:06:23.55,Carino,,0000,0000,0000,,Nie!
Dialogue: 0,0:06:26.48,0:06:28.39,Carino,,0000,0000,0000,,Dia, ďakujem.
Dialogue: 0,0:06:31.53,0:06:32.58,Carino,,0000,0000,0000,,Q-feuille!
Dialogue: 0,0:06:32.96,0:06:34.32,Carino,,0000,0000,0000,,Spomeň si na nás!
Dialogue: 0,0:06:34.94,0:06:37.12,Carino,,0000,0000,0000,,Myslela som, že si bola Pani Dobrá Pamäť!
Dialogue: 0,0:06:37.12,0:06:38.71,Carino,,0000,0000,0000,,Nepoznám vás!
Dialogue: 0,0:06:39.46,0:06:40.89,Carino,,0000,0000,0000,,Som Ascoeur!
Dialogue: 0,0:06:40.89,0:06:42.12,Carino,,0000,0000,0000,,A ja Dia!
Dialogue: 0,0:06:43.12,0:06:45.26,Carino,,0000,0000,0000,,Nepoznám vás, špicatá hlava a dievčatko!
Dialogue: 0,0:06:45.26,0:06:46.99,Carino,,0000,0000,0000,,Ste moji nepriatelia!
Dialogue: 0,0:06:46.99,0:06:47.82,Carino,,0000,0000,0000,,Q-feuille!
Dialogue: 0,0:06:48.22,0:06:50.26,Carino,,0000,0000,0000,,Pôjdem na vás ľahko,
Dialogue: 0,0:06:50.26,0:06:51.63,Carino,,0000,0000,0000,,pretože sú to rozkazy môjho drahého brata.
Dialogue: 0,0:06:52.69,0:06:53.50,Carino,,0000,0000,0000,,Ten chlap...
Dialogue: 0,0:06:54.60,0:06:57.86,Carino,,0000,0000,0000,,Ten chlap nie je tvoj brat, Q-feuille.
Dialogue: 0,0:06:57.86,0:06:59.82,Carino,,0000,0000,0000,,On ťa klame, Q-feuille.
Dialogue: 0,0:06:59.17,0:06:59.82,Carino,,0000,0000,0000,,Zmlkni!
Dialogue: 0,0:07:00.65,0:07:01.68,Carino,,0000,0000,0000,,Q-feuille...?
Dialogue: 0,0:07:03.23,0:07:04.02,Carino,,0000,0000,0000,,Viem.
Dialogue: 0,0:07:04.64,0:07:06.48,Carino,,0000,0000,0000,,Ty si skutočná pravá malá sestra, však?
Dialogue: 0,0:07:06.90,0:07:08.85,Carino,,0000,0000,0000,,Nemôžem byť a je jedno čo spravím!
Dialogue: 0,0:07:09.33,0:07:12.85,Carino,,0000,0000,0000,,Nemyslím si že som jeho malá sestra!
Dialogue: 0,0:07:12.85,0:07:13.82,Carino,,0000,0000,0000,,Môj partner je...
Dialogue: 0,0:07:18.73,0:07:20.08,Carino,,0000,0000,0000,,Ascoo!
Dialogue: 0,0:07:20.08,0:07:22.69,Carino,,0000,0000,0000,,Ak si jeho malá sestra, nerob drahému bratovy také ťažkosti!
Dialogue: 0,0:07:23.39,0:07:25.66,Carino,,0000,0000,0000,,Nie! Q-feuille!
Dialogue: 0,0:07:27.13,0:07:27.86,Carino,,0000,0000,0000,,Znova?
Dialogue: 0,0:07:32.11,0:07:32.84,Carino,,0000,0000,0000,,Som ospalá.
Dialogue: 0,0:07:33.51,0:07:34.79,Carino,,0000,0000,0000,,Aj tak si len malé dieťa.
Dialogue: 0,0:07:35.27,0:07:36.81,Carino,,0000,0000,0000,,Vyzeráš ako keby si už nemala energiu.
Dialogue: 0,0:07:37.50,0:07:39.69,Carino,,0000,0000,0000,,Dia, ak odpadneš na mieste ako je toto...
Dialogue: 0,0:07:48.97,0:07:50.78,Carino,,0000,0000,0000,,Snažíš sa ochrániť to malé dieťa?
Dialogue: 0,0:07:51.56,0:07:53.42,Carino,,0000,0000,0000,,Len tak ostaň.
Dialogue: 0,0:07:56.28,0:07:57.94,Carino,,0000,0000,0000,,Už náas nik neruší.
Dialogue: 0,0:07:59.99,0:08:00.93,Carino,,0000,0000,0000,,Drahý brat.
Dialogue: 0,0:08:01.45,0:08:02.71,Carino,,0000,0000,0000,,Liese-Lotte.
Dialogue: 0,0:08:03.23,0:08:05.67,Carino,,0000,0000,0000,,Takže si zabudla, kto vskutočnosti si...
Dialogue: 0,0:08:06.44,0:08:07.63,Carino,,0000,0000,0000,,Úbohé dievča.
Dialogue: 0,0:08:08.33,0:08:11.09,Carino,,0000,0000,0000,,Špicatá hlava zabudla kto je?
Dialogue: 0,0:08:11.79,0:08:16.32,Carino,,0000,0000,0000,,Teraz, prilož svoje pery na moje, tvojho partnera.
Dialogue: 0,0:08:16.89,0:08:20.08,Carino,,0000,0000,0000,,Potom si spomenieš, kto v skutočnosti si.
Dialogue: 0,0:08:20.43,0:08:22.21,Carino,,0000,0000,0000,,Spomenúť si bozkom?
Dialogue: 0,0:08:22.21,0:08:23.19,Carino,,0000,0000,0000,,Áno.
Dialogue: 0,0:08:23.19,0:08:25.91,Carino,,0000,0000,0000,,Prijmi ma a spomenieš si na všetko.
Dialogue: 0,0:08:27.41,0:08:30.00,Carino,,0000,0000,0000,,Q-feuille, zastav ju svojou schopnosťou.
Dialogue: 0,0:08:30.39,0:08:31.57,Carino,,0000,0000,0000,,Áno.
Dialogue: 0,0:08:31.57,0:08:34.24,Carino,,0000,0000,0000,,Ak me chceš pobozkať, sám zastav čas a sprav to!
Dialogue: 0,0:08:35.49,0:08:37.45,Carino,,0000,0000,0000,,Q-feuille do toho neťahaj!
Dialogue: 0,0:08:38.03,0:08:41.44,Carino,,0000,0000,0000,,Moja sila uspý celý svet.
Dialogue: 0,0:08:41.99,0:08:43.21,Carino,,0000,0000,0000,,To nebude fungovať.
Dialogue: 0,0:08:46.44,0:08:48.47,Carino,,0000,0000,0000,,Takže, nikdy ma...
Dialogue: 0,0:08:51.83,0:08:52.59,Carino,,0000,0000,0000,,..nepobozkáš!
Dialogue: 0,0:08:56.08,0:08:57.66,Carino,,0000,0000,0000,,Aké problénové dieťa...
Dialogue: 0,0:08:58.96,0:09:03.21,Carino,,0000,0000,0000,,Ako sa teraz veci majú, nemôžem sa vryť do teba.
Dialogue: 0,0:09:04.39,0:09:05.14,Carino,,0000,0000,0000,,Pán Geacht'er...
Dialogue: 0,0:09:08.28,0:09:09.94,Carino,,0000,0000,0000,,Chyť ju.
Dialogue: 0,0:09:10.66,0:09:13.11,Carino,,0000,0000,0000,,Nezaujíma ma, ako sa to spravíš, kým bude dýchať.
Dialogue: 0,0:09:18.57,0:09:20.53,Carino,,0000,0000,0000,,Musím aspoň Diu a TAMA dostať z tadeto.
Dialogue: 0,0:09:22.64,0:09:23.42,Carino,,0000,0000,0000,,Tento pocit.
Dialogue: 0,0:09:24.09,0:09:24.82,Carino,,0000,0000,0000,,Rezonácia?
Dialogue: 0,0:09:25.42,0:09:26.16,Carino,,0000,0000,0000,,Q-feuille?
Dialogue: 0,0:09:27.56,0:09:28.38,Carino,,0000,0000,0000,,Tvoje spomienky.
Dialogue: 0,0:09:38.50,0:09:41.92,Carino,,0000,0000,0000,,Akonáhle sa zapletieš do týchto reťazcov, 10,000 krát viac než neunikneš.
Dialogue: 0,0:09:42.67,0:09:45.33,Carino,,0000,0000,0000,,Bola presne tam, kde ste povedala, pani Q-feuille.
Dialogue: 0,0:09:46.56,0:09:47.54,Carino,,0000,0000,0000,,Q-feuille...
Dialogue: 0,0:09:48.44,0:09:52.21,Carino,,0000,0000,0000,,Rezonácia Q-feuille nám povie kde si.
Dialogue: 0,0:09:54.09,0:09:55.65,Carino,,0000,0000,0000,,100 krát sa prestaň hýbať!
Dialogue: 0,0:09:58.79,0:09:59.78,Carino,,0000,0000,0000,,Q-feuille...
Dialogue: 0,0:10:01.47,0:10:04.70,Carino,,0000,0000,0000,,Ak sa pokúsiš s tede dostať, tak sa tvoje telo prereže.
Dialogue: 0,0:10:06.82,0:10:07.78,Carino,,0000,0000,0000,,To som ja.
Dialogue: 0,0:10:09.89,0:10:12.52,Carino,,0000,0000,0000,,Ascoeur!
Dialogue: 0,0:10:14.83,0:10:20.02,Carino,,0000,0000,0000,,Q-fe... Q-feuille, tvoja partnerka... som ja...
Dialogue: 0,0:10:20.02,0:10:21.08,Carino,,0000,0000,0000,,Čo?
Dialogue: 0,0:10:21.84,0:10:25.56,Carino,,0000,0000,0000,,Iba ja som tvoja partnerka!
Dialogue: 0,0:10:26.90,0:10:27.99,Carino,,0000,0000,0000,,Q-feuille...
Dialogue: 0,0:10:28.83,0:10:30.51,Carino,,0000,0000,0000,,Q-feuille...
Dialogue: 0,0:10:31.52,0:10:33.28,Carino,,0000,0000,0000,,Q-feuille!
Dialogue: 0,0:10:36.32,0:10:37.17,Carino,,0000,0000,0000,,Hej...
Dialogue: 0,0:10:39.00,0:10:40.29,Carino,,0000,0000,0000,,Q-feuille...
Dialogue: 0,0:10:40.86,0:10:42.20,Carino,,0000,0000,0000,,Q-feuille...
Dialogue: 0,0:10:43.90,0:10:44.51,Carino,,0000,0000,0000,,Prečo...
Dialogue: 0,0:10:45.12,0:10:47.54,Carino,,0000,0000,0000,,Prečo tie slzy neprestanú?
Dialogue: 0,0:10:51.53,0:10:53.64,Carino,,0000,0000,0000,,Q-feuille!
Dialogue: 0,0:10:53.64,0:10:56.33,Carino,,0000,0000,0000,,Myslím, je to 10,000 krát viac než dosť.
Dialogue: 0,0:11:00.69,0:11:02.45,Carino,,0000,0000,0000,,Q-feuille...
Dialogue: 0,0:11:06.17,0:11:06.95,Carino,,0000,0000,0000,,Prestaň!
Dialogue: 0,0:11:07.60,0:11:09.41,Carino,,0000,0000,0000,,Prestaň, do kelu!
Dialogue: 0,0:11:11.79,0:11:15.96,Carino,,0000,0000,0000,,Muchlovanie
Dialogue: 0,0:11:15.96,0:11:19.05,Carino,,0000,0000,0000,,Originálny tekvicový puding - Tak chutný, až je návykový
Dialogue: 0,0:11:19.05,0:11:20.97,Carino,,0000,0000,0000,,Muchlovanie
Dialogue: 0,0:11:40.34,0:11:43.61,Carino,,0000,0000,0000,,Pozoravia stanica v prestore zmrazeného času, zmizla.
Dialogue: 0,0:11:44.21,0:11:44.70,Carino,,0000,0000,0000,,Áno.
Dialogue: 0,0:11:45.14,0:11:48.71,Carino,,0000,0000,0000,,Zdá sa, že základňa spoločnosti G sa objavila vo vyššie uvedených súradniciach.
Dialogue: 0,0:11:49.41,0:11:51.53,Carino,,0000,0000,0000,,Takže konečne vložili ich pevnosť do hry.
Dialogue: 0,0:11:52.20,0:11:54.26,Carino,,0000,0000,0000,,Chvíľa k prístupu príde,
Dialogue: 0,0:11:54.26,0:11:56.27,Carino,,0000,0000,0000,,keď priestor zmrazeného času bude prístupný.
Dialogue: 0,0:11:56.87,0:11:59.75,Carino,,0000,0000,0000,,Tím Tweedledee už by mal byť na ceste.
Dialogue: 0,0:12:00.61,0:12:02.21,Carino,,0000,0000,0000,,Mali by sme si tiež pohnúť.
Dialogue: 0,0:12:02.21,0:12:02.72,Carino,,0000,0000,0000,,Áno.
Dialogue: 0,0:12:05.75,0:12:06.92,Carino,,0000,0000,0000,,Úbohé dievča.
Dialogue: 0,0:12:07.90,0:12:09.84,Carino,,0000,0000,0000,,Ak by si sa toľko neodolávala,
Dialogue: 0,0:12:09.84,0:12:12.73,Carino,,0000,0000,0000,,mohla si byť výtaná ako moja malá sestra.
Dialogue: 0,0:12:13.46,0:12:14.69,Carino,,0000,0000,0000,,Tak či tak,
Dialogue: 0,0:12:14.69,0:12:17.48,Carino,,0000,0000,0000,,Konečne si prišla, Liese-Lotte...
Dialogue: 0,0:12:18.05,0:12:18.86,Carino,,0000,0000,0000,,Drahý brat.
Dialogue: 0,0:12:19.51,0:12:21.21,Carino,,0000,0000,0000,,Nemal si zájsť tak daleko.
Dialogue: 0,0:12:21.21,0:12:22.73,Carino,,0000,0000,0000,,Mohol si ju len uspať.
Dialogue: 0,0:12:23.45,0:12:26.34,Carino,,0000,0000,0000,,Ubližovaním si sa láska prehlbuje.
Dialogue: 0,0:12:27.95,0:12:31.49,Carino,,0000,0000,0000,,Pre tých, ktorí prišli o všetko a stali sa prázdnymi,
Dialogue: 0,0:12:31.49,0:12:33.47,Carino,,0000,0000,0000,,hocičo čo im dáš je pre ních všetko.
Dialogue: 0,0:12:35.28,0:12:38.59,Carino,,0000,0000,0000,,Teraz, si spomeň, Liese-Lotte...
Dialogue: 0,0:13:03.32,0:13:05.78,Carino,,0000,0000,0000,,Mami, kam ideme?
Dialogue: 0,0:13:06.38,0:13:07.89,Carino,,0000,0000,0000,,Na dobré miesto.
Dialogue: 0,0:13:07.89,0:13:08.61,Carino,,0000,0000,0000,,Dobré miesto?
Dialogue: 0,0:13:09.17,0:13:14.83,Carino,,0000,0000,0000,,Táto správa je pre všetkých hrdých členov klanu Rosenburg, ktorí majú materské znamienko v tvare ruže.
Dialogue: 0,0:13:15.94,0:13:20.24,Carino,,0000,0000,0000,,Galaxie bola pošliapaná stúpencami, ktorích poznáme ako jednoduchí občania.
Dialogue: 0,0:13:20.92,0:13:25.46,Carino,,0000,0000,0000,,Naša šlachtická zem a čistokrvný Nouvlesse je na pokraji podľahnutia pre nich.
Dialogue: 0,0:13:25.94,0:13:31.88,Carino,,0000,0000,0000,,Tak či onak, odmietoli sme moc tých špinavých stúpencov.
Dialogue: 0,0:13:32.56,0:13:34.85,Carino,,0000,0000,0000,,Ako hrdý čistokrvný Terrans.
Dialogue: 0,0:13:35.58,0:13:37.96,Carino,,0000,0000,0000,,Prijmime vznešenú smrt!
Dialogue: 0,0:13:38.79,0:13:39.92,Carino,,0000,0000,0000,,To je dedko.
Dialogue: 0,0:13:43.33,0:13:44.56,Carino,,0000,0000,0000,,Hej, už sme tu?
Dialogue: 0,0:13:45.32,0:13:46.17,Carino,,0000,0000,0000,,Skoro.
Dialogue: 0,0:13:46.72,0:13:48.78,Carino,,0000,0000,0000,,Vravíš to celú dobu.
Dialogue: 0,0:13:49.48,0:13:52.27,Carino,,0000,0000,0000,,Hneď po jej prijatí ako testovací objekt...
Dialogue: 0,0:13:52.27,0:13:56.36,Carino,,0000,0000,0000,,Žiadne záruky  môžu byť dané vášmu dieťaťu v oblasti ľudských práv a života.
Dialogue: 0,0:13:56.36,0:13:58.74,Carino,,0000,0000,0000,,Ste si toho vedomá z vášho predchádzajúceho dieťaťa, správnet?
Dialogue: 0,0:13:59.55,0:14:00.03,Carino,,0000,0000,0000,,Áno.
Dialogue: 0,0:14:00.50,0:14:01.74,Carino,,0000,0000,0000,,Prosím postarajte sa o ňu.
Dialogue: 0,0:14:02.59,0:14:05.07,Carino,,0000,0000,0000,,Teraz, choď s touto osobou.
Dialogue: 0,0:14:05.70,0:14:06.31,Carino,,0000,0000,0000,,Nie.
Dialogue: 0,0:14:06.91,0:14:09.52,Carino,,0000,0000,0000,,Chcem byť s mamou a ostatnými...
Dialogue: 0,0:14:09.52,0:14:10.02,Carino,,0000,0000,0000,,Nie!
Dialogue: 0,0:14:11.22,0:14:13.91,Carino,,0000,0000,0000,,Je to len dovtedy, kým dedko dokončí jeho prácu.
Dialogue: 0,0:14:14.71,0:14:15.67,Carino,,0000,0000,0000,,Vážne?
Dialogue: 0,0:14:15.67,0:14:16.52,Carino,,0000,0000,0000,,Áno.
Dialogue: 0,0:14:16.52,0:14:19.43,Carino,,0000,0000,0000,,Tak dlho, kým budeš dobré dieťa a nebudeš plakať... Slubujem.
Dialogue: 0,0:14:20.69,0:14:21.75,Carino,,0000,0000,0000,,Ja neplačem.
Dialogue: 0,0:14:22.19,0:14:24.13,Carino,,0000,0000,0000,,Liese-Lotte bude dobré dieťal.
Dialogue: 0,0:14:25.81,0:14:27.75,Carino,,0000,0000,0000,,Ľúbim ťa, Liese-Lotte.
Dialogue: 0,0:14:32.06,0:14:33.90,Carino,,0000,0000,0000,,Ži, Liese-Lotte.
Dialogue: 0,0:14:34.39,0:14:36.66,Carino,,0000,0000,0000,,Ži svoj život, akoby bol náš, Liese-Lotte.
Dialogue: 0,0:14:49.88,0:14:51.26,Carino,,0000,0000,0000,,Kde to som?
Dialogue: 0,0:14:51.26,0:14:52.67,Carino,,0000,0000,0000,,Ticho a pokračuj.
Dialogue: 0,0:14:52.67,0:14:53.70,Carino,,0000,0000,0000,,Nezastavuj.
Dialogue: 0,0:14:55.51,0:14:56.64,Carino,,0000,0000,0000,,Tu, dnu.
Dialogue: 0,0:14:57.26,0:14:59.69,Carino,,0000,0000,0000,,Od zajtra, začneme experimentovať na rozvoji schopností.
Dialogue: 0,0:15:00.96,0:15:01.93,Carino,,0000,0000,0000,,Počkať.
Dialogue: 0,0:15:01.93,0:15:03.74,Carino,,0000,0000,0000,,Liese-Lotte bolo dobré dievča.
Dialogue: 0,0:15:04.12,0:15:06.33,Carino,,0000,0000,0000,,Kde je otec? Kde je mama?
Dialogue: 0,0:15:08.11,0:15:09.49,Carino,,0000,0000,0000,,Počkať!
Dialogue: 0,0:15:09.49,0:15:11.00,Carino,,0000,0000,0000,,Otec!
Dialogue: 0,0:15:11.00,0:15:12.85,Carino,,0000,0000,0000,,Mami!
Dialogue: 0,0:15:16.09,0:15:16.90,Carino,,0000,0000,0000,,Kto to je?
Dialogue: 0,0:15:18.85,0:15:19.54,Carino,,0000,0000,0000,,Ty.
Dialogue: 0,0:15:20.26,0:15:22.08,Carino,,0000,0000,0000,,Aké si povedala že je tvoje meno?
Dialogue: 0,0:15:23.88,0:15:25.29,Carino,,0000,0000,0000,,Liese-Lotte.
Dialogue: 0,0:15:27.24,0:15:30.28,Carino,,0000,0000,0000,,Takže si to vážne ty, Liese-Lotte.
Dialogue: 0,0:15:31.18,0:15:33.16,Carino,,0000,0000,0000,,Tiež si tu bola odložená...
Dialogue: 0,0:15:34.32,0:15:35.34,Carino,,0000,0000,0000,,Do tohto pekla.
Dialogue: 0,0:15:37.28,0:15:38.93,Carino,,0000,0000,0000,,Brat Alfried?
Dialogue: 0,0:15:43.11,0:15:45.39,Carino,,0000,0000,0000,,Ostaň tam. Si ťažká.
Dialogue: 0,0:15:45.39,0:15:48.38,Carino,,0000,0000,0000,,Niesi mŕtvy, brat.
Dialogue: 0,0:15:48.38,0:15:49.59,Carino,,0000,0000,0000,,Mŕtvy?
Dialogue: 0,0:15:50.90,0:15:54.43,Carino,,0000,0000,0000,,Chápem... Som považovaný za mŕtveho.
Dialogue: 0,0:15:54.43,0:15:57.29,Carino,,0000,0000,0000,,Niesi mŕtvy, brat Alfried.
Dialogue: 0,0:15:57.29,0:16:00.39,Carino,,0000,0000,0000,,Vzhľadom k tomu, že taký horúci.
Dialogue: 0,0:16:03.49,0:16:05.60,Carino,,0000,0000,0000,,Tak dlho ako sme v poriadku,
Dialogue: 0,0:16:05.60,0:16:07.91,Carino,,0000,0000,0000,,môžeme byť spolu navždy.
Dialogue: 0,0:16:07.91,0:16:08.77,Carino,,0000,0000,0000,,Spolu?
Dialogue: 0,0:16:09.19,0:16:09.92,Carino,,0000,0000,0000,,Áno.
Dialogue: 0,0:16:11.30,0:16:12.82,Carino,,0000,0000,0000,,Nemôžem s tebou ostať navždy.
Dialogue: 0,0:16:13.26,0:16:13.96,Carino,,0000,0000,0000,,Čo?
Dialogue: 0,0:16:15.94,0:16:17.65,Carino,,0000,0000,0000,,V tomto pekle,
Dialogue: 0,0:16:17.65,0:16:19.44,Carino,,0000,0000,0000,,nevydržím už ani deň.
Dialogue: 0,0:16:21.78,0:16:22.40,Carino,,0000,0000,0000,,Áno.
Dialogue: 0,0:16:23.37,0:16:26.82,Carino,,0000,0000,0000,,Zajtra, zajtra ma tu necháš a pôjdeš do neba.
Dialogue: 0,0:16:28.90,0:16:34.17,Carino,,0000,0000,0000,,Liese-Lotte, prišla si sem aby si zničila celú moju nádej?
Dialogue: 0,0:16:35.87,0:16:36.61,Carino,,0000,0000,0000,,Potom...
Dialogue: 0,0:16:39.99,0:16:42.23,Carino,,0000,0000,0000,,Svojimi rukami, ja...
Dialogue: 0,0:16:43.92,0:16:44.70,Carino,,0000,0000,0000,,To je v poriadku.
Dialogue: 0,0:16:46.32,0:16:47.75,Carino,,0000,0000,0000,,Budeme spolu navždy.
Dialogue: 0,0:16:48.68,0:16:51.87,Carino,,0000,0000,0000,,Brat Alfried, nenecám ťa tu.
Dialogue: 0,0:16:52.32,0:16:55.46,Carino,,0000,0000,0000,,Liese-Lotte bude s tebou navždy.
Dialogue: 0,0:17:06.17,0:17:08.31,Carino,,0000,0000,0000,,Tak už nikdy neplač.
Dialogue: 0,0:17:38.58,0:17:39.24,Carino,,0000,0000,0000,,To je...
Dialogue: 0,0:17:42.22,0:17:43.21,Carino,,0000,0000,0000,,Čo sa stalo?
Dialogue: 0,0:17:43.91,0:17:45.14,Carino,,0000,0000,0000,,Čo to?
Dialogue: 0,0:17:52.89,0:17:53.90,Carino,,0000,0000,0000,,Liese-Lotte.
Dialogue: 0,0:17:58.62,0:17:59.61,Carino,,0000,0000,0000,,Liese-Lotte!
Dialogue: 0,0:18:04.39,0:18:05.82,Carino,,0000,0000,0000,,Liese-Lotte...
Dialogue: 0,0:18:08.76,0:18:10.92,Carino,,0000,0000,0000,,Potvrdený život vo vnútri modulu.
Dialogue: 0,0:18:11.60,0:18:13.23,Carino,,0000,0000,0000,,Mimoriadna situácia, úroveň päť.
Dialogue: 0,0:18:14.03,0:18:17.25,Carino,,0000,0000,0000,,Tento záchranný modul bude vystrelený v kryo móde.
Dialogue: 0,0:18:17.90,0:18:20.41,Carino,,0000,0000,0000,,Brat Alfried...
Dialogue: 0,0:18:27.39,0:18:29.24,Carino,,0000,0000,0000,,Liese-Lotte!
Dialogue: 0,0:18:38.78,0:18:39.92,Carino,,0000,0000,0000,,To sú,
Dialogue: 0,0:18:45.83,0:18:47.45,Carino,,0000,0000,0000,,moje schopnosti?
Dialogue: 0,0:18:55.73,0:18:56.94,Carino,,0000,0000,0000,,Nechám tento svet
Dialogue: 0,0:18:57.81,0:19:00.00,Carino,,0000,0000,0000,,zmiznúť, ktorý nikdy predtým nebol videný...
Dialogue: 0,0:19:00.50,0:19:01.48,Carino,,0000,0000,0000,,A bolesť...
Dialogue: 0,0:19:02.33,0:19:05.35,Carino,,0000,0000,0000,,A veci ktoré ste mi robili...
Dialogue: 0,0:19:06.05,0:19:07.87,Carino,,0000,0000,0000,,Všetky... Vám ich vrátim.
Dialogue: 0,0:19:22.39,0:19:24.32,Carino,,0000,0000,0000,,Hej... si v poriadku?
Dialogue: 0,0:19:26.18,0:19:27.14,Carino,,0000,0000,0000,,POKKYUUN?
Dialogue: 0,0:19:37.33,0:19:39.04,Carino,,0000,0000,0000,,Vrátili sa ti spomienky?
Dialogue: 0,0:19:40.14,0:19:41.45,Carino,,0000,0000,0000,,Liese-Lotte...
Dialogue: 0,0:19:42.57,0:19:45.89,Carino,,0000,0000,0000,,Brat Alfried...
Dialogue: 0,0:19:45.89,0:19:46.77,Carino,,0000,0000,0000,,Áno...
Dialogue: 0,0:19:47.54,0:19:49.83,Carino,,0000,0000,0000,,Teraz ti dám schopnosť triedy G.
Dialogue: 0,0:19:50.66,0:19:56.61,Carino,,0000,0000,0000,,Ako pravý partneri, naša rezonácia nám maximalizuje naše schopnosti.
Dialogue: 0,0:20:03.76,0:20:05.20,Carino,,0000,0000,0000,,Pán Geacht'er!
Dialogue: 0,0:20:10.24,0:20:11.24,Carino,,0000,0000,0000,,Q-feuille...
Dialogue: 0,0:20:12.00,0:20:13.36,Carino,,0000,0000,0000,,Čo robíš, Q-feuille?!
Dialogue: 0,0:20:13.95,0:20:16.20,Carino,,0000,0000,0000,,Ospravedlňujem sa... Moje telo sa hýbe samé...
Dialogue: 0,0:20:19.85,0:20:20.86,Carino,,0000,0000,0000,,Aká nepríjemnosť.
Dialogue: 0,0:20:21.66,0:20:24.38,Carino,,0000,0000,0000,,Zbav sa tej hovoriacej bábiky. Už ju viac nepotrebujem.
Dialogue: 0,0:20:25.58,0:20:26.33,Carino,,0000,0000,0000,,Bábiky?
Dialogue: 0,0:20:26.33,0:20:29.64,Carino,,0000,0000,0000,,On volal Q-feuille svojou malou sestričku, ale teraz...
Dialogue: 0,0:20:29.64,0:20:31.13,Carino,,0000,0000,0000,,Zbaviť sa?
Dialogue: 0,0:20:32.20,0:20:33.25,Carino,,0000,0000,0000,,Samozrejme.
Dialogue: 0,0:20:36.45,0:20:40.77,Carino,,0000,0000,0000,,Bábika, ktorá sa nedá použiť, musí byť odstránená.
Dialogue: 0,0:20:58.54,0:20:59.41,Carino,,0000,0000,0000,,Q-feuille!
Dialogue: 0,0:21:04.49,0:21:06.18,Carino,,0000,0000,0000,,Toto dokončí robotu.
Dialogue: 0,0:21:11.87,0:21:12.80,Carino,,0000,0000,0000,,Q-feuille!
Dialogue: 0,0:21:22.04,0:21:23.09,Carino,,0000,0000,0000,,Q-feuille...
Dialogue: 0,0:21:23.84,0:21:25.25,Carino,,0000,0000,0000,,Ja...
Dialogue: 0,0:21:25.60,0:21:28.97,Carino,,0000,0000,0000,,Spomeň si...
Dialogue: 0,0:21:38.28,0:21:38.94,Carino,,0000,0000,0000,,Prestaň!
Dialogue: 0,0:21:39.53,0:21:40.37,Carino,,0000,0000,0000,,Liese-Lotte!
Dialogue: 0,0:21:42.77,0:21:44.36,Carino,,0000,0000,0000,,Áno, prestaň...
Dialogue: 0,0:21:45.33,0:21:48.27,Carino,,0000,0000,0000,,Svet počúva moju vôľu.
Dialogue: 0,0:21:53.32,0:21:54.22,Carino,,0000,0000,0000,,Čo?
Dialogue: 0,0:22:01.83,0:22:04.19,Carino,,0000,0000,0000,,Ona ruší moje sily?
Dialogue: 0,0:22:04.86,0:22:08.38,Carino,,0000,0000,0000,,Ona prebudila svoje sily sama od seba?
Dialogue: 0,0:22:10.34,0:22:12.61,Carino,,0000,0000,0000,,Veľmi pekné, Liese-Lotte...!
Dialogue: 0,0:22:15.88,0:22:17.19,Carino,,0000,0000,0000,,Ascoo?
Dialogue: 0,0:22:17.72,0:22:18.62,Carino,,0000,0000,0000,,Q-feuille?
Dialogue: 0,0:22:34.96,0:22:35.95,Carino,,0000,0000,0000,,Ascoeur...
Dialogue: 0,0:22:37.40,0:22:38.28,Carino,,0000,0000,0000,,Q-feuille?
Dialogue: 0,0:22:42.77,0:22:43.91,Carino,,0000,0000,0000,,Ascoeur...
Dialogue: 0,0:22:44.40,0:22:45.49,Carino,,0000,0000,0000,,Q-feuille...
Dialogue: 0,0:22:47.99,0:22:50.99,Carino,,0000,0000,0000,,{\an8}Pokračovanie nabudúce
Dialogue: 0,0:24:20.96,0:24:23.96,Carino,,0000,0000,0000,,{\an8}V ďalšej časti
Dialogue: 0,0:24:33.09,0:24:35.26,Carino,,0000,0000,0000,,Čas, ktorý začal tiecť
Dialogue: 0,0:24:22.96,0:24:23.94,Carino,,0000,0000,0000,,Ahoj. Tu je Q-feuille.
Dialogue: 0,0:24:24.66,0:24:27.36,Carino,,0000,0000,0000,,Stratené spomienky. Zastavený čas.
Dialogue: 0,0:24:27.71,0:24:29.54,Carino,,0000,0000,0000,,Tieto veci sa začínajú hýbať.
Dialogue: 0,0:24:30.12,0:24:32.94,Carino,,0000,0000,0000,,Ďalšia časť Kiddy Girl-and epizóda 22,
Dialogue: 0,0:24:35.24,0:24:37.84,Carino,,0000,0000,0000,,Budúci týždeň, sa budem snažiť ešte viac!
Dialogue: 0,0:24:37.84,0:24:38.48,Carino,,0000,0000,0000,,Máš to?
Dialogue: 0,0:24:38.48,0:24:40.48,Carino,,0000,0000,0000,,
