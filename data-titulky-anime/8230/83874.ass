Dialogue: 0,0:23:21.23,0:23:23.05,ED japan,,0000,0000,0000,,{\k17}ma{\k13}ji {\k17}da {\k135}yo
Dialogue: 0,0:22:02.95,0:22:06.38,ED slovak,,0000,0000,0000,,Milovať dievčatá, dávaš im svoju lásku.
Dialogue: 0,0:22:06.38,0:22:08.42,ED slovak,,0000,0000,0000,,Je to preplnená mladosť života.
Dialogue: 0,0:22:08.42,0:22:11.16,ED slovak,,0000,0000,0000,,Poďme to pokorne prijať.
Dialogue: 0,0:22:11.16,0:22:13.33,ED slovak,,0000,0000,0000,,Neexistuje žiadny spôsob aby to bolo čisté.
Dialogue: 0,0:22:13.33,0:22:15.79,ED slovak,,0000,0000,0000,,Učiteľ prišiel a vynadal nám.
Dialogue: 0,0:22:15.79,0:22:18.16,ED slovak,,0000,0000,0000,,Za to, že sme chceli získať všetky miesta.
Dialogue: 0,0:22:18.16,0:22:20.94,ED slovak,,0000,0000,0000,,Je to vzrušenie, nech už to je ako chce.
Dialogue: 0,0:22:20.94,0:22:23.95,ED slovak,,0000,0000,0000,,Je to až príliš vážne!
Dialogue: 0,0:22:23.95,0:22:26.49,ED slovak,,0000,0000,0000,,Tí, ktorý sú príliš vážny
Dialogue: 0,0:22:26.49,0:22:29.01,ED slovak,,0000,0000,0000,,a budú len zraňovať.
Dialogue: 0,0:22:29.01,0:22:33.34,ED slovak,,0000,0000,0000,,Bola som nehanebne zmätená.
Dialogue: 0,0:22:33.34,0:22:38.17,ED slovak,,0000,0000,0000,,Ja sa len chcem, s tebou učiť.
Dialogue: 0,0:22:38.17,0:22:41.09,ED slovak,,0000,0000,0000,,Stupeň, z ktorého sme pozerali, ja tomu chcem veriť,
Dialogue: 0,0:22:41.09,0:22:43.46,ED slovak,,0000,0000,0000,,že je to ako to, čo som si predstavovala,
Dialogue: 0,0:22:43.46,0:22:46.01,ED slovak,,0000,0000,0000,,prosím, buď vážny.
Dialogue: 0,0:22:46.01,0:22:51.05,ED slovak,,0000,0000,0000,,Zrážka vášni, ktorou ťa obdivujem.
Dialogue: 0,0:22:51.05,0:22:53.84,ED slovak,,0000,0000,0000,,Čakal som na túto úlohu!
Dialogue: 0,0:22:53.84,0:22:57.60,ED slovak,,0000,0000,0000,,Ukáž mi ducha! Moje pocity
Dialogue: 0,0:22:57.60,0:23:01.16,ED slovak,,0000,0000,0000,,radosti a smútku sa stretli s tebou.
Dialogue: 0,0:23:01.16,0:23:05.09,ED slovak,,0000,0000,0000,,Poďme sa porozprávať.
Dialogue: 0,0:23:05.09,0:23:07.61,ED slovak,,0000,0000,0000,,Vidím víťazstvo,
Dialogue: 0,0:23:07.61,0:23:11.22,ED slovak,,0000,0000,0000,,a preto roním slzy.
Dialogue: 0,0:23:11.22,0:23:15.64,ED slovak,,0000,0000,0000,,Ja tu budem pre teba vždy.
Dialogue: 0,0:23:14.14,0:23:16.40,ED slovak,,0000,0000,0000,,Majikoi pohyb lásky
Dialogue: 0,0:23:16.40,0:23:19.03,ED slovak,,0000,0000,0000,,Majikoi pocit
Dialogue: 0,0:23:19.03,0:23:21.23,ED slovak,,0000,0000,0000,,Majikoi revolúcia.
Dialogue: 0,0:23:21.23,0:23:23.05,ED slovak,,0000,0000,0000,,Je to skutočné!
