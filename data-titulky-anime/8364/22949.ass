Dialogue: 0,0:00:24.16,0:00:28.16,JAP,,0000,0000,0000,,heijoushin ga daiji desu (panpanpanpan)
Dialogue: 0,0:00:28.16,0:00:32.08,JAP,,0000,0000,0000,,hayaru kokoro wo osaete (monmonmonmon)
Dialogue: 0,0:00:32.08,0:00:36.04,JAP,,0000,0000,0000,,joujikijin ni narimashou (panpanpanpan)
Dialogue: 0,0:00:36.04,0:00:39.42,JAP,,0000,0000,0000,,yabai hito nara sugu nareru
Dialogue: 0,0:00:39.42,0:00:47.38,JAP,,0000,0000,0000,,aoi futari no hebun koinu ni naru yume wa (pea)
Dialogue: 0,0:00:47.38,0:00:54.72,JAP,,0000,0000,0000,,yukai tokumori no nou wo toreesu shite
Dialogue: 0,0:00:54.35,0:00:57.28,JAP,,0000,0000,0000,,(sora wo) kakemawaritai
Dialogue: 0,0:00:57.28,0:01:01.36,JAP,,0000,0000,0000,,yureru omoi no shita no kaniku ni wa
Dialogue: 0,0:01:01.36,0:01:05.05,JAP,,0000,0000,0000,,koi no oishisa datte fukuranderu
Dialogue: 0,0:01:05.05,0:01:09.52,JAP,,0000,0000,0000,,sugoi menyuu wo tsukuru kouryakuhou nara
Dialogue: 0,0:01:09.52,0:01:13.66,JAP,,0000,0000,0000,,mattou ni furuutsu hagukumimashou
