Dialogue: 0,0:00:23.91,0:00:27.91,JAP,,0000,0000,0000,,heijoushin ga daiji desu (panpanpanpan)
Dialogue: 0,0:00:27.91,0:00:31.83,JAP,,0000,0000,0000,,hayaru kokoro wo osaete (monmonmonmon)
Dialogue: 0,0:00:31.83,0:00:35.79,JAP,,0000,0000,0000,,joujikijin ni narimashou (panpanpanpan)
Dialogue: 0,0:00:35.79,0:00:39.17,JAP,,0000,0000,0000,,yabai hito nara sugu nareru
Dialogue: 0,0:00:39.17,0:00:47.13,JAP,,0000,0000,0000,,aoi futari no hebun koinu ni naru yume wa (pea)
Dialogue: 0,0:00:47.13,0:00:54.47,JAP,,0000,0000,0000,,yukai tokumori no nou wo toreesu shite
Dialogue: 0,0:00:54.10,0:00:57.03,JAP,,0000,0000,0000,,(sora wo) kakemawaritai
Dialogue: 0,0:00:57.03,0:01:01.11,JAP,,0000,0000,0000,,yureru omoi no shita no kaniku ni wa
Dialogue: 0,0:01:01.11,0:01:04.80,JAP,,0000,0000,0000,,koi no oishisa datte fukuranderu
Dialogue: 0,0:01:04.80,0:01:09.27,JAP,,0000,0000,0000,,sugoi menyuu wo tsukuru kouryakuhou nara
Dialogue: 0,0:01:09.27,0:01:13.41,JAP,,0000,0000,0000,,mattou ni furuutsu hagukumimashou
Dialogue: 0,0:01:13.41,0:01:15.46,JAP,,0000,0000,0000,,dakishimetainda (panpanpan)
Dialogue: 0,0:01:15.46,0:01:17.36,JAP,,0000,0000,0000,,kamishimetainda (waowao)
