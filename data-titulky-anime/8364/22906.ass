Dialogue: 0,0:00:59.61,0:01:03.61,JAP,,0000,0000,0000,,heijoushin ga daiji desu (panpanpanpan)
Dialogue: 0,0:01:03.61,0:01:07.53,JAP,,0000,0000,0000,,hayaru kokoro wo osaete (monmonmonmon)
Dialogue: 0,0:01:07.53,0:01:11.49,JAP,,0000,0000,0000,,joujikijin ni narimashou (panpanpanpan)
Dialogue: 0,0:01:11.49,0:01:14.87,JAP,,0000,0000,0000,,yabai hito nara sugu nareru
Dialogue: 0,0:01:14.87,0:01:22.83,JAP,,0000,0000,0000,,aoi futari no hebun koinu ni naru yume wa (pea)
Dialogue: 0,0:01:22.83,0:01:30.17,JAP,,0000,0000,0000,,yukai tokumori no nou wo toreesu shite
Dialogue: 0,0:01:29.80,0:01:32.73,JAP,,0000,0000,0000,,(sora wo) kakemawaritai
Dialogue: 0,0:01:32.73,0:01:36.81,JAP,,0000,0000,0000,,yureru omoi no shita no kaniku ni wa
Dialogue: 0,0:01:36.81,0:01:40.50,JAP,,0000,0000,0000,,koi no oishisa datte fukuranderu
Dialogue: 0,0:01:40.50,0:01:44.97,JAP,,0000,0000,0000,,sugoi menyuu wo tsukuru kouryakuhou nara
Dialogue: 0,0:01:44.97,0:01:49.11,JAP,,0000,0000,0000,,mattou ni furuutsu hagukumimashou
Dialogue: 0,0:01:49.11,0:01:51.16,JAP,,0000,0000,0000,,dakishimetainda (panpanpan)
Dialogue: 0,0:01:51.16,0:01:53.06,JAP,,0000,0000,0000,,kamishimetainda (waowao)
Dialogue: 0,0:01:53.06,0:01:57.03,JAP,,0000,0000,0000,,odokashitainda ima wa gaman shiyou (panpanpan)
Dialogue: 0,0:01:57.03,0:02:03.42,JAP,,0000,0000,0000,,shugyou wo shiyou (panpanpan) zettai shiyou Coolish Walk
Dialogue: 0,0:00:35.79,0:00:37.79,CZ,,0000,0000,0000,,Chci tě stlačit
Dialogue: 0,0:00:37.79,0:00:39.79,CZ,,0000,0000,0000,,Chci ponořit svoje zuby do tebe
Dialogue: 0,0:00:39.79,0:00:45.84,CZ,,0000,0000,0000,,Chci tě překvapit, ale dnes se držím dále, Drsnou chůzí.
Dialogue: 0,0:00:43.95,0:00:45.84,CZ,,0000,0000,0000,,panpanpan pan pan
