Dialogue: 0,0:01:18.62,0:01:21.08,SK Opening,,0000,0000,0000,,priprav sa na odlet
Dialogue: 0,0:01:21.08,0:01:23.50,SK Opening,,0000,0000,0000,,otvor svoju myseľ
Dialogue: 0,0:01:23.50,0:01:27.17,SK Opening,,0000,0000,0000,,dokiaľ tvoj spád nezhrdzavie
Dialogue: 0,0:01:28.63,0:01:31.25,SK Opening,,0000,0000,0000,,drsný ako filmová hviezda
Dialogue: 0,0:01:31.25,0:01:33.71,SK Opening,,0000,0000,0000,,nádherná ako roková hviezda
Dialogue: 0,0:01:33.71,0:01:36.09,SK Opening,,0000,0000,0000,,ohromí na každej súťaži
Dialogue: 0,0:01:36.09,0:01:38.84,SK Opening,,0000,0000,0000,,sme moderný kovboji
Dialogue: 0,0:01:38.84,0:01:41.35,SK Opening,,0000,0000,0000,,si pripravený na akaciu?
Dialogue: 0,0:01:41.35,0:01:43.93,SK Opening,,0000,0000,0000,,pred tebou nestojí žiadna stena
Dialogue: 0,0:01:43.93,0:01:46.39,SK Opening,,0000,0000,0000,,máš nejakú úlohu?
Dialogue: 0,0:01:46.39,0:01:49.23,SK Opening,,0000,0000,0000,,prikazujem ti, späť do ohňa!
Dialogue: 0,0:01:49.23,0:01:53.11,SK Opening,,0000,0000,0000,,je to neopísateľné
Dialogue: 0,0:01:53.11,0:01:55.99,SK Opening,,0000,0000,0000,,šialenstvo
Dialogue: 0,0:01:55.99,0:01:57.61,SK Opening,,0000,0000,0000,,svojej vôli nemôžem vzdorovať
Dialogue: 0,0:01:59.41,0:02:03.20,SK Opening,,0000,0000,0000,,na afekte nezáleží
Dialogue: 0,0:02:03.20,0:02:08.25,SK Opening,,0000,0000,0000,,prími svoje telo zo tristošesťdesiat stupňov
Dialogue: 0,0:02:08.25,0:02:13.29,SK Opening,,0000,0000,0000,,ži teraz kovboj
Dialogue: 0,0:02:13.30,0:02:18.83,SK Opening,,0000,0000,0000,,akokeby zajtra nemali vyklíčiť kvety
Dialogue: 0,0:02:19.59,0:02:23.39,SK Opening,,0000,0000,0000,,ži kovboj
Dialogue: 0,0:02:23.39,0:02:28.73,SK Opening,,0000,0000,0000,,teraz bež dopredu kovboj
Dialogue: 0,0:23:45.65,0:23:47.65,Default,,0000,0000,0000,,
Dialogue: 0,0:23:47.65,0:23:49.65,Default,,0000,0000,0000,,
Dialogue: 0,0:21:55.23,0:21:57.23,Sk Ending,,0000,0000,0000,,spoľahni sa na dievčenský štýl
Dialogue: 0,0:21:57.23,0:21:58.77,Sk Ending,,0000,0000,0000,,je to silnejšie, však?
Dialogue: 0,0:21:58.77,0:22:00.11,Sk Ending,,0000,0000,0000,,len si to priznaj
Dialogue: 0,0:22:00.11,0:22:02.23,Sk Ending,,0000,0000,0000,,chlapci len podvádzajú
Dialogue: 0,0:22:02.23,0:22:06.24,Sk Ending,,0000,0000,0000,,tak teda do toho, vhodnejšý je bozk
Dialogue: 0,0:22:14.75,0:22:16.75,Sk Ending,,0000,0000,0000,,čo chceš robiť celé dni?
Dialogue: 0,0:22:16.75,0:22:17.91,Sk Ending,,0000,0000,0000,,správne tempo
Dialogue: 0,0:22:17.92,0:22:20.42,Sk Ending,,0000,0000,0000,,s tým tempom, len poď
Dialogue: 0,0:22:20.42,0:22:23.00,Sk Ending,,0000,0000,0000,,stávaš sa prázdnou
Dialogue: 0,0:22:23.00,0:22:25.76,Sk Ending,,0000,0000,0000,,tak povedz svojim skazeným pocitom čav čav čav
Dialogue: 0,0:22:25.76,0:22:26.88,Sk Ending,,0000,0000,0000,,si v poriadku?
Dialogue: 0,0:22:26.88,0:22:29.13,Sk Ending,,0000,0000,0000,,všetci po tebe idú
Dialogue: 0,0:22:29.14,0:22:30.68,Sk Ending,,0000,0000,0000,,šelmi na love
Dialogue: 0,0:22:30.68,0:22:32.93,Sk Ending,,0000,0000,0000,,divne sa maskujú
Dialogue: 0,0:22:32.93,0:22:35.06,Sk Ending,,0000,0000,0000,,odrovnaj ich jednou ranou
Dialogue: 0,0:22:35.06,0:22:39.56,Sk Ending,,0000,0000,0000,,zvyčajne ste stále spolu
Dialogue: 0,0:22:39.56,0:22:45.65,Sk Ending,,0000,0000,0000,,hlúpy žart, a tvoj ďalší zmätený výraz
Dialogue: 0,0:22:45.65,0:22:53.37,Sk Ending,,0000,0000,0000,,a ak bol nudný budeš za zlú
Dialogue: 0,0:22:53.37,0:22:57.66,Sk Ending,,0000,0000,0000,,keď zrazu sme vleteli do provokatívnej zóny
Dialogue: 0,0:22:57.66,0:23:02.67,Sk Ending,,0000,0000,0000,,už žiadne problémi, už vôbec nie
Dialogue: 0,0:23:02.67,0:23:04.50,Sk Ending,,0000,0000,0000,,skáčeme si voľne
Dialogue: 0,0:23:04.50,0:23:07.80,Sk Ending,,0000,0000,0000,,spolu tvoríme zázrak
Dialogue: 0,0:23:07.80,0:23:10.30,Sk Ending,,0000,0000,0000,,nemusíte s nami súhlasiť
Dialogue: 0,0:23:10.30,0:23:13.43,Sk Ending,,0000,0000,0000,,ale nám je to fuk
Dialogue: 0,0:23:13.43,0:23:15.47,Sk Ending,,0000,0000,0000,,spoľahni sa na dievčenský štýl
Dialogue: 0,0:23:15.47,0:23:17.14,Sk Ending,,0000,0000,0000,,sme silnejšie
Dialogue: 0,0:23:17.14,0:23:18.56,Sk Ending,,0000,0000,0000,,len si to priznaj
Dialogue: 0,0:23:18.56,0:23:20.60,Sk Ending,,0000,0000,0000,,chlapci len podvádzajú
Dialogue: 0,0:23:20.60,0:23:22.60,Sk Ending,,0000,0000,0000,,tak teda do toho
Dialogue: 0,0:23:22.60,0:23:24.60,Sk Ending,,0000,0000,0000,,vhodnejšý je bozk
Dialogue: 0,0:21:57.40,0:21:58.20,KopieSk Ending,,0000,0000,0000,,áno
