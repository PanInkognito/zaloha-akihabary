Dialogue: 0,0:00:00.82,0:00:04.10,Default,,0000,0000,0000,,Niekoľko desaťročí po Tretej Svetovej Vojne
Dialogue: 0,0:00:04.10,0:00:11.34,Default,,0000,0000,0000,,bol bombardovaný úsek Tokya oddelený od zvyšku mesta. Teraz sa nazýva Čierny Bod.
Dialogue: 0,0:00:12.94,0:00:19.12,Default,,0000,0000,0000,,Jedného dňa, si tu farmaceutická spoločnosť z mesta, Simeon, založila základňu.
Dialogue: 0,0:00:19.48,0:00:28.66,Default,,0000,0000,0000,,Vodca Simeonu, Adam Arclight a jeho Štvorka sa rozhodli prevziať nadvládu nad Čiernym Bodom a zničiť všetok odpor.
Dialogue: 0,0:00:29.59,0:00:30.50,Default,,0000,0000,0000,,Lenže.
Dialogue: 0,0:00:30.50,0:00:33.85,Default,,0000,0000,0000,,Boli tu aj tí, čo sa postavili na odpor.
Dialogue: 0,0:00:34.51,0:00:37.85,Default,,0000,0000,0000,,Záhadný kňaz, Adam Blade.
Dialogue: 0,0:00:38.98,0:00:41.11,Default,,0000,0000,0000,,Eve Neuschwanstein.
Dialogue: 0,0:00:41.46,0:00:43.66,Default,,0000,0000,0000,,A tí ostatní.
Dialogue: 0,0:00:47.98,0:00:53.44,Default,,0000,0000,0000,,Práve bojujú proti Simeonu vo vnútri ich budovy.
Dialogue: 0,0:02:30.00,0:02:35.00,EPtitle,,0000,0000,0000,,{\blur15\fs70\be1\4c&H000000&\pos(654,504)\3c&H165A05&}Odpor
Dialogue: 0,0:02:36.24,0:02:37.18,Default,,0000,0000,0000,,Umri,
Dialogue: 0,0:02:38.02,0:02:38.99,Default,,0000,0000,0000,,Cruz!
Dialogue: 0,0:02:40.48,0:02:41.88,Default,,0000,0000,0000,,Prečo?
Dialogue: 0,0:02:42.85,0:02:44.38,Default,,0000,0000,0000,,Sestra.
Dialogue: 0,0:02:45.67,0:02:48.00,Default,,0000,0000,0000,,K-Kňaz-sama?
Dialogue: 0,0:02:49.09,0:02:52.59,Default,,0000,0000,0000,,Ten, kto vtedy zachránil jeho život som ja.
Dialogue: 0,0:02:52.59,0:02:54.70,Default,,0000,0000,0000,,Ak ho chceš zabiť, potrebuješ moje povolenie.
Dialogue: 0,0:02:56.05,0:02:56.92,Default,,0000,0000,0000,,Dobre.
Dialogue: 0,0:03:04.73,0:03:05.95,Default,,0000,0000,0000,,Prepáč.
Dialogue: 0,0:03:06.22,0:03:08.71,Default,,0000,0000,0000,,Stále sme ti len na príťaž...
Dialogue: 0,0:03:10.02,0:03:11.20,Default,,0000,0000,0000,,To je v poriadku.
Dialogue: 0,0:03:11.64,0:03:12.72,Default,,0000,0000,0000,,Odpočívajte!
Dialogue: 0,0:03:14.58,0:03:15.97,Default,,0000,0000,0000,,Blade.
Dialogue: 0,0:03:15.97,0:03:19.97,Default,,0000,0000,0000,,Od začiatku som vedel, že to prehráte.
Dialogue: 0,0:03:20.37,0:03:22.98,Default,,0000,0000,0000,,Zabijem ťa ty parchant!
Dialogue: 0,0:03:28.58,0:03:30.65,Default,,0000,0000,0000,,Postaraj sa o tamtoho, Eva!
Dialogue: 0,0:03:39.85,0:03:41.05,Default,,0000,0000,0000,,Sestra.
Dialogue: 0,0:03:45.29,0:03:46.43,Default,,0000,0000,0000,,Sestra.
Dialogue: 0,0:03:48.36,0:03:49.63,Default,,0000,0000,0000,,Cruz-kun!
Dialogue: 0,0:03:53.43,0:03:54.31,Default,,0000,0000,0000,,To je zlé.
Dialogue: 0,0:03:54.31,0:03:55.78,Default,,0000,0000,0000,,Je blízko pri srdci.
Dialogue: 0,0:03:55.78,0:03:57.09,Default,,0000,0000,0000,,Lekárska pomoc, otvoriť.
Dialogue: 0,0:03:57.46,0:03:59.02,Default,,0000,0000,0000,,Cruz-kun, vydrž!
Dialogue: 0,0:03:59.02,0:04:00.97,Default,,0000,0000,0000,,Musíš to vydržať, Cruz-kun!
Dialogue: 0,0:04:01.25,0:04:02.45,Default,,0000,0000,0000,,Zima...
Dialogue: 0,0:04:02.71,0:04:03.98,Default,,0000,0000,0000,,Chlad...
Dialogue: 0,0:04:04.41,0:04:05.39,Default,,0000,0000,0000,,Počuješ ma?
Dialogue: 0,0:04:05.39,0:04:06.10,Default,,0000,0000,0000,,Cruz-kun!
Dialogue: 0,0:04:06.88,0:04:08.07,Default,,0000,0000,0000,,Sestra...
Dialogue: 0,0:04:08.59,0:04:10.82,Default,,0000,0000,0000,,Prečo si tu so Simeonom?
Dialogue: 0,0:04:12.13,0:04:14.86,Default,,0000,0000,0000,,Vtedy sme predsa... boli...
Dialogue: 0,0:04:14.86,0:04:16.28,Default,,0000,0000,0000,,Cruz-kun!
Dialogue: 0,0:04:17.28,0:04:18.36,Default,,0000,0000,0000,,Sestra...
Dialogue: 0,0:04:19.54,0:04:22.23,Default,,0000,0000,0000,,Cruz-kun! Cruz-kun!
Dialogue: 0,0:04:35.53,0:04:36.55,Default,,0000,0000,0000,,Toto je...
Dialogue: 0,0:04:38.75,0:04:40.68,Default,,0000,0000,0000,,Kapitán Zakard sa vrátil.
Dialogue: 0,0:04:42.37,0:04:43.46,Default,,0000,0000,0000,,To je...
Dialogue: 0,0:04:50.06,0:04:51.39,Default,,0000,0000,0000,,Sestra.
Dialogue: 0,0:04:52.57,0:04:57.32,Default,,0000,0000,0000,,Kapitán, čo keby sme si na oslavu porazenia Testamentov vypili?
Dialogue: 0,0:04:59.45,0:05:01.83,Default,,0000,0000,0000,,Ale len pokiaľ nepoletíme cez palubu.
Dialogue: 0,0:05:06.33,0:05:07.28,Default,,0000,0000,0000,,To som...
Dialogue: 0,0:05:07.73,0:05:09.92,Default,,0000,0000,0000,,ja ešte ako člen odporu.
Dialogue: 0,0:05:10.35,0:05:13.42,Default,,0000,0000,0000,,Vytajte doma, Kapitán Zakard, Sestra!
Dialogue: 0,0:05:14.51,0:05:15.42,Default,,0000,0000,0000,,Cruz.
Dialogue: 0,0:05:16.14,0:05:17.82,Default,,0000,0000,0000,,Všetci, skvelá práca.
Dialogue: 0,0:05:17.82,0:05:19.88,Default,,0000,0000,0000,,Postarám sa o údržbu zbraní.
Dialogue: 0,0:05:20.40,0:05:21.58,Default,,0000,0000,0000,,Vďaka ako vždy.
Dialogue: 0,0:05:23.26,0:05:25.97,Default,,0000,0000,0000,,Hej, už nám dochádza chľast.
Dialogue: 0,0:05:26.35,0:05:27.74,Default,,0000,0000,0000,,Pôjdem kúpiť.
Dialogue: 0,0:05:27.74,0:05:29.05,Default,,0000,0000,0000,,Prepáč za tie starosti.
Dialogue: 0,0:05:29.05,0:05:29.85,Default,,0000,0000,0000,,V poriadku.
Dialogue: 0,0:05:33.44,0:05:34.33,Default,,0000,0000,0000,,Cruz.
Dialogue: 0,0:05:35.50,0:05:36.69,Default,,0000,0000,0000,,Sestra.
Dialogue: 0,0:05:40.33,0:05:45.79,Default,,0000,0000,0000,,Vtedy som sem jednostaj chodil, aj keď ma sestra veľakrát vyhodila.
Dialogue: 0,0:05:46.20,0:05:47.96,Default,,0000,0000,0000,,Povedala som ti, že sem nemáš chodiť.
Dialogue: 0,0:05:47.96,0:05:52.51,Default,,0000,0000,0000,,Toto je armáda bojujúca za oslobodenie Čierneho Bodu od Simeonu.
Dialogue: 0,0:05:53.18,0:05:56.55,Default,,0000,0000,0000,,Nie je to miesto nahranie pre deti.
Dialogue: 0,0:05:57.62,0:06:01.80,Default,,0000,0000,0000,,Ale, keď tu ty a všetci bojujete, nemôžem len tak sedieť.
Dialogue: 0,0:06:02.77,0:06:03.73,Default,,0000,0000,0000,,Cruz?
Dialogue: 0,0:06:04.05,0:06:08.65,Default,,0000,0000,0000,,Raz budem silný ako kapitán a porazím tých zlosynov zo Simeonu.
Dialogue: 0,0:06:09.09,0:06:11.98,Default,,0000,0000,0000,,Ja ťa ochránim.
Dialogue: 0,0:06:12.55,0:06:13.68,Default,,0000,0000,0000,,Ty?
Dialogue: 0,0:06:19.11,0:06:22.21,Default,,0000,0000,0000,,Asi je to nemožné, však?
Dialogue: 0,0:06:22.57,0:06:24.81,Default,,0000,0000,0000,,Nemám na viac ako na toto...
Dialogue: 0,0:06:24.81,0:06:25.99,Default,,0000,0000,0000,,Na tom nezáleží.
Dialogue: 0,0:06:26.78,0:06:29.41,Default,,0000,0000,0000,,Len rob to, čo v tejto chvíli môžeš.
Dialogue: 0,0:06:30.17,0:06:32.01,Default,,0000,0000,0000,,Ja ťa ochránim.
Dialogue: 0,0:06:32.48,0:06:34.55,Default,,0000,0000,0000,,Tak, poďme!
Dialogue: 0,0:06:35.57,0:06:36.69,Default,,0000,0000,0000,,Sestra.
Dialogue: 0,0:06:57.77,0:07:00.01,Default,,0000,0000,0000,,Cruz! Ty si triasol tým pivom?
Dialogue: 0,0:07:00.01,0:07:01.66,Default,,0000,0000,0000,,Ee? Nie, ja som...
Dialogue: 0,0:07:03.34,0:07:04.07,Default,,0000,0000,0000,,Prepáč.
Dialogue: 0,0:07:04.39,0:07:05.78,Default,,0000,0000,0000,,Tyyy...
Dialogue: 0,0:07:06.65,0:07:08.33,Default,,0000,0000,0000,,Super, poďme, poďme!
Dialogue: 0,0:07:08.33,0:07:09.84,Default,,0000,0000,0000,,Päť na Bakua.
Dialogue: 0,0:07:09.84,0:07:12.00,Default,,0000,0000,0000,,Vy, prestaňte stávkovať!
Dialogue: 0,0:07:17.74,0:07:18.91,Default,,0000,0000,0000,,Ospravedlňte ma!
Dialogue: 0,0:07:21.06,0:07:24.47,Default,,0000,0000,0000,,Kapitán Zakard, dostal som sa k neuveriteľným informáciám.
Dialogue: 0,0:07:28.40,0:07:29.46,Default,,0000,0000,0000,,Čože?
Dialogue: 0,0:07:29.90,0:07:31.75,Default,,0000,0000,0000,,Všetkých zhromaždi!
Dialogue: 0,0:07:33.31,0:07:41.03,Default,,0000,0000,0000,,Obdržali sme informácie, že veliteľ Simeonu, Adam Arclight, bude onedlho sám opúšťať Čierny Bod.
Dialogue: 0,0:07:44.20,0:07:46.61,Default,,0000,0000,0000,,Ten všivák si neberie žiadnu ochranku?
Dialogue: 0,0:07:47.00,0:07:49.12,Default,,0000,0000,0000,,Ani Štvorku?
Dialogue: 0,0:07:49.47,0:07:50.21,Default,,0000,0000,0000,,Hej.
Dialogue: 0,0:07:50.56,0:07:54.64,Default,,0000,0000,0000,,Doteraz sme sa cez jeho ochranku nedokázali dostať, ale...
Dialogue: 0,0:07:54.97,0:07:58.00,Default,,0000,0000,0000,,teraz máme konečne šancu dostať ho.
Dialogue: 0,0:07:58.57,0:08:00.67,Default,,0000,0000,0000,,Boží zásah, však?
Dialogue: 0,0:08:01.02,0:08:06.13,Default,,0000,0000,0000,,Je to priamo z brány šesťsto šesťdesiatšesť k tej základni, ktorú si Simeon stavia.
Dialogue: 0,0:08:06.49,0:08:09.64,Default,,0000,0000,0000,,Odtiaľto zaútočí moja jednotka.
Dialogue: 0,0:08:10.44,0:08:13.52,Default,,0000,0000,0000,,Aruka, tvoja jednotka nás podporí zozadu.
Dialogue: 0,0:08:14.32,0:08:16.52,Default,,0000,0000,0000,,Myslíte dvojitú formáciu, však?
Dialogue: 0,0:08:16.81,0:08:18.04,Default,,0000,0000,0000,,Správne.
Dialogue: 0,0:08:18.04,0:08:24.53,Default,,0000,0000,0000,,Ak sa mu aj tak podarí utiecť, ďalšie dve skupiny budú na oboch stranách cesty.
Dialogue: 0,0:08:24.84,0:08:28.53,Default,,0000,0000,0000,,Tá jednotka by ich mala oslabiť, ale nemáme na to ľudí.
Dialogue: 0,0:08:28.82,0:08:30.38,Default,,0000,0000,0000,,Najmite žoldákov!
Dialogue: 0,0:08:30.38,0:08:32.69,Default,,0000,0000,0000,,Nezáleží na tom, koľko si vypýtajú.
Dialogue: 0,0:08:33.01,0:08:35.48,Default,,0000,0000,0000,,Ak ho teraz necháme ujsť, ďalšiu šancu nedostaneme.
Dialogue: 0,0:08:35.92,0:08:36.62,Default,,0000,0000,0000,,Koniec.
Dialogue: 0,0:08:36.62,0:08:37.54,Default,,0000,0000,0000,,Pripravte sa!
Dialogue: 0,0:08:37.97,0:08:39.04,Default,,0000,0000,0000,,Rozkaz!
Dialogue: 0,0:08:48.41,0:08:49.45,Default,,0000,0000,0000,,Sestra.
Dialogue: 0,0:08:50.61,0:08:52.85,Default,,0000,0000,0000,,Zaútočíme na Arclighta.
Dialogue: 0,0:08:54.86,0:08:58.67,Default,,0000,0000,0000,,Je to obrovská šanca, ale aj tak sa asi nevyhneme stratám.
Dialogue: 0,0:08:59.27,0:09:00.77,Default,,0000,0000,0000,,Ty ísť nemôžeš.
Dialogue: 0,0:09:01.53,0:09:02.25,Default,,0000,0000,0000,,Dobre?
Dialogue: 0,0:09:03.19,0:09:04.26,Default,,0000,0000,0000,,Sestra.
Dialogue: 0,0:09:16.20,0:09:17.16,Default,,0000,0000,0000,,Vôjdi!
Dialogue: 0,0:09:17.16,0:09:19.09,Default,,0000,0000,0000,,O-Ospravedlňte ma!
Dialogue: 0,0:09:19.61,0:09:20.63,Default,,0000,0000,0000,,Ehm...
Dialogue: 0,0:09:20.63,0:09:22.34,Default,,0000,0000,0000,,Volali ste ma?
Dialogue: 0,0:09:23.03,0:09:24.03,Default,,0000,0000,0000,,Hej.
Dialogue: 0,0:09:26.46,0:09:27.13,Default,,0000,0000,0000,,Kvôli tomuto.
Dialogue: 0,0:09:30.93,0:09:32.11,Default,,0000,0000,0000,,Je to od Aruky.
Dialogue: 0,0:09:32.45,0:09:33.64,Default,,0000,0000,0000,,Od sestry?
Dialogue: 0,0:09:34.30,0:09:37.60,Default,,0000,0000,0000,,"Ak sa mi v nadchádzajúcom boji niečo stane,
Dialogue: 0,0:09:38.43,0:09:42.02,Default,,0000,0000,0000,,chráň to ako to najcennejšie, akoby si chránil mňa"!
Dialogue: 0,0:09:42.42,0:09:43.55,Default,,0000,0000,0000,,To predsa...
Dialogue: 0,0:09:48.26,0:09:49.95,Default,,0000,0000,0000,,Ak sa jej niečo stane...?
Dialogue: 0,0:10:04.66,0:10:08.36,Default,,0000,0000,0000,,Tu a teraz zahájime náš plný útok na Arclighta.
Dialogue: 0,0:10:08.62,0:10:09.84,Default,,0000,0000,0000,,Ste pripravení?
Dialogue: 0,0:10:14.56,0:10:17.34,Default,,0000,0000,0000,,Načo tá vážna tvár?
Dialogue: 0,0:10:17.34,0:10:21.64,Default,,0000,0000,0000,,Máme tu štyristo ľudí a pre každého zbraň.
Dialogue: 0,0:10:22.08,0:10:23.65,Default,,0000,0000,0000,,Bude to hračka.
Dialogue: 0,0:10:24.09,0:10:28.06,Default,,0000,0000,0000,,N-Nepotrebných nesmiete podceňovať!
Dialogue: 0,0:10:32.12,0:10:35.66,Default,,0000,0000,0000,,Nepotrební sú koniec koncov iba ľudia.
Dialogue: 0,0:10:35.66,0:10:37.95,Default,,0000,0000,0000,,Chceš mi povedať, žeby im toto neublížilo?
Dialogue: 0,0:10:40.88,0:10:41.79,Default,,0000,0000,0000,,A-Ale...
Dialogue: 0,0:10:42.31,0:10:44.96,Default,,0000,0000,0000,,Tak tedsa z tou zbraňou skús zastreliť mňa!
Dialogue: 0,0:10:45.29,0:10:46.34,Default,,0000,0000,0000,,Čo si vravela?
Dialogue: 0,0:10:46.68,0:10:47.63,Default,,0000,0000,0000,,Čo je?
Dialogue: 0,0:10:47.99,0:10:49.20,Default,,0000,0000,0000,,Nezvládneš to?
Dialogue: 0,0:10:49.53,0:10:51.47,Default,,0000,0000,0000,,Je mi fuk, že si žoldnier.
Dialogue: 0,0:10:51.47,0:10:56.22,Default,,0000,0000,0000,,Je to proste hamba, že sa takí šmejdi ako ty vyhlasujú za našich spojencov.
Dialogue: 0,0:10:57.98,0:10:59.39,Default,,0000,0000,0000,,Ty mrcha!
Dialogue: 0,0:10:59.85,0:11:00.93,Default,,0000,0000,0000,,Zadržte, vy dvaja!
Dialogue: 0,0:11:11.07,0:11:12.99,Default,,0000,0000,0000,,T-To je...
Dialogue: 0,0:11:14.42,0:11:17.51,Default,,0000,0000,0000,,Ty snáď nie si Nepotrebná?
Dialogue: 0,0:11:17.51,0:11:22.75,Default,,0000,0000,0000,,Dokáže vytvoriť teplotu vysokú aj na tavenie kovu. To je jej schopnosť.
Dialogue: 0,0:11:23.22,0:11:26.25,Default,,0000,0000,0000,,A toto nie je ani náznak Arclightovej sily.
Dialogue: 0,0:11:31.62,0:11:33.27,Default,,0000,0000,0000,,Prečo si tu?
Dialogue: 0,0:11:33.63,0:11:38.30,Default,,0000,0000,0000,,Ch-Chcel som ti nejako byť k úžitku, preto...
Dialogue: 0,0:11:40.11,0:11:41.81,Default,,0000,0000,0000,,Vravela som, že nemáš chodiť.
Dialogue: 0,0:11:48.87,0:11:50.82,Default,,0000,0000,0000,,Ani sa odo mňa nepohni.
Dialogue: 0,0:11:53.11,0:11:54.95,Default,,0000,0000,0000,,Všetci, ideme!
Dialogue: 0,0:11:55.51,0:12:03.58,Default,,0000,0000,0000,,My z Odporu zlikvidujeme toho, ktorý terorizoval občanov lovom na Nepotrebných, vodcu Simeonu, Adama Arclighta...
Dialogue: 0,0:12:03.75,0:12:06.21,Default,,0000,0000,0000,,a oslobodíme Čierny Bod.
Dialogue: 0,0:12:45.40,0:12:46.49,Default,,0000,0000,0000,,Je tu.
Dialogue: 0,0:12:46.49,0:12:48.69,Default,,0000,0000,0000,,Ako sme čakali, nemá žiadnu ochranku.
Dialogue: 0,0:12:50.56,0:12:52.08,Default,,0000,0000,0000,,Páľ!
Dialogue: 0,0:12:57.86,0:12:59.19,Default,,0000,0000,0000,,Dostali sme ho?
Dialogue: 0,0:13:09.74,0:13:10.92,Default,,0000,0000,0000,,Ani škrabanec.
Dialogue: 0,0:13:10.92,0:13:11.91,Default,,0000,0000,0000,,B-Blbosť.
Dialogue: 0,0:13:13.03,0:13:16.17,Default,,0000,0000,0000,,Privítanie hodné návratu kráľa.
Dialogue: 0,0:13:16.17,0:13:17.59,Default,,0000,0000,0000,,Doňho!
Dialogue: 0,0:13:21.00,0:13:22.09,Default,,0000,0000,0000,,Ideme, Cruz!
Dialogue: 0,0:13:22.09,0:13:23.13,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:13:23.13,0:13:25.43,Default,,0000,0000,0000,,Arclight!
Dialogue: 0,0:13:32.34,0:13:36.90,Default,,0000,0000,0000,,Už je to nejaký čas, Všetci z Odporu.
Dialogue: 0,0:13:37.27,0:13:38.52,Default,,0000,0000,0000,,Ty si...
Dialogue: 0,0:13:38.52,0:13:40.23,Default,,0000,0000,0000,,Rokakuji Riru!
Dialogue: 0,0:13:41.05,0:13:43.04,Default,,0000,0000,0000,,Ona je psychokinetička...
Dialogue: 0,0:13:45.66,0:13:46.74,Default,,0000,0000,0000,,Nebojte sa!
Dialogue: 0,0:13:46.74,0:13:48.16,Default,,0000,0000,0000,,Sú tu len dvaja.
Dialogue: 0,0:13:48.90,0:13:50.27,Default,,0000,0000,0000,,Naozaj?
Dialogue: 0,0:14:02.89,0:14:06.51,Default,,0000,0000,0000,,Vy ste... Uten, Saten.
Dialogue: 0,0:14:06.84,0:14:08.77,Default,,0000,0000,0000,,Už nám neutečiete.
Dialogue: 0,0:14:08.77,0:14:10.22,Default,,0000,0000,0000,,Vy potkani.
Dialogue: 0,0:14:10.90,0:14:12.76,poznámka,,0000,0000,0000,,{\fad(100,100)}Shimeon Shiten znamená niečo ako štyria najsilnejší zo Simeonu
Dialogue: 0,0:14:10.91,0:14:13.01,Default,,0000,0000,0000,,Štvorka Simeonu.
Dialogue: 0,0:14:13.01,0:14:14.68,Default,,0000,0000,0000,,Informácie boli mylné.
Dialogue: 0,0:14:14.68,0:14:16.13,Default,,0000,0000,0000,,O strážach tam nič nebolo.
Dialogue: 0,0:14:16.13,0:14:17.48,Default,,0000,0000,0000,,Toto nezvládneme.
Dialogue: 0,0:14:17.48,0:14:18.77,Default,,0000,0000,0000,,Všetci ústup.
Dialogue: 0,0:14:20.64,0:14:21.48,Default,,0000,0000,0000,,Cruz, poď!
Dialogue: 0,0:14:22.09,0:14:22.97,Default,,0000,0000,0000,,Sestra.
Dialogue: 0,0:14:25.32,0:14:26.41,Default,,0000,0000,0000,,Sestra!
Dialogue: 0,0:14:29.46,0:14:30.60,Default,,0000,0000,0000,,Prečo?
Dialogue: 0,0:14:30.60,0:14:33.83,Default,,0000,0000,0000,,Vyzerá to, akoby od začiatku vedeli, že plánujeme útok.
Dialogue: 0,0:14:34.29,0:14:35.39,Default,,0000,0000,0000,,Je to pasca.
Dialogue: 0,0:14:35.39,0:14:39.13,Default,,0000,0000,0000,,Iné vysvetlenie nie je. Prečo by ináč so sebou bral Štvorku?
Dialogue: 0,0:14:39.37,0:14:40.56,Default,,0000,0000,0000,,To potom...
Dialogue: 0,0:14:40.56,0:14:45.84,Default,,0000,0000,0000,,Tá informácia bola sama o sebe pasca? Nalákali nás sem?
Dialogue: 0,0:14:46.20,0:14:48.13,Default,,0000,0000,0000,,Správne!
Dialogue: 0,0:14:51.00,0:14:54.43,Default,,0000,0000,0000,,Aby vylákal malé myšky zo svojich dierok pred Simeon...
Dialogue: 0,0:14:54.43,0:14:57.39,Default,,0000,0000,0000,,sám Arclight-sama zo seba spravil návnadu.
Dialogue: 0,0:14:57.74,0:15:00.04,Default,,0000,0000,0000,,A vy ste mu naleteli.
Dialogue: 0,0:15:00.88,0:15:02.16,Default,,0000,0000,0000,,Ten fagan.
Dialogue: 0,0:15:05.54,0:15:07.63,Default,,0000,0000,0000,,Odparenie Kvapaliny!
Dialogue: 0,0:15:13.24,0:15:15.51,Default,,0000,0000,0000,,Ste vážne hlupák, starý pán.
Dialogue: 0,0:15:15.51,0:15:18.71,Default,,0000,0000,0000,,Vážne ste si mysleli, že toto bude platiť na jedného zo Štvorky Simeonu?
Dialogue: 0,0:15:19.88,0:15:21.96,Default,,0000,0000,0000,,Čo je to za schopnosť?
Dialogue: 0,0:15:27.45,0:15:29.73,Default,,0000,0000,0000,,Sú to kúzla.
Dialogue: 0,0:15:33.55,0:15:36.40,Default,,0000,0000,0000,,Na jeden záťah zničili západnú jednotku?
Dialogue: 0,0:15:36.40,0:15:40.44,Default,,0000,0000,0000,,Informácie, ktoré sme obdržali, všetky boli falošné.
Dialogue: 0,0:15:41.60,0:15:42.55,Default,,0000,0000,0000,,Snáď...
Dialogue: 0,0:15:42.55,0:15:44.65,Default,,0000,0000,0000,,medzi nami nie je špicel?
Dialogue: 0,0:15:44.97,0:15:46.36,Default,,0000,0000,0000,,Je to monštrum.
Dialogue: 0,0:15:46.61,0:15:48.82,Default,,0000,0000,0000,,Takto sa k nemu ani nepriblížime.
Dialogue: 0,0:15:50.44,0:15:51.78,Default,,0000,0000,0000,,To je v poho.
Dialogue: 0,0:15:51.78,0:15:55.61,Default,,0000,0000,0000,,Aj keď je Nepotrebný, ak ho zasiahneme týmto, tak umrie.
Dialogue: 0,0:15:55.61,0:15:57.41,Default,,0000,0000,0000,,To si predsa vravel ty sám, nie?
Dialogue: 0,0:15:57.83,0:15:58.90,Default,,0000,0000,0000,,Je to nemožné.
Dialogue: 0,0:15:58.90,0:16:01.67,Default,,0000,0000,0000,,Ani raketomet nič nespravil.
Dialogue: 0,0:16:02.00,0:16:03.41,Default,,0000,0000,0000,,Sklapni!
Dialogue: 0,0:16:03.41,0:16:04.87,Default,,0000,0000,0000,,Všetci ste padavky.
Dialogue: 0,0:16:04.87,0:16:06.58,Default,,0000,0000,0000,,Prečo sa tak trasiete?
Dialogue: 0,0:16:06.58,0:16:07.92,Default,,0000,0000,0000,,T-Tak to nie je.
Dialogue: 0,0:16:08.31,0:16:10.17,Default,,0000,0000,0000,,J- Je zima.
Dialogue: 0,0:16:10.39,0:16:12.28,Default,,0000,0000,0000,,Z ničoho nič.
Dialogue: 0,0:16:16.81,0:16:18.71,Default,,0000,0000,0000,,Č-Čo je to?
Dialogue: 0,0:16:18.71,0:16:20.60,Default,,0000,0000,0000,,Zem zmŕza?
Dialogue: 0,0:16:26.20,0:16:27.07,Default,,0000,0000,0000,,Prichádzajú.
Dialogue: 0,0:16:40.50,0:16:41.82,Default,,0000,0000,0000,,Aruka.
Dialogue: 0,0:16:42.91,0:16:44.04,Default,,0000,0000,0000,,Hlúposť.
Dialogue: 0,0:16:44.41,0:16:46.54,Default,,0000,0000,0000,,Prečo si tam s ním?
Dialogue: 0,0:16:47.56,0:16:48.52,Default,,0000,0000,0000,,Saten...
Dialogue: 0,0:16:48.88,0:16:51.63,Default,,0000,0000,0000,,títo úbožiaci nemajú žiadne schopnosti.
Dialogue: 0,0:16:52.11,0:16:54.32,Default,,0000,0000,0000,,Nie je možné, žeby pri sebe mali čip.
Dialogue: 0,0:16:54.32,0:16:55.84,Default,,0000,0000,0000,,Rob si s nimi, čo chceš!
Dialogue: 0,0:16:55.84,0:16:57.39,Default,,0000,0000,0000,,Rozkaz!
Dialogue: 0,0:16:57.83,0:16:58.81,Default,,0000,0000,0000,,To snáď...
Dialogue: 0,0:16:59.22,0:17:01.37,Default,,0000,0000,0000,,to ty si bola ten špicel?
Dialogue: 0,0:17:01.37,0:17:03.44,Default,,0000,0000,0000,,Ty zradkyňa!
Dialogue: 0,0:17:03.44,0:17:05.60,Default,,0000,0000,0000,,Nechápte to zle!
Dialogue: 0,0:17:06.18,0:17:09.92,Default,,0000,0000,0000,,Dai Yon Hadou!
Dialogue: 0,0:17:23.93,0:17:25.46,Default,,0000,0000,0000,,Si skutočne vytrvalý.
Dialogue: 0,0:17:26.82,0:17:28.34,Default,,0000,0000,0000,,Ustúp!
Dialogue: 0,0:17:29.40,0:17:30.88,Default,,0000,0000,0000,,Vykonám.
Dialogue: 0,0:17:32.10,0:17:34.79,Default,,0000,0000,0000,,Ty si na čele Odporu, čo?
Dialogue: 0,0:17:34.79,0:17:37.65,Default,,0000,0000,0000,,Zato, že si až doteraz prežil, máš môj obdiv.
Dialogue: 0,0:17:38.10,0:17:39.51,Default,,0000,0000,0000,,Si pripravený?
Dialogue: 0,0:17:39.82,0:17:41.99,Default,,0000,0000,0000,,To platí aj pre teba.
Dialogue: 0,0:17:41.99,0:17:43.97,Default,,0000,0000,0000,,Priprav sa umrieť, Arclight.
Dialogue: 0,0:17:43.97,0:17:47.09,Default,,0000,0000,0000,,Porazím ťa, aj keby som mal pri tom prísť o život.
Dialogue: 0,0:17:47.46,0:17:50.13,poznámka,,0000,0000,0000,,{\fad(100,100)}Felsen Avalanche [フェルゼンアバランシェ]- kamenná lavína
Dialogue: 0,0:17:47.47,0:17:49.71,Default,,0000,0000,0000,,Feruzen Abaranshe!
Dialogue: 0,0:17:58.95,0:18:05.21,Default,,0000,0000,0000,,Toto kamenné brnenie je najpevnejšia ochrana s nekonečnou regeneráciou!
Dialogue: 0,0:18:05.48,0:18:08.36,poznámka,,0000,0000,0000,,{\fad(100,100)}ganseki nadare [岩石崩]- kamenná lavína (japončina)
Dialogue: 0,0:18:08.58,0:18:13.29,Default,,0000,0000,0000,,A tento úder je najsilnejší úder zeme.  Rozdrtí aj železo.
Dialogue: 0,0:18:15.62,0:18:17.01,Default,,0000,0000,0000,,Tak čo, Arclight!?
Dialogue: 0,0:18:18.67,0:18:19.89,Default,,0000,0000,0000,,Pekná ukážka.
Dialogue: 0,0:18:21.53,0:18:22.72,Default,,0000,0000,0000,,Sila!
Dialogue: 0,0:18:28.32,0:18:30.58,Default,,0000,0000,0000,,Obnova brnenia zeme.
Dialogue: 0,0:18:32.04,0:18:35.03,Default,,0000,0000,0000,,Rýchlosť plus Temupesto Suredo!
Dialogue: 0,0:18:46.27,0:18:47.94,Default,,0000,0000,0000,,Psychokinéza!
Dialogue: 0,0:18:58.09,0:19:00.97,Default,,0000,0000,0000,,To pre toto vás voláme odpad.
Dialogue: 0,0:19:01.43,0:19:03.34,Default,,0000,0000,0000,,T-Ty parchant!
Dialogue: 0,0:19:03.34,0:19:05.60,Default,,0000,0000,0000,,Koľko schopností sakra máš?
Dialogue: 0,0:19:05.60,0:19:09.19,Default,,0000,0000,0000,,Zabudol si, že som boh?
Dialogue: 0,0:19:10.23,0:19:11.73,Default,,0000,0000,0000,,Tak mi ho daj!
Dialogue: 0,0:19:13.63,0:19:15.01,Default,,0000,0000,0000,,Ten čip.
Dialogue: 0,0:19:15.01,0:19:17.49,Default,,0000,0000,0000,,Vy s ním nemáte predsa žiadne plány.
Dialogue: 0,0:19:20.35,0:19:22.38,Default,,0000,0000,0000,,Nie je tu.
Dialogue: 0,0:19:22.38,0:19:25.20,Default,,0000,0000,0000,,Smola, bože.
Dialogue: 0,0:19:33.72,0:19:34.76,Default,,0000,0000,0000,,Odpad.
Dialogue: 0,0:19:36.67,0:19:40.94,Default,,0000,0000,0000,,Konečne sme dostali všetky myšky, ktoré vzdorovali Simeonu.
Dialogue: 0,0:19:40.94,0:19:44.51,Default,,0000,0000,0000,,Posledný člen Odporu je jej brat.
Dialogue: 0,0:19:45.28,0:19:48.16,Default,,0000,0000,0000,,Dovoľme im ich posledné rozlúčenie.
Dialogue: 0,0:19:55.11,0:19:56.15,Default,,0000,0000,0000,,Cruz!
Dialogue: 0,0:19:56.76,0:19:57.53,Default,,0000,0000,0000,,Sestra.
Dialogue: 0,0:20:03.24,0:20:04.95,Default,,0000,0000,0000,,Ty odtiaľto uteč!
Dialogue: 0,0:20:05.45,0:20:07.12,Default,,0000,0000,0000,,Ale, ty...
Dialogue: 0,0:20:07.44,0:20:08.39,Default,,0000,0000,0000,,V poriadku.
Dialogue: 0,0:20:08.63,0:20:10.20,Default,,0000,0000,0000,,Mám svoju silu.
Dialogue: 0,0:20:12.25,0:20:13.58,Default,,0000,0000,0000,,Mám Fragment.
Dialogue: 0,0:20:17.07,0:20:19.80,Default,,0000,0000,0000,,Hīto Ekusupurōshon!
Dialogue: 0,0:20:38.49,0:20:39.54,Default,,0000,0000,0000,,Rozumiem.
Dialogue: 0,0:20:41.21,0:20:44.64,Default,,0000,0000,0000,,Takže vtedy už sestra...
Dialogue: 0,0:20:45.37,0:20:48.37,Default,,0000,0000,0000,,Hīto Ekusupurōshon!
Dialogue: 0,0:20:51.37,0:20:53.91,Default,,0000,0000,0000,,Dai Yon Hadou!
Dialogue: 0,0:21:01.74,0:21:07.39,Default,,0000,0000,0000,,Doteraz som prežil len vďaka tomu, že ma sestra ochraňovala.
Dialogue: 0,0:21:08.48,0:21:11.09,Default,,0000,0000,0000,,Vždy sme žili spolu.
Dialogue: 0,0:21:11.60,0:21:17.19,Default,,0000,0000,0000,,Ale možno som na nej bol len závislý.
Dialogue: 0,0:21:18.68,0:21:22.40,Default,,0000,0000,0000,,A preto ma začala sestra nenávidieť...
Dialogue: 0,0:21:23.25,0:21:27.87,Default,,0000,0000,0000,,Chcel som zosilnieť a raz jej to všetko vrátiť.
Dialogue: 0,0:21:28.39,0:21:31.41,Default,,0000,0000,0000,,Lenže, už je na to neskoro.
Dialogue: 0,0:21:33.54,0:21:35.79,Default,,0000,0000,0000,,Zbohom, sestra.
Dialogue: 0,0:21:45.22,0:21:46.43,Default,,0000,0000,0000,,Cruz-kun!
Dialogue: 0,0:21:48.89,0:21:51.81,Default,,0000,0000,0000,,Cruz-kun!
Dialogue: 0,0:23:25.80,0:23:30.43,TituloFinal,,0000,0000,0000,,{\fad(0,300)\fs70\pos(262,198)}Nabudúce
Dialogue: 0,0:23:36.47,0:23:40.81,TituloFinal,,0000,0000,0000,,{\fad(300,0)\fs80\pos(644,279)}Diel 18
Dialogue: 0,0:23:36.47,0:23:40.81,TituloFinal,,0000,0000,0000,,{\fad(300,0)\fs80\pos(638,510)}Agni Shwattas
Dialogue: 0,0:23:26.12,0:23:27.78,Default,,0000,0000,0000,,Cruz, posledné slová...
Dialogue: 0,0:23:28.15,0:23:30.11,Default,,0000,0000,0000,,Sestra, už nebojuj, prosím...
Dialogue: 0,0:23:31.70,0:23:32.82,Default,,0000,0000,0000,,Prečo mi to teraz hovoríš?
Dialogue: 0,0:23:32.82,0:23:36.24,Default,,0000,0000,0000,,No, vieš ako ti v tomto anime nadzdvihuje oblečenie, keď bojuješ?
Dialogue: 0,0:23:36.24,0:23:37.58,Default,,0000,0000,0000,,Sestra, žiadne nemáš, však?
Dialogue: 0,0:23:38.21,0:23:39.25,Default,,0000,0000,0000,,To ma netrápi.
Dialogue: 0,0:23:39.25,0:23:40.20,Default,,0000,0000,0000,,Malo by.
Dialogue: 0,0:23:39.52,0:23:41.52,Default,,0000,0000,0000,,
Dialogue: 0,0:01:00.54,0:01:04.54,názov,,0000,0000,0000,,{\fad(200,200)\pos(829,406)}Pôvodný Autor: Imai Kami
Dialogue: 0,0:01:20.89,0:01:25.14,názov,,0000,0000,0000,,{\fad(200,200)\pos(239,62)}preklad: Marek Satina
Dialogue: 0,0:01:26.27,0:01:30.52,názov,,0000,0000,0000,,{\fad(200,200)\pos(835,430)}korekcia:                  Samuel Adamec
Dialogue: 0,0:01:13.88,0:01:13.88,SK Opening,,0000,0000,0000,,
Dialogue: 0,0:01:13.88,0:01:17.18,SK Opening,,0000,0000,0000,,na rozdiel od nešťastných rozhodnutí
Dialogue: 0,0:01:17.18,0:01:20.72,SK Opening,,0000,0000,0000,,tie čo ťa posúvajú v pred sú v menšine
Dialogue: 0,0:01:20.73,0:01:24.10,SK Opening,,0000,0000,0000,,tým roztriešteným kvapôčkam morálky
Dialogue: 0,0:01:24.10,0:01:26.97,SK Opening,,0000,0000,0000,,musíš dovoliť aby sa usadili
Dialogue: 0,0:01:27.00,0:01:28.86,SK Opening,,0000,0000,0000,,šarlatová bomba
Dialogue: 0,0:01:29.78,0:01:32.61,SK Opening,,0000,0000,0000,,pery v šarlatovej bombe
Dialogue: 0,0:01:34.03,0:01:36.41,SK Opening,,0000,0000,0000,,tvoj pohľad ostrý ako nôž
Dialogue: 0,0:01:36.41,0:01:41.45,SK Opening,,0000,0000,0000,,okúsili sme bolesť, hriech Evy a jablka
Dialogue: 0,0:01:41.45,0:01:44.75,SK Opening,,0000,0000,0000,,tiché, obávané a srdcervúce
Dialogue: 0,0:01:44.75,0:01:49.59,SK Opening,,0000,0000,0000,,koľko krát to len bolo?
Dialogue: 0,0:01:49.96,0:01:53.26,SK Opening,,0000,0000,0000,,a chladné sa stáva horkým
Dialogue: 0,0:01:53.26,0:01:56.47,SK Opening,,0000,0000,0000,,nemožne večná noc
Dialogue: 0,0:01:56.47,0:02:00.10,SK Opening,,0000,0000,0000,,keď si hryziem nechty, hviezdy sú tak blízko
Dialogue: 0,0:02:00.10,0:02:03.60,SK Opening,,0000,0000,0000,,chcem ich vydieť, ale vidieť nie sú
Dialogue: 0,0:02:03.60,0:02:06.98,SK Opening,,0000,0000,0000,,priniesol si mi nekonečný a žiaden život
Dialogue: 0,0:02:06.98,0:02:10.23,SK Opening,,0000,0000,0000,,alebo to nemôžu byť moje sestry
Dialogue: 0,0:02:10.23,0:02:13.78,SK Opening,,0000,0000,0000,,ten okamih keď aj vo sne
Dialogue: 0,0:02:13.78,0:02:16.65,SK Opening,,0000,0000,0000,,sa zrodí smútok
Dialogue: 0,0:02:16.66,0:02:22.33,SK Opening,,0000,0000,0000,,zamilovávam sa do šarlatovje bomby
Dialogue: 0,0:02:23.50,0:02:29.17,SK Opening,,0000,0000,0000,,la-la-la-láska so šarlatovou bombou
Dialogue: 0,0:02:29.17,0:02:29.17,SK Opening,,0000,0000,0000,,
Dialogue: 0,0:02:29.17,0:02:29.17,SK Opening,,0000,0000,0000,,
Dialogue: 0,0:02:29.17,0:22:54.98,SK Opening,,0000,0000,0000,,
Dialogue: 0,0:21:55.88,0:21:57.50,Sk Ending,,0000,0000,0000,,tak poď! obímem ťa!
Dialogue: 0,0:21:57.50,0:21:58.96,Sk Ending,,0000,0000,0000,,neutečieš hľadaný chlapče
Dialogue: 0,0:21:58.96,0:22:00.67,Sk Ending,,0000,0000,0000,,podľa mena si hľadaný
Dialogue: 0,0:22:00.67,0:22:02.67,Sk Ending,,0000,0000,0000,,nič ma nezastavý
Dialogue: 0,0:22:07.85,0:22:09.06,Sk Ending,,0000,0000,0000,,si mi veľmi drahý
Dialogue: 0,0:22:09.06,0:22:11.14,Sk Ending,,0000,0000,0000,,vôbec to nie je v priadku
Dialogue: 0,0:22:11.14,0:22:12.31,Sk Ending,,0000,0000,0000,,kam to zmyzol?
Dialogue: 0,0:22:12.31,0:22:14.27,Sk Ending,,0000,0000,0000,,asi budem naozaj plakať
Dialogue: 0,0:22:14.27,0:22:15.52,Sk Ending,,0000,0000,0000,,to nie je na smiech
Dialogue: 0,0:22:15.52,0:22:17.06,Sk Ending,,0000,0000,0000,,stále vraj nemôže prísť
Dialogue: 0,0:22:17.06,0:22:18.65,Sk Ending,,0000,0000,0000,,to sa ti páči sa takto s niekym pohrávať?
Dialogue: 0,0:22:18.65,0:22:21.48,Sk Ending,,0000,0000,0000,,prosím všimni si to, ľúbim ťa
Dialogue: 0,0:22:21.49,0:22:23.03,Sk Ending,,0000,0000,0000,,všetko čo som teraz  povedala
Dialogue: 0,0:22:23.03,0:22:25.15,Sk Ending,,0000,0000,0000,,nebude len tak bez následkov
Dialogue: 0,0:22:27.32,0:22:32.66,Sk Ending,,0000,0000,0000,,musím si ťa držať pri sebe
Dialogue: 0,0:22:32.66,0:22:35.92,Sk Ending,,0000,0000,0000,,vždyť, vždy ťa potrestám
Dialogue: 0,0:22:35.92,0:22:39.25,Sk Ending,,0000,0000,0000,,vždy spolu sa budeme držať za ruky
Dialogue: 0,0:22:39.25,0:22:42.26,Sk Ending,,0000,0000,0000,,nie aj nie sú súčasťou lásky
Dialogue: 0,0:22:42.26,0:22:45.47,Sk Ending,,0000,0000,0000,,preto chcem počuť tvoj názor
Dialogue: 0,0:22:45.47,0:22:47.01,Sk Ending,,0000,0000,0000,,(čo toto?) stále sa hrajme
Dialogue: 0,0:22:47.01,0:22:48.64,Sk Ending,,0000,0000,0000,,(čo toto?) nezunuj si ma
Dialogue: 0,0:22:48.64,0:22:50.35,Sk Ending,,0000,0000,0000,,sila na sto percent
Dialogue: 0,0:22:50.35,0:22:51.97,Sk Ending,,0000,0000,0000,,musíš nabrať kondičku
Dialogue: 0,0:22:51.97,0:22:55.02,Sk Ending,,0000,0000,0000,,nenávisť aj nenávisť sú tiež láska, však?
Dialogue: 0,0:22:55.02,0:22:58.27,Sk Ending,,0000,0000,0000,,môj rozum pradie, si z toho zmetený?
Dialogue: 0,0:22:58.27,0:23:03.69,Sk Ending,,0000,0000,0000,,chystať pasce a nechať sa do nich chytiť je také zlaté
Dialogue: 0,0:23:04.49,0:23:07.86,Sk Ending,,0000,0000,0000,,len nemaj strach, drž sa pri mne
Dialogue: 0,0:23:07.86,0:23:11.08,Sk Ending,,0000,0000,0000,,budem k tebe nežná
Dialogue: 0,0:23:11.08,0:23:16.46,Sk Ending,,0000,0000,0000,,prstom ti na chrbát napíšem "prepáč"
Dialogue: 0,0:23:16.46,0:23:17.46,Sk Ending,,0000,0000,0000,,(mladá láska?)
Dialogue: 0,0:23:17.46,0:23:19.04,Sk Ending,,0000,0000,0000,,(Čo? nesmiem? vážne ťa chcem objať)
Dialogue: 0,0:23:19.04,0:23:20.54,Sk Ending,,0000,0000,0000,,aj keď nechceš si hľadaný chlapče
Dialogue: 0,0:23:20.54,0:23:22.30,Sk Ending,,0000,0000,0000,,sme dospelé, a chceme byť hľadané
Dialogue: 0,0:23:22.30,0:23:24.59,Sk Ending,,0000,0000,0000,,nikto nás nezastavý, áno!
Dialogue: 0,0:23:24.59,0:23:26.59,Sk Ending,,0000,0000,0000,,
