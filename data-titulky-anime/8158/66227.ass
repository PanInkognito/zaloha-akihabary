Dialogue: 0,0:00:11.10,0:00:15.23,Sign,,0000,0000,0000,,Srpen, 1935. Gallie, Předměstí Aslon.
Dialogue: 0,0:01:04.12,0:01:04.90,Default,,0000,0000,0000,,Nikdo tady není.
Dialogue: 0,0:01:05.24,0:01:06.22,Default,,0000,0000,0000,,Oni Utekli?
Dialogue: 0,0:01:12.23,0:01:12.96,Default,,0000,0000,0000,,To je...
Dialogue: 0,0:01:15.28,0:01:16.77,Default,,0000,0000,0000,,Sakra! To jsou oni!
Dialogue: 0,0:01:16.86,0:01:17.64,Default,,0000,0000,0000,,Kde jsou?!
Dialogue: 0,0:01:17.76,0:01:18.72,Default,,0000,0000,0000,,Ke krytu!
Dialogue: 0,0:01:18.74,0:01:19.73,Default,,0000,0000,0000,,Palte! Palte!
Dialogue: 0,0:01:23.33,0:01:24.63,Default,,0000,0000,0000,,Našli jsme je.
Dialogue: 0,0:01:25.14,0:01:25.80,Default,,0000,0000,0000,,Ale kde jsou?
Dialogue: 0,0:01:26.24,0:01:26.93,Default,,0000,0000,0000,,Radista!
Dialogue: 0,0:01:27.22,0:01:27.66,Default,,0000,0000,0000,,Ano!
Dialogue: 0,0:01:28.35,0:01:29.66,Default,,0000,0000,0000,,Žádost o stahnutí!
Dialogue: 0,0:01:30.00,0:01:30.48,Default,,0000,0000,0000,,Ano, pane!
Dialogue: 0,0:01:35.23,0:01:36.03,Default,,0000,0000,0000,,To jsou oni!
Dialogue: 0,0:01:36.19,0:01:36.83,Default,,0000,0000,0000,,Kde?!
Dialogue: 0,0:01:36.81,0:01:37.75,Default,,0000,0000,0000,,Rozptýlit se!
Dialogue: 0,0:01:45.34,0:01:46.97,Default,,0000,0000,0000,,Ústup! Ústup!
Dialogue: 0,0:01:47.63,0:01:49.44,Default,,0000,0000,0000,,Zdravotník! Zdravotník!
Dialogue: 0,0:01:49.67,0:01:51.42,Default,,0000,0000,0000,,Odkuď střílejí?!
Dialogue: 0,0:01:51.62,0:01:52.88,Default,,0000,0000,0000,,Ve tmě nic nevidím!
Dialogue: 0,0:01:52.92,0:01:53.73,Default,,0000,0000,0000,,Kde jsou?!
Dialogue: 0,0:01:53.84,0:01:54.94,Default,,0000,0000,0000,,Sakra!..
Dialogue: 0,0:01:55.28,0:01:57.65,Default,,0000,0000,0000,,Světlice! Vypalte Světlice!
Dialogue: 0,0:02:06.66,0:02:07.55,Default,,0000,0000,0000,,Támhle jsou!
Dialogue: 0,0:02:07.60,0:02:08.21,Default,,0000,0000,0000,,Tam nahoře!
Dialogue: 0,0:02:17.68,0:02:18.65,Default,,0000,0000,0000,,"BEZEJMENNÝ"!
Dialogue: 0,0:02:27.74,0:02:31.33,OpRom,,0000,0000,0000,,Tagatameni kokoro z
Dialogue: 0,0:02:27.74,0:02:31.33,OpRus,,0000,0000,0000,,Nechápu, pro koho
Dialogue: 0,0:02:31.90,0:02:38.44,OpRom,,0000,0000,0000,,Hakanaku kobore ochiiteku
Dialogue: 0,0:02:31.90,0:02:38.44,OpRus,,0000,0000,0000,,Moje duše je drcená
Dialogue: 0,0:02:39.03,0:02:48.74,OpRom,,0000,0000,0000,,Kikasete hoshii
Dialogue: 0,0:02:39.03,0:02:48.74,OpRus,,0000,0000,0000,,Řekni mi!
Dialogue: 0,0:02:55.65,0:03:13.33,Title,,0000,0000,0000,,Valkyria Chronicles 
Dialogue: 0,0:02:55.65,0:03:13.33,Title,,0000,0000,0000,, Uvadí
Dialogue: 0,0:02:55.65,0:03:13.33,Title2,,0000,0000,0000,,Boj o Někoho
Dialogue: 0,0:03:13.67,0:03:18.51,OpRom,,0000,0000,0000,,Tsusou sogu kono senritsu no naka
Dialogue: 0,0:03:13.67,0:03:18.51,OpRus,,0000,0000,0000,,Nekonečný rytmus, s melodií deště,
Dialogue: 0,0:03:18.74,0:03:25.21,OpRom,,0000,0000,0000,,Nani wo miru itai tsumi ni osowaretemo
Dialogue: 0,0:03:18.74,0:03:25.21,OpRus,,0000,0000,0000,,Ale naše poučení z bolesti z našich těžkých hříchů
Dialogue: 0,0:03:25.47,0:03:26.96,OpRom,,0000,0000,0000,,Mada susume
Dialogue: 0,0:03:25.47,0:03:26.96,OpRus,,0000,0000,0000,,Pokračuji...
Dialogue: 0,0:03:27.03,0:03:30.01,OpRom,,0000,0000,0000,,seigi toka sadame ja nai
Dialogue: 0,0:03:27.03,0:03:30.01,OpRus,,0000,0000,0000,,Nečekejte spravedlnost za svůj osudu,
Dialogue: 0,0:03:30.05,0:03:33.36,OpRom,,0000,0000,0000,,tada mira no tame nara
Dialogue: 0,0:03:30.05,0:03:33.36,OpRus,,0000,0000,0000,,Musíte to hledat v něm, jako v zrcadle
Dialogue: 0,0:03:33.65,0:03:36.86,OpRom,,0000,0000,0000,,boku no wa tsunagu atai
Dialogue: 0,0:03:33.65,0:03:36.86,OpRus,,0000,0000,0000,,Upevněte vazby s přátely,
Dialogue: 0,0:03:36.97,0:03:39.95,OpRom,,0000,0000,0000,,sora wa kizuna no yoru
Dialogue: 0,0:03:36.97,0:03:39.95,OpRus,,0000,0000,0000,,To bude volaní z nebes!
Dialogue: 0,0:03:40.05,0:03:45.82,OpRom,,0000,0000,0000,,kibou sore wa kono yoni sonzai suru no
Dialogue: 0,0:03:40.05,0:03:45.82,OpRus,,0000,0000,0000,,Naděje žije v jakémkoliv čase a místě,
Dialogue: 0,0:03:46.03,0:03:51.59,OpRom,,0000,0000,0000,,asu ga mata ubawarete yuku
Dialogue: 0,0:03:46.03,0:03:51.59,OpRus,,0000,0000,0000,,Dokonce i zítra si to nemůžu vzít si to od tebe
Dialogue: 0,0:03:51.80,0:03:53.01,OpRom,,0000,0000,0000,,tachiagare
Dialogue: 0,0:03:51.80,0:03:53.01,OpRus,,0000,0000,0000,,Vzestoupíme!
Dialogue: 0,0:03:53.11,0:03:59.49,OpRom,,0000,0000,0000,,kaze nami wo saitemo wakerarenai
Dialogue: 0,0:03:53.11,0:03:59.49,OpRus,,0000,0000,0000,,Vítr nás nemůže oddělit, ale může klidně foukat dál,
Dialogue: 0,0:03:59.70,0:04:04.90,OpRom,,0000,0000,0000,,zetsubou wo no hotemo shinjitsu motome
Dialogue: 0,0:03:59.70,0:04:04.90,OpRus,,0000,0000,0000,,Budeme setřeni zoufalstvím a pravdou
Dialogue: 0,0:04:05.08,0:04:06.15,OpRom,,0000,0000,0000,,mou ichido
Dialogue: 0,0:04:05.08,0:04:06.15,OpRus,,0000,0000,0000,,Kdykoliv!
Dialogue: 0,0:04:06.31,0:04:12.72,OpRom,,0000,0000,0000,,ni kikaeshita kita demo hanasanai
Dialogue: 0,0:04:06.31,0:04:12.72,OpRus,,0000,0000,0000,,Černá se nemůže stát bílá, a nemůžem to nechat být,
Dialogue: 0,0:04:12.93,0:04:19.85,OpRom,,0000,0000,0000,,na mo nakimono hashiridase
Dialogue: 0,0:04:12.93,0:04:19.85,OpRus,,0000,0000,0000,,A my vstáneme z mrtvých ještě jednou!
Dialogue: 0,0:04:31.75,0:04:32.71,Default,,0000,0000,0000,,"BEZEJMENNÝ"!
Dialogue: 0,0:04:33.61,0:04:34.71,Default,,0000,0000,0000,,Ze všech míst!..
Dialogue: 0,0:04:46.86,0:04:47.66,Default,,0000,0000,0000,,V žádném případě...
Dialogue: 0,0:04:48.32,0:04:50.00,Default,,0000,0000,0000,,Sestřelila všechny Světlice...
Dialogue: 0,0:04:50.87,0:04:52.66,Default,,0000,0000,0000,,Palte! Palte!
Dialogue: 0,0:04:56.30,0:04:58.02,Default,,0000,0000,0000,,Bylo to nutné?
Dialogue: 0,0:04:59.08,0:05:00.80,Default,,0000,0000,0000,,Je špatná viditelnost teď v noci.
Dialogue: 0,0:05:01.30,0:05:03.07,Default,,0000,0000,0000,,Je to naše jediná šance na výhru.
Dialogue: 0,0:05:03.48,0:05:04.99,Default,,0000,0000,0000,,Nemůžeme vyhrát sami.
Dialogue: 0,0:05:05.50,0:05:06.76,Default,,0000,0000,0000,,Imca má pravdu.
Dialogue: 0,0:05:07.37,0:05:08.06,Default,,0000,0000,0000,,Pohyb!
Dialogue: 0,0:05:09.46,0:05:10.93,Default,,0000,0000,0000,,Ústup! ústup!
Dialogue: 0,0:05:27.02,0:05:28.58,Default,,0000,0000,0000,,Kolik máš nabojů?
Dialogue: 0,0:05:28.97,0:05:30.53,Default,,0000,0000,0000,,Tři. A ty?
Dialogue: 0,0:05:30.83,0:05:31.88,Default,,0000,0000,0000,,Čtyři.
Dialogue: 0,0:05:32.20,0:05:33.67,Default,,0000,0000,0000,,To je náš konec?
Dialogue: 0,0:05:35.62,0:05:37.27,Default,,0000,0000,0000,,Nemůžu tady prostě umřít.
Dialogue: 0,0:05:37.87,0:05:39.52,Default,,0000,0000,0000,,Ještě musím jít s tebou na rande.
Dialogue: 0,0:05:40.07,0:05:42.59,Default,,0000,0000,0000,,Člověk obvykle zemře jako první,za taková slova.
Dialogue: 0,0:05:44.81,0:05:47.95,Default,,0000,0000,0000,,Daite! Doprovoď naší drsnou lady.
Dialogue: 0,0:05:48.23,0:05:48.78,Default,,0000,0000,0000,,Jo!
Dialogue: 0,0:05:51.53,0:05:53.27,Default,,0000,0000,0000,,Kde se schovávají?
Dialogue: 0,0:05:53.98,0:05:55.61,Default,,0000,0000,0000,,Takže, tady umřem?
Dialogue: 0,0:05:57.17,0:06:00.01,Default,,0000,0000,0000,,My nejsme ti, kteří rozhodnou, kde zemřeme.
Dialogue: 0,0:06:00.69,0:06:02.88,Default,,0000,0000,0000,,Jedna kulka to všechno skončí.
Dialogue: 0,0:06:03.32,0:06:05.52,Default,,0000,0000,0000,,Pak můžem o tom přemýšlet později.
Dialogue: 0,0:06:06.07,0:06:07.69,Default,,0000,0000,0000,,Nyní potřebujeme aby se nepřítel dal na ústup.
Dialogue: 0,0:06:15.44,0:06:16.24,Default,,0000,0000,0000,,Neutíkej te!
Dialogue: 0,0:06:16.36,0:06:18.01,Default,,0000,0000,0000,,Vraťte se do svých pozic!
Dialogue: 0,0:06:18.24,0:06:21.31,Default,,0000,0000,0000,,Oni jsou jeden krok před náma. Nemůžem nic dělat!
Dialogue: 0,0:06:21.67,0:06:23.44,Default,,0000,0000,0000,,Máme výhodu v počtech...
Dialogue: 0,0:06:23.81,0:06:25.02,Default,,0000,0000,0000,,Tak proč se to děje?..
Dialogue: 0,0:06:37.17,0:06:37.97,Default,,0000,0000,0000,,Kdy se jim!..
Dialogue: 0,0:06:38.41,0:06:39.49,Default,,0000,0000,0000,,Jsme obklíčeni!
Dialogue: 0,0:06:44.33,0:06:46.64,Default,,0000,0000,0000,,Ústup! Ústup!
Dialogue: 0,0:07:05.35,0:07:09.57,Default,,0000,0000,0000,,Nakonec... utekli se stažením ocasem mezi nohama.
Dialogue: 0,0:07:20.27,0:07:21.78,Default,,0000,0000,0000,,Vše podle plánu, Kaliso.
Dialogue: 0,0:07:22.79,0:07:25.43,Default,,0000,0000,0000,,Je skvělé, že nevěděli, že tady nemáme žádné granáty.
Dialogue: 0,0:07:25.92,0:07:28.01,Default,,0000,0000,0000,,Já už nechci hrát takovouhle roli znovu.
Dialogue: 0,0:07:28.72,0:07:31.45,Default,,0000,0000,0000,,Každý operoval v souladu s mým plánem.
Dialogue: 0,0:07:32.41,0:07:35.98,Default,,0000,0000,0000,,Pokud strategie nemá chybu - bude existovat pouze jeden výsledek.
Dialogue: 0,0:07:37.03,0:07:39.84,Default,,0000,0000,0000,,Pro mě je tento proces mnohem důležitější, než výsledek.
Dialogue: 0,0:07:45.35,0:07:49.46,Sign,,0000,0000,0000,,{\pos(650,611)\fad(500,500)}Pohraniční pevnost Ghirlandaio
Dialogue: 0,0:07:52.41,0:07:57.85,Default,,0000,0000,0000,,Takže, naše velká říše dalo takové úsilí proti nějaké zemi jako je Gallie...
Dialogue: 0,0:07:59.27,0:08:03.78,Default,,0000,0000,0000,,Vaše Výsosti, vy jste mě sem zavolal jen kvůli napomenutí?
Dialogue: 0,0:08:05.92,0:08:07.70,Default,,0000,0000,0000,,Slyšel jste, Veliteli o «Kalamity Raven»...
Dialogue: 0,0:08:08.46,0:08:11.35,Default,,0000,0000,0000,,...je zanmý pod frází «skromná ctnost».
Dialogue: 0,0:08:12.27,0:08:13.23,Default,,0000,0000,0000,,Jak víte...
Dialogue: 0,0:08:13.69,0:08:16.79,Default,,0000,0000,0000,,...Gallianům se vrátí poměrně velká část oblasti.
Dialogue: 0,0:08:17.63,0:08:18.13,Default,,0000,0000,0000,,A však...
Dialogue: 0,0:08:18.69,0:08:24.30,Default,,0000,0000,0000,,I když máme před sebou ještě v našich rukou Dillsburg, hlavní město Gallia, Randgriz sedí s nožem na krku.
Dialogue: 0,0:08:25.17,0:08:26.46,Default,,0000,0000,0000,,Pomocí naší vojenské základny tady...
Dialogue: 0,0:08:26.94,0:08:28.84,Default,,0000,0000,0000,,...je možné, aby jsme potřebovaly tolik síly.
Dialogue: 0,0:08:29.57,0:08:33.27,Default,,0000,0000,0000,,Samozřejmě, Galliani nebudou jen tak sedět a nic nedělat.
Dialogue: 0,0:08:35.03,0:08:37.23,Default,,0000,0000,0000,,Musím chránit naše vojenské základny u Dillsburgu.
Dialogue: 0,0:08:40.00,0:08:42.06,Default,,0000,0000,0000,,To ukáže vaši loajalitu ke mně.
Dialogue: 0,0:08:59.45,0:09:02.63,Default,,0000,0000,0000,,Pouze chléba a polévka bez chuti... Někdy i méně...
Dialogue: 0,0:09:08.02,0:09:08.80,Default,,0000,0000,0000,,Cesta je čistá!
Dialogue: 0,0:09:10.01,0:09:13.04,Default,,0000,0000,0000,,Jeden Kilometr odsud, je potok tak to tam můžem přejít.
Dialogue: 0,0:09:13.95,0:09:17.01,Default,,0000,0000,0000,,Pravidelná armáda bude vést útok na Dillsburg.
Dialogue: 0,0:09:18.06,0:09:20.74,Default,,0000,0000,0000,,Takže bitva se bude pravděpodobně konat na západě.
Dialogue: 0,0:09:21.32,0:09:24.57,Default,,0000,0000,0000,,Kurte, my máme získat oba břehy.
Dialogue: 0,0:09:25.10,0:09:25.83,Default,,0000,0000,0000,,Proto...
Dialogue: 0,0:09:27.21,0:09:30.12,Default,,0000,0000,0000,,...oni si nemyslí, že to můžem dokázat.
Dialogue: 0,0:09:30.78,0:09:35.19,Default,,0000,0000,0000,,A nebudou mít žádný čas zabývat se dezertérskou četou.
Dialogue: 0,0:10:00.69,0:10:06.09,Default,,0000,0000,0000,,Slyšel jsem, že vaše skupina nepříjmá žádné rozkazy od prince Maximilliana, ale jen od vás Darcsensi...
Dialogue: 0,0:10:07.30,0:10:12.18,Default,,0000,0000,0000,,Nechápejte mě špatně. Budu bránit tuto základnu.
Dialogue: 0,0:10:13.22,0:10:16.87,Sign,,0000,0000,0000,,{\pos(648,615)\fad(200,100)}Dillsburg vojenská základna
Dialogue: 0,0:10:14.04,0:10:16.80,Default,,0000,0000,0000,,Můžeš jednoduše chránit své děti.
Dialogue: 0,0:10:18.13,0:10:21.95,Default,,0000,0000,0000,,Doufám, že jsi kluzký Darcsensi alespoň jsi schopný?
Dialogue: 0,0:10:23.90,0:10:24.35,Default,,0000,0000,0000,,Ano, Pane.
Dialogue: 0,0:10:25.32,0:10:27.27,Default,,0000,0000,0000,,No tak, No tak.
Dialogue: 0,0:10:28.33,0:10:30.73,Default,,0000,0000,0000,,Jaký je, jako Velitel Münchhausen?
Dialogue: 0,0:10:31.42,0:10:31.72,Default,,0000,0000,0000,,Zig.
Dialogue: 0,0:10:32.61,0:10:34.98,Default,,0000,0000,0000,,Buďte kdykoli připraveni jednat.
Dialogue: 0,0:10:35.70,0:10:36.06,Default,,0000,0000,0000,,Rozkaz!
Dialogue: 0,0:10:37.77,0:10:39.93,Default,,0000,0000,0000,,Co to k sakru, je za chlapa?
Dialogue: 0,0:10:40.34,0:10:42.70,Default,,0000,0000,0000,,Jo... Jeden z druhů.
Dialogue: 0,0:10:43.30,0:10:46.14,Default,,0000,0000,0000,,Vidím, že Impérium má nedostatek dobrých velitelů.
Dialogue: 0,0:10:46.49,0:10:52.88,Default,,0000,0000,0000,,A Galliani víc, než je nutné, Gusurgu?
Dialogue: 0,0:10:54.23,0:10:56.91,Default,,0000,0000,0000,,To není ten důvod. proč to má Impérium tak těžké?
Dialogue: 0,0:10:58.89,0:10:59.85,Default,,0000,0000,0000,,Poslyš!..
Dialogue: 0,0:10:59.94,0:11:01.00,Default,,0000,0000,0000,,Lydia, nech toho.
Dialogue: 0,0:11:07.39,0:11:09.82,Default,,0000,0000,0000,,Každý stát musí být připraven.
Dialogue: 0,0:11:10.49,0:11:11.08,Default,,0000,0000,0000,,Ano, pane.
Dialogue: 0,0:11:29.97,0:11:33.11,Default,,0000,0000,0000,,Máš opravdu ráda Edelweisse, Isara?
Dialogue: 0,0:11:34.99,0:11:39.10,Default,,0000,0000,0000,,Jo. Můj Otec dal část své duše do konstrukce.
Dialogue: 0,0:11:39.58,0:11:43.54,Default,,0000,0000,0000,,A nikdy neopouštíš Edelweisse sáma, že?
Dialogue: 0,0:11:43.90,0:11:44.33,Default,,0000,0000,0000,,Ano!
Dialogue: 0,0:11:44.78,0:11:45.89,Default,,0000,0000,0000,,Jak chytré!
Dialogue: 0,0:11:46.39,0:11:48.91,Default,,0000,0000,0000,,Je to v pořádku dělat takovou důkladnou údržbu?
Dialogue: 0,0:11:49.21,0:11:51.49,Default,,0000,0000,0000,,Nevíme, kdy půjdem do další operace.
Dialogue: 0,0:11:52.23,0:11:53.88,Default,,0000,0000,0000,,Půjde nastartovat zatimco opravujem.
Dialogue: 0,0:11:54.37,0:11:54.78,Default,,0000,0000,0000,,Eh?
Dialogue: 0,0:11:55.42,0:11:58.29,Default,,0000,0000,0000,,Jak víš, že to nezačne teď, Welkine?
Dialogue: 0,0:11:59.20,0:12:01.17,Default,,0000,0000,0000,,Nepřítel ma pozemní výhodu.
Dialogue: 0,0:12:01.63,0:12:03.70,Default,,0000,0000,0000,,A nejméně pět set vojáků.
Dialogue: 0,0:12:04.41,0:12:06.54,Default,,0000,0000,0000,,Takže jediná šance je přepad.
Dialogue: 0,0:12:07.36,0:12:10.78,Default,,0000,0000,0000,,Naštěstí, v Dillsburg v této části roku mlha není rarita.
Dialogue: 0,0:12:12.50,0:12:15.14,Default,,0000,0000,0000,,Operace «Mlžné ráno».
Dialogue: 0,0:12:58.85,0:13:00.21,Default,,0000,0000,0000,,Konečně se vyjasnilo.
Dialogue: 0,0:13:07.57,0:13:10.02,Default,,0000,0000,0000,,Tepleji! Ať je Teplejší!
Dialogue: 0,0:13:10.02,0:13:12.06,Default,,0000,0000,0000,,Jo, jo... Počkej na moment.
Dialogue: 0,0:13:15.77,0:13:17.31,Default,,0000,0000,0000,,Riela, čas střídaní.
Dialogue: 0,0:13:17.77,0:13:18.60,Default,,0000,0000,0000,,Chápu.
Dialogue: 0,0:13:21.92,0:13:23.84,Default,,0000,0000,0000,,Oh! To je Slavík!
Dialogue: 0,0:13:24.58,0:13:26.30,Default,,0000,0000,0000,,Víš hodně o ptácích.
Dialogue: 0,0:13:29.60,0:13:31.36,Default,,0000,0000,0000,,V místě, kde jsem vyrůstala...
Dialogue: 0,0:13:32.24,0:13:35.92,Default,,0000,0000,0000,,Říká se, že pár, který uslyší jeho zpěv, nalezne štěstí.
Dialogue: 0,0:13:42.14,0:13:43.98,Default,,0000,0000,0000,,Tato pověra nemá žádný vědecký podklad.
Dialogue: 0,0:13:45.83,0:13:47.84,Default,,0000,0000,0000,,Kurte, ty jsi takový!..
Dialogue: 0,0:13:49.24,0:13:50.55,Default,,0000,0000,0000,,Co tě naštvalo?
Dialogue: 0,0:13:52.15,0:13:52.80,Default,,0000,0000,0000,,Co se stalo?
Dialogue: 0,0:14:08.32,0:14:09.28,Default,,0000,0000,0000,,Vydrž!
Dialogue: 0,0:14:11.94,0:14:16.96,Default,,0000,0000,0000,,Takže vaše vesnice byla napadena Impérialními vojáky a oni převzali kontrolu?
Dialogue: 0,0:14:17.43,0:14:17.73,Default,,0000,0000,0000,,Jo...
Dialogue: 0,0:14:23.38,0:14:24.89,Default,,0000,0000,0000,,Tak tedy vysvětlím plán...
Dialogue: 0,0:14:27.72,0:14:31.44,Default,,0000,0000,0000,,Kurte, Já vím, že chceš učinit neuvážené kroky...
Dialogue: 0,0:14:32.39,0:14:35.46,Default,,0000,0000,0000,,Pak přemýšlel o tom co říkaš: "Oh, ano! Správně!"
Dialogue: 0,0:14:36.15,0:14:38.39,Default,,0000,0000,0000,,Ale to je stále bezohledný.
Dialogue: 0,0:14:48.18,0:14:49.67,Default,,0000,0000,0000,,Pojďme si, odpočinout do stanů.
Dialogue: 0,0:14:50.11,0:14:50.52,Default,,0000,0000,0000,,Jo...
Dialogue: 0,0:14:58.73,0:15:01.60,Default,,0000,0000,0000,,Nemůžeme jít do vesnice s naším současným vybavením.
Dialogue: 0,0:15:02.33,0:15:03.78,Default,,0000,0000,0000,,Máme málo munice.
Dialogue: 0,0:15:04.39,0:15:06.68,Default,,0000,0000,0000,,Vrátíme-li se do vesnice, a co dál?
Dialogue: 0,0:15:07.21,0:15:09.92,Default,,0000,0000,0000,,Bez munice a zbraní jsme zbytečná zátěž.
Dialogue: 0,0:15:10.60,0:15:13.17,Default,,0000,0000,0000,,Možná bychom měli hledat naše pravidelné jednotky?
Dialogue: 0,0:15:13.54,0:15:14.94,Default,,0000,0000,0000,,V jiných místech...
Dialogue: 0,0:15:15.27,0:15:17.83,Default,,0000,0000,0000,,...budeme mít těžké ztráty i v akci...
Dialogue: 0,0:15:18.31,0:15:19.34,Default,,0000,0000,0000,,...i s malými Imperialními sílami.
Dialogue: 0,0:15:20.21,0:15:21.15,Default,,0000,0000,0000,,Počkej minutku!
Dialogue: 0,0:15:21.49,0:15:23.37,Default,,0000,0000,0000,,Chceš opustit odžádosti od dívčiny?
Dialogue: 0,0:15:24.22,0:15:26.61,Default,,0000,0000,0000,,Ne tak docela. Ale nemáme dobrou ruku.
Dialogue: 0,0:15:27.45,0:15:30.02,Default,,0000,0000,0000,,Zavoláme pravidelní jednotky. Je to jejich práce.
Dialogue: 0,0:15:30.87,0:15:34.63,Default,,0000,0000,0000,,Co může pro ně znamenat nějaká malá vesnice?
Dialogue: 0,0:15:35.04,0:15:37.45,Default,,0000,0000,0000,,Muži, májí opravdu hlad...
Dialogue: 0,0:15:37.93,0:15:38.85,Default,,0000,0000,0000,,Muži jsou vyčerpaní.
Dialogue: 0,0:15:39.14,0:15:40.41,Default,,0000,0000,0000,,Tank je bez munice.
Dialogue: 0,0:15:40.75,0:15:42.33,Default,,0000,0000,0000,,Nemáme dostatek zásob.
Dialogue: 0,0:15:42.56,0:15:43.36,Default,,0000,0000,0000,,Je to k ničemu.
Dialogue: 0,0:15:43.64,0:15:45.68,Default,,0000,0000,0000,,Ale její rodiče jsou v nebezpečí!
Dialogue: 0,0:15:46.55,0:15:50.40,Default,,0000,0000,0000,,To ale nezvětší naše zásoby a munici...
Dialogue: 0,0:15:52.86,0:15:58.44,Default,,0000,0000,0000,,Právě teď jsme pro Gallianskou armádu jen dezertéři a zrádci.
Dialogue: 0,0:15:59.76,0:16:01.08,Default,,0000,0000,0000,,Raněný a vyčerpaný...
Dialogue: 0,0:16:02.00,0:16:04.16,Default,,0000,0000,0000,,Ale kdo trpí válkou ze všeho nejvíce?
Dialogue: 0,0:16:05.65,0:16:07.02,Default,,0000,0000,0000,,Civilisti...
Dialogue: 0,0:16:08.38,0:16:08.90,Default,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:16:10.10,0:16:13.79,Default,,0000,0000,0000,,Ale nejsou Bezejmenný. Všichni mají jméno.
Dialogue: 0,0:16:14.63,0:16:16.24,Default,,0000,0000,0000,,Víme, jako nikdo jiný.
Dialogue: 0,0:16:17.43,0:16:21.10,Default,,0000,0000,0000,,Že tyto zajatci Imperialních sil potřebují pomoc.
Dialogue: 0,0:16:21.86,0:16:23.71,Default,,0000,0000,0000,,A pouze jediná četa jim může pomoci...
Dialogue: 0,0:16:25.39,0:16:28.53,Default,,0000,0000,0000,,...my, obyčejní dezertéři, "Bezejmenný"!
Dialogue: 0,0:16:33.45,0:16:34.40,Default,,0000,0000,0000,,Kdybychom je nechali...
Dialogue: 0,0:16:36.94,0:16:38.39,Default,,0000,0000,0000,,...nejsme lepší než pravidelní jednotky!
Dialogue: 0,0:16:44.30,0:16:45.29,Default,,0000,0000,0000,,No, já jsem.
Dialogue: 0,0:16:58.15,0:16:58.58,Default,,0000,0000,0000,,Co?
Dialogue: 0,0:16:59.06,0:17:00.37,Default,,0000,0000,0000,,Anna měla pravdu.
Dialogue: 0,0:17:00.51,0:17:02.14,Default,,0000,0000,0000,,Imperialní vojáci jsou všude.
Dialogue: 0,0:17:02.73,0:17:03.59,Default,,0000,0000,0000,,obrněný transportér?
Dialogue: 0,0:17:04.18,0:17:06.51,Default,,0000,0000,0000,,Tady je to. Zdá se, že je pouze jeden.
Dialogue: 0,0:17:07.06,0:17:08.72,Default,,0000,0000,0000,,Dobře, pojďmě na to? lidi.
Dialogue: 0,0:17:18.87,0:17:21.48,Default,,0000,0000,0000,,Vstoupíme do vesnice po půlnoci.
Dialogue: 0,0:17:26.34,0:17:30.24,Default,,0000,0000,0000,,Imcaina skupina zamíří rovnou do nepřatelské centrály.
Dialogue: 0,0:17:31.04,0:17:32.76,Default,,0000,0000,0000,,Musíme zajmout jejich velitele rychle.
Dialogue: 0,0:17:33.63,0:17:34.74,Default,,0000,0000,0000,,To je v pořádku...
Dialogue: 0,0:17:35.06,0:17:36.09,Default,,0000,0000,0000,,Nech toho, prosím!
Dialogue: 0,0:17:38.40,0:17:40.74,Default,,0000,0000,0000,,Co je, děvče? Zůstaň v klidu.
Dialogue: 0,0:17:46.98,0:17:48.90,Default,,0000,0000,0000,,Náš Tank dá signál.
Dialogue: 0,0:17:59.93,0:18:03.41,Default,,0000,0000,0000,,A-a-alarm!
Dialogue: 0,0:18:04.47,0:18:05.64,Default,,0000,0000,0000,,Útok!
Dialogue: 0,0:18:05.73,0:18:08.96,Default,,0000,0000,0000,,Útok? Sakra, co se stalo...
Dialogue: 0,0:18:42.25,0:18:45.89,Default,,0000,0000,0000,,Dobře jdi do centra vesnice, podpoří je Imcaina skupina!
Dialogue: 0,0:18:46.26,0:18:48.46,Default,,0000,0000,0000,,Kurte, mají Tank!
Dialogue: 0,0:18:58.62,0:19:01.48,Default,,0000,0000,0000,,Anna o tom nic neřekla.
Dialogue: 0,0:19:01.74,0:19:03.60,Default,,0000,0000,0000,,Možná, to ta dívka nevěděla.
Dialogue: 0,0:19:04.44,0:19:06.30,Default,,0000,0000,0000,,Glorie, protitankovou střelu!
Dialogue: 0,0:19:07.15,0:19:08.80,Default,,0000,0000,0000,,Vystřelila jsem poslední.
Dialogue: 0,0:19:09.05,0:19:09.81,Default,,0000,0000,0000,,Vážně?!
Dialogue: 0,0:19:23.03,0:19:24.16,Default,,0000,0000,0000,,Kaliso, ustoupit!
Dialogue: 0,0:19:25.02,0:19:25.49,Default,,0000,0000,0000,,Kaliso!
Dialogue: 0,0:19:47.70,0:19:51.51,Default,,0000,0000,0000,,Já opravdu nemám ráda být v úzkých...
Dialogue: 0,0:19:54.69,0:19:58.13,Default,,0000,0000,0000,,Ta Liška! Jsi schovala poslední pro sebe .
Dialogue: 0,0:20:01.11,0:20:01.69,Default,,0000,0000,0000,,Co je sakra?!
Dialogue: 0,0:20:02.17,0:20:04.16,Default,,0000,0000,0000,,Tank a obrněný transportér jsou ztraceny?
Dialogue: 0,0:20:10.62,0:20:12.69,Default,,0000,0000,0000,,Vzdejte se a nikdo už nezemře!
Dialogue: 0,0:20:23.10,0:20:24.29,Default,,0000,0000,0000,,Shromáždili jsme všechny zbraně.
Dialogue: 0,0:20:25.09,0:20:27.61,Default,,0000,0000,0000,,Budete v péči vesničanů.
Dialogue: 0,0:20:28.26,0:20:30.57,Default,,0000,0000,0000,,Budete později předáni Gallianské armádě.
Dialogue: 0,0:20:31.32,0:20:32.60,Default,,0000,0000,0000,,Mami! Tati!
Dialogue: 0,0:20:33.71,0:20:34.30,Default,,0000,0000,0000,,Anna!
Dialogue: 0,0:20:35.25,0:20:36.05,Default,,0000,0000,0000,,Jsi v pořádku?
Dialogue: 0,0:20:36.30,0:20:36.68,Default,,0000,0000,0000,,Ano!
Dialogue: 0,0:20:38.92,0:20:40.47,Default,,0000,0000,0000,,Děkuji, pane Vojáku!
Dialogue: 0,0:20:55.51,0:20:57.90,Default,,0000,0000,0000,,Tento kozí sýr pěkně páchne, ale je výtečný!
Dialogue: 0,0:20:58.42,0:20:59.41,Default,,0000,0000,0000,,Tady máš.
Dialogue: 0,0:20:59.89,0:21:00.60,Default,,0000,0000,0000,,Děkuji!
Dialogue: 0,0:21:10.67,0:21:11.45,Default,,0000,0000,0000,,Jedla jsi?
Dialogue: 0,0:21:13.95,0:21:15.32,Default,,0000,0000,0000,,Nedělej si starosti.
Dialogue: 0,0:21:15.87,0:21:18.23,Default,,0000,0000,0000,,Nemůžeš vědět, kdy bude další jídlo.
Dialogue: 0,0:21:24.38,0:21:25.93,Default,,0000,0000,0000,,Takže budeš jíst, co zmůžeš!
Dialogue: 0,0:21:26.16,0:21:27.95,Default,,0000,0000,0000,,Přestaň! Nech mě jít!
Dialogue: 0,0:21:28.23,0:21:30.50,Default,,0000,0000,0000,,Smím prosit, Princesno?
Dialogue: 0,0:21:32.17,0:21:33.11,Default,,0000,0000,0000,,Netancuju!
Dialogue: 0,0:21:33.27,0:21:35.12,Default,,0000,0000,0000,,Jsi tak elegantní, Imca!
Dialogue: 0,0:21:42.01,0:21:44.32,Default,,0000,0000,0000,,Nejedla jsem tolik, už nějakou dobu.
Dialogue: 0,0:21:44.71,0:21:46.20,Default,,0000,0000,0000,,Ano, to byl svátek.
Dialogue: 0,0:21:46.66,0:21:49.43,Default,,0000,0000,0000,,Ach, vy snad nechcete dělat žadné potíže?
Dialogue: 0,0:21:53.56,0:21:55.62,Default,,0000,0000,0000,,Takže nejsou žádné bezejmenných lidí, správně?
Dialogue: 0,0:21:56.29,0:21:57.43,Default,,0000,0000,0000,,Jako bezejmenný vojáci...
Dialogue: 0,0:21:59.68,0:22:04.45,Default,,0000,0000,0000,,Nechápu, proč jsme nechali všechny zbraně ve Vesnici?
Dialogue: 0,0:22:05.22,0:22:08.95,Default,,0000,0000,0000,,Teď se mohou bránit, i když jsme odešli.
Dialogue: 0,0:22:09.75,0:22:11.81,Default,,0000,0000,0000,,Vesničané potřebují tyto zbraně víc.
Dialogue: 0,0:22:12.38,0:22:15.45,Default,,0000,0000,0000,,Můžeš s námi všemi sdílet takovou radost?
Dialogue: 0,0:22:41.53,0:22:42.44,Default,,0000,0000,0000,,Já, osobně
Dialogue: 0,0:22:43.18,0:22:43.55,Default,,0000,0000,0000,,Ano.
Dialogue: 0,0:22:44.52,0:22:46.81,Default,,0000,0000,0000,,V dopoledních hodinách Galliani zaútočí plnou silou.
Dialogue: 0,0:22:48.00,0:22:50.50,Default,,0000,0000,0000,,Budeš vést přepadení.
Dialogue: 0,0:22:51.54,0:22:51.88,Default,,0000,0000,0000,,Ano, pane!
Dialogue: 0,0:22:52.62,0:22:53.37,Default,,0000,0000,0000,,Rozumím!
Dialogue: 0,0:23:26.87,0:23:29.16,Default,,0000,0000,0000,,tady! Máme munici!
Dialogue: 0,0:23:33.61,0:23:35.10,Default,,0000,0000,0000,,Ah, moje malý náboje!
Dialogue: 0,0:23:35.74,0:23:37.00,Default,,0000,0000,0000,,Oh, támhle jsou 9mm.
Dialogue: 0,0:23:37.63,0:23:39.28,Default,,0000,0000,0000,,A granáty taky.
Dialogue: 0,0:23:39.55,0:23:41.05,Default,,0000,0000,0000,,Neberte si všechno sámi.
Dialogue: 0,0:23:41.26,0:23:43.37,Default,,0000,0000,0000,,Vy a Gloria jste tady hlavní žrouti.
Dialogue: 0,0:23:43.49,0:23:45.02,Default,,0000,0000,0000,,Ha, jak to víš?
Dialogue: 0,0:23:45.94,0:23:48.19,Default,,0000,0000,0000,,Je dobré, že jsme je obdrželi.
Dialogue: 0,0:23:48.54,0:23:51.40,Default,,0000,0000,0000,,Ano, podplukovník Crowe drží své slovo.
Dialogue: 0,0:23:54.48,0:23:57.38,Default,,0000,0000,0000,,Jako by nebyl žádný déšť v poslední době.
Dialogue: 0,0:24:03.57,0:24:05.40,Default,,0000,0000,0000,,Dahaue, nemůžete spát?
Dialogue: 0,0:24:06.14,0:24:06.94,Default,,0000,0000,0000,,Stárnete?
Dialogue: 0,0:24:07.67,0:24:11.80,Default,,0000,0000,0000,,Víš, že k ránu, výchazí Modrý měsíc...
Dialogue: 0,0:24:12.27,0:24:13.32,Default,,0000,0000,0000,,...Bude hustá Mlha?
Dialogue: 0,0:24:15.22,0:24:17.35,Default,,0000,0000,0000,,Takže, čas se blíží?
Dialogue: 0,0:24:18.76,0:24:20.67,Default,,0000,0000,0000,,Ano, konečně.
Dialogue: 0,0:25:04.16,0:25:04.69,Default,,0000,0000,0000,,Do prdele!
Dialogue: 0,0:25:05.94,0:25:07.70,Default,,0000,0000,0000,,Zaútočíli skrz mlhu?
Dialogue: 0,0:25:18.89,0:25:19.43,Default,,0000,0000,0000,,Opravdu?
Dialogue: 0,0:25:20.32,0:25:20.99,Default,,0000,0000,0000,,Samozřejmě.
Dialogue: 0,0:25:22.26,0:25:25.27,Default,,0000,0000,0000,,Naším úkolem je chránit tyto krásné hračky.
Dialogue: 0,0:26:06.39,0:26:07.09,Default,,0000,0000,0000,,Pojď te!
Dialogue: 0,0:26:07.20,0:26:07.78,Default,,0000,0000,0000,,Ano!
Dialogue: 0,0:26:08.03,0:26:08.69,Default,,0000,0000,0000,,Ano, pane!
Dialogue: 0,0:26:30.04,0:26:30.75,Default,,0000,0000,0000,,vydrž!
Dialogue: 0,0:26:31.71,0:26:33.38,Default,,0000,0000,0000,,Ty jsi... Ze 7. čety...
Dialogue: 0,0:26:52.23,0:26:53.47,Default,,0000,0000,0000,,Proč jsi tady?
Dialogue: 0,0:26:54.91,0:26:55.71,Default,,0000,0000,0000,,Pomoz nám, prosím!
Dialogue: 0,0:26:56.29,0:26:58.99,Default,,0000,0000,0000,,Gallianská armáda...Dostali naše vojáky do pasti
Dialogue: 0,0:27:01.86,0:27:02.71,Default,,0000,0000,0000,,Ale jak?
Dialogue: 0,0:27:32.98,0:27:36.87,EdRom,,0000,0000,0000,,kowarete keshiki
Dialogue: 0,0:27:32.98,0:27:36.87,EdRus,,0000,0000,0000,,Ta shnilá scenérie
Dialogue: 0,0:27:38.05,0:27:42.50,EdRom,,0000,0000,0000,,nichi funde yuku kaze
Dialogue: 0,0:27:38.05,0:27:42.50,EdRus,,0000,0000,0000,,Jemně foukaná větrem
Dialogue: 0,0:27:43.55,0:27:47.26,EdRom,,0000,0000,0000,,katachinai mono ni
Dialogue: 0,0:27:43.55,0:27:47.26,EdRus,,0000,0000,0000,,A všechny beztvaré věcí
Dialogue: 0,0:27:47.59,0:27:53.42,EdRom,,0000,0000,0000,,aa... kyou suna ateiru
Dialogue: 0,0:27:47.59,0:27:53.42,EdRus,,0000,0000,0000,,Ach ... stát se písek ...
Dialogue: 0,0:27:54.81,0:27:58.69,EdRom,,0000,0000,0000,,yuuyake no iro
Dialogue: 0,0:27:54.81,0:27:58.69,EdRus,,0000,0000,0000,,V barvách západu slunce
Dialogue: 0,0:27:59.85,0:28:04.30,EdRom,,0000,0000,0000,,yasashiku egaita
Dialogue: 0,0:27:59.85,0:28:04.30,EdRus,,0000,0000,0000,,Váš jemný úsměv je barvený
Dialogue: 0,0:28:05.35,0:28:08.93,EdRom,,0000,0000,0000,,koukai to itami
Dialogue: 0,0:28:05.35,0:28:08.93,EdRus,,0000,0000,0000,,Ale lituje té bolesti
Dialogue: 0,0:28:09.34,0:28:10.65,EdRom,,0000,0000,0000,,mada
Dialogue: 0,0:28:09.34,0:28:10.65,EdRus,,0000,0000,0000,,Stále...
Dialogue: 0,0:28:10.79,0:28:14.06,EdRom,,0000,0000,0000,,shizuka ni furueteru
Dialogue: 0,0:28:10.79,0:28:14.06,EdRus,,0000,0000,0000,,Se mi mráz tiše
Dialogue: 0,0:28:16.57,0:28:21.57,EdRom,,0000,0000,0000,,nani ga tarinai no
Dialogue: 0,0:28:16.57,0:28:21.57,EdRus,,0000,0000,0000,,Co si mohu přát?
Dialogue: 0,0:28:21.68,0:28:26.10,EdRom,,0000,0000,0000,,mamoritaka atta yo
Dialogue: 0,0:28:21.68,0:28:26.10,EdRus,,0000,0000,0000,,Již jste mě chráníly
Dialogue: 0,0:28:27.15,0:28:30.73,EdRom,,0000,0000,0000,,namida ga denai houdou
Dialogue: 0,0:28:27.15,0:28:30.73,EdRus,,0000,0000,0000,,Nemám důvod plákat
Dialogue: 0,0:28:31.22,0:28:40.70,EdRom,,0000,0000,0000,,kanashii no ni mata furikaesu
Dialogue: 0,0:28:31.22,0:28:40.70,EdRus,,0000,0000,0000,,Ale smutek se vrací znovu a znovu!
Dialogue: 0,0:28:41.13,0:28:47.25,EdRom,,0000,0000,0000,,owaranai sekai yureru kibou
Dialogue: 0,0:28:41.13,0:28:47.25,EdRus,,0000,0000,0000,,Nekonečný svět může rozdrtit mé naděje,
Dialogue: 0,0:28:47.52,0:28:52.41,EdRom,,0000,0000,0000,,shiroi hane nakushitemo
Dialogue: 0,0:28:47.52,0:28:52.41,EdRus,,0000,0000,0000,,I můžu ztratit obě svá bílá křídla.
Dialogue: 0,0:28:52.71,0:28:58.40,EdRom,,0000,0000,0000,,hikari no asa wa kitto kuru yo
Dialogue: 0,0:28:52.71,0:28:58.40,EdRus,,0000,0000,0000,,Ale ranní světlo jistě přijde
Dialogue: 0,0:28:58.56,0:29:03.50,EdRom,,0000,0000,0000,,kotae wo mitsukeru
Dialogue: 0,0:28:58.56,0:29:03.50,EdRus,,0000,0000,0000,,A pomoz mi najít odpověď
Dialogue: 0,0:29:03.63,0:29:09.75,EdRom,,0000,0000,0000,,ima kimi no egao ni
