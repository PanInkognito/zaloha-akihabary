Dialogue: 0,0:00:11.45,0:00:18.37,loghtitle,D,0028,0448,0170,,{\a6\pos(182,192)}{\c&HFFFFFF&}LEGENDA
Dialogue: 0,0:00:11.45,0:00:18.37,loghtitle,D,0000,0000,0385,,{\a6\pos(328,368)}{\c&HFFFFFF&}O HRDINOCH GALAXIE
Dialogue: 0,0:01:34.35,0:01:39.05,def2,(null),0000,0000,0000,,{\c&HFFFF80&}Po dobytí pevnosti Iserlohn bol Yang Wenli povýšený na viceadmirála.
Dialogue: 0,0:01:39.05,0:01:42.34,def2,(null),0000,0000,0000,,{\c&HFFFF80&}Bol pozvaný na výročné stretnutie Vojenskej akadémie...
Dialogue: 0,0:01:42.34,0:01:46.14,def2,(null),0000,0000,0000,,{\c&HFFFF80&}...v Telnousene, druhom najväčšom meste planéty Heinessen.
Dialogue: 0,0:01:46.14,0:01:51.20,def2,(null),0000,0000,0000,,{\c&HFFFF80&}Na Telnousen má Yang veľa spomienok zo študentských čias na Akadémii.
Dialogue: 0,0:01:55.72,0:01:58.11,def2,(null),0000,0000,0000,,Sme tu, viceadmirál Yang.
Dialogue: 0,0:01:59.15,0:02:00.30,def2,(null),0000,0000,0000,,Dobre som si pospal.
Dialogue: 0,0:02:06.59,0:02:10.18,def2,(null),0000,0000,0000,,Nemáme presne určený program...
Dialogue: 0,0:02:10.18,0:02:12.08,def2,(null),0000,0000,0000,,...samozrejme s výnimkou ceremónie.
Dialogue: 0,0:02:12.08,0:02:14.67,def2,(null),0000,0000,0000,,Môžem ťa niekam vziať, ak budeš chcieť.
Dialogue: 0,0:02:14.67,0:02:16.97,def2,(null),0000,0000,0000,,O mňa si nerobte starosti.
Dialogue: 0,0:02:16.97,0:02:20.35,def2,(null),0000,0000,0000,,Okrem toho, nechceli ste sa s niekým stretnúť?
Dialogue: 0,0:02:20.35,0:02:21.56,def2,(null),0000,0000,0000,,Hej!
Dialogue: 0,0:02:25.65,0:02:27.05,def2,(null),0000,0000,0000,,Už je tu!
Dialogue: 0,0:02:27.05,0:02:28.44,def2,(null),0000,0000,0000,,To je on!
Dialogue: 0,0:02:29.34,0:02:30.54,def2,(null),0000,0000,0000,,Pozrite sa sem.
Dialogue: 0,0:02:37.13,0:02:39.12,def2,(null),0000,0000,0000,,Ste viceadmirál Yang Wenli?
Dialogue: 0,0:02:39.12,0:02:45.22,def2,(null),0000,0000,0000,,{\a6\fscx96.25\fscy97.5\pos(317,374)}Som Raymond Togliatti, predseda Národnej mierovej asociácie v oblasti Telnousen.
Dialogue: 0,0:02:39.06,0:02:43.35,mactitle,(null),0000,0000,0322,,{\a6\c&HFFFFFF&\fscx77.5\fscy71.25\pos(312,436)}Raymond Togliatti
Dialogue: 0,0:02:46.72,0:02:48.49,def2,(null),0000,0000,0000,,Teší ma.
Dialogue: 0,0:02:48.49,0:02:50.60,def2,(null),0000,0000,0000,,Rád vás poznávam.
Dialogue: 0,0:02:50.60,0:02:54.78,def2,(null),0000,0000,0000,,Vážení, toto je Hrdina z El-Facilu! Zázrak z Iserlohnu!
Dialogue: 0,0:02:54.78,0:02:56.59,def2,(null),0000,0000,0000,,Yang-kúzelník!
Dialogue: 0,0:02:56.59,0:02:59.68,def2,(null),0000,0000,0000,,Predstavujem vám viceadmirála Yang Wenliho!
Dialogue: 0,0:03:00.77,0:03:03.07,def2,(null),0000,0000,0000,,Viceadmirál... kytica!
Dialogue: 0,0:03:12.35,0:03:15.54,def2,(null),0000,0000,0000,,Vážení, toto dieťa je vojnová sirota.
Dialogue: 0,0:03:15.54,0:03:19.03,def2,(null),0000,0000,0000,,Jej otca zabili v bitke pri Astarte.
Dialogue: 0,0:03:19.03,0:03:24.32,def2,(null),0000,0000,0000,,Obetoval svoj život za mier a slobodu svojej vlasti.
Dialogue: 0,0:03:24.32,0:03:28.40,def2,(null),0000,0000,0000,,Existuje nejaký spôsob, ako si uctiť pamiatku jej otca?
Dialogue: 0,0:03:28.40,0:03:29.49,def2,(null),0000,0000,0000,,Existuje!
Dialogue: 0,0:03:29.49,0:03:32.98,def2,(null),0000,0000,0000,,Bojovať až do úplnej porážky Impéria!
Dialogue: 0,0:03:32.98,0:03:37.47,def2,(null),0000,0000,0000,,Zástancovia takzvanej Mierovej strany usilujú o mierovú dohodu s Impériom.
Dialogue: 0,0:03:37.47,0:03:41.58,def2,(null),0000,0000,0000,,Myslia si, že spolunažívanie s despotickým štátom je možné.
Dialogue: 0,0:03:41.58,0:03:44.57,def2,(null),0000,0000,0000,,Ale to je iba zbožné prianie!
Dialogue: 0,0:03:44.57,0:03:48.06,def2,(null),0000,0000,0000,,Tvrdenia protivojnovej strany o Impériu sú nepravdivé.
Dialogue: 0,0:03:48.06,0:03:50.46,def2,(null),0000,0000,0000,,Sme slobodný národ.
Dialogue: 0,0:03:50.46,0:03:53.25,def2,(null),0000,0000,0000,,Preto naša Aliancia toleruje opozíciu voči národnej politike.
Dialogue: 0,0:03:53.25,0:03:55.34,def2,(null),0000,0000,0000,,Ale Mierová strana je naivná.
Dialogue: 0,0:03:55.34,0:03:57.44,def2,(null),0000,0000,0000,,Je to len banda rozmaznaných detí.
Dialogue: 0,0:03:57.44,0:03:59.14,def2,(null),0000,0000,0000,,Presne tak! Presne tak!
Dialogue: 0,0:03:59.14,0:04:02.24,def2,(null),0000,0000,0000,,V nadchádzajúcich telnousenských voľbách...
Dialogue: 0,0:04:02.24,0:04:06.53,def2,(null),0000,0000,0000,,...voľte mňa, Raymonda Togliattiho.
Dialogue: 0,0:04:06.53,0:04:08.82,def2,(null),0000,0000,0000,,Máte slobodnú voľbu...
Dialogue: 0,0:04:08.82,0:04:10.82,def2,(null),0000,0000,0000,,...podieľať sa na našom zajtrajšom víťazstve.
Dialogue: 0,0:04:10.82,0:04:12.51,def2,(null),0000,0000,0000,,Nech žije Aliancia!
Dialogue: 0,0:04:12.51,0:04:14.51,def2,(null),0000,0000,0000,,Nech žije Republika!
Dialogue: 0,0:04:14.51,0:04:16.21,def2,(null),0000,0000,0000,,Smrť Impériu!
Dialogue: 0,0:04:16.21,0:04:17.21,def2,(null),0000,0000,0000,,Nech žije Aliancia!
Dialogue: 0,0:04:17.21,0:04:18.70,def2,(null),0000,0000,0000,,Nech žije Republika!
Dialogue: 0,0:04:18.70,0:04:20.49,def2,(null),0000,0000,0000,,Smrť Impériu!
Dialogue: 0,0:04:20.49,0:04:22.00,def2,(null),0000,0000,0000,,{\pos(324,441)}Togliatti !\N
Dialogue: 0,0:04:22.00,0:04:23.98,def2,(null),0000,0000,0000,,Togliatti !
Dialogue: 0,0:04:23.98,0:04:27.28,def2,(null),0000,0000,0000,,Ďakujem vám. Všetkým vám ďakujem.
Dialogue: 0,0:04:30.97,0:04:32.56,def2,(null),0000,0000,0000,,Som v šoku.
Dialogue: 0,0:04:32.56,0:04:36.95,def2,(null),0000,0000,0000,,Nečakal som, že ma na takom mieste zatiahnu do politickej šaškárne.
Dialogue: 0,0:04:38.65,0:04:42.44,def2,(null),0000,0000,0000,,Vyzerá to, ako by ste sem prišli podporiť toho vojnového agitátora.
Dialogue: 0,0:04:42.44,0:04:45.14,def2,(null),0000,0000,0000,,O to presne tým darebákom šlo.
Dialogue: 0,0:04:45.14,0:04:47.14,def2,(null),0000,0000,0000,,Akým darebákom?
Dialogue: 0,0:04:47.14,0:04:49.83,def2,(null),0000,0000,0000,,Tým, ktorí ma sem poslali.
Dialogue: 0,0:04:49.83,0:04:52.72,def2,(null),0000,0000,0000,,Stavím sa, že v tom má prsty Truhnit.
Dialogue: 0,0:04:52.72,0:04:55.42,def2,(null),0000,0000,0000,,Minister obrany by niečo také urobil?
Dialogue: 0,0:04:55.42,0:04:58.41,def2,(null),0000,0000,0000,,Hej. Mal som byť obozretnejší.
Dialogue: 0,0:04:58.41,0:05:01.11,def2,(null),0000,0000,0000,,Na Akadémii mi neustále hrozilo, že ma vyhodia...
Dialogue: 0,0:05:01.11,0:05:05.72,def2,(null),0000,0000,0000,,...no a teraz ma pozvali na výročné stretnutie.
Dialogue: 0,0:05:07.00,0:05:11.49,def2,(null),0000,0000,0000,,To dievčatko nemalo radosť, že ma vidí.
Dialogue: 0,0:05:12.18,0:05:14.77,def2,(null),0000,0000,0000,,„Prečo si mi zabil otca?“
Dialogue: 0,0:05:14.77,0:05:17.87,def2,(null),0000,0000,0000,,Jej oči ma obviňovali.
Dialogue: 0,0:05:17.87,0:05:22.07,def2,(null),0000,0000,0000,,Ja, čo som prežil, na to nemám odpoveď.
Dialogue: 0,0:05:23.26,0:05:27.15,def2,(null),0000,0000,0000,,Čo je to za človeka, čo zatiahne také dieťa do svojej volebnej kampane?
Dialogue: 0,0:05:27.15,0:05:29.45,def2,(null),0000,0000,0000,,Prekliati politici!
Dialogue: 0,0:05:33.34,0:05:35.43,def2,(null),0000,0000,0000,,Viceadmirál, dáte si čaj?
Dialogue: 0,0:05:35.43,0:05:37.92,def2,(null),0000,0000,0000,,Hej, dal by som si ho s brandy.
Dialogue: 0,0:05:37.92,0:05:39.52,def2,(null),0000,0000,0000,,Pozriem sa.
Dialogue: 0,0:05:42.41,0:05:43.91,def2,(null),0000,0000,0000,,Ja otvorím.
Dialogue: 0,0:05:48.69,0:05:50.40,def2,(null),0000,0000,0000,,Kto ste?
Dialogue: 0,0:05:50.40,0:05:51.50,def2,(null),0000,0000,0000,,Prestaňte!
Dialogue: 0,0:05:52.69,0:05:54.89,def2,(null),0000,0000,0000,,Do čerta s tebou, Yang Wenli!
Dialogue: 0,0:05:58.88,0:06:01.08,def2,(null),0000,0000,0000,,Aký Hrdina z El-Facil!
Dialogue: 0,0:06:01.08,0:06:03.67,def2,(null),0000,0000,0000,,Vojnový štváč je to!
Dialogue: 0,0:06:09.86,0:06:11.72,def2,(null),0000,0000,0000,,Prestaňte!
Dialogue: 0,0:06:17.84,0:06:18.50,def2,(null),0000,0000,0000,,Ahoj.
Dialogue: 0,0:06:18.50,0:06:21.05,mactitle,(null),0000,0000,0347,,{\a6\pos(314,431)\fscx77.5\fscy76.25}{\c&HFFFFFF&}Jessica Edwards
Dialogue: 0,0:06:20.64,0:06:22.83,def2,(null),0000,0000,0347,,{\a6\pos(313,400)}{\c&HFFFF80&}Ochranka! Čo sa tu deje?
Dialogue: 0,0:06:28.17,0:06:29.27,def2,(null),0000,0000,0000,,Prepáčte.
Dialogue: 0,0:06:29.27,0:06:33.26,def2,(null),0000,0000,0000,,Máme tu párty a niekto sa opil.
Dialogue: 0,0:06:33.26,0:06:36.06,def2,(null),0000,0000,0000,,Dospelí sa niekedy nevedia ovládať.
Dialogue: 0,0:06:36.06,0:06:37.30,def2,(null),0000,0000,0000,,Vďaka. Dovidenia.
Dialogue: 0,0:06:43.13,0:06:44.63,def2,(null),0000,0000,0000,,Bolí to?
Dialogue: 0,0:06:44.63,0:06:46.92,def2,(null),0000,0000,0000,,Nie. Nič to nie je.
Dialogue: 0,0:06:47.92,0:06:50.02,def2,(null),0000,0000,0000,,Je mi to veľmi ľúto.
Dialogue: 0,0:06:50.02,0:06:54.37,def2,(null),0000,0000,0000,,Keď som sa dozvedela, čo majú v pláne, ponáhľala som sa sem.
Dialogue: 0,0:06:55.01,0:06:58.00,def2,(null),0000,0000,0000,,Prišla si v pravú chvíľu.
Dialogue: 0,0:06:58.00,0:07:02.49,def2,(null),0000,0000,0000,,Len o niečo neskôr a zajtra ráno by som sa s takou tvárou na ceremónii nemohol ukázať.
Dialogue: 0,0:07:02.49,0:07:04.29,def2,(null),0000,0000,0000,,Ty tam chceš naozaj ísť?
Dialogue: 0,0:07:04.29,0:07:06.29,def2,(null),0000,0000,0000,,Rozkazy zhora.
Dialogue: 0,0:07:06.29,0:07:09.08,def2,(null),0000,0000,0000,,Je to súčasť mojej práce.
Dialogue: 0,0:07:09.08,0:07:10.18,def2,(null),0000,0000,0000,,Chápem.
Dialogue: 0,0:07:10.18,0:07:13.27,def2,(null),0000,0000,0000,,Ale dnešok sa môže opakovať.
Dialogue: 0,0:07:13.27,0:07:14.65,def2,(null),0000,0000,0000,,Dúfam, že chápeš.
Dialogue: 0,0:07:15.57,0:07:16.87,def2,(null),0000,0000,0000,,Áno.
Dialogue: 0,0:07:16.87,0:07:24.15,def2,(null),0000,0000,0000,,Naša protivojnová strana má v špeciálnych voľbách svojho kandidáta.
Dialogue: 0,0:07:24.15,0:07:26.84,def2,(null),0000,0000,0000,,Naše hnutie silnie.
Dialogue: 0,0:07:26.84,0:07:29.13,def2,(null),0000,0000,0000,,Mali sme šancu na víťazstvo.
Dialogue: 0,0:07:29.13,0:07:31.84,def2,(null),0000,0000,0000,,A potom si prišiel ty.
Dialogue: 0,0:07:31.84,0:07:34.63,def2,(null),0000,0000,0000,,Nech už to bolo, ako chce, tvoje potriasanie rukou spravilo dojem.
Dialogue: 0,0:07:34.63,0:07:39.17,def2,(null),0000,0000,0000,,Bolo to v televízii a vo všetkých novinách.
Dialogue: 0,0:07:40.22,0:07:43.91,def2,(null),0000,0000,0000,,Nevieš si predstaviť tú paniku na volebných centrálach.
Dialogue: 0,0:07:43.91,0:07:49.00,def2,(null),0000,0000,0000,,Včerajšie tesné prvenstvo sa hneď obrátilo.
Dialogue: 0,0:07:49.00,0:07:52.68,def2,(null),0000,0000,0000,,Napokon, si „Zázračný Yang.“
Dialogue: 0,0:07:53.68,0:07:56.28,def2,(null),0000,0000,0000,,Chápem, prečo sú tvoji priatelia naštvaní.
Dialogue: 0,0:07:56.28,0:07:58.67,def2,(null),0000,0000,0000,,Bolo to odo mňa nepremyslené.
Dialogue: 0,0:07:58.67,0:08:02.46,def2,(null),0000,0000,0000,,Ak je to tak, potom tam zajtra nechoď!
Dialogue: 0,0:08:02.46,0:08:03.97,def2,(null),0000,0000,0000,,Jessica...
Dialogue: 0,0:08:05.26,0:08:06.56,def2,(null),0000,0000,0000,,To nejde.
Dialogue: 0,0:08:06.56,0:08:09.45,def2,(null),0000,0000,0000,,Taký je verejný život.
Dialogue: 0,0:08:09.45,0:08:11.27,def2,(null),0000,0000,0000,,Aj Jean to povedal.
Dialogue: 0,0:08:12.04,0:08:15.33,def2,(null),0000,0000,0000,,Úprimne sa ti ospravedlňujem za činy svojich priateľov.
Dialogue: 0,0:08:15.33,0:08:18.03,def2,(null),0000,0000,0000,,Som ti vďačná, že si si to nechal pre seba.
Dialogue: 0,0:08:18.03,0:08:19.27,def2,(null),0000,0000,0000,,Maj sa.
Dialogue: 0,0:08:22.22,0:08:23.37,def2,(null),0000,0000,0000,,Jessica...
Dialogue: 0,0:08:26.22,0:08:28.41,def2,(null),0000,0000,0000,,Ale nič.
Dialogue: 0,0:08:28.41,0:08:30.70,def2,(null),0000,0000,0000,,Zbohom.
Dialogue: 0,0:08:39.28,0:08:41.87,def2,(null),0000,0000,0000,,Viceadmirál Yang, ona odišla.
Dialogue: 0,0:08:41.87,0:08:42.87,def2,(null),0000,0000,0000,,Viem.
Dialogue: 0,0:08:42.87,0:08:45.67,def2,(null),0000,0000,0000,,Prečo ste jej nepovedali...
Dialogue: 0,0:08:45.67,0:08:47.56,def2,(null),0000,0000,0000,,...že ste sem prišli kvôli nej?
Dialogue: 0,0:08:47.56,0:08:48.36,def2,(null),0000,0000,0000,,Julian ?
Dialogue: 0,0:08:49.56,0:08:51.66,def2,(null),0000,0000,0000,,Je už čaj hotový?
Dialogue: 0,0:09:07.41,0:09:09.51,def2,(null),0000,0000,0000,,Čo povieš? Dobrá reštaurácia, nie?
Dialogue: 0,0:09:09.51,0:09:11.69,def2,(null),0000,0000,0000,,Byť viceadmirálom je úžasné.
Dialogue: 0,0:09:11.69,0:09:13.50,def2,(null),0000,0000,0000,,Je to výborné!
Dialogue: 0,0:09:13.50,0:09:16.99,def2,(null),0000,0000,0000,,Ale môžete si to dovoliť?
Dialogue: 0,0:09:17.87,0:09:20.89,def2,(null),0000,0000,0000,,Chodil som sem často, ešte keď som chodil na Akadémiu.
Dialogue: 0,0:09:20.89,0:09:22.28,def2,(null),0000,0000,0000,,Odvtedy to tu nezdraželo.
Dialogue: 0,0:09:22.28,0:09:24.38,def2,(null),0000,0000,0000,,Viceadmirál Yang?
Dialogue: 0,0:09:24.38,0:09:26.08,def2,(null),0000,0000,0000,,To som ja.
Dialogue: 0,0:09:26.08,0:09:30.97,def2,(null),0000,0000,0000,,Tieto dámy by chceli váš autogram.
Dialogue: 0,0:09:30.97,0:09:34.46,def2,(null),0000,0000,0000,,Prepáčte, ale dnes som tu súkromne.
Dialogue: 0,0:09:34.46,0:09:37.57,def2,(null),0000,0000,0000,,Ale viceadmirál...
Dialogue: 0,0:09:41.35,0:09:43.57,def2,(null),0000,0000,0000,,Veľmi pekne vám ďakujem.
Dialogue: 0,0:09:59.31,0:10:00.70,def2,(null),0000,0000,0000,,Nech žije viceadmirál Yang!
Dialogue: 0,0:10:05.00,0:10:06.59,def2,(null),0000,0000,0000,,Pôjdeme?
Dialogue: 0,0:10:06.59,0:10:07.75,def2,(null),0000,0000,0000,,Hej.
Dialogue: 0,0:10:10.49,0:10:14.57,def2,(null),0000,0000,0000,,Víťazstvo v týchto voľbách bude asi veľmi tesné.
Dialogue: 0,0:10:14.57,0:10:18.46,def2,(null),0000,0000,0000,,Taká škoda. Odhliadnuc od portrétovaného to má stále umeleckú hodnotu.
Dialogue: 0,0:10:29.24,0:10:30.53,def2,(null),0000,0000,0000,,Bitka?
Dialogue: 0,0:10:30.53,0:10:32.02,def2,(null),0000,0000,0000,,Vyzerá to tak.
Dialogue: 0,0:10:34.43,0:10:36.13,def2,(null),0000,0000,0000,,Vlasteneckí rytieri!
Dialogue: 0,0:10:37.12,0:10:38.62,def2,(null),0000,0000,0322,,{\a6\pos(313,403)}Prestaňte!\NČo to robíte?
Dialogue: 0,0:10:38.62,0:10:39.92,def2,(null),0000,0000,0000,,Ústup!
Dialogue: 0,0:10:43.11,0:10:45.31,def2,(null),0000,0000,0000,,Si v poriadku?
Dialogue: 0,0:10:45.31,0:10:49.40,def2,(null),0000,0000,0000,,Arnold Street... Základňa Mierovej strany...
Dialogue: 0,0:10:49.40,0:10:51.40,def2,(null),0000,0000,0000,,V poriadku. Drž sa ma.
Dialogue: 0,0:10:51.40,0:10:52.69,def2,(null),0000,0000,0000,,Julian, pomôž mi.
Dialogue: 0,0:10:52.69,0:10:53.57,def2,(null),0000,0000,0000,,Hneď!
Dialogue: 0,0:11:15.25,0:11:17.33,def2,(null),0000,0000,0000,,Julian, vráť sa do hotela.
Dialogue: 0,0:11:17.33,0:11:18.94,def2,(null),0000,0000,0000,,A neponocuj.
Dialogue: 0,0:11:50.27,0:11:51.77,def2,(null),0000,0000,0000,,Yang Wenli ?!
Dialogue: 0,0:11:52.76,0:11:53.56,def2,(null),0000,0000,0000,,Peter !
Dialogue: 0,0:11:53.56,0:11:55.66,def2,(null),0000,0000,0000,,Čo si mu spravil?
Dialogue: 0,0:11:55.66,0:11:57.15,def2,(null),0000,0000,0000,,Nerobte unáhlené závery!
Dialogue: 0,0:11:57.15,0:11:59.04,def2,(null),0000,0000,0000,,Ja som mu to nespravil!
Dialogue: 0,0:12:00.19,0:12:01.59,def2,(null),0000,0000,0000,,Yang !
Dialogue: 0,0:12:01.59,0:12:04.92,def2,(null),0000,0000,0000,,Zaútočili naňho Vlasteneckí rytieri.
Dialogue: 0,0:12:04.92,0:12:07.25,def2,(null),0000,0000,0000,,Je dobitý, no nemá nič zlomené.
Dialogue: 0,0:12:13.77,0:12:17.36,def2,(null),0000,0000,0000,,Viceadmirál Yang, zachránili ste jedného z našich aktivistov.
Dialogue: 0,0:12:17.36,0:12:20.12,def2,(null),0000,0000,0000,,Chcem sa vám v jeho mene poďakovať.
Dialogue: 0,0:12:20.12,0:12:20.86,def2,(null),0000,0000,0000,,To nestojí za reč.
Dialogue: 0,0:12:20.86,0:12:23.45,def2,(null),0000,0000,0349,,{\a6\pos(323,436)}Som James Thorndike.
Dialogue: 0,0:12:23.45,0:12:24.92,def2,(null),0000,0000,0000,,Yang Wenli.
Dialogue: 0,0:12:26.04,0:12:27.34,def2,(null),0000,0000,0000,,Prestaň s tým!
Dialogue: 0,0:12:27.34,0:12:28.24,def2,(null),0000,0000,0000,,Jessica...
Dialogue: 0,0:12:28.24,0:12:32.82,def2,(null),0000,0000,0000,,Ak zverejníme tieto fotky, umlčíme tým vojnových štváčov.
Dialogue: 0,0:12:32.82,0:12:35.62,def2,(null),0000,0000,0000,,My nie sme ako oni.
Dialogue: 0,0:12:35.62,0:12:38.11,def2,(null),0000,0000,0000,,Nepoužívane špinavé taktiky.
Dialogue: 0,0:12:38.11,0:12:43.19,def2,(null),0000,0000,0000,,Okrem toho, viceadmirál Yang sa nerád necháva využívať politikmi.
Dialogue: 0,0:12:45.40,0:12:49.09,def2,(null),0000,0000,0000,,Ten Thorndike vyzerá ako správny chlap.
Dialogue: 0,0:12:49.09,0:12:56.37,def2,(null),0000,0000,0000,,Pridal sa k protivojnovému hnutiu po tom, čo prišiel o troch synov v druhom útoku na Iserlohn.
Dialogue: 0,0:12:56.37,0:12:58.07,def2,(null),0000,0000,0000,,Je to mierumilovný človek...
Dialogue: 0,0:12:58.07,0:13:00.20,def2,(null),0000,0000,0000,,...no vojnu nenávidí z hĺbky srdca.
Dialogue: 0,0:13:04.56,0:13:06.05,def2,(null),0000,0000,0000,,Spomínaš si, Yang?
Dialogue: 0,0:13:06.05,0:13:07.65,def2,(null),0000,0000,0000,,Hej.
Dialogue: 0,0:13:07.65,0:13:13.04,def2,(null),0000,0000,0000,,Kedykoľvek tadiaľto prechádzam, zmocní sa ma smútok.
Dialogue: 0,0:13:15.94,0:13:18.13,def2,(null),0000,0000,0000,,Pôjdeme dnu?
Dialogue: 0,0:13:18.13,0:13:19.23,def2,(null),0000,0000,0000,,Zvládneš to?
Dialogue: 0,0:13:19.23,0:13:22.22,def2,(null),0000,0000,0000,,Áno, lebo dnes nie som sama.
Dialogue: 0,0:13:24.72,0:13:26.31,def2,(null),0000,0000,0000,,Aká irónia.
Dialogue: 0,0:13:26.31,0:13:29.91,def2,(null),0000,0000,0000,,Aj keď ste s Jeanom neznášali vojnu...
Dialogue: 0,0:13:29.91,0:13:31.90,def2,(null),0000,0000,0000,,...obaja ste skončili ako vojaci.
Dialogue: 0,0:13:31.90,0:13:34.80,def2,(null),0000,0000,0000,,Bolo na to veľa dôvodov.
Dialogue: 0,0:13:34.80,0:13:36.59,def2,(null),0000,0000,0000,,...veľmi veľa.
Dialogue: 0,0:14:07.06,0:14:08.76,def2,(null),0000,0000,0000,,Vedel som, že tu budeš.
Dialogue: 0,0:14:08.76,0:14:09.86,def2,(null),0000,0000,0000,,Čo má byť?
Dialogue: 0,0:14:09.86,0:14:11.45,def2,(null),0000,0000,0000,,Aké: „Čo má byť?“
Dialogue: 0,0:14:11.45,0:14:13.65,def2,(null),0000,0000,0000,,Zase si vynechal simulácie.
Dialogue: 0,0:14:13.65,0:14:16.24,def2,(null),0000,0000,0000,,Inštruktora skoro porazilo.
Dialogue: 0,0:14:16.24,0:14:19.34,def2,(null),0000,0000,0000,,Behanie mi ako vojenský výcvik úplne stačí.
Dialogue: 0,0:14:19.34,0:14:22.53,def2,(null),0000,0000,0000,,Okrem toho ma simulácie nebavia.
Dialogue: 0,0:14:22.53,0:14:27.52,def2,(null),0000,0000,0000,,Ty si nemyslíš, že chlapík, čo porazil Wideborna, bol vojenský génius?
Dialogue: 0,0:14:27.52,0:14:29.62,def2,(null),0000,0000,0000,,Mal skrátka šťastie.
Dialogue: 0,0:14:30.62,0:14:34.31,def2,(null),0000,0000,0000,,Pôvodne si chcel študovať históriu, ak sa nemýlim.
Dialogue: 0,0:14:34.31,0:14:36.90,def2,(null),0000,0000,0000,,Keby som mal peniaze, nebol by som tu.
Dialogue: 0,0:14:36.90,0:14:38.70,def2,(null),0000,0000,0000,,Ani ja.
Dialogue: 0,0:14:38.70,0:14:43.89,def2,(null),0000,0000,0000,,Ale ak som chcel na univerzitu, Dôstojnícka akadémia bola jediná možnosť.
Dialogue: 0,0:14:43.89,0:14:47.58,def2,(null),0000,0000,0000,,Pre nás, chudobných, je to požehnanie.
Dialogue: 0,0:14:47.58,0:14:52.46,def2,(null),0000,0000,0000,,No ak sa nestaneš vojakom, musíš zaplatiť školné.
Dialogue: 0,0:14:52.46,0:14:55.36,def2,(null),0000,0000,0000,,Niečo za niečo.
Dialogue: 0,0:14:55.36,0:14:58.15,def2,(null),0000,0000,0000,,To by stačilo. Poď so mnou.
Dialogue: 0,0:14:58.15,0:15:00.75,def2,(null),0000,0000,0000,,Hej, ešte som neskončil!
Dialogue: 0,0:15:00.75,0:15:03.13,def2,(null),0000,0000,0000,,Neboj sa, história ti neutečie.
Dialogue: 0,0:15:21.00,0:15:23.40,def2,(null),0000,0000,0000,,Čo povieš? Nie je nádherná?
Dialogue: 0,0:15:23.40,0:15:25.50,def2,(null),0000,0000,0000,,Je učiteľka hudby?
Dialogue: 0,0:15:25.50,0:15:27.49,def2,(null),0000,0000,0000,,Riaditeľova dcéra.
Dialogue: 0,0:15:27.49,0:15:29.19,def2,(null),0000,0000,0000,,Toho starého plešivca?
Dialogue: 0,0:15:29.19,0:15:31.68,def2,(null),0000,0000,0000,,Jablko padlo ďaleko od stromu.
Dialogue: 0,0:15:31.68,0:15:34.68,def2,(null),0000,0000,0000,,Chodí na hudobnú akadémiu.
Dialogue: 0,0:15:34.68,0:15:37.97,def2,(null),0000,0000,0000,,Dnes večer sa koná ples, sponzorovaný hudobnou školou.
Dialogue: 0,0:15:37.97,0:15:40.47,def2,(null),0000,0000,0000,,Je isté, že tam dnes bude.
Dialogue: 0,0:15:40.47,0:15:41.86,def2,(null),0000,0000,0000,,Ideš?
Dialogue: 0,0:15:41.86,0:15:43.16,def2,(null),0000,0000,0000,,Samozrejme.
Dialogue: 0,0:16:01.01,0:16:03.20,def2,(null),0000,0000,0000,,Teraz je naša šanca.
Dialogue: 0,0:16:06.40,0:16:07.50,def2,(null),0000,0000,0000,,Vyber si.
Dialogue: 0,0:16:07.50,0:16:08.52,def2,(null),0000,0000,0000,,Hlava.
Dialogue: 0,0:16:08.52,0:16:09.52,def2,timing???,0000,0000,0000,,Znak.
Dialogue: 0,0:16:11.09,0:16:12.49,def2,(null),0000,0000,0000,,Je mi ľúto, Jean.
Dialogue: 0,0:16:12.49,0:16:15.88,def2,(null),0000,0000,0000,,Uh... zatancujete si so mnou?
Dialogue: 0,0:16:15.88,0:16:19.78,def2,(null),0000,0000,0000,,Som kadet prvého ročníka Vojenskej akadémie, major histórie.
Dialogue: 0,0:16:19.78,0:16:21.67,def2,(null),0000,0000,0000,,Yang Wenli.
Dialogue: 0,0:16:21.67,0:16:24.37,def2,(null),0000,0000,0000,,Jessica Edwards.
Dialogue: 0,0:16:33.75,0:16:35.63,def2,(null),0000,0000,0000,,Prepáčte.
Dialogue: 0,0:16:37.83,0:16:38.44,def2,(null),0000,0000,0000,,Nechcel som...
Dialogue: 0,0:16:50.27,0:16:51.57,def2,(null),0000,0000,0000,,Prepáčte...
Dialogue: 0,0:16:54.41,0:16:57.19,def2,(null),0000,0000,0000,,Čas na striedačku, Yang.
Dialogue: 0,0:16:57.19,0:17:00.40,def2,(null),0000,0000,0000,,Som Jean Robert Lap.
Dialogue: 0,0:17:04.84,0:17:08.82,def2,(null),0000,0000,0000,,Asi som sa mal zapísať do tanečnej.
Dialogue: 0,0:17:30.87,0:17:34.67,def2,(null),0000,0000,0000,,Nikdy som sa nenaučil tancovať.
Dialogue: 0,0:17:36.86,0:17:39.76,def2,(null),0000,0000,0000,,Robila som sa nedostupnou.
Dialogue: 0,0:17:39.76,0:17:43.75,def2,(null),0000,0000,0000,,Myslela som, že ma prídeš ešte požiadať o tanec.
Dialogue: 0,0:17:43.75,0:17:47.54,def2,(null),0000,0000,0000,,Keby si ma znova požiadal, neodmietla by som.
Dialogue: 0,0:17:47.54,0:17:49.04,def2,(null),0000,0000,0000,,Ale...
Dialogue: 0,0:17:49.04,0:17:50.93,def2,(null),0000,0000,0000,,...ty si neprišiel.
Dialogue: 0,0:17:50.93,0:17:53.93,def2,(null),0000,0000,0000,,Pretože Jean bol do teba až po uši zamilovaný.
Dialogue: 0,0:17:53.93,0:17:56.22,def2,(null),0000,0000,0000,,Muži nesúperia so svojimi priateľmi?
Dialogue: 0,0:17:56.22,0:17:58.22,def2,(null),0000,0000,0000,,Hej, niečo také.
Dialogue: 0,0:18:02.61,0:18:03.71,def2,(null),0000,0000,0000,,Jessica...
Dialogue: 0,0:18:03.71,0:18:05.31,def2,(null),0000,0000,0000,,Prosím...
Dialogue: 0,0:18:05.31,0:18:07.65,def2,(null),0000,0000,0000,,...len na chvíľu...
Dialogue: 0,0:18:29.49,0:18:31.59,def2,(null),0000,0000,0000,,To bolo blízko volebnej základne!
Dialogue: 0,0:18:40.08,0:18:41.17,def2,(null),0000,0000,0000,,S dovolením.
Dialogue: 0,0:18:41.17,0:18:42.87,def2,(null),0000,0000,0000,,Prepáčte.
Dialogue: 0,0:18:55.43,0:18:57.03,def2,(null),0000,0000,0000,,To je strašné!
Dialogue: 0,0:19:14.70,0:19:18.49,def2,(null),0000,0000,0000,,Nesiete na pleciach budúcnosť Aliancie.
Dialogue: 0,0:19:18.49,0:19:22.18,def2,(null),0000,0000,0000,,Nech každý z vás nesie to posolstvo v srdci.
Dialogue: 0,0:19:22.18,0:19:24.37,def2,(null),0000,0000,0000,,Za našu slávnu minulosť Aliancie...
Dialogue: 0,0:19:28.96,0:19:31.15,def2,(null),0000,0000,0000,,Ste unavený, viceadmirál?
Dialogue: 0,0:19:31.15,0:19:33.64,def2,(null),0000,0000,0000,,Hej, bolo to únavné.
Dialogue: 0,0:19:54.11,0:19:55.90,def2,(null),0000,0000,0000,,Viceadmirál Yang!
Dialogue: 0,0:19:55.90,0:20:01.09,def2,(null),0000,0000,0000,,Čo si myslíte o včerajšom výbuchu na základni protivojenskej koalície?
Dialogue: 0,0:20:01.09,0:20:03.69,def2,(null),0000,0000,0000,,Je to rúhanie sa voči demokratickej vláde.
Dialogue: 0,0:20:03.69,0:20:05.68,def2,(null),0000,0000,0000,,Nič iné to nie je.
Dialogue: 0,0:20:05.68,0:20:09.68,def2,(null),0000,0000,0000,,Mnohí sa domnievajú, že to bola práca provojenskej strany.
Dialogue: 0,0:20:09.68,0:20:11.27,def2,(null),0000,0000,0000,,Aj ja si to myslím.
Dialogue: 0,0:20:11.27,0:20:12.67,def2,(null),0000,0000,0000,,Musím už ísť.
Dialogue: 0,0:20:12.67,0:20:15.86,def2,(null),0000,0000,0000,,Čo hovoríte na to...
Dialogue: 0,0:20:15.86,0:20:20.64,def2,(null),0000,0000,0000,,...že Jessica Edwards bude kandidovať namiesto Thornidkea?
Dialogue: 0,0:20:23.95,0:20:25.93,def2,(null),0000,0000,0000,,Naozaj?
Dialogue: 0,0:20:30.13,0:20:35.92,def2,(null),0000,0000,0000,,{\c&HFFFF80&}Chcem sa spýtať ľudí, čo majú moc...
Dialogue: 0,0:20:35.92,0:20:38.71,def2,(null),0000,0000,0000,,{\c&HFFFF80&}Kde ste boli?
Dialogue: 0,0:20:38.71,0:20:44.60,def2,(null),0000,0000,0000,,{\c&HFFFF80&}Kde ste boli a čo ste robili, zatiaľ čo ste posielali mladých vojakov na smrť?
Dialogue: 0,0:20:45.49,0:20:47.88,def2,(null),0000,0000,0000,,{\c&HFFFF80&}Výsledky výnimočných volieb na Thelnousene...
Dialogue: 0,0:20:47.88,0:20:52.28,def2,(null),0000,0000,0000,,{\c&HFFFF80&}....ukazujú výrazné víťazstvo Jessicy Edwards.
Dialogue: 0,0:20:52.28,0:20:58.97,def2,(null),0000,0000,0000,,{\c&HFFFF80&}Slečna Jessica Edwards vyhrala s počtom 80% platných hlasov.
Dialogue: 0,0:20:58.97,0:21:03.40,def2,(null),0000,0000,0000,,{\c&HFFFF80&}Hovorí sa, väčšina stúpencov slečny Edwardsovej pochádza z radov vojenských odporcov.
Dialogue: 0,0:21:03.40,0:21:07.50,mactitle,(null),0000,0000,0346,,{\c&HFFFFFF&\pos(322,470)\fscx67.5\fscy66.25}admirál Dwight Greenhill
Dialogue: 0,0:21:05.75,0:21:09.75,def2,(null),0000,0000,0346,,{\a6\pos(312,375)}Útok na Thorndikea sa im nevyplatil.
Dialogue: 0,0:21:09.75,0:21:12.43,def2,(null),0000,0000,0000,,Patriotickí rytieri konajú neuvážene.
Dialogue: 0,0:21:12.43,0:21:14.64,def2,(null),0000,0000,0000,,Kto myslíte, že za tým bol?
Dialogue: 0,0:21:14.64,0:21:17.33,def2,(null),0000,0000,0000,,Pravdepodobne pár hlupákov, čo konalo na vlastnú päsť.
Dialogue: 0,0:21:17.33,0:21:20.12,def2,(null),0000,0000,0000,,Truhnit ich má určite teraz na koberci.
Dialogue: 0,0:21:20.12,0:21:22.12,def2,(null),0000,0000,0000,,Viceadmirál Yang.
Dialogue: 0,0:21:22.12,0:21:25.41,def2,(null),0000,0000,0000,,Všetok personál je v zasadačke 21.
Dialogue: 0,0:21:25.41,0:21:26.71,def2,(null),0000,0000,0000,,Už idem.
Dialogue: 0,0:21:26.71,0:21:27.91,def2,(null),0000,0000,0000,,Frederica.
Dialogue: 0,0:21:27.91,0:21:31.60,def2,(null),0000,0000,0000,,Pozvi dnes viceadmirála Yanga k nám na večeru.
Dialogue: 0,0:21:32.17,0:21:33.20,def2,(null),0000,0000,0000,,Pane...
Dialogue: 0,0:21:33.20,0:21:35.09,def2,(null),0000,0000,0000,,Netvárte sa tak prekvapene.
Dialogue: 0,0:21:35.09,0:21:37.59,def2,(null),0000,0000,0000,,Máte snáď niečo proti mojej dcére?
Dialogue: 0,0:21:38.36,0:21:39.88,def2,(null),0000,0000,0000,,Nie, práve naopak!
Dialogue: 0,0:21:39.88,0:21:43.08,def2,(null),0000,0000,0000,,Ach, áno... Priveďte so sebou aj svojho chovanca...
Dialogue: 0,0:21:43.08,0:21:44.97,def2,(null),0000,0000,0000,,...Julian, je to tak?
Dialogue: 0,0:21:44.97,0:21:46.86,def2,(null),0000,0000,0000,,Áno, pane. Budeme sa tešiť.
Dialogue: 0,0:21:46.86,0:21:48.36,def2,(null),0000,0000,0000,,Dovidenia.
Dialogue: 0,0:21:53.25,0:21:54.64,def2,(null),0000,0000,0000,,{\c&HFFFFFF&}Jessica...
Dialogue: 0,0:21:54.64,0:21:56.84,def2,(null),0000,0000,0000,,{\c&HFFFFFF&}...bude to ťažké. Nevzdávaj sa.
Dialogue: 0,0:21:57.84,0:21:59.64,def2,(null),0000,0000,0000,,{\c&HFFFF80&} O týždeň neskôr...
Dialogue: 0,0:21:59.64,0:22:02.23,def2,(null),0000,0000,0000,,{\c&HFFFF80&}...najvyššia rada Aliancie slobodných planét schválila plán...
Dialogue: 0,0:22:02.23,0:22:07.02,def2,(null),0000,0000,0000,,{\c&HFFFF80&}...na inváziu imperiálneho územia spojeneckými silami.
