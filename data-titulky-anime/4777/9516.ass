Dialogue: 0,0:00:01.11,0:00:02.56,main,,0000,0000,0000,,Tak začneme.
Dialogue: 0,0:00:02.57,0:00:03.61,main,,0000,0000,0000,,Jdeme na to.
Dialogue: 0,0:00:04.19,0:00:05.98,main,,0000,0000,0000,,No tak už začněme!
Dialogue: 0,0:00:06.32,0:00:07.49,op1,,0000,0000,0000,,Okolo 3 cm!
Dialogue: 0,0:00:07.49,0:00:08.78,op1,,0000,0000,0000,,Je to snad málo?
Dialogue: 0,0:00:08.78,0:00:09.03,op1,,0000,0000,0000,,Jejda!
Dialogue: 0,0:00:09.03,0:00:10.86,op1,,0000,0000,0000,,Schované pod naší uniformou!
Dialogue: 0,0:00:10.87,0:00:11.91,op1,,0000,0000,0000,,Mám toho nechat?
Dialogue: 0,0:00:11.91,0:00:12.20,op1,,0000,0000,0000,,JEJ!
Dialogue: 0,0:00:12.20,0:00:13.28,op1,,0000,0000,0000,,Jít do toho?
Dialogue: 0,0:00:13.28,0:00:14.08,op1,,0000,0000,0000,,Snažit se?
Dialogue: 0,0:00:14.08,0:00:15.66,op1,,0000,0000,0000,,Vychutnat si každý okamžik. Jo!
Dialogue: 0,0:00:15.66,0:00:16.25,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.26,0:00:16.50,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:16.50,0:00:16.90,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.91,0:00:17.25,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:17.25,0:00:17.87,op1,,0000,0000,0000,,Jen mezi námi dvěma!
Dialogue: 0,0:00:17.87,0:00:20.58,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:00:22.56,0:00:23.36,logo,,0000,0000,0000,,{\move(108,218,202,218,0,0)}LUCKY
Dialogue: 0,0:00:23.36,0:00:25.08,logo,,0000,0000,0000,,{\pos(202,218)}LUCKY
Dialogue: 0,0:00:22.56,0:00:23.36,logo,,0000,0000,0000,,{\move(638,218,459,218,0,0)}STAR
Dialogue: 0,0:00:23.36,0:00:25.08,logo,,0000,0000,0000,,{\pos(459,218)}STAR
Dialogue: 0,0:00:25.51,0:00:26.09,op1,,0000,0000,0000,,Cítím se nějak nesvá.
Dialogue: 0,0:00:26.09,0:00:26.71,op1,,0000,0000,0000,,Co jsem to jedla?
Dialogue: 0,0:00:26.71,0:00:27.30,op1,,0000,0000,0000,,Mám tě hrozně ráda!
Dialogue: 0,0:00:27.30,0:00:27.51,op1,,0000,0000,0000,,Oops!
Dialogue: 0,0:00:27.51,0:00:28.55,op1,,0000,0000,0000,,Jedno z nich je špatně!
Dialogue: 0,0:00:28.55,0:00:29.30,op1,,0000,0000,0000,,Relaxovat!
Dialogue: 0,0:00:29.30,0:00:29.93,op1,,0000,0000,0000,,Nic nedělat!
Dialogue: 0,0:00:29.93,0:00:30.47,op1,,0000,0000,0000,,Flákat se!
Dialogue: 0,0:00:30.47,0:00:31.55,op1,,0000,0000,0000,,Už toho nechej!
Dialogue: 0,0:00:31.55,0:00:33.21,op1,,0000,0000,0000,,Je ta nová holka oblíbená?
Dialogue: 0,0:00:33.21,0:00:34.85,op1,,0000,0000,0000,,Já bych řekla, že je docela normální!
Dialogue: 0,0:00:34.85,0:00:36.35,op1,,0000,0000,0000,,To jsem jediná překvapená?
Dialogue: 0,0:00:36.35,0:00:36.66,op1,,0000,0000,0000,,Vepřové kousky!
Dialogue: 0,0:00:36.66,0:00:37.06,op1,,0000,0000,0000,,Skleněné nudle!
Dialogue: 0,0:00:37.06,0:00:37.39,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:00:37.39,0:00:38.18,op2,,0000,0000,0000,,DA-DA-DA-DA-DA!
Dialogue: 0,0:00:38.73,0:00:39.52,op1,,0000,0000,0000,,Ooh?
Dialogue: 0,0:00:39.52,0:00:40.31,op1,,0000,0000,0000,,Víš co tím myslím?
Dialogue: 0,0:00:39.89,0:00:40.73,op1up,,0000,0000,0000,,Moje brýle…
Dialogue: 0,0:00:40.73,0:00:41.81,op1up,,0000,0000,0000,,Kde mám brýle?
Dialogue: 0,0:00:41.90,0:00:43.86,op1,,0000,0000,0000,,Nedávno jsme hrála tu samou bojovku.
Dialogue: 0,0:00:42.48,0:00:43.86,op1up,,0000,0000,0000,,Bez nich nic nevidím…
Dialogue: 0,0:00:44.90,0:00:45.48,op2,,0000,0000,0000,,POM! POM!
Dialogue: 0,0:00:45.48,0:00:46.53,op1,,0000,0000,0000,,Roztleskávačky!
Dialogue: 0,0:00:46.53,0:00:48.07,op2,,0000,0000,0000,,DÁME SI SLADKOST!
Dialogue: 0,0:00:48.07,0:00:48.65,op2,,0000,0000,0000,,LA! LA!
Dialogue: 0,0:00:48.65,0:00:49.70,op1,,0000,0000,0000,,Párty začíná!
Dialogue: 0,0:00:49.70,0:00:51.28,op2,,0000,0000,0000,,MÁŠ TO V KRVI!
Dialogue: 0,0:00:51.28,0:00:51.86,op2,,0000,0000,0000,,ANO!
Dialogue: 0,0:00:51.87,0:00:52.87,op1,,0000,0000,0000,,Zanechává to dojem.
Dialogue: 0,0:00:52.87,0:00:54.32,op1,,0000,0000,0000,,Malé planetky.
Dialogue: 0,0:00:54.33,0:00:54.87,op1,,0000,0000,0000,,Narazili!
Dialogue: 0,0:00:54.87,0:00:55.49,op1,,0000,0000,0000,,A shořeli!
Dialogue: 0,0:00:55.49,0:00:55.87,op1,,0000,0000,0000,,Bylo to ohromné!
Dialogue: 0,0:00:55.87,0:00:56.62,op1,,0000,0000,0000,,Z plných plic křičíme!
Dialogue: 0,0:00:56.62,0:00:57.18,op1,,0000,0000,0000,,ZPÍVEJ A TANCUJ!
Dialogue: 0,0:00:57.54,0:00:58.83,op1,,0000,0000,0000,,Přiznej si to!
Dialogue: 0,0:00:58.83,0:01:03.29,op1,,0000,0000,0000,,Nakonec se stejně budeme smát!
Dialogue: 0,0:01:03.29,0:01:07.05,op1,,0000,0000,0000,,Protože mám na sobě námořnickou uniformu.
Dialogue: 0,0:01:07.05,0:01:09.92,op2,,0000,0000,0000,,To je ono!
Dialogue: 0,0:01:10.51,0:01:14.05,op1,,0000,0000,0000,,Je teprve pondělí a já už mám špatnou náladu.
Dialogue: 0,0:01:14.05,0:01:15.85,op1,,0000,0000,0000,,Co mám dělat?
Dialogue: 0,0:01:16.06,0:01:19.89,op1,,0000,0000,0000,,Raději si vezmu moji letní uniformu -
Dialogue: 0,0:01:19.89,0:01:22.77,op1,,0000,0000,0000,,- je rozkošná!
Dialogue: 0,0:01:23.02,0:01:24.19,op1,,0000,0000,0000,,Trojdílná uniforma!
Dialogue: 0,0:01:24.19,0:01:25.61,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:01:25.61,0:01:26.06,op1,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.07,0:01:26.48,op1,,0000,0000,0000,,Dej se do toho!
Dialogue: 0,0:01:26.48,0:01:26.86,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.86,0:01:27.48,op1,,0000,0000,0000,,SNAŽ SE!
Dialogue: 0,0:01:27.48,0:01:27.65,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:27.65,0:01:29.54,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:01:32.96,0:01:34.55,main,,0000,0000,0000,,Když se říká, že podzim je období čtení…
Dialogue: 0,0:01:34.55,0:01:37.26,main,,0000,0000,0000,,…rozhodla jsem se sama něco přečíst.
Dialogue: 0,0:01:37.84,0:01:38.84,main,,0000,0000,0000,,To je neobvyklé.
Dialogue: 0,0:01:39.18,0:01:41.14,main,,0000,0000,0000,,I když je to pro správnou věc…
Dialogue: 0,0:01:41.14,0:01:43.18,main,,0000,0000,0000,,…stojí to spoustu peněz.
Dialogue: 0,0:01:43.18,0:01:44.18,main,,0000,0000,0000,,Nemusíš si je kupovat.
Dialogue: 0,0:01:44.18,0:01:46.39,main,,0000,0000,0000,,Stačí si je půjčit v knihovně.
Dialogue: 0,0:01:46.39,0:01:48.27,main,,0000,0000,0000,,Je velká šance, že budou mít to co chceš.
Dialogue: 0,0:01:48.27,0:01:48.94,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:01:48.94,0:01:51.69,main,,0000,0000,0000,,Pochybuji, že by měli nějakou mangu.
Dialogue: 0,0:01:52.61,0:01:53.90,main,,0000,0000,0000,,Že prej čtení…
Dialogue: 0,0:01:54.82,0:01:56.49,main,,0000,0000,0000,,Říkat mange, čtení…
Dialogue: 0,0:01:55.07,0:01:58.06,whitebig,,0000,0000,0000,,{\frz-90\pos(399,197)}Zvláštní pocit
Dialogue: 0,0:01:58.34,0:02:01.89,distort_violet,,0000,0000,0000,,{\frz-90\pos(556,187)}Učí se na test
Dialogue: 0,0:01:59.74,0:02:01.49,main,,0000,0000,0000,,Nemůžu se soustředit…
Dialogue: 0,0:02:11.08,0:02:14.17,main,,0000,0000,0000,,Přestávky jsou přece důležité.
Dialogue: 0,0:02:14.32,0:02:14.91,main,,0000,0000,0000,,Tak.
Dialogue: 0,0:02:15.46,0:02:17.46,main,,0000,0000,0000,,Alespoň přijdu na jiné myšlenky.
Dialogue: 0,0:02:20.55,0:02:21.72,main,,0000,0000,0000,,To je Kuroi-sensei.
Dialogue: 0,0:02:22.43,0:02:23.18,main,,0000,0000,0000,,Hej!
Dialogue: 0,0:02:23.18,0:02:25.51,main,,0000,0000,0000,,Přestaň hrát a začni se učit!
Dialogue: 0,0:02:25.52,0:02:28.35,main,,0000,0000,0000,,Jestli budeš mít špatné známky tak se vážně naštvu!
Dialogue: 0,0:02:28.69,0:02:29.73,main,,0000,0000,0000,,Sakra!
Dialogue: 0,0:02:29.73,0:02:31.44,main,,0000,0000,0000,,Pořád mě sleduje!
Dialogue: 0,0:02:31.44,0:02:33.56,main,,0000,0000,0000,,Proč mi to dělá?
Dialogue: 0,0:02:33.90,0:02:37.19,main,,0000,0000,0000,,Najednou už ta hra není tak zábavná jak byla.
Dialogue: 0,0:02:38.95,0:02:43.41,main,,0000,0000,0000,,Jak jste se učila na test vy když jste studovala?
Dialogue: 0,0:02:44.95,0:02:47.04,main,,0000,0000,0000,,Když jsem se nemohla soustředit tak jsem hrála nebo spala.
Dialogue: 0,0:02:47.04,0:02:49.08,main,,0000,0000,0000,,Nic zvláštního.
Dialogue: 0,0:02:49.50,0:02:50.96,main,,0000,0000,0000,,Přesně jako já.
Dialogue: 0,0:02:51.58,0:02:53.71,main,,0000,0000,0000,,Ale teď jsem učitelka já, takže se podle toho musím chovat.
Dialogue: 0,0:02:54.00,0:02:55.42,main,,0000,0000,0000,,Sice to chápu…
Dialogue: 0,0:02:55.42,0:02:56.92,main,,0000,0000,0000,,…ale je to děsně nespravedlivé!
Dialogue: 0,0:03:00.05,0:03:04.05,main,,0000,0000,0000,,Tentokrát mám v plánu si udělat nějaký cíl a pomalu se k němu propracovávat.
Dialogue: 0,0:03:04.30,0:03:06.93,main,,0000,0000,0000,,Ségra, pomohla bys mi s tím čemu nerozumím?
Dialogue: 0,0:03:06.93,0:03:08.56,main,,0000,0000,0000,,Chceš se učit společně?
Dialogue: 0,0:03:12.60,0:03:15.61,main,,0000,0000,0000,,Takže v tomto případě bude "x" rovno…
Dialogue: 0,0:03:16.52,0:03:17.15,main,,0000,0000,0000,,Co je?
Dialogue: 0,0:03:17.15,0:03:20.15,main,,0000,0000,0000,,Právě jsem si uvědomila že začal můj pořad v televizi.
Dialogue: 0,0:03:20.65,0:03:21.70,main,,0000,0000,0000,,V dnešní epizodě…
Dialogue: 0,0:03:21.70,0:03:23.66,main,,0000,0000,0000,,se hlavní postava dostane do novin…
Dialogue: 0,0:03:23.66,0:03:26.62,main,,0000,0000,0000,,…a zpráva o jejím těhotenství se rychle rozšíří.
Dialogue: 0,0:03:27.54,0:03:29.04,main,,0000,0000,0000,,Takže se chceš dívat?
Dialogue: 0,0:03:29.83,0:03:30.29,main,,0000,0000,0000,,Ne, to je dobrý.
Dialogue: 0,0:03:30.29,0:03:31.41,main,,0000,0000,0000,,Nahrávám si to.
Dialogue: 0,0:03:44.34,0:03:45.18,main,,0000,0000,0000,,Um…
Dialogue: 0,0:03:47.60,0:03:50.47,main,,0000,0000,0000,,Pokaždé když nerozumím víc věcem zároveň se mi chce spát…
Dialogue: 0,0:03:50.48,0:03:52.23,main,,0000,0000,0000,,To znamená, že si pořád ospalá?
Dialogue: 0,0:03:53.44,0:03:56.02,main,,0000,0000,0000,,Možná bych to neměla přehánět hned ze začátku…
Dialogue: 0,0:03:56.02,0:03:58.15,main,,0000,0000,0000,,…a měla bych se jít vyspat.
Dialogue: 0,0:03:58.15,0:03:58.86,main,,0000,0000,0000,,Fajn.
Dialogue: 0,0:03:58.86,0:04:01.57,main,,0000,0000,0000,,Vidím že už začínáš ztrácet sílu a motivaci.
Dialogue: 0,0:04:02.99,0:04:05.82,main,,0000,0000,0000,,Pořád se nemůžu soustředit!
Dialogue: 0,0:04:06.03,0:04:06.57,main,,0000,0000,0000,,Fajn.
Dialogue: 0,0:04:07.62,0:04:09.79,main,,0000,0000,0000,,Nějaký tip jak se učit na test?
Dialogue: 0,0:04:09.79,0:04:12.58,main,,0000,0000,0000,,Napadlo mě jak asi studuješ ty.
Dialogue: 0,0:04:12.58,0:04:14.21,main,,0000,0000,0000,,No…
Dialogue: 0,0:04:14.21,0:04:17.88,main,,0000,0000,0000,,Pokud se snažíš až moc rychle něco pochopit tak je velká šance, že se brzo unavíš.
Dialogue: 0,0:04:17.88,0:04:21.30,main,,0000,0000,0000,,V mém případě prostě dělám jako vždy.
Dialogue: 0,0:04:21.67,0:04:24.72,main,,0000,0000,0000,,Na dnešní test si dost věřím.
Dialogue: 0,0:04:24.72,0:04:26.55,main,,0000,0000,0000,,Cítím se nad věcí.
Dialogue: 0,0:04:26.55,0:04:29.51,main,,0000,0000,0000,,Tvůj denní režim ale moc přesvědčivý není.
Dialogue: 0,0:04:29.51,0:04:30.81,main,,0000,0000,0000,,Nic jsi nedělala, co?
Dialogue: 0,0:04:31.73,0:04:33.77,main,,0000,0000,0000,,I přes to, že je tak roztomilá…
Dialogue: 0,0:04:34.19,0:04:36.69,main,,0000,0000,0000,,A měla i jed na jejich drápech.
Dialogue: 0,0:04:36.69,0:04:37.52,main,,0000,0000,0000,,Páni…
Dialogue: 0,0:04:37.52,0:04:40.27,main,,0000,0000,0000,,má i svou děsivou stránku.
Dialogue: 0,0:04:40.28,0:04:41.19,main,,0000,0000,0000,,Jo!
Dialogue: 0,0:04:41.90,0:04:42.57,main,,0000,0000,0000,,Kagami.
Dialogue: 0,0:04:42.57,0:04:43.61,main,,0000,0000,0000,,Jak to šlo?
Dialogue: 0,0:04:43.61,0:04:45.70,main,,0000,0000,0000,,Tenhle se mi dost nepodařil.
Dialogue: 0,0:04:45.70,0:04:47.91,main,,0000,0000,0000,,Ale můj průměr si držím.
Dialogue: 0,0:04:47.91,0:04:49.99,main,,0000,0000,0000,,Takže i tobě se občas nezadaří…
Dialogue: 0,0:04:49.99,0:04:53.12,main,,0000,0000,0000,,To proto, že mi před tím pomáhala.
Dialogue: 0,0:04:53.12,0:04:56.12,main,,0000,0000,0000,,A díky tomu jsem si vedla nadprůměrně.
Dialogue: 0,0:04:56.12,0:04:57.42,main,,0000,0000,0000,,Jak si na tom ty?
Dialogue: 0,0:04:57.92,0:05:00.79,main,,0000,0000,0000,,S podivem jsem si vedla špatně.
Dialogue: 0,0:05:00.80,0:05:02.02,main,,0000,0000,0000,,A to jsem si byla jistá, že to zmáknu.
Dialogue: 0,0:05:02.02,0:05:03.51,main,,0000,0000,0000,,Na tom není nic podivného.
Dialogue: 0,0:05:03.51,0:05:05.17,main,,0000,0000,0000,,To se dalo očekávat.
Dialogue: 0,0:05:05.18,0:05:06.34,main,,0000,0000,0000,,Ale víš…
Dialogue: 0,0:05:06.34,0:05:09.39,main,,0000,0000,0000,,myslela jsem si, že lidé se párují s těmi, kteří si sou podobní.
Dialogue: 0,0:05:10.06,0:05:10.89,main,,0000,0000,0000,,Tak například…
Dialogue: 0,0:05:10.89,0:05:14.60,main,,0000,0000,0000,,Kagami a Miyuki jsou obě předsedkyně třídy.
Dialogue: 0,0:05:14.60,0:05:17.39,main,,0000,0000,0000,,A Kagami a já máme obě rády hry.
Dialogue: 0,0:05:17.40,0:05:19.61,main,,0000,0000,0000,,I když jiné žánry.
Dialogue: 0,0:05:19.61,0:05:21.52,main,,0000,0000,0000,,Ty hraješ simulátory a RPG.
Dialogue: 0,0:05:21.52,0:05:23.07,main,,0000,0000,0000,,A já hraji střílečky.
Dialogue: 0,0:05:23.07,0:05:26.70,main,,0000,0000,0000,,Myslela jsem si, že já i Tsukasa jsme obě hloupé…
Dialogue: 0,0:05:26.70,0:05:28.41,main,,0000,0000,0000,,…ale tentokrát se mezi námi udělala velká propast.
Dialogue: 0,0:05:29.32,0:05:32.45,main,,0000,0000,0000,,No, i když jsem si tentokrát vedla špatně, tak mi to moc nevadí.
Dialogue: 0,0:05:32.45,0:05:33.54,main,,0000,0000,0000,,Mělo by!
Dialogue: 0,0:05:34.00,0:05:36.50,main,,0000,0000,0000,,Pokud na to budeš kašlat jen proto, že se to nepovedlo i ostatním…
Dialogue: 0,0:05:36.50,0:05:37.90,main,,0000,0000,0000,,… tak se ti to vymstí.
Dialogue: 0,0:05:41.04,0:05:41.54,main,,0000,0000,0000,,Vlastně…
Dialogue: 0,0:05:41.54,0:05:44.46,main,,0000,0000,0000,,Tentokrát jsem se s otcem nevsadila…
Dialogue: 0,0:05:44.46,0:05:46.34,main,,0000,0000,0000,,…takže mě to moc neštve.
Dialogue: 0,0:05:46.34,0:05:48.93,main,,0000,0000,0000,,Nedokážu si tě představit jako dospělou.
Dialogue: 0,0:05:49.26,0:05:50.84,main,,0000,0000,0000,,Když už kopíruješ Miyuki…
Dialogue: 0,0:05:50.85,0:05:52.47,main,,0000,0000,0000,,…mohla by sis začít psát poznámky…
Dialogue: 0,0:05:52.47,0:05:54.26,main,,0000,0000,0000,,…po celou dobu a ne jen před testy.
Dialogue: 0,0:05:54.27,0:05:56.64,main,,0000,0000,0000,,Miyuki-san je vážně metodická.
Dialogue: 0,0:05:56.64,0:05:58.77,main,,0000,0000,0000,,Je typ člověka A.
Dialogue: 0,0:05:59.65,0:06:01.31,main,,0000,0000,0000,,To mi lidé často říkají…
Dialogue: 0,0:06:01.31,0:06:03.57,main,,0000,0000,0000,,…ale ve skutečnosti jsem O.
Dialogue: 0,0:06:04.19,0:06:05.07,main,,0000,0000,0000,,Ty jsi O?
Dialogue: 0,0:06:05.07,0:06:06.24,main,,0000,0000,0000,,To by mě nikdy nenapadlo.
Dialogue: 0,0:06:07.11,0:06:07.44,main,,0000,0000,0000,,Jo, ale…
Dialogue: 0,0:06:10.45,0:06:15.07,smt_about_blood_BOOK,,0000,0000,0000,,{\frz+2\pos(204,311)}Průvodce krevními skupinami
Dialogue: 0,0:06:10.45,0:06:12.07,main,,0000,0000,0000,,Podle této knihy…
Dialogue: 0,0:06:12.08,0:06:15.16,main,,0000,0000,0000,,…mám spoustu vlastností které jsou typické…
Dialogue: 0,0:06:15.16,0:06:17.08,main,,0000,0000,0000,,…pro typ O.
Dialogue: 0,0:06:17.08,0:06:18.83,main,,0000,0000,0000,,Například…
Dialogue: 0,0:06:18.83,0:06:21.13,main,,0000,0000,0000,,Píšou že typ O je často…
Dialogue: 0,0:06:21.13,0:06:22.79,main,,0000,0000,0000,,…dětinský a nevinný…
Dialogue: 0,0:06:22.79,0:06:24.17,main,,0000,0000,0000,,…což na mě perfektně sedí.
Dialogue: 0,0:06:24.63,0:06:28.42,main,,0000,0000,0000,,Typ O si často vynutí svůj názor, a častou jsou lakomí.
Dialogue: 0,0:06:29.22,0:06:31.55,main,,0000,0000,0000,,Tohle bude asi Miyukina druhá strana.
Dialogue: 0,0:06:31.55,0:06:33.30,main,,0000,0000,0000,,Ty jsi nejspíš typ A.
Dialogue: 0,0:06:33.93,0:06:37.06,main,,0000,0000,0000,,Ale hodně lidí si mě plete s typem B.
Dialogue: 0,0:06:37.06,0:06:39.85,main,,0000,0000,0000,,Typ A jsou společenští lidé co věnují spoustu pozornosti svému okolí.
Dialogue: 0,0:06:39.85,0:06:42.23,main,,0000,0000,0000,,Hledají mezi lidmi spřízněné duše.
Dialogue: 0,0:06:42.56,0:06:43.86,main,,0000,0000,0000,,Je to vůbec správně?
Dialogue: 0,0:06:43.86,0:06:46.48,main,,0000,0000,0000,,Jsem přece všímavé ne?
Dialogue: 0,0:06:46.48,0:06:48.61,main,,0000,0000,0000,,Jen když jde o něco co tě zajímá.
Dialogue: 0,0:06:48.61,0:06:50.45,main,,0000,0000,0000,,Jsem citlivá a ohleduplná k ostatním.
Dialogue: 0,0:06:50.45,0:06:52.16,main,,0000,0000,0000,,Když jde o domácí úkoly.
Dialogue: 0,0:06:54.66,0:06:56.41,main,,0000,0000,0000,,A co ty Kagami?
Dialogue: 0,0:06:56.41,0:06:56.87,main,,0000,0000,0000,,Já?
Dialogue: 0,0:06:57.31,0:06:58.50,main,,0000,0000,0000,,Já jsem typ B.
Dialogue: 0,0:06:58.50,0:07:03.92,main,,0000,0000,0000,,Takže si bezstarostná, sprostá, naivní, urážlivá?
Dialogue: 0,0:07:03.92,0:07:05.09,main,,0000,0000,0000,,To má něco znamenat?
Dialogue: 0,0:07:05.09,0:07:06.75,main,,0000,0000,0000,,Tak potom je Tsukasa typ AB?
Dialogue: 0,0:07:07.17,0:07:08.13,main,,0000,0000,0000,,Jakto?
Dialogue: 0,0:07:08.13,0:07:08.84,main,,0000,0000,0000,,Proč?
Dialogue: 0,0:07:08.84,0:07:09.80,main,,0000,0000,0000,,Jen tak.
Dialogue: 0,0:07:10.18,0:07:12.01,main,,0000,0000,0000,,Když to vezmeme podle osobností…
Dialogue: 0,0:07:12.01,0:07:13.18,main,,0000,0000,0000,,tak by Tsukasa byla typ A…
Dialogue: 0,0:07:13.18,0:07:15.30,main,,0000,0000,0000,,…a Kagami typ B.
Dialogue: 0,0:07:15.64,0:07:17.81,main,,0000,0000,0000,,Obě jsme typ B.
Dialogue: 0,0:07:17.81,0:07:19.43,main,,0000,0000,0000,,Jsme dvojčata.
Dialogue: 0,0:07:19.43,0:07:20.35,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:07:20.35,0:07:21.44,main,,0000,0000,0000,,Úplně jsem zapomněla.
Dialogue: 0,0:07:21.44,0:07:23.31,main,,0000,0000,0000,,I když nejsme tak bezstarostné jako si ty.
Dialogue: 0,0:07:23.77,0:07:25.19,main,,0000,0000,0000,,Když mluvíme o horoskopech…
Dialogue: 0,0:07:25.19,0:07:28.03,main,,0000,0000,0000,,…taky jsem slyšela o jednom používající zvířata a sushi.
Dialogue: 0,0:07:28.03,0:07:28.99,newspaper,,0000,0000,0000,,{\pos(355,209)}Jaké je vaše nejoblíbenější? 
Dialogue: 0,0:07:28.49,0:07:28.99,main,,0000,0000,0000,,Sushi?
Dialogue: 0,0:07:31.15,0:07:31.70,main,,0000,0000,0000,,Tak se podíváme…
Dialogue: 0,0:07:31.70,0:07:32.49,main,,0000,0000,0000,,Miyuki-san je…
Dialogue: 0,0:07:33.82,0:07:35.12,main,,0000,0000,0000,,Takže Miyuki je lososová rolka.
Dialogue: 0,0:07:35.12,0:07:36.49,main,,0000,0000,0000,,I taky patří do vyšší třídy.
Dialogue: 0,0:07:36.87,0:07:38.91,main,,0000,0000,0000,,Miluji lososovou rolku!
Dialogue: 0,0:07:38.91,0:07:39.41,main,,0000,0000,0000,,A?
Dialogue: 0,0:07:39.41,0:07:40.75,main,,0000,0000,0000,,Co já a Tsukasa?
Dialogue: 0,0:07:43.63,0:07:44.96,main,,0000,0000,0000,,Okurková rolka.
Dialogue: 0,0:07:45.25,0:07:46.79,main,,0000,0000,0000,,Okurková?
Dialogue: 0,0:07:46.80,0:07:49.17,main,,0000,0000,0000,,No, myslím, že se to počítá jako sushi.
Dialogue: 0,0:07:49.17,0:07:50.55,main,,0000,0000,0000,,Je to docela přesné.
Dialogue: 0,0:07:50.55,0:07:52.26,main,,0000,0000,0000,,A co ty?
Dialogue: 0,0:07:52.26,0:07:53.18,main,,0000,0000,0000,,Já?
Dialogue: 0,0:07:55.80,0:07:56.43,main,,0000,0000,0000,,Krájený zázvor?
Dialogue: 0,0:07:58.18,0:07:59.10,main,,0000,0000,0000,,Píše se tam zázvor!
Dialogue: 0,0:08:00.89,0:08:02.39,main,,0000,0000,0000,,Nepodceňuj zázvor!
Dialogue: 0,0:08:02.39,0:08:03.10,main,,0000,0000,0000,,No…
Dialogue: 0,0:08:03.10,0:08:05.48,main,,0000,0000,0000,,Bez něj si sushi nedáš…
Dialogue: 0,0:08:05.48,0:08:08.77,main,,0000,0000,0000,,…a popis je pozitivní.
Dialogue: 0,0:08:09.36,0:08:10.32,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:08:10.32,0:08:12.86,main,,0000,0000,0000,,A okurková rolka se počítá jako sushi jo?
Dialogue: 0,0:08:15.41,0:08:15.82,main,,0000,0000,0000,,Ano?
Dialogue: 0,0:08:15.82,0:08:16.78,main,,0000,0000,0000,,Konato?
Dialogue: 0,0:08:16.78,0:08:18.70,main,,0000,0000,0000,,O tom našem dnešním rozhovoru o krevních skupinách…
Dialogue: 0,0:08:18.70,0:08:20.24,main,,0000,0000,0000,,Jen abys věděla…
Dialogue: 0,0:08:20.25,0:08:23.33,main,,0000,0000,0000,,Dvojčata můžou mít různou krevní skupinu…
Dialogue: 0,0:08:23.33,0:08:25.67,main,,0000,0000,0000,,…takže si se nepletla.
Dialogue: 0,0:08:25.67,0:08:28.25,main,,0000,0000,0000,,Zdá se mi, že jsi to vzala moc vážně.
Dialogue: 0,0:08:28.25,0:08:30.75,main,,0000,0000,0000,,Tak ti teda pěkně děkuji.
Dialogue: 0,0:08:30.76,0:08:32.09,main,,0000,0000,0000,,Jsi upřímný člověk.
Dialogue: 0,0:08:32.09,0:08:34.34,main,,0000,0000,0000,,Začínám v tobě vidět typ A.
Dialogue: 0,0:08:34.97,0:08:35.55,main,,0000,0000,0000,,A jo…
Dialogue: 0,0:08:35.55,0:08:38.64,main,,0000,0000,0000,,V TV byl pořad o psychologických testech.
Dialogue: 0,0:08:39.39,0:08:40.39,main,,0000,0000,0000,,Tyhle věci…
Dialogue: 0,0:08:40.39,0:08:45.56,main,,0000,0000,0000,,…se občas pletou a někdy ne…
Dialogue: 0,0:08:45.56,0:08:49.11,main,,0000,0000,0000,,…takže to vždy aspoň z části sedí.
Dialogue: 0,0:08:49.61,0:08:50.77,main,,0000,0000,0000,,Kagami…
Dialogue: 0,0:08:50.78,0:08:53.24,main,,0000,0000,0000,,Nesmíš ne na to dívat tak pragmaticky…
Dialogue: 0,0:08:53.24,0:08:55.28,main,,0000,0000,0000,,Tak si nikdy nic neužiješ.
Dialogue: 0,0:08:55.28,0:08:57.45,main,,0000,0000,0000,,Chovej se víc podle tvého věku.
Dialogue: 0,0:08:58.16,0:08:58.87,main,,0000,0000,0000,,Tsukasa.
Dialogue: 0,0:08:58.87,0:09:00.16,main,,0000,0000,0000,,Tentokrát se vážně snažila.
Dialogue: 0,0:09:02.50,0:09:05.50,main,,0000,0000,0000,,Kagami si vedla hůř než obvykle.
Dialogue: 0,0:09:06.83,0:09:09.79,main,,0000,0000,0000,,Ale i přes to má vyšší počet bodů…
Dialogue: 0,0:09:11.80,0:09:13.55,main,,0000,0000,0000,,Dobrá práce holky.
Dialogue: 0,0:09:14.76,0:09:15.76,main,,0000,0000,0000,,Jak to říct…
Dialogue: 0,0:09:15.76,0:09:17.72,main,,0000,0000,0000,,Dělali jste co jste mohli…
Dialogue: 0,0:09:17.72,0:09:18.34,main,,0000,0000,0000,,Ooh.
Dialogue: 0,0:09:18.35,0:09:19.60,main,,0000,0000,0000,,Tsukasa se snažila.
Dialogue: 0,0:09:20.47,0:09:22.72,main,,0000,0000,0000,,Ale i přes to má Kagami pořád větší počet bodů.
Dialogue: 0,0:09:22.72,0:09:23.93,main,,0000,0000,0000,,Hej, Matsuri.
Dialogue: 0,0:09:23.93,0:09:25.60,main,,0000,0000,0000,,Nemůžeš být víc taktní?
Dialogue: 0,0:09:28.52,0:09:29.98,main,,0000,0000,0000,,Dobré ráno!
Dialogue: 0,0:09:29.98,0:09:31.61,main,,0000,0000,0000,,Ah, dobré ráno.
Dialogue: 0,0:09:32.19,0:09:35.86,main,,0000,0000,0000,,Vy dvě jste ale šikovné mladé slečny.
Dialogue: 0,0:09:35.86,0:09:36.32,main,,0000,0000,0000,,Ale kdepak.
Dialogue: 0,0:09:36.32,0:09:37.32,main,,0000,0000,0000,,Jste moc milá.
Dialogue: 0,0:09:37.32,0:09:41.24,main,,0000,0000,0000,,Takové báječné známky na Ryououské střední…
Dialogue: 0,0:09:41.24,0:09:44.87,main,,0000,0000,0000,,Přála bych si aby mé děti měli zlomek vašich schopností.
Dialogue: 0,0:09:43.45,0:09:44.45,main2,,0000,0000,0000,,Je to jako…
Dialogue: 0,0:09:44.45,0:09:47.16,main2,,0000,0000,0000,,By mě vychvalovala za to, že jsem její sestra…
Dialogue: 0,0:09:44.87,0:09:45.50,main,,0000,0000,0000,,Ale ne.
Dialogue: 0,0:09:45.50,0:09:47.16,main,,0000,0000,0000,,Zase tak dobře si nevedeme.
Dialogue: 0,0:09:47.17,0:09:49.12,main,,0000,0000,0000,,Takže mě to ani moc netěší.
Dialogue: 0,0:09:49.12,0:09:51.99,copyrights,,0000,0000,0000,,{\pos(85,50)}Překlad: paralaX
Dialogue: 0,0:09:49.12,0:09:51.99,copyrights,,0000,0000,0000,,{\pos(169,76)}Domovská stránka: www.otakogu.cz
Dialogue: 0,0:09:54.30,0:09:55.30,main,,0000,0000,0000,,Teď když máme po testech.
Dialogue: 0,0:09:55.30,0:09:57.38,main,,0000,0000,0000,,Co kdyby jsme si někam vyrazili?
Dialogue: 0,0:09:57.97,0:09:58.30,main,,0000,0000,0000,,To zní dobře.
Dialogue: 0,0:09:59.01,0:10:01.18,main,,0000,0000,0000,,Co třeba se podívat na film?
Dialogue: 0,0:10:01.18,0:10:03.06,main,,0000,0000,0000,,Co právě dávají?
Dialogue: 0,0:10:03.18,0:10:05.10,main,,0000,0000,0000,,Právě hrají jeden vážně dobrý film.
Dialogue: 0,0:10:05.10,0:10:05.77,main,,0000,0000,0000,,Co je to?
Dialogue: 0,0:10:06.02,0:10:07.52,main,,0000,0000,0000,,Někdo by to mohl nazvat krvákem…
Dialogue: 0,0:10:07.52,0:10:11.40,main,,0000,0000,0000,,…ale pořád má spoustu děsivých psychologických horrorových prvků.
Dialogue: 0,0:10:12.40,0:10:13.94,main,,0000,0000,0000,,Tak jdeme!
Dialogue: 0,0:10:13.94,0:10:16.40,main,,0000,0000,0000,,Co jít první domů a až pak se sejít?
Dialogue: 0,0:10:16.40,0:10:18.28,main,,0000,0000,0000,,Jdeme! Jdeme!
Dialogue: 0,0:10:19.03,0:10:20.32,main,,0000,0000,0000,,Tohle není zábava.
Dialogue: 0,0:10:20.32,0:10:22.57,main,,0000,0000,0000,,Takto se nemůžu bavit!
Dialogue: 0,0:10:25.95,0:10:27.33,main,,0000,0000,0000,,Pořád máme čas. Co budeme dělat?
Dialogue: 0,0:10:27.33,0:10:29.42,main,,0000,0000,0000,,Konata má zpoždění.
Dialogue: 0,0:10:29.42,0:10:30.79,main,,0000,0000,0000,,To zase přijde pozdě?
Dialogue: 0,0:10:30.79,0:10:31.75,main,,0000,0000,0000,,Možná…
Dialogue: 0,0:10:31.96,0:10:35.63,main,,0000,0000,0000,,Přála bych si aby jednou přišla dřív než my.
Dialogue: 0,0:10:36.30,0:10:37.21,main,,0000,0000,0000,,Vážně…
Dialogue: 0,0:10:37.22,0:10:38.97,main,,0000,0000,0000,,Film začne co nevidět.
Dialogue: 0,0:10:42.05,0:10:43.05,main,,0000,0000,0000,,Kdy…
Dialogue: 0,0:10:43.39,0:10:45.51,main,,0000,0000,0000,,Nepoznala jsem tě bez toho…
Dialogue: 0,0:10:46.68,0:10:49.27,main,,0000,0000,0000,,No každopádně máme málo času, tak pojďme.
Dialogue: 0,0:10:49.64,0:10:50.35,main,,0000,0000,0000,,Jo.
Dialogue: 0,0:10:50.35,0:10:53.23,main,,0000,0000,0000,,Předstírejte, že jsme sestry.
Dialogue: 0,0:10:53.86,0:10:54.77,main,,0000,0000,0000,,Proč?
Dialogue: 0,0:10:54.77,0:10:55.44,main,,0000,0000,0000,,No…
Dialogue: 0,0:10:55.44,0:10:58.32,main,,0000,0000,0000,,Potom si můžu koupit dětský lístek aniž bych vypadala divně.
Dialogue: 0,0:10:58.32,0:10:59.45,main,,0000,0000,0000,,Spratek!
Dialogue: 0,0:10:59.45,0:11:01.36,main,,0000,0000,0000,,Pořád využívá její výšky…
Dialogue: 0,0:11:01.36,0:11:03.37,main,,0000,0000,0000,,…a přitom si na ni stěžuje!
Dialogue: 0,0:11:03.37,0:11:04.91,main,,0000,0000,0000,,To bylo strašidelné!
Dialogue: 0,0:11:04.91,0:11:06.37,main,,0000,0000,0000,,Jsi tak roztomilá.
Dialogue: 0,0:11:06.37,0:11:07.91,main,,0000,0000,0000,,Mám docela hlad.
Dialogue: 0,0:11:07.91,0:11:08.58,main,,0000,0000,0000,,Já taky.
Dialogue: 0,0:11:09.00,0:11:11.50,main,,0000,0000,0000,,A Tsukase asi taky vytrávilo po tom všem křičení.
Dialogue: 0,0:11:13.00,0:11:14.17,main,,0000,0000,0000,,Co si dáme?
Dialogue: 0,0:11:14.17,0:11:15.96,main,,0000,0000,0000,,Co třeba jít do bufetu?
Dialogue: 0,0:11:15.96,0:11:16.96,main,,0000,0000,0000,,To zní dobře!
Dialogue: 0,0:11:17.21,0:11:18.71,main,,0000,0000,0000,,Bufet…
Dialogue: 0,0:11:18.72,0:11:21.43,main,,0000,0000,0000,,Ráj pro všechny ženské potřeby!
Dialogue: 0,0:11:22.76,0:11:25.22,main,,0000,0000,0000,,Mají strašnou spoustu zákusků a nápojů.
Dialogue: 0,0:11:25.22,0:11:26.60,main,,0000,0000,0000,,To jo.
Dialogue: 0,0:11:26.60,0:11:28.72,main,,0000,0000,0000,,Zajímalo by mě, jestli by bylo možné zkusit každý.
Dialogue: 0,0:11:28.73,0:11:30.27,main,,0000,0000,0000,,Pro Kagami je to hračka.
Dialogue: 0,0:11:31.31,0:11:34.48,main,,0000,0000,0000,,Náš časový limit je jedna hodina na zákazníka.
Dialogue: 0,0:11:34.48,0:11:37.02,main,,0000,0000,0000,,Něco na pití naleznete tam v tom rohu.
Dialogue: 0,0:11:37.03,0:11:39.82,main,,0000,0000,0000,,A zákusky támhle.
Dialogue: 0,0:11:39.82,0:11:41.49,main,,0000,0000,0000,,Prosím poslužte si.
Dialogue: 0,0:11:41.49,0:11:42.20,main,,0000,0000,0000,,Dobře!
Dialogue: 0,0:11:45.49,0:11:47.87,main,,0000,0000,0000,,A pokud nedojíte abnormální množství jídla…
Dialogue: 0,0:11:47.87,0:11:51.66,main,,0000,0000,0000,,…bude ho muset uhradit extra.
Dialogue: 0,0:11:51.67,0:11:53.62,main,,0000,0000,0000,,Mějte to prosím na paměti.
Dialogue: 0,0:11:54.88,0:11:56.75,main,,0000,0000,0000,,Tak a to je vše - dobrou chuť.
Dialogue: 0,0:11:58.59,0:12:00.51,main,,0000,0000,0000,,Co považují za abnormální množství?
Dialogue: 0,0:12:00.84,0:12:03.72,main,,0000,0000,0000,,Někteří lidé si naberou hromadu a pak mají problém to dojíst.
Dialogue: 0,0:12:03.72,0:12:04.22,main,,0000,0000,0000,,No tak.
Dialogue: 0,0:12:04.22,0:12:04.89,main,,0000,0000,0000,,Jdeme.
Dialogue: 0,0:12:06.35,0:12:07.35,main,,0000,0000,0000,,To je hodně…
Dialogue: 0,0:12:07.72,0:12:08.97,main,,0000,0000,0000,,Jsi si jistá, že to sníš?
Dialogue: 0,0:12:09.39,0:12:10.68,main,,0000,0000,0000,,A co ty?
Dialogue: 0,0:12:11.98,0:12:14.40,main,,0000,0000,0000,,Tohle je normál.
Dialogue: 0,0:12:14.40,0:12:15.69,main,,0000,0000,0000,,Nemám s tím žádný problém.
Dialogue: 0,0:12:15.90,0:12:17.40,main,,0000,0000,0000,,Itadakimasu!
Dialogue: 0,0:12:23.07,0:12:24.15,main,,0000,0000,0000,,Výtečné!
Dialogue: 0,0:12:24.70,0:12:25.66,main,,0000,0000,0000,,A tam ten!
Dialogue: 0,0:12:25.87,0:12:27.16,main,,0000,0000,0000,,A tento!
Dialogue: 0,0:12:34.62,0:12:36.79,main,,0000,0000,0000,,To je boles…
Dialogue: 0,0:12:37.21,0:12:40.00,main,,0000,0000,0000,,Po prvním talíři jsme měli zpomalit.
Dialogue: 0,0:12:40.84,0:12:42.51,main,,0000,0000,0000,,Už nemůžu.
Dialogue: 0,0:12:43.34,0:12:45.93,main,,0000,0000,0000,,Ale pokud něco necháme tak to budeme muset platit extra.
Dialogue: 0,0:12:46.43,0:12:48.05,main,,0000,0000,0000,,To je špatné!
Dialogue: 0,0:12:53.98,0:12:55.14,main,,0000,0000,0000,,To bolí…
Dialogue: 0,0:12:55.15,0:12:56.73,main,,0000,0000,0000,,Já to nedokážu…
Dialogue: 0,0:12:58.69,0:12:59.61,main,,0000,0000,0000,,Skoro nám už došel čas…
Dialogue: 0,0:13:03.78,0:13:04.99,main,,0000,0000,0000,,Oh, Kagami!
Dialogue: 0,0:13:04.99,0:13:06.70,main,,0000,0000,0000,,Nabrala druhý dech!
Dialogue: 0,0:13:06.70,0:13:08.20,main,,0000,0000,0000,,Jinak by to bylo plýtvání!
Dialogue: 0,0:13:12.58,0:13:13.41,main,,0000,0000,0000,,Nemůžu.
Dialogue: 0,0:13:14.12,0:13:15.12,main,,0000,0000,0000,,Ne! Ne!
Dialogue: 0,0:13:15.42,0:13:17.17,main,,0000,0000,0000,,Pokud vypiješ moc vody…
Dialogue: 0,0:13:17.17,0:13:19.21,main,,0000,0000,0000,,…budeš brzo sytá!
Dialogue: 0,0:13:19.21,0:13:22.13,main,,0000,0000,0000,,Prostě se cpi dokud to jde…
Dialogue: 0,0:13:22.13,0:13:24.88,main,,0000,0000,0000,,…a až nakonec to spláchni vodou!
Dialogue: 0,0:13:25.38,0:13:26.88,main,,0000,0000,0000,,No tak jen tak neseď!
Dialogue: 0,0:13:26.89,0:13:28.55,main,,0000,0000,0000,,Dej se do jídla!
Dialogue: 0,0:13:28.55,0:13:29.93,main,,0000,0000,0000,,Už nemůžu…
Dialogue: 0,0:13:29.93,0:13:32.01,main,,0000,0000,0000,,Puding ještě zvládneš!
Dialogue: 0,0:13:32.52,0:13:33.14,main,,0000,0000,0000,,Tady.
Dialogue: 0,0:13:34.02,0:13:35.89,main,,0000,0000,0000,,Nemůžeš to sníst ty?
Dialogue: 0,0:13:35.89,0:13:38.02,main,,0000,0000,0000,,Já už taky nemůžu!
Dialogue: 0,0:13:38.02,0:13:40.19,main,,0000,0000,0000,,Já už jsem mimo hru…
Dialogue: 0,0:13:40.19,0:13:41.90,main,,0000,0000,0000,,Prostě si ten puding nacpi do krku!
Dialogue: 0,0:13:43.07,0:13:44.11,main,,0000,0000,0000,,Nemáme už moc času!
Dialogue: 0,0:13:44.82,0:13:46.15,main,,0000,0000,0000,,Tohle není zábava!
Dialogue: 0,0:13:46.15,0:13:48.36,main,,0000,0000,0000,,Tohle mě nebaví!
Dialogue: 0,0:13:48.87,0:13:50.07,main,,0000,0000,0000,,Testy jsou u konce…
Dialogue: 0,0:13:50.08,0:13:52.87,main,,0000,0000,0000,,…tak jsem si pořádně zahrála…
Dialogue: 0,0:13:53.12,0:13:55.00,main,,0000,0000,0000,,…to proto jsem tak ospalá…
Dialogue: 0,0:13:55.00,0:13:57.16,main,,0000,0000,0000,,Kdybys tak naplno studovala…
Dialogue: 0,0:13:57.17,0:13:58.42,main,,0000,0000,0000,,…tvoje testy by hned vypadaly jinak.
Dialogue: 0,0:13:58.92,0:14:04.62,main,,0000,0000,0000,,Poslední dobou mají d-simulátory spoustu save pointů…
Dialogue: 0,0:14:04.62,0:14:06.82,main,,0000,0000,0000,,…ale i přes to nemůžu pořád přestat.
Dialogue: 0,0:14:06.82,0:14:08.63,main,,0000,0000,0000,,Přemýšlela jsem…
Dialogue: 0,0:14:08.64,0:14:10.22,main,,0000,0000,0000,,Že ještě nejsi dost stará na to aby sis je mohla kupovat.
Dialogue: 0,0:14:10.22,0:14:11.41,main,,0000,0000,0000,,Jak to teda děláš?
Dialogue: 0,0:14:12.31,0:14:14.22,main,,0000,0000,0000,,Můj taťka mi je kupuje.
Dialogue: 0,0:14:14.43,0:14:14.77,main,,0000,0000,0000,,To ne!
Dialogue: 0,0:14:14.77,0:14:15.89,main,,0000,0000,0000,,Jak jsem si myslela!
Dialogue: 0,0:14:15.89,0:14:17.98,main,,0000,0000,0000,,Tvoje rodina je dost zvrácená!
Dialogue: 0,0:14:17.98,0:14:19.73,main,,0000,0000,0000,,Takže on je vážně chodí kupovat?
Dialogue: 0,0:14:19.73,0:14:21.19,main,,0000,0000,0000,,A ty by ses ho jako dcera neměla o takové věci ptát.
Dialogue: 0,0:14:21.61,0:14:24.15,main,,0000,0000,0000,,Takže jsem vážně ospalá.
Dialogue: 0,0:14:24.15,0:14:26.94,main,,0000,0000,0000,,Ty asi nehraješ hry, co Miyuki?
Dialogue: 0,0:14:27.61,0:14:29.78,main,,0000,0000,0000,,Nemám konzoli…
Dialogue: 0,0:14:29.78,0:14:31.95,main,,0000,0000,0000,,…ale občas si zahraji na počítači.
Dialogue: 0,0:14:32.62,0:14:34.03,main,,0000,0000,0000,,Co hraješ?
Dialogue: 0,0:14:34.04,0:14:35.24,main,,0000,0000,0000,,To mě překvapilo!
Dialogue: 0,0:14:35.25,0:14:37.62,main,,0000,0000,0000,,Hry jako je Solitair a Miny.
Dialogue: 0,0:14:37.62,0:14:39.29,main,,0000,0000,0000,,Občas se do toho vážně dostanu.
Dialogue: 0,0:14:39.96,0:14:41.50,main,,0000,0000,0000,,Takovýto hry…
Dialogue: 0,0:14:41.50,0:14:43.42,main,,0000,0000,0000,,…tak to jsem nečekala.
Dialogue: 0,0:14:43.71,0:14:45.34,main,,0000,0000,0000,,Vím co myslíš.
Dialogue: 0,0:14:45.34,0:14:47.21,main,,0000,0000,0000,,Tyhle hry…
Dialogue: 0,0:14:47.22,0:14:49.30,main,,0000,0000,0000,,…jsou docela návykové.
Dialogue: 0,0:14:49.30,0:14:51.43,main,,0000,0000,0000,,Jaké je tvé skóre Yuki-chan?
Dialogue: 0,0:14:51.89,0:14:53.68,main,,0000,0000,0000,,Solitaire je o štěstí…
Dialogue: 0,0:14:53.68,0:14:56.31,main,,0000,0000,0000,,…ale často mám přes desetitisíc.
Dialogue: 0,0:14:56.31,0:14:59.94,main,,0000,0000,0000,,A co se týká min tak ty jsem nedávno dokončila na obtížnost expert pod sto sekund.
Dialogue: 0,0:15:01.31,0:15:04.69,main,,0000,0000,0000,,Lidé jako Miyuki se do těhle her vážně zažerou…
Dialogue: 0,0:15:07.65,0:15:10.90,main,,0000,0000,0000,,Letos byla spousta sportovních aktivit.
Dialogue: 0,0:15:11.57,0:15:15.91,main,,0000,0000,0000,,Dokonce i tam ten dostal zlatou na Olympiádě za bruslení.
Dialogue: 0,0:15:15.91,0:15:18.50,main,,0000,0000,0000,,A dokonce i školní baseball byl zajímavý.
Dialogue: 0,0:15:18.50,0:15:20.25,main,,0000,0000,0000,,To byl neuvěřitelně dlouhý zápas.
Dialogue: 0,0:15:20.92,0:15:22.21,main,,0000,0000,0000,,A Handkerchief prince.
Dialogue: 0,0:15:22.54,0:15:23.79,main,,0000,0000,0000,,Když mluvíme o baseballu…
Dialogue: 0,0:15:23.79,0:15:25.79,main,,0000,0000,0000,,Tam ten hráč odjel do Ameriky, co?
Dialogue: 0,0:15:26.71,0:15:28.88,main,,0000,0000,0000,,Už je japonská série u konce?
Dialogue: 0,0:15:28.88,0:15:29.76,main,,0000,0000,0000,,Dávno.
Dialogue: 0,0:15:29.76,0:15:30.42,main,,0000,0000,0000,,To ne!
Dialogue: 0,0:15:30.43,0:15:32.13,main,,0000,0000,0000,,Byly i tentokrát slevy?
Dialogue: 0,0:15:32.14,0:15:35.47,main,,0000,0000,0000,,Japonská série byla mezi tam tím a tam tím týmem… (vážně to tak říkají)
Dialogue: 0,0:15:35.47,0:15:38.97,main,,0000,0000,0000,,…takže do toho nebyly zatažený žádný obchody.
Dialogue: 0,0:15:38.98,0:15:41.23,main,,0000,0000,0000,,Ale měly nějaké výprodeje na oslavách.
Dialogue: 0,0:15:43.19,0:15:46.57,main,,0000,0000,0000,,Ale to byla jen šunka a takové věci, že?
Dialogue: 0,0:15:46.57,0:15:48.61,main,,0000,0000,0000,,To nevím…
Dialogue: 0,0:15:48.61,0:15:51.49,main,,0000,0000,0000,,Nicméně si na to musím dát příští rok pozor.
Dialogue: 0,0:15:51.49,0:15:52.91,main,,0000,0000,0000,,Jak tě znám…
Dialogue: 0,0:15:52.91,0:15:54.87,main,,0000,0000,0000,,tak si stejně nejspíš jen koupíš tucet sladkostí.
Dialogue: 0,0:15:54.87,0:15:55.99,main,,0000,0000,0000,,To ne.
Dialogue: 0,0:15:55.99,0:15:58.41,main,,0000,0000,0000,,Potřebuji věci jako je sůl, cukr a sojovou omáčku.
Dialogue: 0,0:15:58.41,0:16:00.66,main,,0000,0000,0000,,Takové ty základní věci.
Dialogue: 0,0:16:02.04,0:16:03.08,main,,0000,0000,0000,,No…
Dialogue: 0,0:16:03.08,0:16:06.59,main,,0000,0000,0000,,Kagami tomu asi stejně nerozumí, co?
Dialogue: 0,0:16:06.59,0:16:09.59,main,,0000,0000,0000,,No tak promiň! Ale nejsem na tohle dobrá!
Dialogue: 0,0:16:14.51,0:16:15.59,main,,0000,0000,0000,,Oční kapky?
Dialogue: 0,0:16:15.60,0:16:16.85,main,,0000,0000,0000,,Chceš abych ti pomohla?
Dialogue: 0,0:16:17.60,0:16:18.81,main,,0000,0000,0000,,Mockrát děkuji.
Dialogue: 0,0:16:19.27,0:16:19.64,main,,0000,0000,0000,,Fajn.
Dialogue: 0,0:16:19.64,0:16:21.23,main,,0000,0000,0000,,Lehni si.
Dialogue: 0,0:16:22.77,0:16:23.64,main,,0000,0000,0000,,Dobře…
Dialogue: 0,0:16:23.65,0:16:25.35,main,,0000,0000,0000,,Relaxuj.
Dialogue: 0,0:16:25.36,0:16:26.48,main,,0000,0000,0000,,Ano.
Dialogue: 0,0:16:26.94,0:16:28.07,main,,0000,0000,0000,,Tak jo.
Dialogue: 0,0:16:31.90,0:16:32.57,main,,0000,0000,0000,,Dobře.
Dialogue: 0,0:16:32.57,0:16:33.99,main,,0000,0000,0000,,Uvolni se.
Dialogue: 0,0:16:33.99,0:16:34.82,main,,0000,0000,0000,,Jo.
Dialogue: 0,0:16:39.20,0:16:39.95,main,,0000,0000,0000,,Um…
Dialogue: 0,0:16:39.95,0:16:41.08,main,,0000,0000,0000,,Omlouvám se.
Dialogue: 0,0:16:41.08,0:16:42.96,main,,0000,0000,0000,,Nedělám to schválně…
Dialogue: 0,0:16:44.00,0:16:45.08,main,,0000,0000,0000,,Ale tvůj obličej…
Dialogue: 0,0:16:45.08,0:16:46.25,main,,0000,0000,0000,,…vypadá děsivě!
Dialogue: 0,0:16:52.80,0:16:54.05,main,,0000,0000,0000,,Špatný na výhrách je…
Dialogue: 0,0:16:54.05,0:16:57.01,main,,0000,0000,0000,,…že trvá hrozně dlouho než ti je pošlou.
Dialogue: 0,0:16:57.43,0:17:02.56,main,,0000,0000,0000,,Proto jsem přišla se způsobem jak si při tom užít dvojnásobek zábavy.
Dialogue: 0,0:17:02.56,0:17:03.27,main,,0000,0000,0000,,Jakto?
Dialogue: 0,0:17:03.52,0:17:05.94,main,,0000,0000,0000,,Musíš to odeslat a rychle se začít bavit jinak abys na to zapoměla.
Dialogue: 0,0:17:05.94,0:17:10.48,main,,0000,0000,0000,,A když ti pak přijde tak z toho máš extra radost.
Dialogue: 0,0:17:10.48,0:17:12.28,main,,0000,0000,0000,,Dva v jednom.
Dialogue: 0,0:17:12.99,0:17:16.07,main,,0000,0000,0000,,Nemůžu snést hlavně to čekání…
Dialogue: 0,0:17:16.07,0:17:18.57,main,,0000,0000,0000,,Jak můžeš být tak pozitivní jen k věcem co máš ráda?
Dialogue: 0,0:17:19.06,0:17:22.04,copyrights,,0000,0000,0000,,{\pos(85,50)}Překlad: paralaX
Dialogue: 0,0:17:19.06,0:17:22.04,copyrights,,0000,0000,0000,,{\pos(169,76)}Domovská stránka: www.otakogu.cz
Dialogue: 0,0:17:24.91,0:17:26.21,main,,0000,0000,0000,,Oha-Lucky!
Dialogue: 0,0:17:26.33,0:17:27.08,main,,0000,0000,0000,,Ahoj!
Dialogue: 0,0:17:27.08,0:17:30.04,main,,0000,0000,0000,,S radostí vám přináším dnešní epizodu Lucky Channelu!
Dialogue: 0,0:17:30.55,0:17:33.67,main,,0000,0000,0000,,Tohle je už naše devátá epizoda!
Dialogue: 0,0:17:33.67,0:17:35.80,main,,0000,0000,0000,,Já jsem vaše průvodkyně Akira Kogami!
Dialogue: 0,0:17:35.80,0:17:37.68,main,,0000,0000,0000,,A osoba které se producent Itou-san chystá říct…
Dialogue: 0,0:17:37.68,0:17:41.56,main,,0000,0000,0000,,…"Hej, Shiraishi, pojď sem na moment" je…
Dialogue: 0,0:17:42.18,0:17:43.14,main,,0000,0000,0000,,Vážně?!
Dialogue: 0,0:17:43.81,0:17:45.02,main,,0000,0000,0000,,Já jsem asistent Akiri-sam…
Dialogue: 0,0:17:45.02,0:17:46.39,main,,0000,0000,0000,,100% připravený…
Dialogue: 0,0:17:46.39,0:17:47.64,main,,0000,0000,0000,,Minoru Shiraishi.
Dialogue: 0,0:17:47.65,0:17:49.19,main,,0000,0000,0000,,Je mi potěšením.
Dialogue: 0,0:17:49.19,0:17:49.73,main,,0000,0000,0000,,Fajn.
Dialogue: 0,0:17:49.73,0:17:51.73,main,,0000,0000,0000,,Dnes vám představíme třetí postavu.
Dialogue: 0,0:17:51.73,0:17:53.32,main,,0000,0000,0000,,Na řadě je Tsukasa Hiiragi!
Dialogue: 0,0:17:51.98,0:17:52.94,main2,,0000,0000,0000,,To je překvapení…
Dialogue: 0,0:17:52.94,0:17:54.69,main2,,0000,0000,0000,,Tady.
Dialogue: 0,0:17:54.36,0:17:56.70,main,,0000,0000,0000,,Ty blbý, pomalý, nemotorný…
Dialogue: 0,0:17:56.70,0:17:57.68,main,,0000,0000,0000,,Tsu-
Dialogue: 0,0:17:56.70,0:17:57.68,main2,,0000,0000,0000,,Thi-
Dialogue: 0,0:17:58.28,0:17:59.07,main,,0000,0000,0000,,Omlouvám se.
Dialogue: 0,0:17:59.07,0:18:01.24,main,,0000,0000,0000,,Vážně to začíná být zajímavé!
Dialogue: 0,0:18:01.24,0:18:01.99,main,,0000,0000,0000,,Ano!
Dialogue: 0,0:18:01.99,0:18:03.04,main,,0000,0000,0000,,Utáhněme si zástěry a pusťme se do toho…
Dialogue: 0,0:18:03.04,0:18:04.95,main,,0000,0000,0000,,…jak jen můžeme!
Dialogue: 0,0:18:04.95,0:18:06.00,main,,0000,0000,0000,,Že, Akiro?
Dialogue: 0,0:18:06.00,0:18:06.83,main,,0000,0000,0000,,Zástěry?
Dialogue: 0,0:18:08.08,0:18:10.17,main,,0000,0000,0000,,Já mám být idol…
Dialogue: 0,0:18:10.17,0:18:12.29,main,,0000,0000,0000,,takže taký věci nejsou nic pro mě…
Dialogue: 0,0:18:12.30,0:18:13.96,main,,0000,0000,0000,,Ty se o to postarej.
Dialogue: 0,0:18:14.59,0:18:16.38,main,,0000,0000,0000,,To je pravda…
Dialogue: 0,0:18:16.38,0:18:17.55,main,,0000,0000,0000,,Uh…
Dialogue: 0,0:18:18.34,0:18:20.68,main,,0000,0000,0000,,Takže si je pořádně utáhněte dokud je nebudete mít zařezané…
Dialogue: 0,0:18:20.93,0:18:21.76,main,,0000,0000,0000,,Hej.
Dialogue: 0,0:18:21.76,0:18:24.01,main,,0000,0000,0000,,Nech mě tě varovat.
Dialogue: 0,0:18:24.02,0:18:25.64,main,,0000,0000,0000,,Varovat?
Dialogue: 0,0:18:25.64,0:18:27.48,main,,0000,0000,0000,,Jakmile to začne jít s komikem z kopce…
Dialogue: 0,0:18:27.48,0:18:28.56,main,,0000,0000,0000,,…je konec.
Dialogue: 0,0:18:28.56,0:18:29.77,main,,0000,0000,0000,,Takže já jsem komik…
Dialogue: 0,0:18:29.77,0:18:30.31,main,,0000,0000,0000,,A taky…
Dialogue: 0,0:18:30.31,0:18:32.52,main,,0000,0000,0000,,Poslední dobou používáš spoustu věcí od jiných lidí…
Dialogue: 0,0:18:32.52,0:18:34.19,main,,0000,0000,0000,,…takže si dávej pozor.
Dialogue: 0,0:18:34.19,0:18:34.61,main,,0000,0000,0000,,Ano!
Dialogue: 0,0:18:34.61,0:18:35.48,main,,0000,0000,0000,,Um…
Dialogue: 0,0:18:35.49,0:18:36.57,main,,0000,0000,0000,,Co že to dnes děláme?
Dialogue: 0,0:18:37.57,0:18:39.15,main,,0000,0000,0000,,Představujeme Tsukasu Hiiragi-san.
Dialogue: 0,0:18:40.62,0:18:41.49,main,,0000,0000,0000,,That To Hear--
Dialogue: 0,0:18:41.49,0:18:43.03,main,,0000,0000,0000,,Mi-Mi-Miracle!
Dialogue: 0,0:18:43.03,0:18:43.74,main,,0000,0000,0000,,Kamigishi…
Dialogue: 0,0:18:43.74,0:18:45.37,main,,0000,0000,0000,,Minorun-run!
Dialogue: 0,0:18:45.37,0:18:45.95,main,,0000,0000,0000,,Že?
Dialogue: 0,0:18:46.75,0:18:48.21,main,,0000,0000,0000,,Takže to můžeš říct?
Dialogue: 0,0:18:48.21,0:18:49.00,main,,0000,0000,0000,,Jak to myslíš?
Dialogue: 0,0:18:49.00,0:18:51.92,main,,0000,0000,0000,,Připomíná jednu starou oblíbenou postavu.
Dialogue: 0,0:18:52.17,0:18:54.55,main,,0000,0000,0000,,Um, myslíte?
Dialogue: 0,0:18:54.55,0:18:58.30,main,,0000,0000,0000,,No asi by se to dalo říct…
Dialogue: 0,0:18:59.05,0:19:00.09,main,,0000,0000,0000,,Nerozesmívej mě!
Dialogue: 0,0:19:00.97,0:19:03.22,main,,0000,0000,0000,,Tsukasa Hiiragi a idol?
Dialogue: 0,0:19:03.22,0:19:04.60,main,,0000,0000,0000,,Kde jste to vzala?
Dialogue: 0,0:19:04.60,0:19:05.97,main,,0000,0000,0000,,Víš…
Dialogue: 0,0:19:05.97,0:19:08.06,main,,0000,0000,0000,,Teď když pracuješ těsně vedle mě.
Dialogue: 0,0:19:08.06,0:19:10.73,main,,0000,0000,0000,,Měl by ses naučit co to skutečný idol je.
Dialogue: 0,0:19:10.73,0:19:11.90,main,,0000,0000,0000,,A-ano madam.
Dialogue: 0,0:19:11.90,0:19:14.61,main,,0000,0000,0000,,Tsukasa je ten citliví charakter…
Dialogue: 0,0:19:14.61,0:19:17.28,main,,0000,0000,0000,,…s nevinným pohledem.
Dialogue: 0,0:19:18.45,0:19:20.78,main,,0000,0000,0000,,Nevinným pohledem…
Dialogue: 0,0:19:20.99,0:19:21.66,main,,0000,0000,0000,,Každopádně…
Dialogue: 0,0:19:21.87,0:19:23.32,main,,0000,0000,0000,,…koho zajímá nějaká Tsukasa?
Dialogue: 0,0:19:21.87,0:19:23.32,main2,,0000,0000,0000,,Ani otec mě nikdy neuhodil!
Dialogue: 0,0:19:23.78,0:19:27.87,main,,0000,0000,0000,,Naučím tě základy toho kdo je to idol.
Dialogue: 0,0:19:27.87,0:19:29.83,main,,0000,0000,0000,,Zaprvé je důležité být fotogenický.
Dialogue: 0,0:19:30.46,0:19:31.37,main,,0000,0000,0000,,Sem se dívej.
Dialogue: 0,0:19:31.62,0:19:34.59,main,,0000,0000,0000,,Tohle pro tebe bude užitečná lekce.
Dialogue: 0,0:19:34.59,0:19:36.42,main,,0000,0000,0000,,Takže dávej pozor.
Dialogue: 0,0:19:36.63,0:19:37.00,main,,0000,0000,0000,,Ano!
Dialogue: 0,0:19:37.01,0:19:39.88,main,,0000,0000,0000,,Víš jak si tě najednou fotí lidi když jsi ve městě?
Dialogue: 0,0:19:41.05,0:19:41.59,main,,0000,0000,0000,,Prosím.
Dialogue: 0,0:19:41.59,0:19:42.63,main,,0000,0000,0000,,Zmáčkni spoušť.
Dialogue: 0,0:19:42.64,0:19:45.01,main,,0000,0000,0000,,Nevinný pohled…
Dialogue: 0,0:19:42.64,0:19:45.01,main2,,0000,0000,0000,,*mrk* *mrk* *mrk* 
Dialogue: 0,0:19:46.39,0:19:47.10,main,,0000,0000,0000,,Jo.
Dialogue: 0,0:19:47.10,0:19:48.77,main,,0000,0000,0000,,Takto.
Dialogue: 0,0:19:48.77,0:19:51.31,main,,0000,0000,0000,,Rekvizity jsou taky důležité.
Dialogue: 0,0:19:51.31,0:19:53.35,main,,0000,0000,0000,,Vždy bys je měl mít u sebe.
Dialogue: 0,0:19:53.36,0:19:53.98,main,,0000,0000,0000,,Jak?
Dialogue: 0,0:19:53.98,0:19:59.49,main,,0000,0000,0000,,Nikdy nesmíš ztratit pozornost když jdeš po městě…
Dialogue: 0,0:19:59.49,0:20:00.69,main,,0000,0000,0000,,…vždy buď ve střehu.
Dialogue: 0,0:20:01.40,0:20:01.86,main,,0000,0000,0000,,A…
Dialogue: 0,0:20:01.86,0:20:03.61,main,,0000,0000,0000,,…ujisti se, že máš zářivě bílé zuby.
Dialogue: 0,0:20:03.87,0:20:04.87,main,,0000,0000,0000,,Jako panenka.
Dialogue: 0,0:20:04.87,0:20:05.95,main,,0000,0000,0000,,Zajímavé.
Dialogue: 0,0:20:05.95,0:20:06.62,main,,0000,0000,0000,,A taky…
Dialogue: 0,0:20:06.62,0:20:09.08,main,,0000,0000,0000,,…ujisti se, že máš naplno otevřené oči…
Dialogue: 0,0:20:09.08,0:20:11.12,main,,0000,0000,0000,,…a že se usmíváš těmito svaly.
Dialogue: 0,0:20:11.12,0:20:11.87,main,,0000,0000,0000,,Děsivé!
Dialogue: 0,0:20:11.87,0:20:14.79,main,,0000,0000,0000,,Ale musíš při tom vypadat přirozeně.
Dialogue: 0,0:20:14.79,0:20:18.09,main,,0000,0000,0000,,Když na tebe někdo z ničeho nic namíří foťák…
Dialogue: 0,0:20:18.09,0:20:19.92,main,,0000,0000,0000,,Akira udělá - „whoa!“
Dialogue: 0,0:20:20.59,0:20:21.59,main,,0000,0000,0000,,Takto.
Dialogue: 0,0:20:22.80,0:20:24.30,main,,0000,0000,0000,,Přirozené…
Dialogue: 0,0:20:24.30,0:20:26.64,main,,0000,0000,0000,,A taky pro změnu…
Dialogue: 0,0:20:26.64,0:20:27.55,main,,0000,0000,0000,,…když jdeš s klukem do-
Dialogue: 0,0:20:28.06,0:20:29.31,main,,0000,0000,0000,,Ah, ne!
Dialogue: 0,0:20:29.31,0:20:31.10,main,,0000,0000,0000,,Je čas se rozloučit!
Dialogue: 0,0:20:31.31,0:20:39.53,main,,0000,0000,0000,,Prosím, posílejte vám vaše komentáře a náměty k show!
Dialogue: 0,0:20:39.53,0:20:41.94,main,,0000,0000,0000,,Také nám posílejte komentáře ohledně radio show a naší webové stránky!
Dialogue: 0,0:20:41.95,0:20:42.40,main,,0000,0000,0000,,Tak jo!
Dialogue: 0,0:20:42.40,0:20:44.15,main,,0000,0000,0000,,Uvidíme se příští týden.
Dialogue: 0,0:20:44.16,0:20:44.99,main,,0000,0000,0000,,Bye-bíí!
Dialogue: 0,0:20:46.95,0:20:48.95,main,,0000,0000,0000,,Ty jsi byl zrovna na focení co?
Dialogue: 0,0:20:48.95,0:20:49.91,main,,0000,0000,0000,,Jo.
Dialogue: 0,0:20:49.91,0:20:52.10,main,,0000,0000,0000,,Ve svetru.
Dialogue: 0,0:20:52.71,0:20:54.58,main,,0000,0000,0000,,To bylo roztomilé…
Dialogue: 0,0:20:53.71,0:20:54.58,main2,,0000,0000,0000,,Děkuji!
Dialogue: 0,0:20:55.71,0:20:57.33,main,,0000,0000,0000,,Bylo to roztomilé?
Dialogue: 0,0:20:55.71,0:20:57.33,main2,,0000,0000,0000,,Co se děje?
Dialogue: 0,0:20:57.34,0:20:58.00,main,,0000,0000,0000,,No…
Dialogue: 0,0:20:58.00,0:20:59.55,main,,0000,0000,0000,,Každý říkal že bylo…
Dialogue: 0,0:20:59.55,0:21:00.13,main,,0000,0000,0000,,Asi jo.
Dialogue: 0,0:21:00.03,0:21:00.26,main2,,0000,0000,0000,,Jop.
Dialogue: 0,0:21:00.13,0:21:02.46,main,,0000,0000,0000,,Ty teda válíš.
Dialogue: 0,0:21:01.13,0:21:02.46,main2,,0000,0000,0000,,To jo.
Dialogue: 0,0:21:02.47,0:21:03.67,main,,0000,0000,0000,,Vypadalo to jako zábava.
Dialogue: 0,0:21:03.68,0:21:05.18,main,,0000,0000,0000,,Oční linky…
Dialogue: 0,0:21:05.18,0:21:05.88,main,,0000,0000,0000,,…s…
Dialogue: 0,0:21:05.89,0:21:07.18,main,,0000,0000,0000,,…rukama takto polozenýma pod tváří…
Dialogue: 0,0:21:07.18,0:21:08.18,main,,0000,0000,0000,,…byla to zábava!
Dialogue: 0,0:21:08.68,0:21:09.10,main,,0000,0000,0000,,Jo.
Dialogue: 0,0:21:09.10,0:21:09.99,main,,0000,0000,0000,,Jsi na tom teď závislý?
Dialogue: 0,0:21:09.78,0:21:11.22,main2,,0000,0000,0000,,Jo, včetně převlékání.
Dialogue: 0,0:21:11.22,0:21:12.14,main,,0000,0000,0000,,Jsem na tom závislý.
Dialogue: 0,0:21:12.14,0:21:13.23,main,,0000,0000,0000,,Viděls fotky?
Dialogue: 0,0:21:13.23,0:21:14.14,main,,0000,0000,0000,,Ne, ještě ne.
Dialogue: 0,0:21:14.73,0:21:15.77,main,,0000,0000,0000,,Manager…
Dialogue: 0,0:21:15.77,0:21:16.27,main,,0000,0000,0000,,Ano.
Dialogue: 0,0:21:16.27,0:21:17.35,main,,0000,0000,0000,,…se na ně díval…
Dialogue: 0,0:21:17.36,0:21:17.60,main,,0000,0000,0000,,Ano.
Dialogue: 0,0:21:17.61,0:21:20.94,main,,0000,0000,0000,,…a řekl že aby byl upřímný tak že se mu nějaké nezdály.
Dialogue: 0,0:21:20.94,0:21:23.19,main,,0000,0000,0000,,No měl jsem spoustu jako Mokkori…
Dialogue: 0,0:21:23.19,0:21:24.49,main,,0000,0000,0000,,…takže to je asi ono.
Dialogue: 0,0:21:25.45,0:21:26.74,main,,0000,0000,0000,,Omlouvám se!
Dialogue: 0,0:21:26.74,0:21:28.53,main,,0000,0000,0000,,Taková chyba!
Dialogue: 0,0:21:28.53,0:21:30.03,main,,0000,0000,0000,,Teď už pořádně.
Dialogue: 0,0:21:30.04,0:21:32.41,main,,0000,0000,0000,,A už sis objednala další song.
Dialogue: 0,0:21:32.41,0:21:33.91,main,,0000,0000,0000,,Půjč ten ovladač i mě.
Dialogue: 0,0:21:33.91,0:21:34.91,main,,0000,0000,0000,,Fajn.
Dialogue: 0,0:21:34.91,0:21:36.67,main,,0000,0000,0000,,Už ses rozhodla, Tsukaso?
Dialogue: 0,0:21:36.67,0:21:37.96,main,,0000,0000,0000,,Ještě chvilku.
Dialogue: 0,0:21:37.96,0:21:40.67,main,,0000,0000,0000,,Protože moc muziku neposlouchám neznám moc písniček.
Dialogue: 0,0:21:40.67,0:21:41.38,main,,0000,0000,0000,,To je v pořádku.
Dialogue: 0,0:21:41.38,0:21:43.17,main,,0000,0000,0000,,Zatím budu zpívat já.
Dialogue: 0,0:21:43.17,0:21:43.96,main,,0000,0000,0000,,Hej.
Dialogue: 0,0:21:43.97,0:21:45.84,main,,0000,0000,0000,,Hlavně nezpívej jen písničky z anime.
Dialogue: 0,0:21:45.84,0:21:47.63,main,,0000,0000,0000,,Tohle není z anime.
Dialogue: 0,0:21:53.52,0:21:54.89,main,,0000,0000,0000,,Tohle jsem už slyšela.
Dialogue: 0,0:21:54.89,0:21:55.73,main,,0000,0000,0000,,Co je to?
Dialogue: 0,0:21:55.73,0:21:57.23,main,,0000,0000,0000,,To je KyonKyon.
Dialogue: 0,0:21:57.23,0:22:00.06,main,,0000,0000,0000,,Zpívávala jsem to s tátou.
Dialogue: 0,0:22:00.07,0:22:02.52,main,,0000,0000,0000,,Takže od anime ke starým písním.
Dialogue: 0,0:22:03.07,0:22:06.52,main,,0000,0000,0000,,To je urážka KyonKyona!
Dialogue: 0,0:22:09.16,0:22:12.87,mainup2,,0000,0000,0000,,Nový začátek čeká ve větru,
Dialogue: 0,0:22:12.99,0:22:17.12,mainup2,,0000,0000,0000,,ode dne kdy jsem se zamilovala.
Dialogue: 0,0:22:12.99,0:22:13.58,main,,0000,0000,0000,,Tsukaso.
Dialogue: 0,0:22:13.58,0:22:15.37,main,,0000,0000,0000,,Proč si nevybereš něco jednoduchého?
Dialogue: 0,0:22:15.37,0:22:16.54,main,,0000,0000,0000,,Jako nějakou dětskou písničku.
Dialogue: 0,0:22:17.33,0:22:21.34,mainup2,,0000,0000,0000,,Před tím než jsem si to uvědomila…
Dialogue: 0,0:22:17.33,0:22:19.54,main,,0000,0000,0000,,No, dívám se i do té sekce.
Dialogue: 0,0:22:19.54,0:22:20.84,main,,0000,0000,0000,,Omlouvám se.
Dialogue: 0,0:22:20.84,0:22:21.34,main,,0000,0000,0000,,To je v pořádku.
Dialogue: 0,0:22:21.46,0:22:25.38,mainup2,,0000,0000,0000,,…mé srdce pro tebe již plálo.
Dialogue: 0,0:22:21.46,0:22:23.34,main,,0000,0000,0000,,Není kam spěchat.
Dialogue: 0,0:22:23.34,0:22:25.38,main,,0000,0000,0000,,Pořád máme spoustu času.
Dialogue: 0,0:22:25.38,0:22:29.64,mainup2,,0000,0000,0000,,Neplač miláčku.
Dialogue: 0,0:22:29.64,0:22:33.76,mainup2,,0000,0000,0000,,Mé přání se vyplní…
Dialogue: 0,0:22:33.77,0:22:37.98,mainup2,,0000,0000,0000,,Překročím řeku slz…
Dialogue: 0,0:22:37.98,0:22:41.98,mainup2,,0000,0000,0000,,A na vše zapomenu…
Dialogue: 0,0:22:42.65,0:22:46.32,mainup2,,0000,0000,0000,,Jaká je to bolestivá láska…
Dialogue: 0,0:22:46.32,0:22:51.28,mainup2,,0000,0000,0000,,…když ji neopětuješ.
Dialogue: 0,0:22:51.28,0:22:53.28,mainup2,,0000,0000,0000,,
