Dialogue: 0,0:00:00.09,0:00:05.92,whitebig,,0000,0000,0000,,{\pos(225,126)}
Dialogue: 0,0:00:00.09,0:00:05.92,whitebig,,0000,0000,0000,,{\pos(226,152)}
Dialogue: 0,0:00:01.11,0:00:02.56,main,,0000,0000,0000,,Tak začneme.
Dialogue: 0,0:00:02.57,0:00:03.61,main,,0000,0000,0000,,Jdeme na to.
Dialogue: 0,0:00:04.19,0:00:05.98,main,,0000,0000,0000,,No tak už začněme!
Dialogue: 0,0:00:06.32,0:00:07.49,op1,,0000,0000,0000,,Okolo 3 cm!
Dialogue: 0,0:00:07.49,0:00:08.78,op1,,0000,0000,0000,,Je to snad málo?
Dialogue: 0,0:00:08.78,0:00:09.03,op1,,0000,0000,0000,,Jejda!
Dialogue: 0,0:00:09.03,0:00:10.86,op1,,0000,0000,0000,,Schované pod naší uniformou!
Dialogue: 0,0:00:10.87,0:00:11.91,op1,,0000,0000,0000,,Mám toho nechat?
Dialogue: 0,0:00:11.91,0:00:12.20,op1,,0000,0000,0000,,JEJ!
Dialogue: 0,0:00:12.20,0:00:13.28,op1,,0000,0000,0000,,Jít do toho?
Dialogue: 0,0:00:13.28,0:00:14.08,op1,,0000,0000,0000,,Snažit se?
Dialogue: 0,0:00:14.08,0:00:15.66,op1,,0000,0000,0000,,Vychutnat si každý okamžik. Jo!
Dialogue: 0,0:00:15.66,0:00:16.25,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.26,0:00:16.50,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:16.50,0:00:16.90,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.91,0:00:17.25,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:17.25,0:00:17.87,op1,,0000,0000,0000,,Jen mezi námi dvěma!
Dialogue: 0,0:00:17.87,0:00:20.58,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:00:22.56,0:00:23.36,logo,,0000,0000,0000,,{\move(108,218,202,218,0,0)}LUCKY
Dialogue: 0,0:00:23.36,0:00:25.08,logo,,0000,0000,0000,,{\pos(202,218)}LUCKY
Dialogue: 0,0:00:22.56,0:00:23.36,logo,,0000,0000,0000,,{\move(638,218,459,218,0,0)}STAR
Dialogue: 0,0:00:23.36,0:00:25.08,logo,,0000,0000,0000,,{\pos(459,218)}STAR
Dialogue: 0,0:00:25.51,0:00:26.09,op1,,0000,0000,0000,,Cítím se nějak nesvá.
Dialogue: 0,0:00:26.09,0:00:26.71,op1,,0000,0000,0000,,Co jsem to jedla?
Dialogue: 0,0:00:26.71,0:00:27.30,op1,,0000,0000,0000,,Mám tě hrozně ráda!
Dialogue: 0,0:00:27.30,0:00:27.51,op1,,0000,0000,0000,,Oops!
Dialogue: 0,0:00:27.51,0:00:28.55,op1,,0000,0000,0000,,Jedno z nich je špatně!
Dialogue: 0,0:00:28.55,0:00:29.30,op1,,0000,0000,0000,,Relaxovat!
Dialogue: 0,0:00:29.30,0:00:29.93,op1,,0000,0000,0000,,Nic nedělat!
Dialogue: 0,0:00:29.93,0:00:30.47,op1,,0000,0000,0000,,Flákat se!
Dialogue: 0,0:00:30.47,0:00:31.55,op1,,0000,0000,0000,,Už toho nechej!
Dialogue: 0,0:00:31.55,0:00:33.21,op1,,0000,0000,0000,,Je ta nová holka oblíbená?
Dialogue: 0,0:00:33.21,0:00:34.85,op1,,0000,0000,0000,,Já bych řekla, že je docela normální!
Dialogue: 0,0:00:34.85,0:00:36.35,op1,,0000,0000,0000,,To jsem jediná překvapená?
Dialogue: 0,0:00:36.35,0:00:36.66,op1,,0000,0000,0000,,Vepřové kousky!
Dialogue: 0,0:00:36.66,0:00:37.06,op1,,0000,0000,0000,,Skleněné nudle!
Dialogue: 0,0:00:37.06,0:00:37.39,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:00:37.39,0:00:38.18,op2,,0000,0000,0000,,DA-DA-DA-DA-DA!
Dialogue: 0,0:00:38.73,0:00:39.52,op1,,0000,0000,0000,,Ooh?
Dialogue: 0,0:00:39.52,0:00:40.31,op1,,0000,0000,0000,,Víš co tím myslím?
Dialogue: 0,0:00:39.89,0:00:40.73,op1up,,0000,0000,0000,,Moje brýle…
Dialogue: 0,0:00:40.73,0:00:41.81,op1up,,0000,0000,0000,,Kde mám brýle?
Dialogue: 0,0:00:41.90,0:00:43.86,op1,,0000,0000,0000,,Nedávno jsme hrála tu samou bojovku.
Dialogue: 0,0:00:42.48,0:00:43.86,op1up,,0000,0000,0000,,Bez nich nic nevidím…
Dialogue: 0,0:00:44.90,0:00:45.48,op2,,0000,0000,0000,,POM! POM!
Dialogue: 0,0:00:45.48,0:00:46.53,op1,,0000,0000,0000,,Roztleskávačky!
Dialogue: 0,0:00:46.53,0:00:48.07,op2,,0000,0000,0000,,DÁME SI SLADKOST!
Dialogue: 0,0:00:48.07,0:00:48.65,op2,,0000,0000,0000,,LA! LA!
Dialogue: 0,0:00:48.65,0:00:49.70,op1,,0000,0000,0000,,Párty začíná!
Dialogue: 0,0:00:49.70,0:00:51.28,op2,,0000,0000,0000,,MÁŠ TO V KRVI!
Dialogue: 0,0:00:51.28,0:00:51.86,op2,,0000,0000,0000,,ANO!
Dialogue: 0,0:00:51.87,0:00:52.87,op1,,0000,0000,0000,,Zanechává to dojem.
Dialogue: 0,0:00:52.87,0:00:54.32,op1,,0000,0000,0000,,Malé planetky.
Dialogue: 0,0:00:54.33,0:00:54.87,op1,,0000,0000,0000,,Narazili!
Dialogue: 0,0:00:54.87,0:00:55.49,op1,,0000,0000,0000,,A shořeli!
Dialogue: 0,0:00:55.49,0:00:55.87,op1,,0000,0000,0000,,Bylo to ohromné!
Dialogue: 0,0:00:55.87,0:00:56.62,op1,,0000,0000,0000,,Z plných plic křičíme!
Dialogue: 0,0:00:56.62,0:00:57.18,op1,,0000,0000,0000,,ZPÍVEJ A TANCUJ!
Dialogue: 0,0:00:57.54,0:00:58.83,op1,,0000,0000,0000,,Přiznej si to!
Dialogue: 0,0:00:58.83,0:01:03.29,op1,,0000,0000,0000,,Nakonec se stejně budeme smát!
Dialogue: 0,0:01:03.29,0:01:07.05,op1,,0000,0000,0000,,Protože mám na sobě námořnickou uniformu.
Dialogue: 0,0:01:07.05,0:01:09.92,op2,,0000,0000,0000,,To je ono!
Dialogue: 0,0:01:10.51,0:01:14.05,op1,,0000,0000,0000,,Je teprve pondělí a já už mám špatnou náladu.
Dialogue: 0,0:01:14.05,0:01:15.85,op1,,0000,0000,0000,,Co mám dělat?
Dialogue: 0,0:01:16.06,0:01:19.89,op1,,0000,0000,0000,,Raději si vezmu moji letní uniformu -
Dialogue: 0,0:01:19.89,0:01:22.77,op1,,0000,0000,0000,,- je rozkošná!
Dialogue: 0,0:01:23.02,0:01:24.19,op1,,0000,0000,0000,,Trojdílná uniforma!
Dialogue: 0,0:01:24.19,0:01:25.61,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:01:25.61,0:01:26.06,op1,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.07,0:01:26.48,op1,,0000,0000,0000,,Dej se do toho!
Dialogue: 0,0:01:26.48,0:01:26.86,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.86,0:01:27.48,op1,,0000,0000,0000,,SNAŽ SE!
Dialogue: 0,0:01:27.48,0:01:27.65,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:27.65,0:01:29.54,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:01:31.09,0:01:32.13,main,,0000,0000,0000,,To je fakt.
Dialogue: 0,0:01:32.50,0:01:34.55,main,,0000,0000,0000,,Ode dneška nosíme letní uniformy.
Dialogue: 0,0:01:34.55,0:01:37.22,main,,0000,0000,0000,,Někdo jako Konata se nejspíš objeví ještě v zimní.
Dialogue: 0,0:01:37.93,0:01:40.68,main,,0000,0000,0000,,Alespoň jeden ze třídy na to zapomene.
Dialogue: 0,0:01:41.43,0:01:42.85,main,,0000,0000,0000,,Dobré ráno, ségra.
Dialogue: 0,0:01:48.98,0:01:50.65,main,,0000,0000,0000,,Jeden je v tomto domě.
Dialogue: 0,0:01:50.86,0:01:53.65,white,,0000,0000,0000,,{\frz-90\pos(389,161)}lidí
Dialogue: 0,0:01:50.86,0:01:53.65,white,,0000,0000,0000,,{\frz-90\pos(305,211)}Různé druhy
Dialogue: 0,0:01:53.94,0:01:54.65,main,,0000,0000,0000,,Krysy!
Dialogue: 0,0:01:55.32,0:01:58.82,main,,0000,0000,0000,,Zapoměla jsem se přihlásit do té televizní soutěže.
Dialogue: 0,0:01:59.53,0:02:01.07,main,,0000,0000,0000,,Docela jsem chtěla tu cenu.
Dialogue: 0,0:02:01.07,0:02:02.20,main,,0000,0000,0000,,Zástěra z Apron of Love.
Dialogue: 0,0:02:02.49,0:02:04.03,main,,0000,0000,0000,,Nedostatek lásky.
Dialogue: 0,0:02:05.12,0:02:07.96,main,,0000,0000,0000,,Zapoměla jsi protože nemáš dost lásky.
Dialogue: 0,0:02:07.96,0:02:08.75,main,,0000,0000,0000,,To ne.
Dialogue: 0,0:02:08.75,0:02:12.71,main,,0000,0000,0000,,Spíš jsem byla zaneprázdněna se studenskou radou a přípravou na testy.
Dialogue: 0,0:02:12.71,0:02:13.42,main,,0000,0000,0000,,Ne, ne.
Dialogue: 0,0:02:13.42,0:02:15.09,main,,0000,0000,0000,,To jsou jen výmluvy.
Dialogue: 0,0:02:15.59,0:02:17.38,main,,0000,0000,0000,,Nikdy jsem nezapomněla.
Dialogue: 0,0:02:17.38,0:02:20.38,main,,0000,0000,0000,,Nesrovnávej mě s někým kdo celé dny nedělá nic jiného, než se jen baví!
Dialogue: 0,0:02:20.39,0:02:22.34,main,,0000,0000,0000,,A to jak jsi mi to řekla mě vážně naštvalo!
Dialogue: 0,0:02:23.10,0:02:25.76,main,,0000,0000,0000,,Jak vyhraješ tu soutěž?
Dialogue: 0,0:02:25.77,0:02:26.64,main,,0000,0000,0000,,Jednoduše…
Dialogue: 0,0:02:26.93,0:02:28.31,main,,0000,0000,0000,,Láska!
Dialogue: 0,0:02:28.64,0:02:29.44,main,,0000,0000,0000,,Jo, jo.
Dialogue: 0,0:02:29.44,0:02:31.77,main,,0000,0000,0000,,Jsem prostě nemilující ženská.
Dialogue: 0,0:02:31.77,0:02:32.35,main,,0000,0000,0000,,No…
Dialogue: 0,0:02:32.36,0:02:33.56,main,,0000,0000,0000,,Pokud to uděláš jako každý jiný…
Dialogue: 0,0:02:33.57,0:02:35.52,main,,0000,0000,0000,,… tak nevyhraješ.
Dialogue: 0,0:02:36.11,0:02:37.99,main,,0000,0000,0000,,Co dáváš na svoje kresby?
Dialogue: 0,0:02:37.99,0:02:39.57,main,,0000,0000,0000,,Věci co mi pomůžou vyhrát.
Dialogue: 0,0:02:39.86,0:02:41.07,main,,0000,0000,0000,,Jaké?
Dialogue: 0,0:02:41.07,0:02:43.62,main,,0000,0000,0000,,Základ je dobrý pozdrav.
Dialogue: 0,0:02:43.62,0:02:47.95,main,,0000,0000,0000,,Pohlednici musíš upravit, tak aby prosila o poslání.
Dialogue: 0,0:02:47.95,0:02:48.95,main,,0000,0000,0000,,Jako například?
Dialogue: 0,0:02:49.21,0:02:50.58,main,,0000,0000,0000,,Jako…
Dialogue: 0,0:02:50.58,0:02:53.67,main,,0000,0000,0000,,Použít zvýrazňovač na okraje pohlednice.
Dialogue: 0,0:02:54.63,0:02:55.92,main,,0000,0000,0000,,To zazáří.
Dialogue: 0,0:02:56.55,0:03:00.42,main,,0000,0000,0000,,Pak přidáš obrázek související s cenou.
Dialogue: 0,0:03:00.43,0:03:02.01,main,,0000,0000,0000,,Obrázek?
Dialogue: 0,0:03:03.35,0:03:05.10,main,,0000,0000,0000,,Moc dobře ale nemaluji.
Dialogue: 0,0:03:05.10,0:03:06.43,main,,0000,0000,0000,,V tom případě…
Dialogue: 0,0:03:06.97,0:03:09.68,main,,0000,0000,0000,,…jich prostě odešli co nejvíc.
Dialogue: 0,0:03:11.02,0:03:12.27,main,,0000,0000,0000,,To bych mohla zvládnout.
Dialogue: 0,0:03:12.27,0:03:13.23,main,,0000,0000,0000,,Ale…
Dialogue: 0,0:03:13.23,0:03:15.52,main,,0000,0000,0000,,Ať už jich pošleš kolik chceš…
Dialogue: 0,0:03:15.52,0:03:19.78,main,,0000,0000,0000,,…musíš každou napsat ručně.
Dialogue: 0,0:03:20.28,0:03:22.03,main,,0000,0000,0000,,A poslat jich pokaždé sto.
Dialogue: 0,0:03:22.03,0:03:23.07,main,,0000,0000,0000,,Sto?!
Dialogue: 0,0:03:23.07,0:03:25.12,main,,0000,0000,0000,,Je snad něco špatně?
Dialogue: 0,0:03:25.12,0:03:26.87,main,,0000,0000,0000,,Nebude to stát spoustu peněz?
Dialogue: 0,0:03:26.87,0:03:29.29,main,,0000,0000,0000,,Bojuješ za něco co chceš.
Dialogue: 0,0:03:29.29,0:03:31.71,main,,0000,0000,0000,,To vyžaduje vášeň a nasazení!
Dialogue: 0,0:03:31.71,0:03:34.63,main,,0000,0000,0000,,Ale nebylo by levnější to prostě koupit?
Dialogue: 0,0:03:34.63,0:03:37.34,main,,0000,0000,0000,,Některé věci si koupit nemůžeš.
Dialogue: 0,0:03:38.59,0:03:40.71,main,,0000,0000,0000,,Vleze se sto pohledů do schránky?
Dialogue: 0,0:03:40.72,0:03:43.00,main,,0000,0000,0000,,Neposíláš je zároveň…
Dialogue: 0,0:03:43.00,0:03:46.01,main,,0000,0000,0000,,…ale pár teď, pár potom a tak.
Dialogue: 0,0:03:46.01,0:03:47.76,main,,0000,0000,0000,,A to během několika dnů.
Dialogue: 0,0:03:48.89,0:03:51.60,main,,0000,0000,0000,,To záleží na konkrétní soutěži.
Dialogue: 0,0:03:51.60,0:03:54.14,main,,0000,0000,0000,,Někdy je to i risk.
Dialogue: 0,0:03:54.15,0:03:57.48,main,,0000,0000,0000,,Proto je někdy lepší to zkusit bez toho.
Dialogue: 0,0:03:57.48,0:03:59.23,main,,0000,0000,0000,,A někdy je to přesně naopak.
Dialogue: 0,0:03:59.23,0:04:00.69,main,,0000,0000,0000,,Takže co teď?
Dialogue: 0,0:04:00.69,0:04:01.57,main,,0000,0000,0000,,Jak jsem řekla…
Dialogue: 0,0:04:01.57,0:04:02.11,main,,0000,0000,0000,,Láska!
Dialogue: 0,0:04:02.11,0:04:02.78,main,,0000,0000,0000,,Jo, jo.
Dialogue: 0,0:04:03.15,0:04:05.11,main,,0000,0000,0000,,Nicméně, pokud chceš vyhrát…
Dialogue: 0,0:04:05.12,0:04:08.28,main,,0000,0000,0000,,podívej se do pár novin a magazínů
Dialogue: 0,0:04:08.28,0:04:12.33,main,,0000,0000,0000,,a hledej kresby, které pravděpodobně nebudou přitahovat tolik lidí.
Dialogue: 0,0:04:12.33,0:04:14.42,main,,0000,0000,0000,,Budeš mít velkou šanci na úspěch.
Dialogue: 0,0:04:15.13,0:04:16.08,main,,0000,0000,0000,,Zkusím to!
Dialogue: 0,0:04:16.08,0:04:17.50,main,,0000,0000,0000,,Kde je láska teď?
Dialogue: 0,0:04:21.21,0:04:22.97,main,,0000,0000,0000,,No, na to že jste dvojčata…
Dialogue: 0,0:04:22.97,0:04:24.97,main,,0000,0000,0000,,…nevypadáte jedna jako druhá.
Dialogue: 0,0:04:26.18,0:04:31.18,main,,0000,0000,0000,,Jsme totiž dvouvaječná dvojčata.
Dialogue: 0,0:04:31.93,0:04:34.23,main,,0000,0000,0000,,Jednovaječná vypadají stejně…
Dialogue: 0,0:04:34.23,0:04:35.48,main,,0000,0000,0000,,…ale dvou můžou být…
Dialogue: 0,0:04:35.48,0:04:37.69,main,,0000,0000,0000,,…hodně rozdílné jak ve vzhledu, tak v osobnosti.
Dialogue: 0,0:04:37.69,0:04:39.02,main,,0000,0000,0000,,Chápu.
Dialogue: 0,0:04:39.02,0:04:41.15,main,,0000,0000,0000,,Tsakasa je pohodář…
Dialogue: 0,0:04:41.15,0:04:42.15,main,,0000,0000,0000,,…ale Kagami je pěknej blázen.
Dialogue: 0,0:04:42.15,0:04:42.78,main,,0000,0000,0000,,Co že to?!
Dialogue: 0,0:04:43.82,0:04:45.90,main,,0000,0000,0000,,Pokud by jste měli stejné účesy s copánky…
Dialogue: 0,0:04:45.91,0:04:47.57,main,,0000,0000,0000,,…mohli by jste vypadat docela stejně.
Dialogue: 0,0:04:48.24,0:04:49.32,main,,0000,0000,0000,,Kana-chan!
Dialogue: 0,0:04:50.16,0:04:50.66,main,,0000,0000,0000,,To ne.
Dialogue: 0,0:04:50.66,0:04:52.08,main,,0000,0000,0000,,Kagami by nikdy nemohla být roztomilý typ.
Dialogue: 0,0:04:52.08,0:04:52.95,main,,0000,0000,0000,,Sklapni!
Dialogue: 0,0:04:52.95,0:04:55.25,main,,0000,0000,0000,,Nezahrnuj mě tvýma zvrácenýma představama!
Dialogue: 0,0:04:55.83,0:04:57.37,main,,0000,0000,0000,,Můžete používat telepatii?
Dialogue: 0,0:04:58.29,0:04:58.88,main,,0000,0000,0000,,Šak víš.
Dialogue: 0,0:04:58.88,0:04:59.96,main,,0000,0000,0000,,Dvojčata…
Dialogue: 0,0:04:59.96,0:05:01.84,main,,0000,0000,0000,,I když jsou vychována odděleně…
Dialogue: 0,0:05:01.84,0:05:06.64,main,,0000,0000,0000,,…mají podobný životní styl a další věci.
Dialogue: 0,0:05:06.64,0:05:08.59,main,,0000,0000,0000,,A občas si vezmou jiný pár dvojčat.
Dialogue: 0,0:05:09.10,0:05:11.22,main,,0000,0000,0000,,A když je jeden z nich v nebezpečí…
Dialogue: 0,0:05:11.22,0:05:14.77,main,,0000,0000,0000,,…ten druhý to dokáže vycítit na velkou vzdálenost.
Dialogue: 0,0:05:15.27,0:05:17.60,main,,0000,0000,0000,,Jsou tu různé záhadné úkazy.
Dialogue: 0,0:05:17.60,0:05:19.69,main,,0000,0000,0000,,Takže, můžete používat telepatii?
Dialogue: 0,0:05:19.69,0:05:20.98,main,,0000,0000,0000,,No tak do toho.
Dialogue: 0,0:05:20.98,0:05:21.52,main,,0000,0000,0000,,Abrakadabra!
Dialogue: 0,0:05:21.52,0:05:23.36,main,,0000,0000,0000,,Čteš až moc mangy.
Dialogue: 0,0:05:23.69,0:05:26.15,comments,,0000,0000,0000,,
Dialogue: 0,0:05:23.69,0:05:24.86,main,,0000,0000,0000,,To samé s ManaKanou…
Dialogue: 0,0:05:25.24,0:05:26.15,main,,0000,0000,0000,,ManaKana?
Dialogue: 0,0:05:26.53,0:05:29.66,main,,0000,0000,0000,,Slyšela jsem že mají pár rozdílu.
Dialogue: 0,0:05:29.66,0:05:32.16,main,,0000,0000,0000,,Mimochodem, Mana je ta pozorná…
Dialogue: 0,0:05:32.16,0:05:33.87,main,,0000,0000,0000,,… a Kana je ta optimistická.
Dialogue: 0,0:05:33.87,0:05:37.41,main,,0000,0000,0000,,A dokonce zní i trochu jinak když zpívají.
Dialogue: 0,0:05:37.42,0:05:38.42,main,,0000,0000,0000,,To mi připomíná…
Dialogue: 0,0:05:38.42,0:05:42.63,main,,0000,0000,0000,,Přesto, že ManaKana byli dlouho dvouvaječná dvojčata…
Dialogue: 0,0:05:42.63,0:05:45.71,main,,0000,0000,0000,,…nedávno zjistili, že jsou vlastně jednovaječná.
Dialogue: 0,0:05:46.38,0:05:47.55,main,,0000,0000,0000,,Vážně?!
Dialogue: 0,0:05:47.55,0:05:49.34,main,,0000,0000,0000,,Je to vážně tak překvapující?
Dialogue: 0,0:05:49.34,0:05:51.55,main,,0000,0000,0000,,Definice pro jedno a dvouvaječná dvojčata…
Dialogue: 0,0:05:51.55,0:05:53.10,main,,0000,0000,0000,,…se nedávno změnily.
Dialogue: 0,0:05:53.52,0:05:56.85,main,,0000,0000,0000,,Což znamená, že by jste mohli být jednovaječná.
Dialogue: 0,0:05:57.39,0:05:59.27,main,,0000,0000,0000,,Vaše identické rysy by byly…
Dialogue: 0,0:06:00.73,0:06:02.52,main,,0000,0000,0000,,Obě máte rády sladké, že?
Dialogue: 0,0:06:03.40,0:06:03.90,main,,0000,0000,0000,,Miluji ho.
Dialogue: 0,0:06:04.28,0:06:04.65,main,,0000,0000,0000,,Asi.
Dialogue: 0,0:06:05.19,0:06:06.82,main,,0000,0000,0000,,Obě píšete levou rukou.
Dialogue: 0,0:06:07.53,0:06:08.20,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:06:08.61,0:06:09.41,main,,0000,0000,0000,,Jaká je vaše oblíbená barva?
Dialogue: 0,0:06:09.91,0:06:10.45,main,,0000,0000,0000,,Asi bílá.
Dialogue: 0,0:06:10.82,0:06:11.24,main,,0000,0000,0000,,Černá.
Dialogue: 0,0:06:11.53,0:06:13.37,main,,0000,0000,0000,,Stejná barva jako vaše duše.
Dialogue: 0,0:06:13.37,0:06:14.45,main,,0000,0000,0000,,Cos to řekla?!
Dialogue: 0,0:06:18.42,0:06:22.29,main,,0000,0000,0000,,Z toho povídání o vejcích a černém a bílém cukroví mám hlad.
Dialogue: 0,0:06:22.29,0:06:24.09,main,,0000,0000,0000,,Nikdo se nebavil o jídle.
Dialogue: 0,0:06:30.55,0:06:32.18,main,,0000,0000,0000,,Je tu hrozné dusno.
Dialogue: 0,0:06:32.76,0:06:35.18,main,,0000,0000,0000,,Sotva jsme nastoupili a já se už potím.
Dialogue: 0,0:06:35.18,0:06:37.27,main,,0000,0000,0000,,To je tou roční dobou.
Dialogue: 0,0:06:37.48,0:06:39.14,main,,0000,0000,0000,,Horko…
Dialogue: 0,0:06:39.14,0:06:41.19,main,,0000,0000,0000,,Ještě nezapnuli klimatizaci.
Dialogue: 0,0:06:46.23,0:06:47.07,main,,0000,0000,0000,,H-Hej!
Dialogue: 0,0:06:47.40,0:06:49.78,main,,0000,0000,0000,,Horko…
Dialogue: 0,0:06:50.20,0:06:53.41,main,,0000,0000,0000,,Ladybug Samba, punková verze.
Dialogue: 0,0:06:50.20,0:06:53.41,newspaperup,,0000,0000,0000,,
Dialogue: 0,0:06:56.16,0:06:59.87,main,,0000,0000,0000,,Jedna z mých sestřenic se tento měsíc vdává.
Dialogue: 0,0:07:01.25,0:07:03.04,main,,0000,0000,0000,,Takže bude červnová nevěsta.
Dialogue: 0,0:07:03.04,0:07:04.08,main,,0000,0000,0000,,To zní báječně.
Dialogue: 0,0:07:04.09,0:07:05.21,main,,0000,0000,0000,,Jak tahle…
Dialogue: 0,0:07:05.71,0:07:10.42,main,,0000,0000,0000,,…upocená a úmorná roční doba představuje nějaké zvláštní štěstí?
Dialogue: 0,0:07:13.09,0:07:16.64,main,,0000,0000,0000,,Co je radostného na svatbě v červnu?
Dialogue: 0,0:07:17.52,0:07:19.85,main,,0000,0000,0000,,Jméno červen /anglicky June/…
Dialogue: 0,0:07:19.85,0:07:21.56,main,,0000,0000,0000,,…je obecně považováno…
Dialogue: 0,0:07:21.56,0:07:24.19,main,,0000,0000,0000,,…za odvozené ze jména řecké bohyně, Juno.
Dialogue: 0,0:07:24.19,0:07:25.06,main,,0000,0000,0000,,V řecké mytologii…
Dialogue: 0,0:07:25.07,0:07:27.48,main,,0000,0000,0000,,…je Juno známá jako Hera…
Dialogue: 0,0:07:27.48,0:07:29.65,main,,0000,0000,0000,,…bohyně manželství a plodnosti.
Dialogue: 0,0:07:30.15,0:07:33.99,main,,0000,0000,0000,,To proto se říká, že ti kdo se provdají v červnu, měsíci June…
Dialogue: 0,0:07:33.99,0:07:36.78,main,,0000,0000,0000,,…dostanou její ochranu a požehnání.
Dialogue: 0,0:07:37.62,0:07:40.33,main,,0000,0000,0000,,Ale nebyla Hera manželkou Zeuse?
Dialogue: 0,0:07:40.33,0:07:41.54,main,,0000,0000,0000,,Ano, to je pravda.
Dialogue: 0,0:07:42.00,0:07:45.08,main,,0000,0000,0000,,Představuji si Heru extrémně žárlivou…
Dialogue: 0,0:07:45.09,0:07:47.59,main,,0000,0000,0000,,…takže necítím to štěstí a radost.
Dialogue: 0,0:07:48.34,0:07:48.67,main,,0000,0000,0000,,Cože?
Dialogue: 0,0:07:49.21,0:07:52.17,main,,0000,0000,0000,,Chcete mít na své svatbě raději šaty nebo kimono?
Dialogue: 0,0:07:52.63,0:07:54.76,main,,0000,0000,0000,,Raději šaty.
Dialogue: 0,0:07:54.76,0:07:57.60,main,,0000,0000,0000,,Je tu můj sen, už od mala.
Dialogue: 0,0:07:58.43,0:08:00.56,main,,0000,0000,0000,,Také bych si vzala šaty.
Dialogue: 0,0:08:00.56,0:08:02.77,main,,0000,0000,0000,,Ale ani kimono by nebylo špatné.
Dialogue: 0,0:08:04.44,0:08:05.85,main,,0000,0000,0000,,Chtěla bych oboje!
Dialogue: 0,0:08:06.73,0:08:10.23,main,,0000,0000,0000,,Dneska mají minisukně a pestrobarevné šaty.
Dialogue: 0,0:08:11.15,0:08:12.90,main,,0000,0000,0000,,Minisukně na svatbu?
Dialogue: 0,0:08:12.90,0:08:16.20,main,,0000,0000,0000,,Se spoustou ozdob po lemu by to vypadalo roztomile.
Dialogue: 0,0:08:16.57,0:08:17.78,main,,0000,0000,0000,,Spousta snů!
Dialogue: 0,0:08:17.78,0:08:18.87,main,,0000,0000,0000,,Spousta ozdob!
Dialogue: 0,0:08:19.24,0:08:20.29,main,,0000,0000,0000,,Že?
Dialogue: 0,0:08:20.29,0:08:22.29,main,,0000,0000,0000,,Jsi Angel Salvia.
Dialogue: 0,0:08:22.29,0:08:22.87,main,,0000,0000,0000,,Počkat.
Dialogue: 0,0:08:22.87,0:08:25.21,main,,0000,0000,0000,,S tvými vlasy vypadáš spíš jako Daisy.
Dialogue: 0,0:08:25.75,0:08:28.46,main,,0000,0000,0000,,Takže žlutý vršek a minisukně.
Dialogue: 0,0:08:30.38,0:08:31.26,main,,0000,0000,0000,,Tsukasa-san.
Dialogue: 0,0:08:31.26,0:08:32.84,main,,0000,0000,0000,,Minisukně by ti nejspíš slušela.
Dialogue: 0,0:08:32.88,0:08:33.88,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:08:33.88,0:08:35.05,main,,0000,0000,0000,,Oblečení je důležité…
Dialogue: 0,0:08:35.05,0:08:37.39,main,,0000,0000,0000,,…ale osoba, kterou si bereš je důležitá stejně.
Dialogue: 0,0:08:37.85,0:08:39.47,main,,0000,0000,0000,,Přítel z dětství?
Dialogue: 0,0:08:39.47,0:08:41.14,main,,0000,0000,0000,,Nebo někdo bohatý ?
Dialogue: 0,0:08:41.14,0:08:42.90,main,,0000,0000,0000,,Těžké rozhodnutí.
Dialogue: 0,0:08:42.90,0:08:45.23,main,,0000,0000,0000,,Jeden má spoustu peněz…
Dialogue: 0,0:08:45.23,0:08:48.40,main,,0000,0000,0000,,…a schopnost vykouzlit Beho*mi a l***zun.
Dialogue: 0,0:08:49.07,0:08:50.11,main,,0000,0000,0000,,Ale pokud se zvažují city…
Dialogue: 0,0:08:50.36,0:08:51.78,main,,0000,0000,0000,,Ruce dolů Bianco!
Dialogue: 0,0:08:52.11,0:08:53.32,main,,0000,0000,0000,,Oh, Kagami.
Dialogue: 0,0:08:53.32,0:08:54.82,main,,0000,0000,0000,,Ty si ten typ co je na skutečnou lásku, heh?
Dialogue: 0,0:08:54.82,0:08:56.74,main,,0000,0000,0000,,Myslela jsem, že jsi na peníze.
Dialogue: 0,0:08:57.32,0:08:58.45,main,,0000,0000,0000,,Žádný comeback, heh?
Dialogue: 0,0:09:01.41,0:09:02.62,main,,0000,0000,0000,,Hoj, Konato.
Dialogue: 0,0:09:03.83,0:09:06.87,main,,0000,0000,0000,,Mám otázku ohledně hry co jsem si včera půjčila.
Dialogue: 0,0:09:06.88,0:09:07.50,main,,0000,0000,0000,,Oh.
Dialogue: 0,0:09:07.50,0:09:09.13,main,,0000,0000,0000,,Rovnou zadrž, Kagaminyo.
Dialogue: 0,0:09:09.46,0:09:12.59,main,,0000,0000,0000,,Není to větší zábava, když na to přijdeš sama?
Dialogue: 0,0:09:13.63,0:09:14.80,main,,0000,0000,0000,,To máš pravdu.
Dialogue: 0,0:09:14.80,0:09:15.38,main,,0000,0000,0000,,Jo, jo.
Dialogue: 0,0:09:15.76,0:09:18.93,main,,0000,0000,0000,,Když myslíš, že je lepší pokud to vyřeším sama…
Dialogue: 0,0:09:18.93,0:09:21.14,main,,0000,0000,0000,,…budeš si i ty od teď dělat domácí úkoly sama ?
Dialogue: 0,0:09:23.31,0:09:25.68,main,,0000,0000,0000,,Nesetkávají se detektivové v manze a anime…
Dialogue: 0,0:09:25.69,0:09:28.44,main,,0000,0000,0000,,…se strašnou spoustou vražd?
Dialogue: 0,0:09:28.86,0:09:30.77,main,,0000,0000,0000,,Jako v Conanovi nebo Kindaichi.
Dialogue: 0,0:09:30.77,0:09:33.73,main,,0000,0000,0000,,Jinak by neměli příběh.
Dialogue: 0,0:09:33.74,0:09:34.53,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:09:34.53,0:09:36.65,main,,0000,0000,0000,,Nemůžou prostě řešit normální věci?
Dialogue: 0,0:09:36.65,0:09:37.36,main,,0000,0000,0000,,Normální?
Dialogue: 0,0:09:37.66,0:09:41.24,main,,0000,0000,0000,,Jako cestovat po Japonsku a hledat zmizelé osoby.
Dialogue: 0,0:09:41.79,0:09:44.62,main,,0000,0000,0000,,Sledování lidí by šlo dobře přenést do hry.
Dialogue: 0,0:09:44.62,0:09:46.79,main,,0000,0000,0000,,S pořádnou dávkou napětí.
Dialogue: 0,0:09:46.79,0:09:47.92,main,,0000,0000,0000,,Myslím.
Dialogue: 0,0:09:47.92,0:09:51.38,main,,0000,0000,0000,,Také bys mohla sledovat nevěrné manželky.
Dialogue: 0,0:09:51.38,0:09:53.21,main,,0000,0000,0000,,Nebo hledat domácí mazlíčky co se zatoulali?
Dialogue: 0,0:09:53.21,0:09:54.34,main,,0000,0000,0000,,Jo, jo!
Dialogue: 0,0:09:54.67,0:09:57.22,main,,0000,0000,0000,,Ale byla by zábava se na to dívat?
Dialogue: 0,0:09:57.84,0:09:59.89,main,,0000,0000,0000,,Nejspíš vůbec ne.
Dialogue: 0,0:09:59.89,0:10:00.39,main,,0000,0000,0000,,Že?
Dialogue: 0,0:10:00.64,0:10:01.72,main,,0000,0000,0000,,Taky…
Dialogue: 0,0:10:01.72,0:10:03.18,main,,0000,0000,0000,,Případy ve skutečném životě…
Dialogue: 0,0:10:03.18,0:10:06.10,main,,0000,0000,0000,,…neobsahují žádné brilantní triky…
Dialogue: 0,0:10:06.10,0:10:07.60,main,,0000,0000,0000,,…jako ty co najdeš v mange a anime.
Dialogue: 0,0:10:08.98,0:10:10.35,main,,0000,0000,0000,,Myslím že ne.
Dialogue: 0,0:10:10.36,0:10:14.23,main,,0000,0000,0000,,Uměle vyrobené, nevystopovatelné otisky prstů, podle kterých nejde vystopovat pachatel.
Dialogue: 0,0:10:14.78,0:10:17.28,main,,0000,0000,0000,,Věci které jsou o krok napřed před forensními laboratořemi.
Dialogue: 0,0:10:17.28,0:10:17.99,main,,0000,0000,0000,,No…
Dialogue: 0,0:10:17.99,0:10:20.57,main,,0000,0000,0000,,Vše moc dobré by mohlo směřovat k napodobeninám.
Dialogue: 0,0:10:20.57,0:10:22.57,main,,0000,0000,0000,,Nebo být moc citlivé pro odvysílání.
Dialogue: 0,0:10:22.58,0:10:23.74,main,,0000,0000,0000,,Ah, nebo…
Dialogue: 0,0:10:23.95,0:10:28.21,main,,0000,0000,0000,,Opravdu těžké případy skončí neobjasněny nebo…
Dialogue: 0,0:10:28.21,0:10:28.87,main,,0000,0000,0000,,…nevyřešeny.
Dialogue: 0,0:10:28.87,0:10:31.21,main,,0000,0000,0000,,To by bylo špatné ze spousty úhlů!
Dialogue: 0,0:10:32.46,0:10:35.17,main,,0000,0000,0000,,Neštve tě jak několik posledních dnů stále prší?
Dialogue: 0,0:10:35.17,0:10:36.88,main,,0000,0000,0000,,To máš pravdu…
Dialogue: 0,0:10:38.76,0:10:42.05,main,,0000,0000,0000,,Ale já mám ráda déšť…
Dialogue: 0,0:10:42.76,0:10:44.26,main,,0000,0000,0000,,Ale když zmokneš…
Dialogue: 0,0:10:44.26,0:10:46.10,main,,0000,0000,0000,,…tak si ani nemůžeš dát věci ven na usušení…
Dialogue: 0,0:10:46.10,0:10:47.64,main,,0000,0000,0000,,a proto jsem já déšť nikdy ráda neměla.
Dialogue: 0,0:10:48.39,0:10:50.56,main,,0000,0000,0000,,Proč ho máš ráda ty?
Dialogue: 0,0:10:50.56,0:10:55.36,main,,0000,0000,0000,,Protože zruší baseball a můžu se tak dívat na anime.
Dialogue: 0,0:10:55.36,0:10:56.57,main,,0000,0000,0000,,Chápu.
Dialogue: 0,0:10:56.57,0:10:58.30,main,,0000,0000,0000,,Zní to jako něco co bys řekla.
Dialogue: 0,0:10:58.30,0:11:01.03,main,,0000,0000,0000,,Teď mají mnohem víc uzavřených stadiónů.
Dialogue: 0,0:11:05.95,0:11:06.87,main,,0000,0000,0000,,Děje se něco?
Dialogue: 0,0:11:07.37,0:11:13.13,main,,0000,0000,0000,,V anime často vidíš idioty a génie chodit do stejné školy…
Dialogue: 0,0:11:13.67,0:11:14.79,main,,0000,0000,0000,,Hned toho nech.
Dialogue: 0,0:11:15.09,0:11:16.00,main,,0000,0000,0000,,Odpal!
Dialogue: 0,0:11:16.00,0:11:17.80,main,,0000,0000,0000,,Vypadni, pako!
Dialogue: 0,0:11:18.21,0:11:20.67,main,,0000,0000,0000,,Není to ve skutečnosti docela zvláštní?
Dialogue: 0,0:11:20.68,0:11:21.97,main,,0000,0000,0000,,Když si to tak vezmu…
Dialogue: 0,0:11:21.97,0:11:23.22,main,,0000,0000,0000,,…v některých anime…
Dialogue: 0,0:11:23.22,0:11:27.26,main,,0000,0000,0000,,…vypadá že je učiteli stejně jako žákům.
Dialogue: 0,0:11:27.27,0:11:28.52,main,,0000,0000,0000,,Nebo ještě míň.
Dialogue: 0,0:11:28.98,0:11:29.48,main,,0000,0000,0000,,Učitelko.
Dialogue: 0,0:11:30.02,0:11:31.23,main,,0000,0000,0000,,Ano, Ta*****na-san?
Dialogue: 0,0:11:31.64,0:11:35.31,main,,0000,0000,0000,,Svět se vždy zajímal o sledování…
Dialogue: 0,0:11:35.32,0:11:38.73,main,,0000,0000,0000,,…spojitosti mezi fyzickým a mentálním věkem.
Dialogue: 0,0:11:38.74,0:11:39.23,main,,0000,0000,0000,,Nechápu.
Dialogue: 0,0:11:39.69,0:11:42.36,main,,0000,0000,0000,,Není se na co ptát.
Dialogue: 0,0:11:42.36,0:11:45.32,main,,0000,0000,0000,,Ve skutečnosti, nebyl by student který je odlišný od ostatních…
Dialogue: 0,0:11:45.33,0:11:47.03,main,,0000,0000,0000,,…opravdu záhadný?
Dialogue: 0,0:11:49.08,0:11:49.91,main,,0000,0000,0000,,Jo.
Dialogue: 0,0:11:49.91,0:11:52.04,main,,0000,0000,0000,,To stoprocentně souhlasím.
Dialogue: 0,0:11:52.62,0:11:54.29,main,,0000,0000,0000,,Neměla bych to říkat…
Dialogue: 0,0:11:54.29,0:11:56.59,main,,0000,0000,0000,,…ale nevím jak ses na tuhle školu dostala ty, Konato.
Dialogue: 0,0:11:57.13,0:11:58.84,main,,0000,0000,0000,,Seš dobrá v dohánění věcí den předem…
Dialogue: 0,0:11:58.84,0:12:00.88,main,,0000,0000,0000,,…takže ses noc před tím neučila?
Dialogue: 0,0:12:06.01,0:12:07.47,main,,0000,0000,0000,,Cože?
Dialogue: 0,0:12:08.14,0:12:08.47,main,,0000,0000,0000,,Nah.
Dialogue: 0,0:12:08.47,0:12:10.77,main,,0000,0000,0000,,Za jednu noc bych to nestihla.
Dialogue: 0,0:12:11.27,0:12:12.64,main,,0000,0000,0000,,To mě vyděsilo.
Dialogue: 0,0:12:12.64,0:12:15.42,main,,0000,0000,0000,,Jsem ale překvapena, že stále studuješ.
Dialogue: 0,0:12:15.42,0:12:18.27,main,,0000,0000,0000,,To vyšel na DSko nějaký kvíz?
Dialogue: 0,0:12:18.27,0:12:20.57,main,,0000,0000,0000,,Otec stanovil určité podmínky.
Dialogue: 0,0:12:20.99,0:12:22.32,main,,0000,0000,0000,,D by znamenalo Pippin.
Dialogue: 0,0:12:22.32,0:12:23.95,main,,0000,0000,0000,,C by bylo 3DO.
Dialogue: 0,0:12:23.95,0:12:25.99,main,,0000,0000,0000,,B pak PS2.
Dialogue: 0,0:12:25.99,0:12:26.99,main,,0000,0000,0000,,Izuminin otče,
Dialogue: 0,0:12:26.99,0:12:29.28,main,,0000,0000,0000,,vy vážně víte jak jednat s vaší dcerou, co?
Dialogue: 0,0:12:29.74,0:12:30.83,main,,0000,0000,0000,,Zajímalo by mne…
Dialogue: 0,0:12:36.33,0:12:39.46,main,,0000,0000,0000,,Není vážná záhada jak to, že je Tsukasa ve stejné škole jako my?
Dialogue: 0,0:12:39.46,0:12:41.63,main,,0000,0000,0000,,Řekla to naprosto bez mrknutí!
Dialogue: 0,0:12:44.07,0:12:45.00,main,,0000,0000,0000,,Woo-hoo!
Dialogue: 0,0:12:44.47,0:12:46.14,mainup,,0000,0000,0000,,Vrať se za deset let!
Dialogue: 0,0:12:45.00,0:12:47.01,main,,0000,0000,0000,,Zase jsem vyhrála!
Dialogue: 0,0:12:46.18,0:12:48.02,main2,,0000,0000,0000,,Sakra! V bojovkách ji neporazím.
Dialogue: 0,0:12:48.02,0:12:49.22,main,,0000,0000,0000,,Strávila u toho mnohem víc času než já.
Dialogue: 0,0:12:49.31,0:12:52.10,main,,0000,0000,0000,,Klidně mě vyzvi v jiné hře, jestli chceš.
Dialogue: 0,0:12:53.39,0:12:54.69,main,,0000,0000,0000,,Jseš si jistá?
Dialogue: 0,0:12:54.69,0:12:56.15,main,,0000,0000,0000,,Tak potom…
Dialogue: 0,0:12:56.56,0:12:58.11,main,,0000,0000,0000,,Kvíz!
Dialogue: 0,0:12:58.69,0:12:59.77,main,,0000,0000,0000,,T-to ne…
Dialogue: 0,0:12:59.77,0:13:02.57,main,,0000,0000,0000,,Prohrála jsem s Konatou ve vědomostní soutěži?
Dialogue: 0,0:13:02.57,0:13:03.19,main,,0000,0000,0000,,Neuvěřitelné…
Dialogue: 0,0:13:04.40,0:13:05.53,main,,0000,0000,0000,,Neříkej mi…
Dialogue: 0,0:13:05.53,0:13:07.87,main,,0000,0000,0000,,…že si zapamatovala pořadí otázek?
Dialogue: 0,0:13:07.87,0:13:10.58,main,,0000,0000,0000,,Když jde o hry tak má neuvěřitelnou paměť.
Dialogue: 0,0:13:11.29,0:13:12.58,main,,0000,0000,0000,,Co si zahrajeme dál?
Dialogue: 0,0:13:12.58,0:13:13.75,main,,0000,0000,0000,,Nespěchej tak.
Dialogue: 0,0:13:14.58,0:13:16.42,main,,0000,0000,0000,,Heh? To je divné.
Dialogue: 0,0:13:16.42,0:13:19.67,main,,0000,0000,0000,,Neměla jsi Party hru?
Dialogue: 0,0:13:19.67,0:13:22.92,main,,0000,0000,0000,,Jak to, že si pamatuješ všechny hry co mám?
Dialogue: 0,0:13:22.92,0:13:25.63,main,,0000,0000,0000,,V té by jsi mě možná mohla porazit.
Dialogue: 0,0:13:26.72,0:13:28.05,main,,0000,0000,0000,,Hrozně to páchlo!
Dialogue: 0,0:13:28.85,0:13:31.85,main,,0000,0000,0000,,Kagami, sotva si snědla nějaké sušenky.
Dialogue: 0,0:13:31.85,0:13:33.85,main,,0000,0000,0000,,Obvykle je schramstneš.
Dialogue: 0,0:13:33.85,0:13:35.43,main,,0000,0000,0000,,Není tu důvod.
Dialogue: 0,0:13:35.89,0:13:36.73,main,,0000,0000,0000,,Já vím.
Dialogue: 0,0:13:36.73,0:13:38.98,main,,0000,0000,0000,,Hubneš na léto?
Dialogue: 0,0:13:38.98,0:13:40.65,main,,0000,0000,0000,,Tak to není.
Dialogue: 0,0:13:41.02,0:13:43.15,main,,0000,0000,0000,,Né že bys měla někoho komu se můžeš bez plavek ukazovat.
Dialogue: 0,0:13:43.15,0:13:43.90,main,,0000,0000,0000,,Sklapni!
Dialogue: 0,0:13:43.90,0:13:45.19,main,,0000,0000,0000,,To není tvůj problém!
Dialogue: 0,0:13:45.20,0:13:45.86,main,,0000,0000,0000,,Ah, jo.
Dialogue: 0,0:13:45.86,0:13:47.99,main,,0000,0000,0000,,Nenarodili jste se na Tanabatu?
Dialogue: 0,0:13:47.99,0:13:49.87,main,,0000,0000,0000,,Nemáte zanedlouho narozeniny?
Dialogue: 0,0:13:49.87,0:13:50.91,main,,0000,0000,0000,,Jak to že to víš?
Dialogue: 0,0:13:50.91,0:13:53.24,main,,0000,0000,0000,,Pamatuje si naše narozeniny.
Dialogue: 0,0:13:53.95,0:13:55.45,main,,0000,0000,0000,,Sedmnáct, co?
Dialogue: 0,0:13:55.46,0:13:57.62,main,,0000,0000,0000,,Rok před velkýma.
Dialogue: 0,0:13:57.62,0:13:59.38,main,,0000,0000,0000,,Co to znamená?
Dialogue: 0,0:13:59.38,0:14:00.92,main,,0000,0000,0000,,Rok před těma velkýma.
Dialogue: 0,0:14:00.92,0:14:03.80,main,,0000,0000,0000,,…se stávají nešťastné věci.
Dialogue: 0,0:14:04.80,0:14:06.97,main,,0000,0000,0000,,Jak se blíží naše narozeniny…
Dialogue: 0,0:14:06.97,0:14:08.47,main,,0000,0000,0000,,…začínám se bát…
Dialogue: 0,0:14:09.05,0:14:11.18,main,,0000,0000,0000,,…takže jsem koupila několik věcí pro štěstí…
Dialogue: 0,0:14:11.18,0:14:13.14,main,,0000,0000,0000,,…a nějaké magické kameny.
Dialogue: 0,0:14:13.56,0:14:15.35,main,,0000,0000,0000,,To zní jako zábava.
Dialogue: 0,0:14:15.35,0:14:16.56,main,,0000,0000,0000,,To je!
Dialogue: 0,0:14:16.56,0:14:18.31,main,,0000,0000,0000,,Nemohla jsem je přestat nakupovat.
Dialogue: 0,0:14:18.31,0:14:19.14,main,,0000,0000,0000,,A…
Dialogue: 0,0:14:19.15,0:14:20.81,main,,0000,0000,0000,,A já mám amulety proti hněvu.
Dialogue: 0,0:14:20.81,0:14:21.56,main,,0000,0000,0000,,Er, ne.
Dialogue: 0,0:14:21.56,0:14:22.65,main,,0000,0000,0000,,Amulety proti zlu.
Dialogue: 0,0:14:22.65,0:14:24.36,main,,0000,0000,0000,,Amulety proti zuření, aha.
Dialogue: 0,0:14:25.19,0:14:25.94,main,,0000,0000,0000,,Jak…
Dialogue: 0,0:14:25.94,0:14:29.82,main,,0000,0000,0000,,Jak by šlo tě ochránit před otěhotněním a dopravní nehodou zároveň.
Dialogue: 0,0:14:29.82,0:14:30.66,main,,0000,0000,0000,,Jo, jo.
Dialogue: 0,0:14:31.12,0:14:32.28,main,,0000,0000,0000,,Oh, jo.
Dialogue: 0,0:14:32.28,0:14:35.70,main,,0000,0000,0000,,Víte že na vaše narozeniny je den culíku?
Dialogue: 0,0:14:36.37,0:14:37.58,main,,0000,0000,0000,,To nevím.
Dialogue: 0,0:14:37.58,0:14:40.00,main,,0000,0000,0000,,Ale proč 7. červen?
Dialogue: 0,0:14:40.00,0:14:43.84,main,,0000,0000,0000,,Protože Vega z legendy o Tanabatě ho měla.
Dialogue: 0,0:14:43.84,0:14:45.84,main,,0000,0000,0000,,To zní zvláštně.
Dialogue: 0,0:14:45.84,0:14:49.30,main,,0000,0000,0000,,V Japonsku je Společenství Culíku…
Dialogue: 0,0:14:49.30,0:14:54.43,main,,0000,0000,0000,,…a každý rok udělují cenu celebritě o které si myslí, že by jí slušel culík.
Dialogue: 0,0:14:55.56,0:14:56.85,main,,0000,0000,0000,,Vážně…
Dialogue: 0,0:14:56.85,0:15:00.06,main,,0000,0000,0000,,Ale já si myslím, že s culíkem by vypadal dobře každý.
Dialogue: 0,0:15:00.52,0:15:01.65,main,,0000,0000,0000,,Jsi si jistá?
Dialogue: 0,0:15:07.99,0:15:11.07,main,,0000,0000,0000,,Moje vlasy jsou nejkratší…
Dialogue: 0,0:15:11.07,0:15:14.00,main,,0000,0000,0000,,Ale malej culík je dost roztomilý.
Dialogue: 0,0:15:14.00,0:15:16.12,main,,0000,0000,0000,,I tobě to sluší Kona-chan.
Dialogue: 0,0:15:16.12,0:15:18.29,main,,0000,0000,0000,,Mohla bys ho klidně nosit denně.
Dialogue: 0,0:15:18.83,0:15:19.87,main,,0000,0000,0000,,Myslíš?
Dialogue: 0,0:15:20.92,0:15:21.71,main,,0000,0000,0000,,Jasně.
Dialogue: 0,0:15:27.51,0:15:28.88,main,,0000,0000,0000,,S-ségra také…
Dialogue: 0,0:15:28.88,0:15:29.92,main,,0000,0000,0000,,Kagami je…
Dialogue: 0,0:15:29.92,0:15:31.09,main,,0000,0000,0000,,Jasně!
Dialogue: 0,0:15:31.09,0:15:31.84,main,,0000,0000,0000,,Rozumím.
Dialogue: 0,0:15:31.84,0:15:33.34,main,,0000,0000,0000,,Vrátím to jak to bylo!
Dialogue: 0,0:15:33.72,0:15:35.14,main,,0000,0000,0000,,Mužná jako válečník.
Dialogue: 0,0:15:35.14,0:15:36.22,main,,0000,0000,0000,,Cože to!
Dialogue: 0,0:15:37.97,0:15:41.73,main,,0000,0000,0000,,Nenosí opilci kolem hlav často zavázané kravaty?
Dialogue: 0,0:15:41.73,0:15:43.65,main,,0000,0000,0000,,Má to nějaký význam?
Dialogue: 0,0:15:43.65,0:15:44.90,main,,0000,0000,0000,,Oh, tohle?
Dialogue: 0,0:15:44.90,0:15:46.48,main,,0000,0000,0000,,Není to proto, že když si povolí kravatu…
Dialogue: 0,0:15:46.48,0:15:48.48,main,,0000,0000,0000,,…a snaží se jí přetáhnout přes hlavu…
Dialogue: 0,0:15:48.49,0:15:50.44,main,,0000,0000,0000,,…tak se zasekne?
Dialogue: 0,0:15:52.11,0:15:53.36,main,,0000,0000,0000,,V tom případě…
Dialogue: 0,0:15:53.37,0:15:56.91,main,,0000,0000,0000,,…je o dost divnější proč si sebou vždy nosí krabičku.
Dialogue: 0,0:15:56.91,0:15:58.20,main,,0000,0000,0000,,Krabičku?
Dialogue: 0,0:15:58.20,0:16:00.62,main,,0000,0000,0000,,Tu věc co sebou nosí Kato-chan z Drifters.
Dialogue: 0,0:16:01.25,0:16:02.83,main,,0000,0000,0000,,Nechápu…
Dialogue: 0,0:16:02.83,0:16:04.79,main,,0000,0000,0000,,Dnešní opilci…
Dialogue: 0,0:16:04.79,0:16:09.63,main,,0000,0000,0000,,…mají skoro vždy kolem hlav kravatu a nosí si krabičku.
Dialogue: 0,0:16:09.63,0:16:10.38,main,,0000,0000,0000,,Že?
Dialogue: 0,0:16:10.72,0:16:14.34,main,,0000,0000,0000,,Doufám, že Vega a Altair se letos potkají.
Dialogue: 0,0:16:14.80,0:16:16.60,main,,0000,0000,0000,,Sviť. Sviť. Sviť.
Dialogue: 0,0:16:16.60,0:16:19.39,main,,0000,0000,0000,,Doufám, že ty tvoje amulety dokážou zázrak.
Dialogue: 0,0:16:19.39,0:16:20.72,main,,0000,0000,0000,,Starej se o sebe.
Dialogue: 0,0:16:21.81,0:16:24.81,main,,0000,0000,0000,,Na počítačovém festivalu budou všechny varianty QUO kart.
Dialogue: 0,0:16:25.27,0:16:26.11,main,,0000,0000,0000,,Oh, jo…
Dialogue: 0,0:16:26.11,0:16:28.23,main,,0000,0000,0000,,Tys je vlastně vždycky sbírala…
Dialogue: 0,0:16:28.23,0:16:29.69,main,,0000,0000,0000,,…ale použila si je někdy?
Dialogue: 0,0:16:30.10,0:16:31.10,main,,0000,0000,0000,,To ne.
Dialogue: 0,0:16:31.95,0:16:33.28,main,,0000,0000,0000,,Tak proč se snažit?
Dialogue: 0,0:16:33.74,0:16:35.91,main,,0000,0000,0000,,Není to přirozené, chtít každou věc…
Dialogue: 0,0:16:35.91,0:16:37.91,main,,0000,0000,0000,,… související s anime které máš ráda?
Dialogue: 0,0:16:37.91,0:16:39.83,main,,0000,0000,0000,,Jako fanoušek. Jako sběratel.
Dialogue: 0,0:16:39.83,0:16:40.87,main,,0000,0000,0000,,Ne tak docela…
Dialogue: 0,0:16:40.87,0:16:42.16,main,,0000,0000,0000,,…pokud je nikdy nepoužiješ…
Dialogue: 0,0:16:42.16,0:16:43.83,main,,0000,0000,0000,,Nemůžeš je použít!
Dialogue: 0,0:16:43.83,0:16:45.54,main,,0000,0000,0000,,Musíš se o ně dobře starat!
Dialogue: 0,0:16:45.54,0:16:49.67,main,,0000,0000,0000,,Vážně mě štve když vidím jak je někdo používá.
Dialogue: 0,0:16:49.67,0:16:52.13,main,,0000,0000,0000,,Tyhle věci se nesmí používat!
Dialogue: 0,0:16:52.13,0:16:55.93,main,,0000,0000,0000,,Tím spíš je prodat nebo vydražit.
Dialogue: 0,0:16:55.93,0:16:57.55,main,,0000,0000,0000,,Pokud je použiješ…
Dialogue: 0,0:16:57.55,0:16:59.39,main,,0000,0000,0000,,…tak prohraješ jako fanoušek i jako sběratel!
Dialogue: 0,0:17:00.39,0:17:01.97,main,,0000,0000,0000,,Nemám co říct, když se tak rozjedeš.
Dialogue: 0,0:17:02.43,0:17:05.89,main,,0000,0000,0000,,V mém případě potřebuji alespoň tři.
Dialogue: 0,0:17:05.90,0:17:07.90,main,,0000,0000,0000,,Jednu k zachování.
Dialogue: 0,0:17:07.90,0:17:10.27,main,,0000,0000,0000,,Mám na ni speciální krabičku.
Dialogue: 0,0:17:10.28,0:17:13.44,main,,0000,0000,0000,,Pak jednu na kterou se občas podívám.
Dialogue: 0,0:17:13.45,0:17:14.36,main,,0000,0000,0000,,A na co tu poslední?
Dialogue: 0,0:17:14.36,0:17:15.11,main,,0000,0000,0000,,Extra.
Dialogue: 0,0:17:15.11,0:17:16.03,main,,0000,0000,0000,,Na co?
Dialogue: 0,0:17:16.03,0:17:21.54,main,,0000,0000,0000,,Nechceš vždy dva nebo tři kusy od něčeho co máš ráda?
Dialogue: 0,0:17:24.04,0:17:24.71,main,,0000,0000,0000,,Jo, jo.
Dialogue: 0,0:17:24.71,0:17:25.54,main,,0000,0000,0000,,Ty vlastně ne.
Dialogue: 0,0:17:25.54,0:17:27.12,main,,0000,0000,0000,,To jen já jsem geek.
Dialogue: 0,0:17:27.13,0:17:27.92,main,,0000,0000,0000,,Podivín.
Dialogue: 0,0:17:27.92,0:17:28.63,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:17:28.63,0:17:31.67,main,,0000,0000,0000,,Celkem rozumím co tím myslíš.
Dialogue: 0,0:17:32.67,0:17:34.97,main,,0000,0000,0000,,Čekala bych, že Tsukasa je populární u kluků…
Dialogue: 0,0:17:34.97,0:17:36.93,main,,0000,0000,0000,,…ale nikdy jsem nic takového neslyšela.
Dialogue: 0,0:17:37.84,0:17:40.64,main,,0000,0000,0000,,Tsukasa je docela normální a moc nevyčnívá.
Dialogue: 0,0:17:41.60,0:17:44.14,main,,0000,0000,0000,,Není to tím, že má šílenou sestru, která ji střeží jako orel?
Dialogue: 0,0:17:44.14,0:17:45.35,main,,0000,0000,0000,,Řeklas něco?
Dialogue: 0,0:17:45.85,0:17:47.81,main,,0000,0000,0000,,Nejsem si jistá, jestli jsem úplně normální…
Dialogue: 0,0:17:47.81,0:17:50.23,main,,0000,0000,0000,,…je to spíš tím že Kona-chan a Yuki-chan jsou vážně unikátní.
Dialogue: 0,0:17:50.65,0:17:53.73,main,,0000,0000,0000,,Miyuki zná vše možné.
Dialogue: 0,0:17:53.74,0:17:54.65,main,,0000,0000,0000,,A je pěkná.
Dialogue: 0,0:17:54.74,0:17:55.65,main,,0000,0000,0000,,Má dobrou postavu.
Dialogue: 0,0:17:55.65,0:17:56.86,main,,0000,0000,0000,,Nosí brýle.
Dialogue: 0,0:17:56.86,0:17:57.91,main,,0000,0000,0000,,Bojí se zubaře.
Dialogue: 0,0:17:57.91,0:17:58.41,main,,0000,0000,0000,,Je to nešika.
Dialogue: 0,0:17:58.41,0:17:59.57,main,,0000,0000,0000,,Neví si rady ve spoustě věcí.
Dialogue: 0,0:17:59.00,0:18:01.05,main2,,0000,0000,0000,,Hej. Tos trochu vykolejila.
Dialogue: 0,0:18:01.70,0:18:04.75,main,,0000,0000,0000,,Ale je pravda, že jsem o Miyuki nic neslyšela.
Dialogue: 0,0:18:04.75,0:18:06.21,main,,0000,0000,0000,,Možná je to proto, že je tak pěkná…
Dialogue: 0,0:18:06.21,0:18:07.67,main,,0000,0000,0000,,…že se ji každý bojí oslovit?
Dialogue: 0,0:18:07.67,0:18:09.17,main,,0000,0000,0000,,To pochybuji.
Dialogue: 0,0:18:09.17,0:18:10.00,main,,0000,0000,0000,,Je milá…
Dialogue: 0,0:18:10.00,0:18:11.38,main,,0000,0000,0000,,…a je ostatním hodně nápomocná.
Dialogue: 0,0:18:11.38,0:18:12.63,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:18:12.63,0:18:14.92,main,,0000,0000,0000,,Možná ji zve spousta kluků.
Dialogue: 0,0:18:14.92,0:18:16.67,main,,0000,0000,0000,,Ale ona je odmítá?
Dialogue: 0,0:18:16.68,0:18:18.01,main,,0000,0000,0000,,Odmítá, heh…
Dialogue: 0,0:18:18.01,0:18:18.59,main,,0000,0000,0000,,Nah.
Dialogue: 0,0:18:18.59,0:18:21.18,main,,0000,0000,0000,,Možná si neuvědomuje, že ji zvou ven na rande.
Dialogue: 0,0:18:21.18,0:18:22.81,main,,0000,0000,0000,,To je možné.
Dialogue: 0,0:18:22.81,0:18:24.81,main,,0000,0000,0000,,Yuki-chan může být v tomto trochu nemotorná…
Dialogue: 0,0:18:24.81,0:18:27.18,main,,0000,0000,0000,,…takže nepřímý přístup nemusí fungovat.
Dialogue: 0,0:18:27.19,0:18:30.69,main,,0000,0000,0000,,Páni, kluci z naší třídy jsou kničemu.
Dialogue: 0,0:18:30.69,0:18:33.11,main,,0000,0000,0000,,To ani jeden nemá odvahu…
Dialogue: 0,0:18:33.11,0:18:35.94,main,,0000,0000,0000,,…aby řekl co cítí k Miyuki?
Dialogue: 0,0:18:35.94,0:18:37.24,main,,0000,0000,0000,,Asi ne…
Dialogue: 0,0:18:37.24,0:18:40.28,main,,0000,0000,0000,,Ve třídě nemáme moc maniaků.
Dialogue: 0,0:18:40.78,0:18:43.95,main,,0000,0000,0000,,Miyukin typ je největší obětí maniaků a úchlů.
Dialogue: 0,0:18:45.66,0:18:47.08,main,,0000,0000,0000,,Maniaků, heh?
Dialogue: 0,0:18:48.12,0:18:49.04,main,,0000,0000,0000,,Že?
Dialogue: 0,0:18:49.04,0:18:49.67,main,,0000,0000,0000,,Oh.
Dialogue: 0,0:18:49.67,0:18:51.88,main,,0000,0000,0000,,Už je tak pozdě.
Dialogue: 0,0:18:51.88,0:18:53.79,main,,0000,0000,0000,,Budu muset jít.
Dialogue: 0,0:18:54.09,0:18:54.84,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:18:55.30,0:18:58.92,main,,0000,0000,0000,,Užijte si to tu.
Dialogue: 0,0:18:59.47,0:19:01.26,main,,0000,0000,0000,,Tak já půjdu.
Dialogue: 0,0:19:01.76,0:19:02.97,main,,0000,0000,0000,,Zatím!
Dialogue: 0,0:19:02.97,0:19:04.22,main,,0000,0000,0000,,Pa-pa.
Dialogue: 0,0:19:06.27,0:19:07.22,main,,0000,0000,0000,,Nicméně…
Dialogue: 0,0:19:07.23,0:19:10.64,main,,0000,0000,0000,,Jak jsem vám říkala ten příběh…
Dialogue: 0,0:19:11.77,0:19:12.52,main,,0000,0000,0000,,O-Oh?
Dialogue: 0,0:19:13.31,0:19:14.44,main,,0000,0000,0000,,M-Moc se omlouvám!
Dialogue: 0,0:19:14.69,0:19:15.82,main,,0000,0000,0000,,Jaj!
Dialogue: 0,0:19:16.28,0:19:17.65,main,,0000,0000,0000,,Vážně se omlouvám.
Dialogue: 0,0:19:17.65,0:19:19.19,main,,0000,0000,0000,,To nic…
Dialogue: 0,0:19:19.20,0:19:20.90,main,,0000,0000,0000,,To se stane vždycky, co?
Dialogue: 0,0:19:20.91,0:19:23.16,main,,0000,0000,0000,,Já to udělala včera.
Dialogue: 0,0:19:23.16,0:19:24.16,main,,0000,0000,0000,,Já také.
Dialogue: 0,0:19:24.58,0:19:27.08,main,,0000,0000,0000,,Každý to udělal alespoň jednou.
Dialogue: 0,0:19:27.08,0:19:28.29,main,,0000,0000,0000,,Že, Konato?
Dialogue: 0,0:19:28.29,0:19:29.00,main,,0000,0000,0000,,Já ne.
Dialogue: 0,0:19:29.71,0:19:32.42,main,,0000,0000,0000,,To je to co bys nazvala schopnost rozjet lidi.
Dialogue: 0,0:19:33.13,0:19:33.88,main,,0000,0000,0000,,Miyuki-san.
Dialogue: 0,0:19:34.42,0:19:35.09,main,,0000,0000,0000,,Dobrá práce!
Dialogue: 0,0:19:35.13,0:19:38.13,copyrights,,0000,0000,0000,,{\pos(185,50)}Překlad od paralaXe.
Dialogue: 0,0:19:35.13,0:19:38.13,copyrights,,0000,0000,0000,,{\pos(199,76)}Domovská stránka - www.otakogu.cz
Dialogue: 0,0:19:40.97,0:19:42.09,main,,0000,0000,0000,,Hodně štěstí!
Dialogue: 0,0:19:42.47,0:19:43.18,main,,0000,0000,0000,,Ahoj!
Dialogue: 0,0:19:43.18,0:19:46.18,main,,0000,0000,0000,,Dnes je tu další žůžovní epizoda našeho Lucky Channelu!
Dialogue: 0,0:19:46.85,0:19:48.93,main,,0000,0000,0000,,Já jsme váš průvodce, Akira Kogami.
Dialogue: 0,0:19:48.93,0:19:49.39,main,,0000,0000,0000,,A…
Dialogue: 0,0:19:49.39,0:19:50.27,main,,0000,0000,0000,,Uh.
Dialogue: 0,0:19:50.27,0:19:53.02,main,,0000,0000,0000,,Já jsem Akira-samin asistent, Minoru Shiraishi.
Dialogue: 0,0:19:53.02,0:19:53.60,main,,0000,0000,0000,,Je mi ctí.
Dialogue: 0,0:19:53.98,0:19:54.94,main,,0000,0000,0000,,Okaj, okaj.
Dialogue: 0,0:19:54.94,0:19:57.02,main,,0000,0000,0000,,Tak tohle máme za sebou…
Dialogue: 0,0:19:57.03,0:19:58.32,main,,0000,0000,0000,,Ta-da!
Dialogue: 0,0:19:58.32,0:19:59.94,main,,0000,0000,0000,,Dnes již po třetí!
Dialogue: 0,0:19:59.94,0:20:01.53,main,,0000,0000,0000,,Co si o tom myslíš, Shiraishi-sane?
Dialogue: 0,0:20:01.53,0:20:02.70,main,,0000,0000,0000,,Už si na to zvykáš?
Dialogue: 0,0:20:02.70,0:20:03.36,main,,0000,0000,0000,,No…
Dialogue: 0,0:20:03.37,0:20:04.62,main,,0000,0000,0000,,Ještě ne…
Dialogue: 0,0:20:04.62,0:20:05.12,main,,0000,0000,0000,,Omlouvám se.
Dialogue: 0,0:20:06.37,0:20:10.87,main,,0000,0000,0000,,Cítím, že se mi Shiraishi-san ještě neotevřel.
Dialogue: 0,0:20:11.50,0:20:13.71,main,,0000,0000,0000,,A vypadá to, že sedíme dál od sebe než normálně.
Dialogue: 0,0:20:14.21,0:20:16.59,main,,0000,0000,0000,,Tomuhle říkají emoční odstup?
Dialogue: 0,0:20:16.59,0:20:17.04,main,,0000,0000,0000,,Fňuk.
Dialogue: 0,0:20:17.71,0:20:19.67,main,,0000,0000,0000,,T-T-To není pravda!
Dialogue: 0,0:20:20.26,0:20:21.05,main,,0000,0000,0000,,Okaj!
Dialogue: 0,0:20:21.05,0:20:23.68,main,,0000,0000,0000,,Jdeme na to!
Dialogue: 0,0:20:23.68,0:20:24.39,main,,0000,0000,0000,,Um…
Dialogue: 0,0:20:24.39,0:20:27.35,main,,0000,0000,0000,,Dnes máme něco co budou fanoušci milovat!
Dialogue: 0,0:20:27.35,0:20:28.35,main,,0000,0000,0000,,Hádejte co?
Dialogue: 0,0:20:28.35,0:20:31.48,main,,0000,0000,0000,,Budeme dělat naše první představení členů normálního natáčení!
Dialogue: 0,0:20:31.48,0:20:32.35,main,,0000,0000,0000,,Da! Da-da-dum!
Dialogue: 0,0:20:32.73,0:20:33.60,main,,0000,0000,0000,,Okaj, uh…
Dialogue: 0,0:20:33.60,0:20:35.85,main,,0000,0000,0000,,Dnes vám představíme Miyuki Takara-san.
Dialogue: 0,0:20:36.27,0:20:37.40,main,,0000,0000,0000,,Tak se podíváme…
Dialogue: 0,0:20:37.40,0:20:41.74,main,,0000,0000,0000,,Reprezentuje třídu, dobrá výchova, zna-
Dialogue: 0,0:20:41.74,0:20:42.78,main,,0000,0000,0000,,Znalosti.
Dialogue: 0,0:20:42.78,0:20:43.86,main,,0000,0000,0000,,Znalosti…
Dialogue: 0,0:20:43.86,0:20:46.41,main,,0000,0000,0000,,…ve všech možných směrech. Perfektní člověk.
Dialogue: 0,0:20:46.41,0:20:47.28,main,,0000,0000,0000,,Shiraishi-sane.
Dialogue: 0,0:20:47.28,0:20:49.49,main,,0000,0000,0000,,Ty znáš Miyuki-san, že?
Dialogue: 0,0:20:49.49,0:20:51.25,main,,0000,0000,0000,,Ale samozřejmě!
Dialogue: 0,0:20:51.25,0:20:52.91,main,,0000,0000,0000,,Je hrozně skvělá, že?
Dialogue: 0,0:20:52.91,0:20:54.50,main,,0000,0000,0000,,Ty její brýle!
Dialogue: 0,0:20:54.50,0:20:56.21,main,,0000,0000,0000,,A taky je hrozně krásná.
Dialogue: 0,0:20:56.21,0:20:57.67,main,,0000,0000,0000,,Ten její pohled…
Dialogue: 0,0:20:57.67,0:21:00.46,main,,0000,0000,0000,,…čekaly byste, že je nepřístupná…
Dialogue: 0,0:21:00.46,0:21:03.30,main,,0000,0000,0000,,…ale ve skutečnosti je velice přátelská…
Dialogue: 0,0:21:03.30,0:21:06.22,main,,0000,0000,0000,,Přála bych si abys tak moc nemluvil v radio show…
Dialogue: 0,0:21:07.55,0:21:09.68,main,,0000,0000,0000,,Když jde o Miyuki tak nemůžeš přestat mluvit.
Dialogue: 0,0:21:10.47,0:21:11.18,main,,0000,0000,0000,,Ne!
Dialogue: 0,0:21:11.18,0:21:12.47,main,,0000,0000,0000,,Tak to není…
Dialogue: 0,0:21:12.48,0:21:13.52,main,,0000,0000,0000,,No, jo.
Dialogue: 0,0:21:13.52,0:21:15.39,main,,0000,0000,0000,,Takže Miyuki Takara-san?
Dialogue: 0,0:21:15.40,0:21:16.77,main,,0000,0000,0000,,Pěkná tvářička.
Dialogue: 0,0:21:16.77,0:21:17.94,main,,0000,0000,0000,,Velké prsa.
Dialogue: 0,0:21:17.94,0:21:19.40,main,,0000,0000,0000,,A další hezké části.
Dialogue: 0,0:21:19.95,0:21:22.23,main,,0000,0000,0000,,To má ale štěstí, že je tak pěkná, co?
Dialogue: 0,0:21:20.65,0:21:22.23,main2,,0000,0000,0000,,Akiro-sama, to je trochu…
Dialogue: 0,0:21:22.24,0:21:24.11,main,,0000,0000,0000,,Maniaci ji milují.
Dialogue: 0,0:21:24.11,0:21:24.82,main,,0000,0000,0000,,Ale šak víš…
Dialogue: 0,0:21:24.11,0:21:24.82,main2,,0000,0000,0000,,Miniaci, to je trochu…
Dialogue: 0,0:21:24.82,0:21:27.91,main,,0000,0000,0000,,Pokud de o maniaky, tak já mám obrovský úspěch u pedofilů…
Dialogue: 0,0:21:27.91,0:21:29.58,main,,0000,0000,0000,,To je špatné!
Dialogue: 0,0:21:29.58,0:21:31.45,main,,0000,0000,0000,,Je čas se rozloučit!
Dialogue: 0,0:21:31.58,0:21:32.45,main,,0000,0000,0000,,A tak…
Dialogue: 0,0:21:32.45,0:21:39.34,main,,0000,0000,0000,,Posílejte nám vaše komentáře a příspěvky k show prosím!
Dialogue: 0,0:21:39.79,0:21:41.46,main,,0000,0000,0000,,Těšte se na příště!
Dialogue: 0,0:21:41.46,0:21:42.92,main,,0000,0000,0000,,Ahoj!
Dialogue: 0,0:21:44.13,0:21:45.80,main,,0000,0000,0000,,Tak jaká je ve skutečnosti?
Dialogue: 0,0:21:46.22,0:21:47.30,main,,0000,0000,0000,,Naprosto ta samá.
Dialogue: 0,0:21:47.30,0:21:48.34,main,,0000,0000,0000,,To ne!
Dialogue: 0,0:21:48.35,0:21:51.51,main,,0000,0000,0000,,Nevykouří o přestávce v zákulisí krabičku cigaret?
Dialogue: 0,0:21:51.51,0:21:53.47,main,,0000,0000,0000,,To se neboj. Není jako ty.
Dialogue: 0,0:21:57.48,0:21:58.98,main,,0000,0000,0000,,To byla zábava!
Dialogue: 0,0:21:58.98,0:22:00.61,main,,0000,0000,0000,,Zvláštní písnička.
Dialogue: 0,0:22:00.61,0:22:03.78,main,,0000,0000,0000,,Ráda konečně slyším písničku kterou si všichni můžou vychutnat.
Dialogue: 0,0:22:04.32,0:22:06.28,main,,0000,0000,0000,,Znáš další, Kagami.
Dialogue: 0,0:22:06.86,0:22:08.61,main,,0000,0000,0000,,Uhádneš to podle intra.
Dialogue: 0,0:22:08.62,0:22:10.82,main,,0000,0000,0000,,Je to další bláznivost?
Dialogue: 0,0:22:14.75,0:22:15.87,main,,0000,0000,0000,,Cože to je za písničku?
Dialogue: 0,0:22:15.87,0:22:17.12,main,,0000,0000,0000,,Nevím…
Dialogue: 0,0:22:17.12,0:22:17.79,main,,0000,0000,0000,,Kagami.
Dialogue: 0,0:22:17.79,0:22:18.79,main,,0000,0000,0000,,Odpověď je…
Dialogue: 0,0:22:19.71,0:22:21.38,main,,0000,0000,0000,,Je to z FMP. FMP. (Full Metal Panic)
Dialogue: 0,0:22:22.30,0:22:24.42,main,,0000,0000,0000,,A je to Kagamina oblíbená…
Dialogue: 0,0:22:24.42,0:22:25.51,main,,0000,0000,0000,,Fumoffu!
Dialogue: 0,0:22:25.51,0:22:26.63,main,,0000,0000,0000,,Vážně…
Dialogue: 0,0:22:26.63,0:22:28.51,main,,0000,0000,0000,,Já jen četla mangu…
Dialogue: 0,0:22:28.51,0:22:29.89,main,,0000,0000,0000,,…a viděla jen kousek.
Dialogue: 0,0:22:29.89,0:22:39.85,mainup,,0000,0000,0000,,Řekni dne kdy slzy začnou téct…
Dialogue: 0,0:22:33.68,0:22:35.22,main,,0000,0000,0000,,Jsi vážně dobrá Kona-chan!
Dialogue: 0,0:22:35.23,0:22:37.48,main,,0000,0000,0000,,Zníš jako dospělá.
Dialogue: 0,0:22:37.48,0:22:39.85,main,,0000,0000,0000,,To to jen imituje?
Dialogue: 0,0:22:40.02,0:22:46.66,mainup,,0000,0000,0000,,…chci tě mít u sebe.
Dialogue: 0,0:22:43.32,0:22:43.94,main,,0000,0000,0000,,Tak?
Dialogue: 0,0:22:43.94,0:22:45.94,main,,0000,0000,0000,,Co budete zpívat?
Dialogue: 0,0:22:46.61,0:22:48.45,main,,0000,0000,0000,,Zajímalo by mě, jestli je tady něco co dokážu zazpívat…
Dialogue: 0,0:22:48.45,0:22:50.57,main,,0000,0000,0000,,Neznám moc písniček…
Dialogue: 0,0:22:49.61,0:22:59.25,mainup,,0000,0000,0000,,Těžké časy přijdou jistě…
Dialogue: 0,0:22:50.57,0:22:53.24,main,,0000,0000,0000,,To máš fuk.
Dialogue: 0,0:22:53.24,0:22:55.95,main,,0000,0000,0000,,Tak zpívej co chceš!
Dialogue: 0,0:22:55.95,0:22:56.75,main,,0000,0000,0000,,Um…
Dialogue: 0,0:22:56.75,0:22:58.29,main,,0000,0000,0000,,Tak se podívám.
Dialogue: 0,0:22:58.29,0:23:00.54,main,,0000,0000,0000,,Nejsem si moc jistá mým zpíváním…
Dialogue: 0,0:22:59.25,0:23:08.01,mainup,,0000,0000,0000,,Čím víc se snažíme, tím víc ztrácíme potuchy o odpovědi.
Dialogue: 0,0:23:00.54,0:23:01.96,main,,0000,0000,0000,,To je v pohodě.
Dialogue: 0,0:23:01.96,0:23:03.59,main,,0000,0000,0000,,Slyšíme tě jen my.
Dialogue: 0,0:23:03.59,0:23:05.09,main,,0000,0000,0000,,Jen v klidu.
Dialogue: 0,0:23:05.09,0:23:06.63,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:23:06.63,0:23:07.59,main,,0000,0000,0000,,V tom případě…
Dialogue: 0,0:23:08.84,0:23:16.85,mainup,,0000,0000,0000,,Pokud vidíš duhy po dešti nad naším městem…
Dialogue: 0,0:23:17.23,0:23:20.64,mainup,,0000,0000,0000,,Tak se odhodlej.
Dialogue: 0,0:23:20.77,0:23:24.90,mainup,,0000,0000,0000,,Něco má právě začít…
Dialogue: 0,0:23:24.90,0:23:26.90,mainup,,0000,0000,0000,,Další epizoda
Dialogue: 0,0:23:26.90,0:23:28.90,mainup,,0000,0000,0000,,
