Dialogue: 0,0:00:01.11,0:00:02.56,main,,0000,0000,0000,,Tak začneme.
Dialogue: 0,0:00:02.57,0:00:03.61,main,,0000,0000,0000,,Jdeme na to.
Dialogue: 0,0:00:04.19,0:00:05.98,main,,0000,0000,0000,,No tak už začněme!
Dialogue: 0,0:00:06.32,0:00:07.49,op1,,0000,0000,0000,,Okolo 3 cm!
Dialogue: 0,0:00:07.49,0:00:08.78,op1,,0000,0000,0000,,Je to snad málo?
Dialogue: 0,0:00:08.78,0:00:09.03,op1,,0000,0000,0000,,Jejda!
Dialogue: 0,0:00:09.03,0:00:10.86,op1,,0000,0000,0000,,Schované pod naší uniformou!
Dialogue: 0,0:00:10.87,0:00:11.91,op1,,0000,0000,0000,,Mám toho nechat?
Dialogue: 0,0:00:11.91,0:00:12.20,op1,,0000,0000,0000,,JEJ!
Dialogue: 0,0:00:12.20,0:00:13.28,op1,,0000,0000,0000,,Jít do toho?
Dialogue: 0,0:00:13.28,0:00:14.08,op1,,0000,0000,0000,,Snažit se?
Dialogue: 0,0:00:14.08,0:00:15.66,op1,,0000,0000,0000,,Vychutnat si každý okamžik. Jo!
Dialogue: 0,0:00:15.66,0:00:16.25,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.26,0:00:16.50,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:16.50,0:00:16.90,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.91,0:00:17.25,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:17.25,0:00:17.87,op1,,0000,0000,0000,,Jen mezi námi dvěma!
Dialogue: 0,0:00:17.87,0:00:20.58,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:00:22.56,0:00:23.36,logo,,0000,0000,0000,,{\move(108,218,202,218,0,0)}LUCKY
Dialogue: 0,0:00:23.36,0:00:25.08,logo,,0000,0000,0000,,{\pos(202,218)}LUCKY
Dialogue: 0,0:00:22.56,0:00:23.36,logo,,0000,0000,0000,,{\move(638,218,459,218,0,0)}STAR
Dialogue: 0,0:00:23.36,0:00:25.08,logo,,0000,0000,0000,,{\pos(459,218)}STAR
Dialogue: 0,0:00:25.51,0:00:26.09,op1,,0000,0000,0000,,Cítím se nějak nesvá.
Dialogue: 0,0:00:26.09,0:00:26.71,op1,,0000,0000,0000,,Co jsem to jedla?
Dialogue: 0,0:00:26.71,0:00:27.30,op1,,0000,0000,0000,,Mám tě hrozně ráda!
Dialogue: 0,0:00:27.30,0:00:27.51,op1,,0000,0000,0000,,Oops!
Dialogue: 0,0:00:27.51,0:00:28.55,op1,,0000,0000,0000,,Jedno z nich je špatně!
Dialogue: 0,0:00:28.55,0:00:29.30,op1,,0000,0000,0000,,Relaxovat!
Dialogue: 0,0:00:29.30,0:00:29.93,op1,,0000,0000,0000,,Nic nedělat!
Dialogue: 0,0:00:29.93,0:00:30.47,op1,,0000,0000,0000,,Flákat se!
Dialogue: 0,0:00:30.47,0:00:31.55,op1,,0000,0000,0000,,Už toho nechej!
Dialogue: 0,0:00:31.55,0:00:33.21,op1,,0000,0000,0000,,Je ta nová holka oblíbená?
Dialogue: 0,0:00:33.21,0:00:34.85,op1,,0000,0000,0000,,Já bych řekla, že je docela normální!
Dialogue: 0,0:00:34.85,0:00:36.35,op1,,0000,0000,0000,,To jsem jediná překvapená?
Dialogue: 0,0:00:36.35,0:00:36.66,op1,,0000,0000,0000,,Vepřové kousky!
Dialogue: 0,0:00:36.66,0:00:37.06,op1,,0000,0000,0000,,Skleněné nudle!
Dialogue: 0,0:00:37.06,0:00:37.39,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:00:37.39,0:00:38.18,op2,,0000,0000,0000,,DA-DA-DA-DA-DA!
Dialogue: 0,0:00:38.73,0:00:39.52,op1,,0000,0000,0000,,Ooh?
Dialogue: 0,0:00:39.52,0:00:40.31,op1,,0000,0000,0000,,Víš co tím myslím?
Dialogue: 0,0:00:39.89,0:00:40.73,op1up,,0000,0000,0000,,Moje brýle…
Dialogue: 0,0:00:40.73,0:00:41.81,op1up,,0000,0000,0000,,Kde mám brýle?
Dialogue: 0,0:00:41.90,0:00:43.86,op1,,0000,0000,0000,,Nedávno jsme hrála tu samou bojovku.
Dialogue: 0,0:00:42.48,0:00:43.86,op1up,,0000,0000,0000,,Bez nich nic nevidím…
Dialogue: 0,0:00:44.90,0:00:45.48,op2,,0000,0000,0000,,POM! POM!
Dialogue: 0,0:00:45.48,0:00:46.53,op1,,0000,0000,0000,,Roztleskávačky!
Dialogue: 0,0:00:46.53,0:00:48.07,op2,,0000,0000,0000,,DÁME SI SLADKOST!
Dialogue: 0,0:00:48.07,0:00:48.65,op2,,0000,0000,0000,,LA! LA!
Dialogue: 0,0:00:48.65,0:00:49.70,op1,,0000,0000,0000,,Párty začíná!
Dialogue: 0,0:00:49.70,0:00:51.28,op2,,0000,0000,0000,,MÁŠ TO V KRVI!
Dialogue: 0,0:00:51.28,0:00:51.86,op2,,0000,0000,0000,,ANO!
Dialogue: 0,0:00:51.87,0:00:52.87,op1,,0000,0000,0000,,Zanechává to dojem.
Dialogue: 0,0:00:52.87,0:00:54.32,op1,,0000,0000,0000,,Malé planetky.
Dialogue: 0,0:00:54.33,0:00:54.87,op1,,0000,0000,0000,,Narazili!
Dialogue: 0,0:00:54.87,0:00:55.49,op1,,0000,0000,0000,,A shořeli!
Dialogue: 0,0:00:55.49,0:00:55.87,op1,,0000,0000,0000,,Bylo to ohromné!
Dialogue: 0,0:00:55.87,0:00:56.62,op1,,0000,0000,0000,,Z plných plic křičíme!
Dialogue: 0,0:00:56.62,0:00:57.18,op1,,0000,0000,0000,,ZPÍVEJ A TANCUJ!
Dialogue: 0,0:00:57.54,0:00:58.83,op1,,0000,0000,0000,,Přiznej si to!
Dialogue: 0,0:00:58.83,0:01:03.29,op1,,0000,0000,0000,,Nakonec se stejně budeme smát!
Dialogue: 0,0:01:03.29,0:01:07.05,op1,,0000,0000,0000,,Protože mám na sobě námořnickou uniformu.
Dialogue: 0,0:01:07.05,0:01:09.92,op2,,0000,0000,0000,,To je ono!
Dialogue: 0,0:01:10.51,0:01:14.05,op1,,0000,0000,0000,,Je teprve pondělí a já už mám špatnou náladu.
Dialogue: 0,0:01:14.05,0:01:15.85,op1,,0000,0000,0000,,Co mám dělat?
Dialogue: 0,0:01:16.06,0:01:19.89,op1,,0000,0000,0000,,Raději si vezmu moji letní uniformu -
Dialogue: 0,0:01:19.89,0:01:22.77,op1,,0000,0000,0000,,- je rozkošná!
Dialogue: 0,0:01:23.02,0:01:24.19,op1,,0000,0000,0000,,Trojdílná uniforma!
Dialogue: 0,0:01:24.19,0:01:25.61,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:01:25.61,0:01:26.06,op1,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.07,0:01:26.48,op1,,0000,0000,0000,,Dej se do toho!
Dialogue: 0,0:01:26.48,0:01:26.86,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.86,0:01:27.48,op1,,0000,0000,0000,,SNAŽ SE!
Dialogue: 0,0:01:27.48,0:01:27.65,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:27.65,0:01:29.54,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:01:32.04,0:01:33.16,main,,0000,0000,0000,,Připravit!
Dialogue: 0,0:01:33.75,0:01:34.79,main,,0000,0000,0000,,Pozor!
Dialogue: 0,0:01:44.63,0:01:45.84,main,,0000,0000,0000,,Wow, zase jsi to zvládla.
Dialogue: 0,0:01:45.84,0:01:46.97,main,,0000,0000,0000,,To bylo úžasné.
Dialogue: 0,0:01:47.76,0:01:49.55,main,,0000,0000,0000,,Jak to, že nejsi v žádné sportovním klubu,
Dialogue: 0,0:01:49.55,0:01:52.18,main,,0000,0000,0000,,když jsi tak dobrá?
Dialogue: 0,0:01:52.18,0:01:54.18,main,,0000,0000,0000,,Protože, kdybych se do nějakého přidala…
Dialogue: 0,0:01:54.81,0:01:57.44,main,,0000,0000,0000,,…nestíhala bych vysílání anime.
Dialogue: 0,0:01:57.59,0:02:00.48,white,,0000,0000,0000,,{\frz-90\pos(283,142)}Rychlá
Dialogue: 0,0:01:57.59,0:02:00.48,white,,0000,0000,0000,,{\frz-90\pos(380,222)}holka
Dialogue: 0,0:02:07.41,0:02:08.57,main,,0000,0000,0000,,Ty, Tsukaso.
Dialogue: 0,0:02:08.57,0:02:10.53,main,,0000,0000,0000,,Z jakého konce jíš čokoládový kornout?
Dialogue: 0,0:02:12.37,0:02:13.54,main,,0000,0000,0000,,Myslím, že od hlavy.
Dialogue: 0,0:02:14.25,0:02:15.00,main,,0000,0000,0000,,Chápu.
Dialogue: 0,0:02:16.17,0:02:17.16,main,,0000,0000,0000,,Mimochodem…
Dialogue: 0,0:02:17.17,0:02:18.37,main,,0000,0000,0000,,…která strana je hlava?
Dialogue: 0,0:02:18.38,0:02:19.71,main,,0000,0000,0000,,Tenká nebo tlustá strana?
Dialogue: 0,0:02:21.13,0:02:24.80,main,,0000,0000,0000,,Myslím, že ta tenká.
Dialogue: 0,0:02:24.80,0:02:25.84,main,,0000,0000,0000,,Aha.
Dialogue: 0,0:02:25.84,0:02:28.51,main,,0000,0000,0000,,Já myslela, že je to ta tlustá.
Dialogue: 0,0:02:29.43,0:02:31.51,main,,0000,0000,0000,,Proč bys řekla, že je to ta tenká?
Dialogue: 0,0:02:31.51,0:02:33.10,main,,0000,0000,0000,,No, nepřipomíná ti ulitu?
Dialogue: 0,0:02:33.81,0:02:36.02,main,,0000,0000,0000,,Proč sis ty myslela, že je to ta tlustá?
Dialogue: 0,0:02:36.02,0:02:36.98,main,,0000,0000,0000,,No, nepřipomíná ti…
Dialogue: 0,0:02:36.98,0:02:38.19,main,,0000,0000,0000,,…housenku?
Dialogue: 0,0:02:39.13,0:02:39.98,main,,0000,0000,0000,,Housenku?
Dialogue: 0,0:02:39.98,0:02:43.48,main,,0000,0000,0000,,Ale asi je lepší si představit ulitu.
Dialogue: 0,0:02:59.46,0:03:00.33,main,,0000,0000,0000,,U-Um…
Dialogue: 0,0:03:01.46,0:03:03.17,main,,0000,0000,0000,,Mohla bys odloupnout tenký konec…
Dialogue: 0,0:03:03.17,0:03:06.13,main,,0000,0000,0000,,…a sníst ho s přebytečnou čokoládou.
Dialogue: 0,0:03:07.13,0:03:08.55,main,,0000,0000,0000,,Už to chápu.
Dialogue: 0,0:03:12.14,0:03:13.68,main,,0000,0000,0000,,Dobrý postřeh, Miyuki-san.
Dialogue: 0,0:03:13.68,0:03:14.97,main,,0000,0000,0000,,Jsi tak chytrá.
Dialogue: 0,0:03:14.97,0:03:15.76,main,,0000,0000,0000,,Ne, tak zase ne.
Dialogue: 0,0:03:15.77,0:03:18.10,main,,0000,0000,0000,,Každý má svůj vlastní styl.
Dialogue: 0,0:03:18.51,0:03:20.73,main,,0000,0000,0000,,Jak jíš větrník?
Dialogue: 0,0:03:21.15,0:03:22.90,main,,0000,0000,0000,,Větrníky?
Dialogue: 0,0:03:22.94,0:03:23.77,main,,0000,0000,0000,,No…
Dialogue: 0,0:03:24.02,0:03:26.40,main,,0000,0000,0000,,První oddělím spodní a horní poloviny od sebe…
Dialogue: 0,0:03:26.40,0:03:29.65,main,,0000,0000,0000,,…a pak sním vršek, který si namáčím do krému vespodu.
Dialogue: 0,0:03:29.65,0:03:33.41,main,,0000,0000,0000,,A potom sním spodní polovinu.
Dialogue: 0,0:03:33.41,0:03:36.70,main,,0000,0000,0000,,Takto se nemusím obávat, že mi spadne nějaký krém…
Dialogue: 0,0:03:36.70,0:03:39.83,main,,0000,0000,0000,,…a navíc mám v rovnováze množství větrníku a krému.
Dialogue: 0,0:03:41.71,0:03:45.34,main,,0000,0000,0000,,A co ty, co mají uvnitř krém i puding?
Dialogue: 0,0:03:45.34,0:03:47.09,main,,0000,0000,0000,,Jak jíš je?
Dialogue: 0,0:03:47.09,0:03:48.09,main,,0000,0000,0000,,Smícháš je dohromady?
Dialogue: 0,0:03:48.09,0:03:48.71,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:03:48.72,0:03:50.59,main,,0000,0000,0000,,To bych neudělala.
Dialogue: 0,0:03:51.38,0:03:53.30,main,,0000,0000,0000,,Ale, jak bys je chtěla zamíchat?
Dialogue: 0,0:03:53.85,0:03:55.10,main,,0000,0000,0000,,Jen se ptám.
Dialogue: 0,0:03:55.18,0:03:57.26,main,,0000,0000,0000,,Kdy jíš jahody na dortu…
Dialogue: 0,0:03:57.27,0:03:59.73,main,,0000,0000,0000,,…kaštan na vrcholu Mont Blancu?
Dialogue: 0,0:03:59.73,0:04:01.19,main,,0000,0000,0000,,Takže…
Dialogue: 0,0:04:01.19,0:04:05.90,main,,0000,0000,0000,,Jsou tu také jahody mezi jednotlivými vrstvami.
Dialogue: 0,0:04:05.90,0:04:11.15,main,,0000,0000,0000,,Snažím se dodržovat rovnoměrné rozdělení krému, jahod a piškotu.
Dialogue: 0,0:04:11.91,0:04:15.87,main,,0000,0000,0000,,Takže nemám vyhraněný čas na jahody.
Dialogue: 0,0:04:16.62,0:04:17.79,main,,0000,0000,0000,,Ty se vážně vyznáš.
Dialogue: 0,0:04:18.45,0:04:21.96,main,,0000,0000,0000,,Já si vždy nechám jahodu až na konec.
Dialogue: 0,0:04:21.96,0:04:24.46,main,,0000,0000,0000,,Ale někdy jsem tak plná…
Dialogue: 0,0:04:24.46,0:04:26.00,main,,0000,0000,0000,,…že ji ani nesním…
Dialogue: 0,0:04:26.00,0:04:27.96,main,,0000,0000,0000,,…nebo si ji dá někdo z rodiny.
Dialogue: 0,0:04:27.96,0:04:31.09,main,,0000,0000,0000,,Vážně bys měla sníst to nejlepší už na začátku.
Dialogue: 0,0:04:31.09,0:04:35.14,main,,0000,0000,0000,,Když tak o tom přemýšlím, tak jsem nikdy neviděla, že by si někdo vzal jahodu od mé sestry.
Dialogue: 0,0:04:35.14,0:04:36.22,main,,0000,0000,0000,,To je celá Kagami.
Dialogue: 0,0:04:36.22,0:04:37.30,main,,0000,0000,0000,,Pořád obezřetná.
Dialogue: 0,0:04:37.31,0:04:41.73,main,,0000,0000,0000,,Dokážeš sníst poslední kousek nanuku, aniž by ti spadl?
Dialogue: 0,0:04:41.73,0:04:42.81,main,,0000,0000,0000,,Spadl?
Dialogue: 0,0:04:43.19,0:04:44.73,main,,0000,0000,0000,,Pokud nejsi pozorná…
Dialogue: 0,0:04:46.65,0:04:47.98,main,,0000,0000,0000,,…tak ti poslední kousek spadne, že?
Dialogue: 0,0:04:49.15,0:04:50.57,main,,0000,0000,0000,,Jak to děláš ty, Kona-chan?
Dialogue: 0,0:04:50.57,0:04:51.78,main,,0000,0000,0000,,Slížu ho do posledního kousku.
Dialogue: 0,0:04:53.11,0:04:56.03,main,,0000,0000,0000,,Mohla bys s ním otáčet.
Dialogue: 0,0:04:56.37,0:04:57.57,main,,0000,0000,0000,,To je dobrý nápad.
Dialogue: 0,0:04:57.58,0:05:00.58,main,,0000,0000,0000,,Já nemám zrovna v oblibě zmrzlinové kornouty.
Dialogue: 0,0:05:01.20,0:05:02.25,main,,0000,0000,0000,,Proč?
Dialogue: 0,0:05:03.12,0:05:05.75,main,,0000,0000,0000,,Nesnesu, když mi zůstane pouze kornout.
Dialogue: 0,0:05:06.13,0:05:07.17,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:05:07.17,0:05:09.30,main,,0000,0000,0000,,Vždy ti nakonec nějaký zůstane.
Dialogue: 0,0:05:09.30,0:05:13.97,main,,0000,0000,0000,,Já tlačím zmrzlinu dolů, když ho jím.
Dialogue: 0,0:05:14.88,0:05:16.97,main,,0000,0000,0000,,Čím ji tlačíš?
Dialogue: 0,0:05:16.97,0:05:19.01,main,,0000,0000,0000,,Trochu ji tam zašťouchám.
Dialogue: 0,0:05:19.72,0:05:21.14,main,,0000,0000,0000,,Zašťoucháš?
Dialogue: 0,0:05:21.81,0:05:24.06,main,,0000,0000,0000,,Yuki-chan, ty si potrpíš na vyváženost, je to tak?
Dialogue: 0,0:05:24.06,0:05:25.23,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:05:25.23,0:05:27.40,main,,0000,0000,0000,,Tak co děláš s rýží s kari?
Dialogue: 0,0:05:27.40,0:05:28.94,main,,0000,0000,0000,,Smícháš omáčku s rýží?
Dialogue: 0,0:05:29.40,0:05:30.11,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:05:30.11,0:05:31.86,main,,0000,0000,0000,,Nesmíchávám je…
Dialogue: 0,0:05:31.86,0:05:33.49,main,,0000,0000,0000,,…ale dám si záležet, aby byli dobře vyvážené.
Dialogue: 0,0:05:33.86,0:05:35.32,main,,0000,0000,0000,,Já je míchám.
Dialogue: 0,0:05:35.53,0:05:36.57,main,,0000,0000,0000,,Co ty, Kono-chan?
Dialogue: 0,0:05:36.57,0:05:37.99,main,,0000,0000,0000,,To záleží na kari.
Dialogue: 0,0:05:37.99,0:05:40.53,main,,0000,0000,0000,,Pokud je na tom nějaká worcestrová omáčka nebo syrové vejce…
Dialogue: 0,0:05:40.54,0:05:41.70,main,,0000,0000,0000,,…tak je promíchám.
Dialogue: 0,0:05:42.08,0:05:43.29,main,,0000,0000,0000,,Worcestrová omáčka na kari?
Dialogue: 0,0:05:43.79,0:05:44.79,main,,0000,0000,0000,,Je to dobrý.
Dialogue: 0,0:05:44.79,0:05:47.00,main,,0000,0000,0000,,Dáváš ji i na smažená vejce?
Dialogue: 0,0:05:47.67,0:05:49.29,main,,0000,0000,0000,,Na ty dávám sojovou omáčku.
Dialogue: 0,0:05:50.21,0:05:52.13,main,,0000,0000,0000,,To já používám majonézu.
Dialogue: 0,0:05:52.13,0:05:53.55,main,,0000,0000,0000,,Na smažená vejce?
Dialogue: 0,0:05:53.55,0:05:54.72,main,,0000,0000,0000,,Nemyslím vařená.
Dialogue: 0,0:05:55.80,0:05:57.01,main,,0000,0000,0000,,Majonéza…
Dialogue: 0,0:05:57.01,0:05:58.89,main,,0000,0000,0000,,…se hodí k většině jídel z vajec.
Dialogue: 0,0:05:58.89,0:05:59.97,main,,0000,0000,0000,,Dáváš si ji na kari?
Dialogue: 0,0:06:01.60,0:06:03.64,main,,0000,0000,0000,,Musí to, ale být bílá rýže.
Dialogue: 0,0:06:03.64,0:06:06.60,main,,0000,0000,0000,,Ale ta sladkost se hodí i k hovězím miskám.
Dialogue: 0,0:06:06.60,0:06:07.69,main,,0000,0000,0000,,Hodí se i k Oyako-don.
Dialogue: 0,0:06:08.23,0:06:10.69,main,,0000,0000,0000,,Víš jako jsou vejce a kuřecí maso jako děti a rodiče.
Dialogue: 0,0:06:10.94,0:06:14.15,main,,0000,0000,0000,,Takže, když přidám majonézu, která je také z vajec…
Dialogue: 0,0:06:14.36,0:06:16.61,main,,0000,0000,0000,,…říkám tomu vylepšené Oyako-don.
Dialogue: 0,0:06:18.24,0:06:19.07,main,,0000,0000,0000,,A taky…
Dialogue: 0,0:06:19.07,0:06:20.66,main,,0000,0000,0000,,…normálně nesnesu natto…
Dialogue: 0,0:06:20.66,0:06:22.74,main,,0000,0000,0000,,…ale když k tomu přidám majonézu, je to hned lepší.
Dialogue: 0,0:06:23.24,0:06:24.20,main,,0000,0000,0000,,To si nedokážu představit.
Dialogue: 0,0:06:24.87,0:06:25.62,main,,0000,0000,0000,,Ale…
Dialogue: 0,0:06:25.62,0:06:29.79,main,,0000,0000,0000,,…mojonéza na vejcí na hniličku může být nejlepší.
Dialogue: 0,0:06:29.79,0:06:32.29,main,,0000,0000,0000,,Smažená vejce jsou u nás taky tekutá.
Dialogue: 0,0:06:33.13,0:06:35.34,main,,0000,0000,0000,,Můj otec je tak má rád.
Dialogue: 0,0:06:35.34,0:06:38.88,main,,0000,0000,0000,,Rád z nich vycucne žloutek.
Dialogue: 0,0:06:38.89,0:06:42.22,main,,0000,0000,0000,,A když jsou moc uvařená, tak to nejde.
Dialogue: 0,0:06:42.81,0:06:44.68,main,,0000,0000,0000,,A co třeba kuře na špízu?
Dialogue: 0,0:06:44.68,0:06:45.85,main,,0000,0000,0000,,Co je s ním?
Dialogue: 0,0:06:45.85,0:06:47.48,main,,0000,0000,0000,,Necháváte ho na něm?
Dialogue: 0,0:06:47.48,0:06:49.69,main,,0000,0000,0000,,Nebo použijete hůlky a sundáte ho?
Dialogue: 0,0:06:49.69,0:06:51.15,main,,0000,0000,0000,,Hm…
Dialogue: 0,0:06:51.15,0:06:54.28,main,,0000,0000,0000,,Když ho jím sama, tak ho nechám na špízu.
Dialogue: 0,0:06:54.28,0:06:56.82,main,,0000,0000,0000,,Ale když jsem s lidmi, tak ho sundám.
Dialogue: 0,0:06:56.82,0:06:57.95,main,,0000,0000,0000,,Jo, jo.
Dialogue: 0,0:06:57.95,0:07:00.74,main,,0000,0000,0000,,Je to trochu nešikovné, když zůstane jen jedno.
Dialogue: 0,0:07:01.49,0:07:04.08,main,,0000,0000,0000,,Nikdy se nemůžu přimět si ho dát.
Dialogue: 0,0:07:04.08,0:07:06.54,main,,0000,0000,0000,,A jak ubíhá čas a nikdo ho nechce sníst…
Dialogue: 0,0:07:07.96,0:07:09.42,main,,0000,0000,0000,,…začne osychat…
Dialogue: 0,0:07:09.42,0:07:10.95,main,,0000,0000,0000,,…a nevypadá vůbec dobře…
Dialogue: 0,0:07:10.95,0:07:13.02,main,,0000,0000,0000,,…jenže nikdo si ho nechce dát…
Dialogue: 0,0:07:13.02,0:07:13.92,main,,0000,0000,0000,,…ale když přijde číšník tak řekneš…
Dialogue: 0,0:07:13.92,0:07:15.80,main,,0000,0000,0000,,„Moment, ještě jsem nedojedli.“
Dialogue: 0,0:07:15.80,0:07:17.09,main,,0000,0000,0000,,…jaksi ze zvyku.
Dialogue: 0,0:07:17.72,0:07:20.47,main,,0000,0000,0000,,Ale nakonec ho nikdo nesní.
Dialogue: 0,0:07:20.72,0:07:22.05,main,,0000,0000,0000,,Takové plýtvání.
Dialogue: 0,0:07:22.05,0:07:23.64,main,,0000,0000,0000,,Je mi ho trochu líto.
Dialogue: 0,0:07:24.97,0:07:26.22,main,,0000,0000,0000,,A když se griluje…
Dialogue: 0,0:07:26.22,0:07:29.89,main,,0000,0000,0000,,…tak je mi líto masa, které zůstane připečené ke grilu.
Dialogue: 0,0:07:29.89,0:07:33.40,main,,0000,0000,0000,,Měla bys jíst při grilovaní a bavení se…
Dialogue: 0,0:07:33.40,0:07:35.23,main,,0000,0000,0000,,…takže je toho dost.
Dialogue: 0,0:07:35.23,0:07:38.99,main,,0000,0000,0000,,Je to ještě horší s hovězími jazyky, ty jsou hotové skoro hned.
Dialogue: 0,0:07:38.99,0:07:40.32,main,,0000,0000,0000,,To máš pravdu.
Dialogue: 0,0:07:40.32,0:07:43.78,main,,0000,0000,0000,,Jsou hotové už po jednom otočení.
Dialogue: 0,0:07:43.78,0:07:44.83,main,,0000,0000,0000,,Otočení?
Dialogue: 0,0:07:45.79,0:07:46.58,main,,0000,0000,0000,,Hovězí jazyky?
Dialogue: 0,0:07:47.37,0:07:49.50,main,,0000,0000,0000,,Ty se neotáčejí.
Dialogue: 0,0:07:50.54,0:07:51.12,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:07:51.50,0:07:52.83,main,,0000,0000,0000,,Pokud je otočíš…
Dialogue: 0,0:07:52.83,0:07:55.09,main,,0000,0000,0000,,…cibule nahoře spadne dolů.
Dialogue: 0,0:07:55.75,0:07:56.75,main,,0000,0000,0000,,Oh, aha.
Dialogue: 0,0:07:56.76,0:08:01.47,main,,0000,0000,0000,,Udělají se, bez jakéhokoliv otáčení.
Dialogue: 0,0:08:01.72,0:08:02.59,main,,0000,0000,0000,,Rozumím…
Dialogue: 0,0:08:02.59,0:08:04.26,main,,0000,0000,0000,,Od teď to budu tak dělat.
Dialogue: 0,0:08:04.26,0:08:06.56,main,,0000,0000,0000,,Nikdy je neotáčej.
Dialogue: 0,0:08:06.56,0:08:07.84,main,,0000,0000,0000,,Vážně…
Dialogue: 0,0:08:13.19,0:08:16.52,main,,0000,0000,0000,,Mimochodem, co je hlava čokoládového kornoutu? Tenká nebo tlustá strana?
Dialogue: 0,0:08:18.03,0:08:19.32,main,,0000,0000,0000,,To je teda přísné.
Dialogue: 0,0:08:17.63,0:08:19.59,newspaper,,0000,0000,0000,,{\frz-125\pos(518,288)}Chtěl jsem aby byla mým otrokem
Dialogue: 0,0:08:17.63,0:08:19.59,newspaper,,0000,0000,0000,,{\frz-20\pos(522,113)}Uvězněna na dva měsíce
Dialogue: 0,0:08:20.20,0:08:22.78,main,,0000,0000,0000,,Poslední dobou je tu spousta únosů.
Dialogue: 0,0:08:22.78,0:08:26.45,main,,0000,0000,0000,,Chlapi co nutí aby jim holky říkali „Pane“ a tak.
Dialogue: 0,0:08:27.45,0:08:28.49,main,,0000,0000,0000,,Možná to není až tak…
Dialogue: 0,0:08:28.50,0:08:29.54,main,,0000,0000,0000,,…problém morálky…
Dialogue: 0,0:08:29.54,0:08:32.71,main,,0000,0000,0000,,…jako spíš, že hrají moc erotických her?
Dialogue: 0,0:08:32.72,0:08:34.50,main,,0000,0000,0000,,V těch hrách je to docela běžné.
Dialogue: 0,0:08:34.54,0:08:36.42,main,,0000,0000,0000,,To tu mluvíš o sobě?!
Dialogue: 0,0:08:38.42,0:08:39.42,main,,0000,0000,0000,,Hej, počkej moment.
Dialogue: 0,0:08:39.42,0:08:41.47,main,,0000,0000,0000,,Jak to, že znáš tak dobře co je v těch hrách…
Dialogue: 0,0:08:41.47,0:08:42.24,main,,0000,0000,0000,,Slečno středoškolačko?
Dialogue: 0,0:08:42.90,0:08:45.78,copyrights,,0000,0000,0000,,{\pos(85,50)}Překlad od paralaXe
Dialogue: 0,0:08:42.90,0:08:45.78,copyrights,,0000,0000,0000,,{\pos(129,76)}navštivte www.otakogu.cz
Dialogue: 0,0:08:45.99,0:08:47.64,table,,0000,0000,0000,,{\pos(372,70)}První semestr průběžné hodnocení
Dialogue: 0,0:08:45.99,0:08:47.64,table,,0000,0000,0000,,{\frz-90\pos(317,257)}Miyuki Takara
Dialogue: 0,0:08:48.31,0:08:51.31,main,,0000,0000,0000,,Tvoje známky jsou vždy tak dobré.
Dialogue: 0,0:08:52.39,0:08:54.52,main,,0000,0000,0000,,To nemáš žádné neúspěchy?
Dialogue: 0,0:08:55.15,0:08:56.73,main,,0000,0000,0000,,Stydím se to říct…
Dialogue: 0,0:08:56.73,0:08:59.28,main,,0000,0000,0000,,…ale občas se zapomenu…
Dialogue: 0,0:08:59.32,0:09:01.07,main,,0000,0000,0000,,…nebo jen tak hledím do prázdna.
Dialogue: 0,0:09:01.57,0:09:04.41,main,,0000,0000,0000,,Pořezala jsem se, když jsem škrábala brambory.
Dialogue: 0,0:09:03.99,0:09:04.41,main,,0000,0000,0000,,Au!
Dialogue: 0,0:09:04.74,0:09:07.62,main,,0000,0000,0000,,A zakopla jsem, když jsem pospíchala.
Dialogue: 0,0:09:08.29,0:09:11.75,main,,0000,0000,0000,,A taky sem si přivřela tašku do dveří v metru.
Dialogue: 0,0:09:12.91,0:09:14.00,main,,0000,0000,0000,,Miyuki-san.
Dialogue: 0,0:09:14.00,0:09:17.33,main,,0000,0000,0000,,To nejsou neúspěchy, to je to čemu říkáme moé.
Dialogue: 0,0:09:17.96,0:09:18.59,main,,0000,0000,0000,,Moé?
Dialogue: 0,0:09:18.59,0:09:19.84,main,,0000,0000,0000,,Nemotorná.
Dialogue: 0,0:09:19.84,0:09:22.88,main,,0000,0000,0000,,Co se teď děje v Kono-chaniné hlavě!?
Dialogue: 0,0:09:23.38,0:09:26.39,main,,0000,0000,0000,,Miyuki-san, vypadáš vážně pěkně, proč vlastně nosíš brýle?
Dialogue: 0,0:09:26.39,0:09:28.60,main,,0000,0000,0000,,Takto se líbíš jen určitému typu kluků.
Dialogue: 0,0:09:28.60,0:09:29.64,main,,0000,0000,0000,,Určitému typu…
Dialogue: 0,0:09:30.22,0:09:32.60,main,,0000,0000,0000,,Když si odmyslíme oblíbenost u kluků…
Dialogue: 0,0:09:32.60,0:09:34.31,main,,0000,0000,0000,,…proč nenosíš čočky?
Dialogue: 0,0:09:34.31,0:09:37.02,main,,0000,0000,0000,,Slyšela jsem, že brýle zhoršují zrak.
Dialogue: 0,0:09:37.02,0:09:37.98,main,,0000,0000,0000,,Eh…
Dialogue: 0,0:09:37.98,0:09:40.48,main,,0000,0000,0000,,Je mi to trapné říkat, když jsme už na střední…
Dialogue: 0,0:09:41.32,0:09:42.18,main,,0000,0000,0000,,A-Ale…
Dialogue: 0,0:09:42.53,0:09:43.90,main,,0000,0000,0000,,Já se…
Dialogue: 0,0:09:43.90,0:09:45.11,main,,0000,0000,0000,,…jich bojím!
Dialogue: 0,0:09:45.11,0:09:46.57,main,,0000,0000,0000,,Bojím se čoček!
Dialogue: 0,0:09:46.57,0:09:47.82,main,,0000,0000,0000,,Abych byla přesnější…
Dialogue: 0,0:09:47.82,0:09:50.20,main,,0000,0000,0000,,…bojím se dávat si něco do očí.
Dialogue: 0,0:09:50.20,0:09:52.70,main,,0000,0000,0000,,Dokonce ani nedokážu otevřít oči, když jsem pod vodou.
Dialogue: 0,0:09:52.91,0:09:53.62,main,,0000,0000,0000,,Zatraceně!
Dialogue: 0,0:09:53.62,0:09:55.37,main,,0000,0000,0000,,Dokonce je i roztomilá!
Dialogue: 0,0:09:55.37,0:09:56.21,main,,0000,0000,0000,,To musí být přirozené.
Dialogue: 0,0:09:57.42,0:09:58.88,main,,0000,0000,0000,,Není to Miyuki-san?
Dialogue: 0,0:10:00.21,0:10:02.00,main,,0000,0000,0000,,Je tu kvůli očím?
Dialogue: 0,0:10:02.01,0:10:03.01,main,,0000,0000,0000,,Na čočky?
Dialogue: 0,0:10:06.01,0:10:09.09,main,,0000,0000,0000,,Nejspíš je takto lepší.
Dialogue: 0,0:10:09.10,0:10:10.15,main,,0000,0000,0000,,To asi bolelo…
Dialogue: 0,0:10:10.15,0:10:11.64,main,,0000,0000,0000,,Má to vrozené.
Dialogue: 0,0:10:14.35,0:10:16.35,main,,0000,0000,0000,,Kagami je doma s nachlazením?
Dialogue: 0,0:10:17.10,0:10:20.19,main,,0000,0000,0000,,Vypadá to ale, že nemá chřipku.
Dialogue: 0,0:10:20.19,0:10:23.90,main,,0000,0000,0000,,Já jsem hloupá, takže se o chřipku nemusím bát.
Dialogue: 0,0:10:24.78,0:10:26.24,main,,0000,0000,0000,,Slyšela jsem, že to platí o nachlazení…
Dialogue: 0,0:10:26.24,0:10:28.20,main,,0000,0000,0000,,…ale nevím jestli i na chřipku.
Dialogue: 0,0:10:30.83,0:10:33.04,main,,0000,0000,0000,,On je mezi nimi rozdíl?
Dialogue: 0,0:10:33.04,0:10:35.29,main,,0000,0000,0000,,Myslela jsem, že chřipka je něco jako super-nachlazení.
Dialogue: 0,0:10:35.29,0:10:37.33,main,,0000,0000,0000,,M-Myslím že jsou rozdílné…
Dialogue: 0,0:10:37.92,0:10:41.09,main,,0000,0000,0000,,Ale teď si nejsem jistá…
Dialogue: 0,0:10:42.00,0:10:44.13,main,,0000,0000,0000,,Rozdíl mezi chřipkou a nachlazením?
Dialogue: 0,0:10:45.59,0:10:46.80,main,,0000,0000,0000,,No…
Dialogue: 0,0:10:47.26,0:10:49.72,main,,0000,0000,0000,,Chřipka je způsobena virem.
Dialogue: 0,0:10:49.72,0:10:53.01,main,,0000,0000,0000,,Příznaky jsou horečka a bolest celého těla.
Dialogue: 0,0:10:53.01,0:10:55.89,main,,0000,0000,0000,,A je tu riziko komplikací.
Dialogue: 0,0:10:55.89,0:10:56.68,main,,0000,0000,0000,,Na druhé straně…
Dialogue: 0,0:10:56.69,0:10:59.27,main,,0000,0000,0000,,…příznaky nachlazení jsou bolest v krku a rýma…
Dialogue: 0,0:10:59.27,0:11:01.40,main,,0000,0000,0000,,…a horečka není tak vysoká.
Dialogue: 0,0:11:01.40,0:11:03.82,main,,0000,0000,0000,,A je tu navíc spousta menších rozdílů.
Dialogue: 0,0:11:05.53,0:11:06.40,main,,0000,0000,0000,,Takže…
Dialogue: 0,0:11:06.40,0:11:08.45,main,,0000,0000,0000,,to jsou rozdílné nemoci.
Dialogue: 0,0:11:09.28,0:11:10.20,main,,0000,0000,0000,,Chápu…
Dialogue: 0,0:11:11.48,0:11:12.60,main,,0000,0000,0000,,N-Nicméně…
Dialogue: 0,0:11:12.95,0:11:15.41,main,,0000,0000,0000,,No, mají i dost společného…
Dialogue: 0,0:11:15.41,0:11:19.29,main,,0000,0000,0000,,…takže bys možná mohla říct, že je to vylepšené nachlazení…
Dialogue: 0,0:11:19.29,0:11:20.00,main,,0000,0000,0000,,Věděla jsem to!
Dialogue: 0,0:11:22.46,0:11:22.96,main,,0000,0000,0000,,Kono-chan.
Dialogue: 0,0:11:22.96,0:11:25.30,main,,0000,0000,0000,,Co ten zamyšlený výraz?
Dialogue: 0,0:11:25.30,0:11:27.17,main,,0000,0000,0000,,Ale, to nic.
Dialogue: 0,0:11:27.17,0:11:29.30,main,,0000,0000,0000,,Jen jsem přemýšlela o našem ranním rozhovoru.
Dialogue: 0,0:11:29.84,0:11:31.34,main,,0000,0000,0000,,Ohledně nachlazení a chřipky?
Dialogue: 0,0:11:31.64,0:11:35.35,main,,0000,0000,0000,,Ty jsi neřekla že nesouhlasíš, když jsem řekla že jsem hloupá.
Dialogue: 0,0:11:35.35,0:11:36.18,main,,0000,0000,0000,,N-Ne!
Dialogue: 0,0:11:36.18,0:11:37.22,main,,0000,0000,0000,,Tak to není!
Dialogue: 0,0:11:42.94,0:11:44.02,main,,0000,0000,0000,,Konato-chan.
Dialogue: 0,0:11:44.02,0:11:44.98,main,,0000,0000,0000,,Vítej.
Dialogue: 0,0:11:44.98,0:11:47.03,main,,0000,0000,0000,,Přišla navštívit sestru.
Dialogue: 0,0:11:47.90,0:11:49.32,main,,0000,0000,0000,,Děkujeme za návštěvu.
Dialogue: 0,0:11:49.32,0:11:49.95,main,,0000,0000,0000,,To nic.
Dialogue: 0,0:11:49.95,0:11:51.66,main,,0000,0000,0000,,Promiňte, že vyrušuji.
Dialogue: 0,0:11:56.45,0:11:58.87,main,,0000,0000,0000,,Kagama vypadá docela roztomile, když spí.
Dialogue: 0,0:11:59.25,0:12:01.55,main,,0000,0000,0000,,Co to děláš?              Je vzhůru. Je vzhůru.
Dialogue: 0,0:12:02.46,0:12:03.21,main,,0000,0000,0000,,Navštívit?
Dialogue: 0,0:12:03.21,0:12:04.04,main,,0000,0000,0000,,Ty?
Dialogue: 0,0:12:04.04,0:12:05.79,main,,0000,0000,0000,,Přišla jsi celou tu cestu, abys mě viděla?
Dialogue: 0,0:12:05.80,0:12:06.71,main,,0000,0000,0000,,Tak to je.
Dialogue: 0,0:12:06.71,0:12:08.88,main,,0000,0000,0000,,A-Ale já nechci, abys to ode mě chytila.
Dialogue: 0,0:12:08.88,0:12:10.34,main,,0000,0000,0000,,Ale vážně si toho cením.
Dialogue: 0,0:12:11.01,0:12:11.59,main,,0000,0000,0000,,Ah.
Dialogue: 0,0:12:11.59,0:12:13.39,main,,0000,0000,0000,,To je milé.
Dialogue: 0,0:12:13.39,0:12:15.55,main,,0000,0000,0000,,Bála se o mě…
Dialogue: 0,0:12:15.56,0:12:17.77,main,,0000,0000,0000,,…protože jsem nemocná?
Dialogue: 0,0:12:17.77,0:12:21.69,main,,0000,0000,0000,,Myslela jsem si, že má v hlavě jen hry a anime.
Dialogue: 0,0:12:21.69,0:12:23.56,main,,0000,0000,0000,,Trochu se mě to dotklo.
Dialogue: 0,0:12:25.15,0:12:25.61,main,,0000,0000,0000,,No…
Dialogue: 0,0:12:25.61,0:12:27.99,main,,0000,0000,0000,,…taky jsem chtěla vidět ten úkol z minula.
Dialogue: 0,0:12:28.00,0:12:30.44,main,,0000,0000,0000,,A pak viděla jsem jak pěkně spíš…
Dialogue: 0,0:12:30.45,0:12:31.78,main,,0000,0000,0000,,…tak to ani neřeš.
Dialogue: 0,0:12:31.78,0:12:32.24,main,,0000,0000,0000,,Vypadni!
Dialogue: 0,0:12:34.07,0:12:35.07,main,,0000,0000,0000,,Kagami!
Dialogue: 0,0:12:35.08,0:12:37.83,main,,0000,0000,0000,,Miyuki-chan tě přišla navštívit!
Dialogue: 0,0:12:38.33,0:12:38.91,main,,0000,0000,0000,,Fajn!
Dialogue: 0,0:12:39.50,0:12:40.41,main,,0000,0000,0000,,Promiň, že ruším.
Dialogue: 0,0:12:41.33,0:12:43.12,main,,0000,0000,0000,,Je mi líto, že sem jdeš, když máš volno.
Dialogue: 0,0:12:43.12,0:12:45.00,main,,0000,0000,0000,,Je v pořádku, že jsi takto vzhůru?
Dialogue: 0,0:12:45.50,0:12:46.42,main,,0000,0000,0000,,Děkuji.                Tady máš něco ode mě.
Dialogue: 0,0:12:46.92,0:12:48.55,main,,0000,0000,0000,,Už jsem o dost lepší.
Dialogue: 0,0:12:48.55,0:12:50.42,main,,0000,0000,0000,,Po tom co jsem si vzala nějaké léky mi klesla horečka.
Dialogue: 0,0:12:50.42,0:12:50.76,main,,0000,0000,0000,,Tady.
Dialogue: 0,0:12:51.47,0:12:53.13,main,,0000,0000,0000,,Květiny a ovoce! To jsi nemusela!
Dialogue: 0,0:12:53.97,0:12:54.97,main,,0000,0000,0000,,Posaď se.
Dialogue: 0,0:12:54.97,0:12:56.26,main,,0000,0000,0000,,Moc děkuji.
Dialogue: 0,0:12:56.76,0:13:00.56,main,,0000,0000,0000,,Chtěla jsem tě navštívit se Izumi-san a ostatními…
Dialogue: 0,0:13:00.56,0:13:03.23,main,,0000,0000,0000,,…ale bohužel jsem si nemohla udělat čas.
Dialogue: 0,0:13:03.23,0:13:04.35,main,,0000,0000,0000,,To je v pohodě.
Dialogue: 0,0:13:04.90,0:13:08.36,main,,0000,0000,0000,,Mám pro tebe nějaké věci ze studentské rady.
Dialogue: 0,0:13:08.36,0:13:09.53,main,,0000,0000,0000,,Díky!
Dialogue: 0,0:13:09.99,0:13:14.07,main,,0000,0000,0000,,Pokud by jsi se na ně mohla jen mrknout…
Dialogue: 0,0:13:14.91,0:13:18.20,main,,0000,0000,0000,,A tady jsou rozpočty pro každý klub.
Dialogue: 0,0:13:18.20,0:13:20.87,main,,0000,0000,0000,,A co jsem to ještě chtěla?
Dialogue: 0,0:13:20.87,0:13:21.87,main,,0000,0000,0000,,Em…
Dialogue: 0,0:13:22.41,0:13:24.37,main,,0000,0000,0000,,Pokud bys chtěla odpočívat…
Dialogue: 0,0:13:24.37,0:13:25.48,main,,0000,0000,0000,,…tak já hned odejdu.
Dialogue: 0,0:13:26.21,0:13:28.29,main,,0000,0000,0000,,Takto má vypadat správná návštěva!
Dialogue: 0,0:13:28.30,0:13:30.30,main,,0000,0000,0000,,S-Stalo se něco?
Dialogue: 0,0:13:30.76,0:13:32.97,main,,0000,0000,0000,,No, Konata se tu včera ukázala…
Dialogue: 0,0:13:34.68,0:13:36.26,main,,0000,0000,0000,,Jak ti říkám.
Dialogue: 0,0:13:36.26,0:13:40.72,main,,0000,0000,0000,,Možná se takto projevuje.
Dialogue: 0,0:13:40.72,0:13:42.98,main,,0000,0000,0000,,Ten domácí úkol mohl být pokus o vtip…
Dialogue: 0,0:13:42.98,0:13:46.27,main,,0000,0000,0000,,…aby tě rozveselila.
Dialogue: 0,0:13:46.27,0:13:50.02,main,,0000,0000,0000,,A možná, že tě viděla spát…
Dialogue: 0,0:13:50.03,0:13:51.69,main,,0000,0000,0000,,…protože tě nechtěla vzbudit.
Dialogue: 0,0:13:52.24,0:13:54.15,main,,0000,0000,0000,,To je vždy jen pozitivní!?
Dialogue: 0,0:13:54.15,0:13:55.70,main,,0000,0000,0000,,To by se mi nikdy nestalo.
Dialogue: 0,0:13:56.16,0:13:57.28,main,,0000,0000,0000,,Tak mě napadá…
Dialogue: 0,0:13:57.28,0:13:59.70,main,,0000,0000,0000,,Dneska tu Tsukasa-san není?
Dialogue: 0,0:14:00.45,0:14:01.79,main,,0000,0000,0000,,Dobré ráno.
Dialogue: 0,0:14:02.50,0:14:05.87,main,,0000,0000,0000,,Je ti líp, ségra?
Dialogue: 0,0:14:09.17,0:14:10.17,main,,0000,0000,0000,,Jak vidíš…
Dialogue: 0,0:14:10.17,0:14:13.05,main,,0000,0000,0000,,Je víc rozespalá, než nemocný člověk.
Dialogue: 0,0:14:13.76,0:14:15.05,main,,0000,0000,0000,,U-um…
Dialogue: 0,0:14:15.05,0:14:17.09,main,,0000,0000,0000,,Takto to není každé ráno…
Dialogue: 0,0:14:17.09,0:14:20.14,main,,0000,0000,0000,,…jen jsem si ještě nezvykla na změnu času…
Dialogue: 0,0:14:20.68,0:14:21.89,main,,0000,0000,0000,,Je to trochu pozdě, ale vítej.
Dialogue: 0,0:14:22.72,0:14:24.06,main,,0000,0000,0000,,Naprosto ti rozumím.
Dialogue: 0,0:14:24.06,0:14:26.69,main,,0000,0000,0000,,Taky mám problémy se vzbudit.
Dialogue: 0,0:14:26.69,0:14:28.31,main,,0000,0000,0000,,T-To je pravda.
Dialogue: 0,0:14:28.31,0:14:29.90,main,,0000,0000,0000,,Taky se cítíš tak ospalá?
Dialogue: 0,0:14:29.90,0:14:31.36,main,,0000,0000,0000,,Opravdu.
Dialogue: 0,0:14:31.36,0:14:33.53,main,,0000,0000,0000,,A dnes je navíc obzvláště horko.
Dialogue: 0,0:14:34.24,0:14:35.11,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:14:35.11,0:14:36.24,main,,0000,0000,0000,,Jinak to asi nejde.
Dialogue: 0,0:14:36.74,0:14:39.32,main,,0000,0000,0000,,A poslední dny bylo taky horko.
Dialogue: 0,0:14:40.20,0:14:43.04,main,,0000,0000,0000,,Když jsem se probudila, tak už bylo odpoledne.
Dialogue: 0,0:14:43.04,0:14:45.25,main,,0000,0000,0000,,Většinu času jsem prospala.
Dialogue: 0,0:14:45.25,0:14:48.04,main,,0000,0000,0000,,Není na škodu si odpočinout, když máš volno.
Dialogue: 0,0:14:48.04,0:14:51.13,main,,0000,0000,0000,,Takže jsem nakonec na sobě měla moje pyžamo celý den.
Dialogue: 0,0:14:51.67,0:14:52.50,main,,0000,0000,0000,,A teď ho taky mám.
Dialogue: 0,0:14:53.80,0:14:56.13,main,,0000,0000,0000,,To je pyžamo?
Dialogue: 0,0:14:56.84,0:14:57.88,main,,0000,0000,0000,,Ano.
Dialogue: 0,0:14:57.88,0:14:59.59,main,,0000,0000,0000,,Je docela rozkošné.
Dialogue: 0,0:14:59.59,0:15:01.22,main,,0000,0000,0000,,Neřekla bych, že to jsou pyžama.
Dialogue: 0,0:15:01.22,0:15:02.05,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:15:02.89,0:15:04.77,main,,0000,0000,0000,,Zajímalo by mě, jestli bych v něm mohla zůstat.
Dialogue: 0,0:15:05.23,0:15:06.93,main,,0000,0000,0000,,Klidně v něm můžeš i ven.
Dialogue: 0,0:15:07.27,0:15:09.23,main,,0000,0000,0000,,Tenhle rozhovor snad nemá konce.
Dialogue: 0,0:15:08.27,0:15:10.65,main2,,0000,0000,0000,,Nikdo si toho ani nevšimne.
Dialogue: 0,0:15:09.23,0:15:10.65,mainup,,0000,0000,0000,,Mohla bych v něm do obchodu?
Dialogue: 0,0:15:10.57,0:15:11.82,Nurse,,0000,0000,0000,,{\frz15\pos(353,182)}Ošetřovna
Dialogue: 0,0:15:12.65,0:15:14.32,main,,0000,0000,0000,,Přibrala jsem…
Dialogue: 0,0:15:15.44,0:15:16.07,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:15:16.07,0:15:17.15,main,,0000,0000,0000,,Tím to nebude.
Dialogue: 0,0:15:17.15,0:15:20.11,main,,0000,0000,0000,,Dnes mám na sobě podprsenku s drátěnou výztuhou…
Dialogue: 0,0:15:20.12,0:15:21.74,main,,0000,0000,0000,,…takže se to projevilo.
Dialogue: 0,0:15:21.74,0:15:22.70,main,,0000,0000,0000,,To bude ono.
Dialogue: 0,0:15:22.70,0:15:24.79,main,,0000,0000,0000,,Zajímalo by mě, kolik gramů to tak dělá.
Dialogue: 0,0:15:24.79,0:15:26.12,main,,0000,0000,0000,,Když to odečtu…
Dialogue: 0,0:15:26.12,0:15:26.83,Nurse,,0000,0000,0000,,{\frz15\pos(353,182)}Ošetřovna
Dialogue: 0,0:15:29.17,0:15:30.58,main,,0000,0000,0000,,Je to za mnou…
Dialogue: 0,0:15:31.00,0:15:32.29,main,,0000,0000,0000,,To sem tomu dala.
Dialogue: 0,0:15:32.29,0:15:34.13,main,,0000,0000,0000,,Úplně jsem zapomněla, že jsou dneska prohlídky…
Dialogue: 0,0:15:34.13,0:15:36.80,main,,0000,0000,0000,,…a měla jsem svoje kalhotky s potiskem.
Dialogue: 0,0:15:36.80,0:15:38.63,main,,0000,0000,0000,,To je trapas!
Dialogue: 0,0:15:39.97,0:15:41.47,main,,0000,0000,0000,,Vůbec jsem nevyrostla…
Dialogue: 0,0:15:42.64,0:15:43.47,main,,0000,0000,0000,,Zatraceně!
Dialogue: 0,0:15:43.47,0:15:45.81,main,,0000,0000,0000,,Jediná jsi spokojená!
Dialogue: 0,0:15:46.32,0:15:47.60,main,,0000,0000,0000,,Hoj.
Dialogue: 0,0:15:47.60,0:15:48.23,main,,0000,0000,0000,,Jsem tady.
Dialogue: 0,0:15:48.52,0:15:49.73,main,,0000,0000,0000,,Ségra.
Dialogue: 0,0:15:49.73,0:15:51.10,main,,0000,0000,0000,,Napadlo mě…
Dialogue: 0,0:15:51.86,0:15:54.20,main,,0000,0000,0000,,Kagami sem chodí docela často.
Dialogue: 0,0:15:54.20,0:15:56.28,main,,0000,0000,0000,,Máš ve své třídě nějaké kamarády?
Dialogue: 0,0:15:56.28,0:15:57.90,main,,0000,0000,0000,,Nesrovnávej mě s tebou.
Dialogue: 0,0:15:57.90,0:15:59.19,main,,0000,0000,0000,,Chceš se snad dohadovat?
Dialogue: 0,0:15:59.90,0:16:01.53,main,,0000,0000,0000,,Myslela jsem, že nemáš kamarády.
Dialogue: 0,0:16:01.53,0:16:02.62,main,,0000,0000,0000,,Nestarej se o mě…
Dialogue: 0,0:16:02.62,0:16:05.03,main,,0000,0000,0000,,…ale spíš o tvůj nedostatek společenského života.
Dialogue: 0,0:16:05.54,0:16:07.54,main,,0000,0000,0000,,O to se neboj.
Dialogue: 0,0:16:07.54,0:16:10.46,main,,0000,0000,0000,,Věř tomu nebo, ale mám kolem sebe stovky přátel.
Dialogue: 0,0:16:10.46,0:16:13.71,main,,0000,0000,0000,,A jsem dost dobrá v navazování známostí.
Dialogue: 0,0:16:13.71,0:16:15.59,main,,0000,0000,0000,,A každou noc jsem na párty.
Dialogue: 0,0:16:16.42,0:16:21.18,main,,0000,0000,0000,,Já a moji společníci jsem se snažili získat zpět ukradené světlo z krystalu, aby jsme zachránili svět, před záhubou…
Dialogue: 0,0:16:21.18,0:16:22.05,main,,0000,0000,0000,,…a osvobodili unesenou princeznu…
Dialogue: 0,0:16:22.05,0:16:23.59,main,,0000,0000,0000,,To mluvíš o hře?!
Dialogue: 0,0:16:23.60,0:16:26.06,main,,0000,0000,0000,,Dlouho jsem ho neviděla.
Dialogue: 0,0:16:26.07,0:16:27.01,main,,0000,0000,0000,,Koho?
Dialogue: 0,0:16:27.02,0:16:29.14,main,,0000,0000,0000,,Nespíš mluví o někom ze hry.
Dialogue: 0,0:16:30.23,0:16:31.94,main,,0000,0000,0000,,Hrál za válečníka.
Dialogue: 0,0:16:31.94,0:16:37.73,main,,0000,0000,0000,,Šel někde lovit monstra, aby si vylepšil své schopnost a od té doby jsem ho neviděla.
Dialogue: 0,0:16:39.49,0:16:41.11,main,,0000,0000,0000,,Slyšela jsem…
Dialogue: 0,0:16:41.11,0:16:43.70,main,,0000,0000,0000,,…že leží ve stokách.
Dialogue: 0,0:16:43.70,0:16:45.07,main,,0000,0000,0000,,Zajímalo by mě, jestli je v pořádku.
Dialogue: 0,0:16:45.08,0:16:48.33,main,,0000,0000,0000,,Všichni se o něj bojí.
Dialogue: 0,0:16:48.33,0:16:50.25,main,,0000,0000,0000,,To se v té hře izoloval?
Dialogue: 0,0:16:50.25,0:16:51.96,main,,0000,0000,0000,,Nevím podrobnosti…
Dialogue: 0,0:16:51.96,0:16:55.92,main,,0000,0000,0000,,…ale podle jeho výpisu přihlášení to vypadá, že je online po celou dobu.
Dialogue: 0,0:16:55.92,0:16:57.96,main,,0000,0000,0000,,Takže se izoloval ve virtuálním i reálném světě?!
Dialogue: 0,0:16:57.96,0:17:00.13,main,,0000,0000,0000,,No, přece jen je to jeden člověk.
Dialogue: 0,0:17:00.55,0:17:01.38,main,,0000,0000,0000,,Tak mě napadá…
Dialogue: 0,0:17:01.38,0:17:04.34,main,,0000,0000,0000,,Jak jste se vy dvě seznámili?
Dialogue: 0,0:17:04.93,0:17:09.47,main,,0000,0000,0000,,Nějaký hromotluk z ciziny se chystal unést Tsukasu pryč…
Dialogue: 0,0:17:09.47,0:17:11.89,main,,0000,0000,0000,,…když v tom jsem se objevila.
Dialogue: 0,0:17:11.89,0:17:13.31,main,,0000,0000,0000,,Použitím mého běsnícího úderu…
Dialogue: 0,0:17:13.31,0:17:15.44,main,,0000,0000,0000,,…a ukázkového zdrcujícího kopu…
Dialogue: 0,0:17:15.44,0:17:16.77,main,,0000,0000,0000,,…jsem ji zachránila.
Dialogue: 0,0:17:16.77,0:17:19.53,main,,0000,0000,0000,,Proč se vždycky nakonec bavíme o hrách?
Dialogue: 0,0:17:19.53,0:17:23.32,main,,0000,0000,0000,,No, to se stává, když natrefíš na zkušeného bojovníka.
Dialogue: 0,0:17:23.32,0:17:25.57,main,,0000,0000,0000,,Nejsem si jistá, jestli ses měla tak rychle uchýlit k násilí…
Dialogue: 0,0:17:25.57,0:17:26.57,main,,0000,0000,0000,,…ale poradila sis dobře.
Dialogue: 0,0:17:27.03,0:17:30.58,main,,0000,0000,0000,,Ten cizinec se mě vlastně jen ptal na směr…
Dialogue: 0,0:17:30.58,0:17:33.62,main,,0000,0000,0000,,…ale moc jsem mu nerozuměla, když v tom se objevila Kona-chan…
Dialogue: 0,0:17:34.17,0:17:35.17,main,,0000,0000,0000,,To jsi to naprosto zpackala!
Dialogue: 0,0:17:35.17,0:17:36.58,main,,0000,0000,0000,,A proč se tváříš tak hrdě?!
Dialogue: 0,0:17:37.13,0:17:40.30,main,,0000,0000,0000,,Čas znova nadešel…
Dialogue: 0,0:17:40.80,0:17:42.13,main,,0000,0000,0000,,Má se něco stát?
Dialogue: 0,0:17:42.13,0:17:42.76,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:17:42.76,0:17:45.22,main,,0000,0000,0000,,Jen mě napadlo, že zase začíná sezóna baseballu…
Dialogue: 0,0:17:45.22,0:17:48.64,main,,0000,0000,0000,,Ty nemáš ráda sporty?
Dialogue: 0,0:17:48.64,0:17:50.47,main,,0000,0000,0000,,Jsi tak sportovní…
Dialogue: 0,0:17:50.47,0:17:52.60,main,,0000,0000,0000,,…takže jsem myslela, že je máš ráda.
Dialogue: 0,0:17:52.60,0:17:55.23,main,,0000,0000,0000,,Když se protáhnou a hrají přesčas…
Dialogue: 0,0:17:55.23,0:17:58.23,main,,0000,0000,0000,,…vysílání nových dílů anime se naprosto pomíchá.
Dialogue: 0,0:17:58.53,0:18:02.08,main,,0000,0000,0000,,Někdy je dokonce show úplně zrušena.
Dialogue: 0,0:18:02.08,0:18:05.28,main,,0000,0000,0000,,Není příjemné, když přijdeš o něco co máš ráda.
Dialogue: 0,0:18:05.28,0:18:08.24,main,,0000,0000,0000,,Když jíš jahodu na jahodovém dortu, Kagami?
Dialogue: 0,0:18:08.24,0:18:08.95,main,,0000,0000,0000,,Proč se ptáš?
Dialogue: 0,0:18:09.24,0:18:12.58,main,,0000,0000,0000,,Jen mě zajímalo, jestli si ten typ co si nechává nejlepší nakonec.
Dialogue: 0,0:18:12.58,0:18:15.21,main,,0000,0000,0000,,Obvykle jím to nejlepší až nakonec…
Dialogue: 0,0:18:15.21,0:18:17.37,main,,0000,0000,0000,,…ale s jahodami to záleží na situaci.
Dialogue: 0,0:18:17.38,0:18:19.46,main,,0000,0000,0000,,A griluješ obě dvě strany hovězího jazyku?
Dialogue: 0,0:18:19.92,0:18:21.25,main,,0000,0000,0000,,Hovězího jazyku?
Dialogue: 0,0:18:22.38,0:18:24.05,main,,0000,0000,0000,,Je hotový skoro hned…
Dialogue: 0,0:18:24.05,0:18:25.80,main,,0000,0000,0000,,…ale je to ještě rychlejší, když griluješ obě strany.
Dialogue: 0,0:18:25.80,0:18:26.88,main,,0000,0000,0000,,To nesmíš.
Dialogue: 0,0:18:26.89,0:18:28.34,main,,0000,0000,0000,,Cibule by spadla.
Dialogue: 0,0:18:28.35,0:18:31.10,main,,0000,0000,0000,,Nepřidáváš cibuli, až nakonec.
Dialogue: 0,0:18:31.10,0:18:32.85,main,,0000,0000,0000,,Tak by se ale neopekla.
Dialogue: 0,0:18:33.34,0:18:36.35,main,,0000,0000,0000,,Ale nejsou tam jen kvůli chuti?
Dialogue: 0,0:18:36.44,0:18:37.85,main,,0000,0000,0000,,Ty to prostě nechápeš…
Dialogue: 0,0:18:37.85,0:18:40.55,main,,0000,0000,0000,,Nešlo by to tak i tak?
Dialogue: 0,0:18:41.19,0:18:44.90,main,,0000,0000,0000,,Jaká strana čokoládového kornoutu je podle tebe hlava? Tenká nebo Tlustá?
Dialogue: 0,0:18:46.49,0:18:47.32,main,,0000,0000,0000,,Tlustá?
Dialogue: 0,0:18:47.82,0:18:49.32,main,,0000,0000,0000,,Taky si to myslím.
Dialogue: 0,0:18:49.32,0:18:49.87,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:18:50.24,0:18:51.62,main,,0000,0000,0000,,A z jakého konce ho jíš?
Dialogue: 0,0:18:52.66,0:18:53.95,main,,0000,0000,0000,,Nemáš odloupnout tenký konec…
Dialogue: 0,0:18:53.95,0:18:55.83,main,,0000,0000,0000,,…a pomocí něho jíst přebývající čokoládu?
Dialogue: 0,0:19:03.95,0:19:07.84,main,,0000,0000,0000,,Máš kromě náš nějaké jiné reálné kamarády?
Dialogue: 0,0:19:09.05,0:19:12.81,main,,0000,0000,0000,,Ještě na základce jsem měla celkem blízké kamarády.
Dialogue: 0,0:19:13.60,0:19:14.69,main,,0000,0000,0000,,Takže máš.
Dialogue: 0,0:19:15.43,0:19:17.81,main,,0000,0000,0000,,Ale dlouho jsem s ní nebyla…
Dialogue: 0,0:19:17.81,0:19:19.52,main,,0000,0000,0000,,Zajímalo by mě co asi dělá…
Dialogue: 0,0:19:22.36,0:19:24.98,main,,0000,0000,0000,,Na základce napsala…
Dialogue: 0,0:19:24.99,0:19:26.82,main,,0000,0000,0000,,…že její sen je stát se čarodějem.
Dialogue: 0,0:19:26.82,0:19:27.95,main,,0000,0000,0000,,Takže je stejná jako ty?!
Dialogue: 0,0:19:27.95,0:19:30.95,copyrights,,0000,0000,0000,,{\pos(85,50)}Překlad od paralaXe
Dialogue: 0,0:19:27.95,0:19:30.95,copyrights,,0000,0000,0000,,{\pos(129,76)}navštivte www.otakogu.cz
Dialogue: 0,0:19:33.91,0:19:34.95,main,,0000,0000,0000,,Hodně štěstí!
Dialogue: 0,0:19:35.45,0:19:38.25,main,,0000,0000,0000,,Konečně nadešel čas pro Lucky Channel!
Dialogue: 0,0:19:38.25,0:19:40.08,main,,0000,0000,0000,,Konečně jsme v televizi!
Dialogue: 0,0:19:40.08,0:19:41.08,main,,0000,0000,0000,,Fajn!
Dialogue: 0,0:19:41.08,0:19:43.09,main,,0000,0000,0000,,Zdravím všechny co jsou s námi poprvé!
Dialogue: 0,0:19:43.09,0:19:45.46,main,,0000,0000,0000,,Budu vaším průvodcem! Jsem Akira Kogami.
Dialogue: 0,0:19:46.55,0:19:47.17,main,,0000,0000,0000,,Já jsem Shiraishi.
Dialogue: 0,0:19:49.34,0:19:49.88,main,,0000,0000,0000,,Kdo že jsi?
Dialogue: 0,0:19:50.09,0:19:51.34,main,,0000,0000,0000,,Jen žertuji!
Dialogue: 0,0:19:51.34,0:19:54.14,main,,0000,0000,0000,,Bude mi tu pomáhat.
Dialogue: 0,0:19:54.14,0:19:55.68,main,,0000,0000,0000,,Minoru Shiraishi-san!
Dialogue: 0,0:19:55.68,0:19:57.02,main,,0000,0000,0000,,Da-dada-dum!
Dialogue: 0,0:19:58.23,0:20:00.73,main,,0000,0000,0000,,Jsem tady jako asistent Akiri.
Dialogue: 0,0:20:00.73,0:20:02.40,main,,0000,0000,0000,,Moje jméno je Minoru Shiraishi.
Dialogue: 0,0:20:02.40,0:20:02.73,main,,0000,0000,0000,,Rád vás poznávám.
Dialogue: 0,0:20:03.15,0:20:04.15,main,,0000,0000,0000,,Víš co?
Dialogue: 0,0:20:04.15,0:20:05.73,main,,0000,0000,0000,,Jsi hrozně nervózní.
Dialogue: 0,0:20:06.23,0:20:06.98,main,,0000,0000,0000,,No tak.
Dialogue: 0,0:20:06.99,0:20:08.61,main,,0000,0000,0000,,Teď jsme v televizi.
Dialogue: 0,0:20:08.61,0:20:10.32,main,,0000,0000,0000,,Buď trochu bláznivější!
Dialogue: 0,0:20:10.32,0:20:10.86,main,,0000,0000,0000,,Jen žertuji!
Dialogue: 0,0:20:10.86,0:20:11.66,main,,0000,0000,0000,,Promiň.
Dialogue: 0,0:20:11.66,0:20:12.57,main,,0000,0000,0000,,Nejsem na to zvyklý.
Dialogue: 0,0:20:12.57,0:20:13.12,main,,0000,0000,0000,,Fajn!
Dialogue: 0,0:20:13.12,0:20:14.12,main,,0000,0000,0000,,Jdeme na to!
Dialogue: 0,0:20:14.12,0:20:16.87,main,,0000,0000,0000,,Máme tu nějaké dopisy od našich diváků!
Dialogue: 0,0:20:16.87,0:20:17.87,main,,0000,0000,0000,,Ah, ano.
Dialogue: 0,0:20:17.87,0:20:18.75,main,,0000,0000,0000,,Ehm…
Dialogue: 0,0:20:18.75,0:20:22.04,main,,0000,0000,0000,,Tento je od fandy Berryze z Osaky.
Dialogue: 0,0:20:22.04,0:20:24.88,main,,0000,0000,0000,,Gratuluji, že jste se dostali do televize.
Dialogue: 0,0:20:24.88,0:20:25.59,main,,0000,0000,0000,,Díky!
Dialogue: 0,0:20:25.96,0:20:28.46,main,,0000,0000,0000,,Mimochodem, mám otázku na Akiru-san.
Dialogue: 0,0:20:28.47,0:20:31.47,main,,0000,0000,0000,,Jsi dobře známou hvězdou.
Dialogue: 0,0:20:31.47,0:20:33.72,main,,0000,0000,0000,,Kolik dostává celebrita jako ty zaplaceno?
Dialogue: 0,0:20:33.72,0:20:34.64,main,,0000,0000,0000,,Dej mi prosím vědět.
Dialogue: 0,0:20:38.43,0:20:39.18,main,,0000,0000,0000,,Zaplaceno?
Dialogue: 0,0:20:40.81,0:20:43.81,main,,0000,0000,0000,,To je docela přímá otázka.
Dialogue: 0,0:20:43.81,0:20:45.44,main,,0000,0000,0000,,Šak víš…
Dialogue: 0,0:20:45.44,0:20:47.61,main,,0000,0000,0000,,Jaký má vůbec smysl se na to ptát?
Dialogue: 0,0:20:47.61,0:20:48.73,main,,0000,0000,0000,,A-Akiro-sama?
Dialogue: 0,0:20:48.74,0:20:51.15,main,,0000,0000,0000,,Tak jestli to vážně chceš vědět, tak ti to řeknu.
Dialogue: 0,0:20:51.15,0:20:52.66,main,,0000,0000,0000,,Je to vlastně jedno.
Dialogue: 0,0:20:52.66,0:20:54.41,main,,0000,0000,0000,,Občas to vážně za moc nestojí…
Dialogue: 0,0:20:54.41,0:20:55.66,main,,0000,0000,0000,,…a ještě jsou tu tvoji manažeři.
Dialogue: 0,0:20:55.66,0:20:57.24,main,,0000,0000,0000,,Mám pořád pevný plat…
Dialogue: 0,0:20:57.24,0:21:00.20,main,,0000,0000,0000,,…takže, kdo ví o kolik mě oškubávají.
Dialogue: 0,0:21:00.21,0:21:02.67,main,,0000,0000,0000,,A pracuji už od tří…
Dialogue: 0,0:21:02.67,0:21:05.42,main,,0000,0000,0000,,…takže jdou všechny peníze rodičům.
Dialogue: 0,0:21:05.42,0:21:08.09,main,,0000,0000,0000,,Takže mi dávají něco jako kapesné.
Dialogue: 0,0:21:08.09,0:21:10.21,main,,0000,0000,0000,,A vážně na mě škudlí.
Dialogue: 0,0:21:10.72,0:21:13.97,main,,0000,0000,0000,,Když o tom tak přemýšlím tak mamka nedávno kupovala spoustu značkových věcí.
Dialogue: 0,0:21:13.97,0:21:16.39,main,,0000,0000,0000,,To za ně platí mými penězi?
Dialogue: 0,0:21:16.39,0:21:17.97,main,,0000,0000,0000,,Nezahrávejte si se mnou!
Dialogue: 0,0:21:17.97,0:21:19.77,main,,0000,0000,0000,,Nejsem žádná Popelka!
Dialogue: 0,0:21:19.77,0:21:22.31,main,,0000,0000,0000,,Jen proto, že ti otec neposlal peníze…
Dialogue: 0,0:21:21.77,0:21:22.31,main2,,0000,0000,0000,,Zadrž!
Dialogue: 0,0:21:22.52,0:21:24.44,main,,0000,0000,0000,,…ze mě nedělá tvoji pokladničku!
Dialogue: 0,0:21:22.52,0:21:24.44,main2,,0000,0000,0000,,O tom už mluvit nemusíš!
Dialogue: 0,0:21:25.73,0:21:26.73,main,,0000,0000,0000,,Ah ne!
Dialogue: 0,0:21:26.73,0:21:28.65,main,,0000,0000,0000,,Je čas se rozloučit!
Dialogue: 0,0:21:28.65,0:21:30.28,main,,0000,0000,0000,,Akira je tak smutná!
Dialogue: 0,0:21:30.28,0:21:31.24,main,,0000,0000,0000,,Takže…
Dialogue: 0,0:21:31.24,0:21:37.78,main,,0000,0000,0000,,…posílejte nám vaše otázky a názory na naši show prosím!
Dialogue: 0,0:21:37.78,0:21:39.95,main,,0000,0000,0000,,Rozluč se s nimi taky, Shiraishi-sane!
Dialogue: 0,0:21:40.70,0:21:43.33,main,,0000,0000,0000,,Doufáme, že se uvidíme i příště.
Dialogue: 0,0:21:43.33,0:21:44.71,main,,0000,0000,0000,,Zatím!
Dialogue: 0,0:21:45.83,0:21:46.46,main,,0000,0000,0000,,Jo, a…
Dialogue: 0,0:21:46.46,0:21:48.88,main,,0000,0000,0000,,To se vážně ukážeš v této show?
Dialogue: 0,0:21:49.75,0:21:50.88,main,,0000,0000,0000,,Kdo ti to řekl?
Dialogue: 0,0:21:52.76,0:21:55.34,main,,0000,0000,0000,,Už jsme dlouho nebyli na karaoke.
Dialogue: 0,0:21:55.34,0:21:58.51,main,,0000,0000,0000,,Já sem moc nechodím.
Dialogue: 0,0:21:58.51,0:21:59.35,main,,0000,0000,0000,,No tak. No tak.
Dialogue: 0,0:21:59.35,0:22:01.97,main,,0000,0000,0000,,Máme jen dvě hodiny!
Dialogue: 0,0:22:01.98,0:22:03.60,main,,0000,0000,0000,,Nemusíš nikam spěchat.
Dialogue: 0,0:22:03.60,0:22:04.94,main,,0000,0000,0000,,Pořád máme čas.
Dialogue: 0,0:22:04.94,0:22:07.40,main,,0000,0000,0000,,Je to docela vzrušující.
Dialogue: 0,0:22:07.40,0:22:09.11,main,,0000,0000,0000,,Je to vážně zábava.
Dialogue: 0,0:22:09.11,0:22:10.94,main,,0000,0000,0000,,Je to skvělý pocit, když zpíváš.
Dialogue: 0,0:22:10.94,0:22:11.32,main,,0000,0000,0000,,Heh?
Dialogue: 0,0:22:11.32,0:22:12.23,main,,0000,0000,0000,,Kde je katalog?
Dialogue: 0,0:22:12.28,0:22:13.07,main,,0000,0000,0000,,Katalog?
Dialogue: 0,0:22:13.40,0:22:14.24,main,,0000,0000,0000,,Katalog.
Dialogue: 0,0:22:14.24,0:22:15.53,main,,0000,0000,0000,,Říkáš katalog?
Dialogue: 0,0:22:15.53,0:22:16.11,main,,0000,0000,0000,,Šak víš.
Dialogue: 0,0:22:16.11,0:22:17.70,main,,0000,0000,0000,,Ta věc s čísly písní.
Dialogue: 0,0:22:18.20,0:22:18.95,main,,0000,0000,0000,,Tohle?
Dialogue: 0,0:22:18.95,0:22:19.45,main,,0000,0000,0000,,Ano.
Dialogue: 0,0:22:19.45,0:22:20.99,main,,0000,0000,0000,,Tak začněme!
Dialogue: 0,0:22:20.99,0:22:22.08,main,,0000,0000,0000,,Tak rychle?
Dialogue: 0,0:22:22.62,0:22:24.20,main,,0000,0000,0000,,A už tam má tři písničky!
Dialogue: 0,0:22:24.21,0:22:26.62,main,,0000,0000,0000,,Je hrozné plýtvání jen tak nezpívat.
Dialogue: 0,0:22:29.34,0:22:31.13,main,,0000,0000,0000,,Co je to za písničku?
Dialogue: 0,0:22:31.13,0:22:33.84,main,,0000,0000,0000,,Vesmírný železný muž Kyodain! 
Dialogue: 0,0:22:33.84,0:22:35.84,main,,0000,0000,0000,,Musíš začínat s tímhle!
Dialogue: 0,0:22:35.84,0:22:37.38,main,,0000,0000,0000,,Nikdy jsem o něm neslyšela.
Dialogue: 0,0:22:36.57,0:22:39.90,mainup,,0000,0000,0000,,Da-da-da-da-da Da-da planety
Dialogue: 0,0:22:37.39,0:22:39.05,main,,0000,0000,0000,,Do toho, Kono-chan!
Dialogue: 0,0:22:39.05,0:22:39.80,main,,0000,0000,0000,,Konato!
Dialogue: 0,0:22:40.01,0:22:41.06,main,,0000,0000,0000,,Máš to moc nahlas!
Dialogue: 0,0:22:40.11,0:22:43.28,mainup,,0000,0000,0000,,armáda robotů přepadá svět.
Dialogue: 0,0:22:42.52,0:22:44.43,main,,0000,0000,0000,,Zeslab to!
Dialogue: 0,0:22:43.28,0:22:46.62,mainup,,0000,0000,0000,,Leť bráško do nebes!
Dialogue: 0,0:22:46.02,0:22:48.52,main,,0000,0000,0000,,Jak to, že zná tuhle písničku?
Dialogue: 0,0:22:46.62,0:22:49.87,mainup,,0000,0000,0000,,Skyzei!
Dialogue: 0,0:22:49.87,0:22:53.38,mainup,,0000,0000,0000,,Běž bráško!
Dialogue: 0,0:22:53.38,0:22:56.71,mainup,,0000,0000,0000,,Granzel!
Dialogue: 0,0:22:56.71,0:23:00.05,mainup,,0000,0000,0000,,Dvě ocelové paže!
Dialogue: 0,0:23:00.05,0:23:06.76,mainup,,0000,0000,0000,,Záblesk a z galaxie je ohňostroj!
Dialogue: 0,0:23:06.77,0:23:10.06,mainup,,0000,0000,0000,,Jeď! Jeď! Tryskáči! Jeď! Jeď! Auto!
Dialogue: 0,0:23:10.06,0:23:16.56,mainup,,0000,0000,0000,,Vesmírný železný muž Kyodain!
Dialogue: 0,0:23:16.56,0:23:18.56,mainup,,0000,0000,0000,,S pozdravem od paralaXe!
