Dialogue: 0,0:00:00.00,0:00:05.44,whitebig,,0000,0000,0000,,{\pos(225,126)}
Dialogue: 0,0:00:00.00,0:00:05.44,whitebig,,0000,0000,0000,,{\pos(226,152)}
Dialogue: 0,0:00:00.60,0:00:02.08,main,,0000,0000,0000,,Tak začneme.
Dialogue: 0,0:00:02.08,0:00:03.12,main,,0000,0000,0000,,Jdeme na to.
Dialogue: 0,0:00:03.68,0:00:05.48,main,,0000,0000,0000,,No tak už začněme!
Dialogue: 0,0:00:05.84,0:00:07.00,op1,,0000,0000,0000,,Okolo 3 cm!
Dialogue: 0,0:00:07.00,0:00:08.28,op1,,0000,0000,0000,,Je to snad málo?
Dialogue: 0,0:00:08.28,0:00:08.52,op1,,0000,0000,0000,,Jejda!
Dialogue: 0,0:00:08.52,0:00:10.36,op1,,0000,0000,0000,,Schované pod naší uniformou!
Dialogue: 0,0:00:10.36,0:00:11.40,op1,,0000,0000,0000,,Mám toho nechat?
Dialogue: 0,0:00:11.40,0:00:11.72,op1,,0000,0000,0000,,JEJ!
Dialogue: 0,0:00:11.72,0:00:12.80,op1,,0000,0000,0000,,Jít do toho?
Dialogue: 0,0:00:12.80,0:00:13.60,op1,,0000,0000,0000,,Snažit se?
Dialogue: 0,0:00:13.60,0:00:15.16,op1,,0000,0000,0000,,Vychutnat si každý okamžik. Jo!
Dialogue: 0,0:00:15.16,0:00:15.76,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:15.76,0:00:16.00,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:16.00,0:00:16.40,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.40,0:00:16.76,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:16.76,0:00:17.36,op1,,0000,0000,0000,,Jen mezi námi dvěma!
Dialogue: 0,0:00:17.36,0:00:20.08,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:00:22.08,0:00:22.88,logo,,0000,0000,0000,,{\move(108,218,202,218,0,0)}LUCKY
Dialogue: 0,0:00:22.88,0:00:24.60,logo,,0000,0000,0000,,{\pos(202,218)}LUCKY
Dialogue: 0,0:00:22.08,0:00:22.88,logo,,0000,0000,0000,,{\move(638,218,459,218,0,0)}STAR
Dialogue: 0,0:00:22.88,0:00:24.60,logo,,0000,0000,0000,,{\pos(459,218)}STAR
Dialogue: 0,0:00:25.00,0:00:25.60,op1,,0000,0000,0000,,Cítím se nějak nesvá.
Dialogue: 0,0:00:25.60,0:00:26.20,op1,,0000,0000,0000,,Co jsem to jedla?
Dialogue: 0,0:00:26.20,0:00:26.80,op1,,0000,0000,0000,,Mám tě hrozně ráda!
Dialogue: 0,0:00:26.80,0:00:27.00,op1,,0000,0000,0000,,Oops!
Dialogue: 0,0:00:27.00,0:00:28.04,op1,,0000,0000,0000,,Jedno z nich je špatně!
Dialogue: 0,0:00:28.04,0:00:28.80,op1,,0000,0000,0000,,Relaxovat!
Dialogue: 0,0:00:28.80,0:00:29.44,op1,,0000,0000,0000,,Nic nedělat!
Dialogue: 0,0:00:29.44,0:00:29.96,op1,,0000,0000,0000,,Flákat se!
Dialogue: 0,0:00:29.96,0:00:31.04,op1,,0000,0000,0000,,Už toho nechej!
Dialogue: 0,0:00:31.04,0:00:32.72,op1,,0000,0000,0000,,Je ta nová holka oblíbená?
Dialogue: 0,0:00:32.72,0:00:34.36,op1,,0000,0000,0000,,Já bych řekla, že je docela normální!
Dialogue: 0,0:00:34.36,0:00:35.84,op1,,0000,0000,0000,,To jsem jediná překvapená?
Dialogue: 0,0:00:35.84,0:00:36.16,op1,,0000,0000,0000,,Vepřové kousky!
Dialogue: 0,0:00:36.16,0:00:36.56,op1,,0000,0000,0000,,Skleněné nudle!
Dialogue: 0,0:00:36.56,0:00:36.88,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:00:36.88,0:00:37.68,op2,,0000,0000,0000,,DA-DA-DA-DA-DA!
Dialogue: 0,0:00:38.24,0:00:39.04,op1,,0000,0000,0000,,Ooh?
Dialogue: 0,0:00:39.04,0:00:39.80,op1,,0000,0000,0000,,Víš co tím myslím?
Dialogue: 0,0:00:39.40,0:00:40.24,op1up,,0000,0000,0000,,Moje brýle…
Dialogue: 0,0:00:40.24,0:00:41.32,op1up,,0000,0000,0000,,Kde mám brýle?
Dialogue: 0,0:00:41.40,0:00:43.36,op1,,0000,0000,0000,,Nedávno jsme hrála tu samou bojovku.
Dialogue: 0,0:00:42.00,0:00:43.36,op1up,,0000,0000,0000,,Bez nich nic nevidím…
Dialogue: 0,0:00:44.40,0:00:45.00,op2,,0000,0000,0000,,POM! POM!
Dialogue: 0,0:00:45.00,0:00:46.04,op1,,0000,0000,0000,,Roztleskávačky!
Dialogue: 0,0:00:46.04,0:00:47.56,op2,,0000,0000,0000,,DÁME SI SLADKOST!
Dialogue: 0,0:00:47.56,0:00:48.16,op2,,0000,0000,0000,,LA! LA!
Dialogue: 0,0:00:48.16,0:00:49.20,op1,,0000,0000,0000,,Párty začíná!
Dialogue: 0,0:00:49.20,0:00:50.80,op2,,0000,0000,0000,,MÁŠ TO V KRVI!
Dialogue: 0,0:00:50.80,0:00:51.36,op2,,0000,0000,0000,,ANO!
Dialogue: 0,0:00:51.36,0:00:52.36,op1,,0000,0000,0000,,Zanechává to dojem.
Dialogue: 0,0:00:52.36,0:00:53.84,op1,,0000,0000,0000,,Malé planetky.
Dialogue: 0,0:00:53.84,0:00:54.36,op1,,0000,0000,0000,,Narazili!
Dialogue: 0,0:00:54.36,0:00:55.00,op1,,0000,0000,0000,,A shořeli!
Dialogue: 0,0:00:55.00,0:00:55.36,op1,,0000,0000,0000,,Bylo to ohromné!
Dialogue: 0,0:00:55.36,0:00:56.12,op1,,0000,0000,0000,,Z plných plic křičíme!
Dialogue: 0,0:00:56.12,0:00:56.68,op1,,0000,0000,0000,,ZPÍVEJ A TANCUJ!
Dialogue: 0,0:00:57.04,0:00:58.32,op1,,0000,0000,0000,,Přiznej si to!
Dialogue: 0,0:00:58.32,0:01:02.80,op1,,0000,0000,0000,,Nakonec se stejně budeme smát!
Dialogue: 0,0:01:02.80,0:01:06.56,op1,,0000,0000,0000,,Protože mám na sobě námořnickou uniformu.
Dialogue: 0,0:01:06.56,0:01:09.44,op2,,0000,0000,0000,,To je ono!
Dialogue: 0,0:01:10.00,0:01:13.56,op1,,0000,0000,0000,,Je teprve pondělí a já už mám špatnou náladu.
Dialogue: 0,0:01:13.56,0:01:15.36,op1,,0000,0000,0000,,Co mám dělat?
Dialogue: 0,0:01:15.56,0:01:19.40,op1,,0000,0000,0000,,Raději si vezmu moji letní uniformu -
Dialogue: 0,0:01:19.40,0:01:22.28,op1,,0000,0000,0000,,- je rozkošná!
Dialogue: 0,0:01:22.52,0:01:23.68,op1,,0000,0000,0000,,Trojdílná uniforma!
Dialogue: 0,0:01:23.68,0:01:25.12,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:01:25.12,0:01:25.56,op1,,0000,0000,0000,,JO!
Dialogue: 0,0:01:25.56,0:01:26.00,op1,,0000,0000,0000,,Dej se do toho!
Dialogue: 0,0:01:26.00,0:01:26.36,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.36,0:01:27.00,op1,,0000,0000,0000,,SNAŽ SE!
Dialogue: 0,0:01:27.00,0:01:27.16,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:27.16,0:01:29.04,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:01:30.84,0:01:31.52,main,,0000,0000,0000,,Že?
Dialogue: 0,0:01:31.52,0:01:32.84,main,,0000,0000,0000,,Smrdí to?
Dialogue: 0,0:01:32.84,0:01:33.88,main,,0000,0000,0000,,To teda jo!
Dialogue: 0,0:01:35.08,0:01:37.68,main,,0000,0000,0000,,Zlatý týden je skoro u konce…
Dialogue: 0,0:01:37.68,0:01:40.16,main,,0000,0000,0000,,Už jste se věnovali úkolům?
Dialogue: 0,0:01:41.68,0:01:43.48,main,,0000,0000,0000,,No, vlastně ještě nee…
Dialogue: 0,0:01:44.36,0:01:47.80,main,,0000,0000,0000,,Šak víš, jak je o prázdninách těžké začít něco dělat?
Dialogue: 0,0:01:47.80,0:01:49.00,main,,0000,0000,0000,,Teď máme co dohánět!
Dialogue: 0,0:01:49.60,0:01:50.40,main,,0000,0000,0000,,Že, ségra?
Dialogue: 0,0:01:51.08,0:01:51.80,main,,0000,0000,0000,,Promiň.
Dialogue: 0,0:01:51.84,0:01:53.96,main,,0000,0000,0000,,Já už mám skoro hotovo.
Dialogue: 0,0:01:54.60,0:01:56.52,main,,0000,0000,0000,,Ale přece sis se mnou celou dobu hrála…
Dialogue: 0,0:01:57.08,0:02:00.12,white,,0000,0000,0000,,{\frz-90\pos(390,133)}Snaha
Dialogue: 0,0:01:57.08,0:02:00.12,white,,0000,0000,0000,,{\frz-90\pos(305,215)}a výsledky
Dialogue: 0,0:02:15.52,0:02:16.72,main,,0000,0000,0000,,Tak jsem dojedla.
Dialogue: 0,0:02:21.40,0:02:22.92,main,,0000,0000,0000,,Mám otázku, ohledně této části…
Dialogue: 0,0:02:22.92,0:02:25.68,main,,0000,0000,0000,,Jak bych mohla znát odpověď, když ji nevíš ani ty?
Dialogue: 0,0:02:25.72,0:02:26.80,main,,0000,0000,0000,,Nejsi na výšce?!
Dialogue: 0,0:02:29.44,0:02:31.40,main,,0000,0000,0000,,Jdi vzbudit Tsukasu.
Dialogue: 0,0:02:31.40,0:02:32.20,main,,0000,0000,0000,,To ne.
Dialogue: 0,0:02:32.20,0:02:33.44,main,,0000,0000,0000,,Ona nevstane.
Dialogue: 0,0:02:34.52,0:02:36.32,main,,0000,0000,0000,,Tak už vstávej!
Dialogue: 0,0:02:37.48,0:02:39.16,main,,0000,0000,0000,,Taťka a ségra mají zpoždění.
Dialogue: 0,0:02:39.48,0:02:41.48,main,,0000,0000,0000,,Nemají tohle ráno, něco důležitého?
Dialogue: 0,0:02:41.60,0:02:44.16,main,,0000,0000,0000,,Cože? To je už ráno?
Dialogue: 0,0:02:43.40,0:02:44.08,main2,,0000,0000,0000,,Jsem doma.
Dialogue: 0,0:02:44.20,0:02:45.48,main,,0000,0000,0000,,Už je ráno.
Dialogue: 0,0:02:45.52,0:02:46.20,main,,0000,0000,0000,,Vítej.
Dialogue: 0,0:02:47.44,0:02:48.52,main,,0000,0000,0000,,Dobré ráno…
Dialogue: 0,0:02:48.52,0:02:50.00,main,,0000,0000,0000,,Už je odpoledne.
Dialogue: 0,0:02:50.00,0:02:52.12,main,,0000,0000,0000,,Nemůžu uvěřit, jak dlouho vydržíš spát.
Dialogue: 0,0:02:53.60,0:02:56.52,main,,0000,0000,0000,,Viděli jste včera speciální epizodu Apron of Love?
Dialogue: 0,0:02:56.52,0:02:57.40,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:02:57.44,0:02:59.00,main,,0000,0000,0000,,V půlce jsem usnula.
Dialogue: 0,0:02:59.44,0:02:59.84,main,,0000,0000,0000,,Usnula jsi?
Dialogue: 0,0:03:00.72,0:03:02.44,main,,0000,0000,0000,,Bylo to šílené!
Dialogue: 0,0:03:02.44,0:03:04.96,main,,0000,0000,0000,,Na konci chlápek balancoval na hlavě s červenou mísou!
Dialogue: 0,0:03:15.24,0:03:16.08,main,,0000,0000,0000,,Kagami!
Dialogue: 0,0:03:16.48,0:03:18.68,main,,0000,0000,0000,,Volá ti Miyuki-chan!
Dialogue: 0,0:03:18.68,0:03:19.48,main,,0000,0000,0000,,Okaj.
Dialogue: 0,0:03:19.96,0:03:23.76,main,,0000,0000,0000,,Nechtěla by ses sejít a zkontrolovat si domácí úkoly?
Dialogue: 0,0:03:24.76,0:03:25.40,main,,0000,0000,0000,,To zní dobře!
Dialogue: 0,0:03:25.40,0:03:26.08,main,,0000,0000,0000,,Tak fajn!
Dialogue: 0,0:03:26.48,0:03:27.44,main,,0000,0000,0000,,Kagami!
Dialogue: 0,0:03:27.44,0:03:29.40,main,,0000,0000,0000,,Volá ti Konata-chan!
Dialogue: 0,0:03:30.28,0:03:33.52,main,,0000,0000,0000,,Co kdyby jsme se sešli a dodělali si úkoly?
Dialogue: 0,0:03:33.52,0:03:34.40,main,,0000,0000,0000,,Počkej moment!
Dialogue: 0,0:03:34.40,0:03:36.96,main,,0000,0000,0000,,Ty si to chceš jen opsat, že jo?!
Dialogue: 0,0:03:37.24,0:03:39.68,main,,0000,0000,0000,,Nech mě, opsat si úkol.
Dialogue: 0,0:03:39.68,0:03:40.96,main,,0000,0000,0000,,Rovnou na věc, co?
Dialogue: 0,0:03:41.48,0:03:43.52,main,,0000,0000,0000,,Můžeš si je opsat…
Dialogue: 0,0:03:43.52,0:03:45.04,main,,0000,0000,0000,,ale pokud si nebudeš dělat úkoly sama…
Dialogue: 0,0:03:45.04,0:03:47.84,main,,0000,0000,0000,,…totálně vyhoříš u testu po prázdninách.
Dialogue: 0,0:03:48.36,0:03:49.48,main,,0000,0000,0000,,Máš pravdu…
Dialogue: 0,0:03:50.08,0:03:51.92,main,,0000,0000,0000,,Vlastně si tím jen ubližujeme…
Dialogue: 0,0:03:52.36,0:03:55.12,main,,0000,0000,0000,,Jsem dobrá ve šprtání se den předem, takže je to ok.
Dialogue: 0,0:03:55.12,0:03:55.88,main,,0000,0000,0000,,Tak mě to nech opsat.
Dialogue: 0,0:03:55.88,0:03:57.36,main,,0000,0000,0000,,Alespoň předstírej, že to není správné!
Dialogue: 0,0:03:59.76,0:04:00.80,main,,0000,0000,0000,,Promiňte, že vyrušuji.
Dialogue: 0,0:04:01.20,0:04:02.76,main,,0000,0000,0000,,Vítej!
Dialogue: 0,0:04:01.20,0:04:02.76,main2,,0000,0000,0000,,Oh, vítej!
Dialogue: 0,0:04:02.76,0:04:03.24,main,,0000,0000,0000,,Oh.
Dialogue: 0,0:04:03.40,0:04:05.36,main,,0000,0000,0000,,Ty jsi taky přišla studovat, Izumi-san?
Dialogue: 0,0:04:05.36,0:04:07.20,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:04:08.16,0:04:09.08,main,,0000,0000,0000,,Nicméně…
Dialogue: 0,0:04:09.08,0:04:10.92,main,,0000,0000,0000,,Sotva jste se do toho dali.
Dialogue: 0,0:04:11.64,0:04:13.76,main,,0000,0000,0000,,Co jste dělali během volna?
Dialogue: 0,0:04:14.56,0:04:15.32,main,,0000,0000,0000,,Ehm…
Dialogue: 0,0:04:15.32,0:04:17.20,pinkblack,,0000,0000,0000,,{\frz-90\pos(320,115)}První den
Dialogue: 0,0:04:17.00,0:04:23.00,main,,0000,0000,0000,,Dvacet tři stránek matiky, dvacet stránek historie a třicet stránek angličtiny…
Dialogue: 0,0:04:19.64,0:04:23.20,main2,,0000,0000,0000,,Konečně jsem u Brány vítězství…
Dialogue: 0,0:04:23.84,0:04:26.20,main,,0000,0000,0000,,Máme pět dní volna…
Dialogue: 0,0:04:26.20,0:04:29.12,main,,0000,0000,0000,,Takže po pěti, ne po čtyřech hodinách denně…
Dialogue: 0,0:04:30.20,0:04:32.40,main,,0000,0000,0000,,To znamená 1,15 stránek matiky za hodinu.
Dialogue: 0,0:04:33.32,0:04:35.28,pinkblack,,0000,0000,0000,,{\frz-90\pos(320,115)}Druhý den
Dialogue: 0,0:04:34.40,0:04:36.68,main,,0000,0000,0000,,Dostávám mnohem víc expů, než jsem si myslela.
Dialogue: 0,0:04:35.44,0:04:37.76,main2,,0000,0000,0000,,Jejda. Dneska jsem se ještě neučila!
Dialogue: 0,0:04:37.96,0:04:41.36,main,,0000,0000,0000,,Zajímalo by mě kolik mi chybí do dalšího levelu?
Dialogue: 0,0:04:40.28,0:04:43.40,main2,,0000,0000,0000,,Mám ještě tři dny. Takže se musím učit 6 hodin denně?
Dialogue: 0,0:04:42.56,0:04:43.96,main,,0000,0000,0000,,Tohle je docela vzácné!
Dialogue: 0,0:04:44.00,0:04:45.84,pinkblack,,0000,0000,0000,,{\frz-90\pos(320,115)}Třetí den
Dialogue: 0,0:04:44.56,0:04:47.00,main,,0000,0000,0000,,Nikdo si nepamatuje jeho jméno…
Dialogue: 0,0:04:46.60,0:04:49.56,main2,,0000,0000,0000,,Už to začíná jít vážně pomalu.
Dialogue: 0,0:04:47.00,0:04:49.08,main,,0000,0000,0000,,a to deset let pomáhal na stavbě.
Dialogue: 0,0:04:49.08,0:04:49.60,main,,0000,0000,0000,,Je to Yutaka.
Dialogue: 0,0:04:49.56,0:04:53.16,main2,,0000,0000,0000,,Začíná jít vážně do tuhého! Takže tohle je vše co mám…
Dialogue: 0,0:04:50.52,0:04:53.04,main,,0000,0000,0000,,Už jen dva dny!
Dialogue: 0,0:04:53.16,0:04:54.52,main,,0000,0000,0000,,Musím si vážně pohnout…
Dialogue: 0,0:04:53.48,0:04:55.00,pinkblack,,0000,0000,0000,,{\frz-90\pos(320,115)}Čtvrtý den
Dialogue: 0,0:04:54.52,0:04:56.88,main,,0000,0000,0000,,Zlatý týden je už skoro u konce…
Dialogue: 0,0:04:56.88,0:04:59.36,main,,0000,0000,0000,,Už jste se podívali na vaše úkoly?
Dialogue: 0,0:05:00.44,0:05:03.32,main,,0000,0000,0000,,Co kdyby jsme se sešli a dodělali si úkoly?
Dialogue: 0,0:05:03.76,0:05:07.12,main,,0000,0000,0000,,Chtěla jsem se držet plánu…
Dialogue: 0,0:05:07.16,0:05:08.60,main,,0000,0000,0000,,Mise neúspěšná.
Dialogue: 0,0:05:08.60,0:05:10.20,main,,0000,0000,0000,,Lepší je si to ani neplánovat.
Dialogue: 0,0:05:10.20,0:05:11.28,main,,0000,0000,0000,,Ty jsi ještě horší!
Dialogue: 0,0:05:20.44,0:05:23.16,main,,0000,0000,0000,,Takže to nakonec udělala sama?
Dialogue: 0,0:05:23.96,0:05:27.40,main,,0000,0000,0000,,Myslím, že pořád hluboko uvnitř je dříč.
Dialogue: 0,0:05:30.68,0:05:33.44,main,,0000,0000,0000,,Ale její přesnost dost mizivá.
Dialogue: 0,0:05:33.44,0:05:34.16,main,,0000,0000,0000,,Patetické…
Dialogue: 0,0:05:34.16,0:05:35.28,main,,0000,0000,0000,,Prostě patetické!
Dialogue: 0,0:05:35.56,0:05:36.80,main,,0000,0000,0000,,Ahoj!
Dialogue: 0,0:05:36.80,0:05:38.76,main,,0000,0000,0000,,Tak co? Bavili jste se?
Dialogue: 0,0:05:38.76,0:05:39.84,main,,0000,0000,0000,,Tak znova…
Dialogue: 0,0:05:39.84,0:05:41.44,main,,0000,0000,0000,,Měli jste kupu úkolů…
Dialogue: 0,0:05:41.44,0:05:43.56,main,,0000,0000,0000,,…a test hned po prázdninách…
Dialogue: 0,0:05:43.56,0:05:46.40,main,,0000,0000,0000,,…takže jste asi moc času na zábavu neměli, co?
Dialogue: 0,0:05:46.76,0:05:49.44,main,,0000,0000,0000,,Sotva se to podobalo prázdninám.
Dialogue: 0,0:05:49.44,0:05:50.32,main,,0000,0000,0000,,To je smutné.
Dialogue: 0,0:05:50.32,0:05:52.28,main,,0000,0000,0000,,To já si krásně odpočinula!
Dialogue: 0,0:05:52.44,0:05:53.24,main,,0000,0000,0000,,Paní učitelko.
Dialogue: 0,0:05:53.76,0:05:55.92,main,,0000,0000,0000,,Já si taky odpočinula!
Dialogue: 0,0:05:55.92,0:05:56.20,main,,0000,0000,0000,,Hej.
Dialogue: 0,0:05:58.20,0:06:01.56,main,,0000,0000,0000,,Nejsou tyhle věci navržené, abys nevyhrála?
Dialogue: 0,0:06:02.32,0:06:03.60,main,,0000,0000,0000,,To záleží co chceš vytáhnout.
Dialogue: 0,0:06:03.60,0:06:05.92,main,,0000,0000,0000,,Některé věci je skoro nemožné vytáhnout.
Dialogue: 0,0:06:06.56,0:06:07.44,main,,0000,0000,0000,,Že?
Dialogue: 0,0:06:07.44,0:06:09.16,main,,0000,0000,0000,,Já bych se toho nikdy ani nedotkla.
Dialogue: 0,0:06:14.00,0:06:14.96,main,,0000,0000,0000,,Neuvěřitelné!
Dialogue: 0,0:06:14.96,0:06:15.68,main,,0000,0000,0000,,Nah.
Dialogue: 0,0:06:15.68,0:06:17.36,main,,0000,0000,0000,,Už jsem věděla, že se dá vytáhnout.
Dialogue: 0,0:06:18.56,0:06:19.36,main,,0000,0000,0000,,Takže možná…
Dialogue: 0,0:06:19.36,0:06:21.56,main,,0000,0000,0000,,…kdybych šla po ní…
Dialogue: 0,0:06:21.56,0:06:22.72,main,,0000,0000,0000,,Mohla bych ho vytáhnout.
Dialogue: 0,0:06:23.12,0:06:24.52,main,,0000,0000,0000,,Tak o tom ty přemýšlíš?
Dialogue: 0,0:06:25.12,0:06:26.88,main,,0000,0000,0000,,NE! To ne!
Dialogue: 0,0:06:27.52,0:06:30.68,main,,0000,0000,0000,,A-ale…
Dialogue: 0,0:06:30.68,0:06:31.88,main,,0000,0000,0000,,Jdi na to, ségra!
Dialogue: 0,0:06:37.04,0:06:38.60,main,,0000,0000,0000,,Myslela jsem si, že ho mám…
Dialogue: 0,0:06:38.60,0:06:39.52,main,,0000,0000,0000,,To je zlé…
Dialogue: 0,0:06:42.16,0:06:42.60,main,,0000,0000,0000,,Heh?
Dialogue: 0,0:06:47.88,0:06:48.32,main,,0000,0000,0000,,To ne!
Dialogue: 0,0:06:48.68,0:06:49.32,main,,0000,0000,0000,,Proč?!
Dialogue: 0,0:06:49.84,0:06:51.56,main,,0000,0000,0000,,Významný člověk kdysi řekl…
Dialogue: 0,0:06:51.56,0:06:54.04,main,,0000,0000,0000,,…že ovládat „chytače“ je jako si zakládat a spořit na účet.
Dialogue: 0,0:06:54.04,0:06:55.76,main,,0000,0000,0000,,Chytrý chlápek…
Dialogue: 0,0:06:59.04,0:07:00.40,main,,0000,0000,0000,,To ne! Zkazila jsem to!
Dialogue: 0,0:07:03.68,0:07:05.76,main,,0000,0000,0000,,Vůbec mi to nešlo…
Dialogue: 0,0:07:03.76,0:07:05.20,mainup,,0000,0000,0000,,A vaše score jsou!
Dialogue: 0,0:07:05.20,0:07:06.68,mainup,,0000,0000,0000,,Dosáhla jste úrovně Taiko 1!
Dialogue: 0,0:07:06.68,0:07:08.76,main,,0000,0000,0000,,Máš smysl pro rytmus Kona-chan.
Dialogue: 0,0:07:07.20,0:07:09.52,mainup,,0000,0000,0000,,Dosáhla jste úrovně Taiko 5!
Dialogue: 0,0:07:08.76,0:07:09.12,main,,0000,0000,0000,,Neh.
Dialogue: 0,0:07:09.12,0:07:10.88,main,,0000,0000,0000,,To nemá co dělat s rytmem.
Dialogue: 0,0:07:11.40,0:07:16.40,main,,0000,0000,0000,,Schopnosti co si odneseš s hraním bývají v normálním světě pěkně neužitečné.
Dialogue: 0,0:07:16.40,0:07:18.60,main,,0000,0000,0000,,Ty jsi toho živoucím důkazem.
Dialogue: 0,0:07:38.24,0:07:39.88,main,,0000,0000,0000,,Jsi silná.
Dialogue: 0,0:07:39.88,0:07:41.08,main,,0000,0000,0000,,To jo.
Dialogue: 0,0:07:41.08,0:07:43.72,main,,0000,0000,0000,,Jak často u toho vysedává?
Dialogue: 0,0:07:44.12,0:07:45.12,main,,0000,0000,0000,,Izumi?
Dialogue: 0,0:07:46.52,0:07:47.96,main,,0000,0000,0000,,Izumi dneska chybí?
Dialogue: 0,0:07:48.56,0:07:49.64,main,,0000,0000,0000,,Paní Kuroi.
Dialogue: 0,0:07:49.64,0:07:51.16,main,,0000,0000,0000,,Volá vám Izumi-san.
Dialogue: 0,0:07:51.80,0:07:52.32,main,,0000,0000,0000,,Ano.
Dialogue: 0,0:07:52.32,0:07:53.64,main,,0000,0000,0000,,To jsem já.
Dialogue: 0,0:07:54.16,0:07:55.60,main,,0000,0000,0000,,Paní učitelko, moc se omlouvám…
Dialogue: 0,0:07:55.60,0:07:57.68,main,,0000,0000,0000,,…ale dnes je mi zle…
Dialogue: 0,0:07:58.16,0:07:59.68,main,,0000,0000,0000,,To je neobvyklé, Izumi.
Dialogue: 0,0:07:59.68,0:08:00.24,main,,0000,0000,0000,,Nachladila jsi se?
Dialogue: 0,0:08:00.48,0:08:01.12,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:08:01.12,0:08:02.56,main,,0000,0000,0000,,Je to moc špatná květnová nemoc.
Dialogue: 0,0:08:03.04,0:08:05.00,main,,0000,0000,0000,,Sotva se můžu pohnout…
Dialogue: 0,0:08:05.00,0:08:06.24,main,,0000,0000,0000,,Prostě se sem dovleč!
Dialogue: 0,0:08:09.04,0:08:09.96,main,,0000,0000,0000,,Hej!
Dialogue: 0,0:08:09.96,0:08:10.80,main,,0000,0000,0000,,Odcházíme.
Dialogue: 0,0:08:13.48,0:08:16.12,main,,0000,0000,0000,,Jak to, že máš tak plnou kabelu?
Dialogue: 0,0:08:16.60,0:08:18.28,main,,0000,0000,0000,,Zítra jsou testy…
Dialogue: 0,0:08:18.28,0:08:21.40,main,,0000,0000,0000,,…takže si musím vzít vše domů na celo-noční učící maraton.
Dialogue: 0,0:08:21.40,0:08:22.40,main,,0000,0000,0000,,Neříkej mi…
Dialogue: 0,0:08:22.40,0:08:24.68,main,,0000,0000,0000,,…že se to chceš za noc všechno naučit?
Dialogue: 0,0:08:25.44,0:08:26.40,main,,0000,0000,0000,,Opravdu…
Dialogue: 0,0:08:26.40,0:08:29.44,main,,0000,0000,0000,,Čekat den před testem na odnesení knih domů…
Dialogue: 0,0:08:29.44,0:08:31.04,main,,0000,0000,0000,,je vážně pozdě!
Dialogue: 0,0:08:31.56,0:08:33.84,main,,0000,0000,0000,,Ale pokaždé to takhle zmáknu.
Dialogue: 0,0:08:34.20,0:08:35.96,main,,0000,0000,0000,,Zníš přesvědčivě.
Dialogue: 0,0:08:35.96,0:08:36.40,main,,0000,0000,0000,,No.
Dialogue: 0,0:08:36.40,0:08:38.64,main,,0000,0000,0000,,Šak se předvedeš.
Dialogue: 0,0:08:38.80,0:08:39.64,main,,0000,0000,0000,,První semestr.
Dialogue: 0,0:08:48.68,0:08:49.52,main,,0000,0000,0000,,Okaj!
Dialogue: 0,0:08:49.52,0:08:51.72,main,,0000,0000,0000,,Čas na vrácení testů!
Dialogue: 0,0:08:53.12,0:08:53.80,main,,0000,0000,0000,,Aah!
Dialogue: 0,0:08:54.20,0:08:55.16,main,,0000,0000,0000,,Slušné.
Dialogue: 0,0:08:55.60,0:08:56.48,main,,0000,0000,0000,,Jak sis vedla?
Dialogue: 0,0:08:56.96,0:08:58.68,main,,0000,0000,0000,,Raději se neptej…
Dialogue: 0,0:08:58.68,0:08:59.96,main,,0000,0000,0000,,Ale snažila jsem se…
Dialogue: 0,0:09:00.36,0:09:01.08,main,,0000,0000,0000,,No, teď…
Dialogue: 0,0:09:01.08,0:09:02.04,main,,0000,0000,0000,,Co slečna „jednunocpředtestem“?
Dialogue: 0,0:09:02.40,0:09:03.64,main,,0000,0000,0000,,V pohodě, díky za optání.
Dialogue: 0,0:09:03.64,0:09:04.64,main,,0000,0000,0000,,To je nefér!
Dialogue: 0,0:09:10.40,0:09:11.24,main,,0000,0000,0000,,Jsem hrozně unavená…
Dialogue: 0,0:09:11.28,0:09:13.28,main,,0000,0000,0000,,Nic se mi nechce dělat…
Dialogue: 0,0:09:13.28,0:09:13.88,main,,0000,0000,0000,,No tak.
Dialogue: 0,0:09:13.88,0:09:14.72,main,,0000,0000,0000,,Jdeme.
Dialogue: 0,0:09:16.76,0:09:19.72,main,,0000,0000,0000,,Ale stejně se tu s námi poflakuješ, co?
Dialogue: 0,0:09:20.56,0:09:21.64,main,,0000,0000,0000,,Počítačový festival!
Dialogue: 0,0:09:21.64,0:09:23.28,main,,0000,0000,0000,,Sleva na všechny QUO karty!
Dialogue: 0,0:09:23.28,0:09:24.40,main,,0000,0000,0000,,Musím je mít všechny!
Dialogue: 0,0:09:24.76,0:09:25.40,main,,0000,0000,0000,,Okaj.
Dialogue: 0,0:09:25.40,0:09:26.44,main,,0000,0000,0000,,Čas si najít práci.
Dialogue: 0,0:09:26.80,0:09:28.44,main,,0000,0000,0000,,Jsi tak průhledná.
Dialogue: 0,0:09:30.88,0:09:31.96,main,,0000,0000,0000,,Vážně…
Dialogue: 0,0:09:31.96,0:09:35.84,main,,0000,0000,0000,,Ještě jsem nikdy neslyšela o nikom kdo by zůstal doma kvůli květnové únavě.
Dialogue: 0,0:09:35.84,0:09:37.84,main,,0000,0000,0000,,Nicméně dokážu si představit, jak ji je.
Dialogue: 0,0:09:37.84,0:09:39.68,main,,0000,0000,0000,,Nemá to v názvu nemoc?
Dialogue: 0,0:09:39.68,0:09:41.52,main,,0000,0000,0000,,Nemělo by se to počítat jako vážná choroba?
Dialogue: 0,0:09:41.56,0:09:42.96,main,,0000,0000,0000,,To myslíš vážně?
Dialogue: 0,0:09:44.76,0:09:47.32,main,,0000,0000,0000,,Co je vlastně květnová nemoc?
Dialogue: 0,0:09:47.84,0:09:49.88,main,,0000,0000,0000,,Miyuki.
Dialogue: 0,0:09:50.20,0:09:51.40,main,,0000,0000,0000,,No takže…
Dialogue: 0,0:09:51.40,0:09:53.24,main,,0000,0000,0000,,Většinou se dostavuje po tom co dosáhneš nějakého cíle…
Dialogue: 0,0:09:53.28,0:09:56.56,main,,0000,0000,0000,,jako je například získání nové práce nebo zvládnutí přijímaček na vysokou.
Dialogue: 0,0:09:56.56,0:10:01.00,main,,0000,0000,0000,,Nové vztahy v dubnu taky nejdou zrovna hladce…
Dialogue: 0,0:10:01.00,0:10:03.60,main,,0000,0000,0000,,…nedokážeš příjmout rozdíl mezi…
Dialogue: 0,0:10:03.60,0:10:05.92,main,,0000,0000,0000,,…skutečným světem a tvou vysněnou realitou.
Dialogue: 0,0:10:05.92,0:10:08.76,main,,0000,0000,0000,,Takže se nakonec cítíš bezmocná a unavená.
Dialogue: 0,0:10:09.36,0:10:13.76,main,,0000,0000,0000,,Takže čistá otupělost by se neměla považovat za květnovou nemoc…
Dialogue: 0,0:10:13.80,0:10:16.32,main,,0000,0000,0000,,Krom toho to nemá co dělat s…
Dialogue: 0,0:10:16.32,0:10:18.20,main,,0000,0000,0000,,…druhým ročníkem na střední.
Dialogue: 0,0:10:18.20,0:10:22.20,main,,0000,0000,0000,,Já jsem na jaře vždycky taková pomalejší.
Dialogue: 0,0:10:22.20,0:10:23.28,main,,0000,0000,0000,,Oh, vážně.
Dialogue: 0,0:10:23.76,0:10:25.76,main,,0000,0000,0000,,Ale pořád na tom nejsem tak špatně jako Konata…
Dialogue: 0,0:10:25.76,0:10:27.88,main,,0000,0000,0000,,…ale cítím se neobvykle unavená.
Dialogue: 0,0:10:28.44,0:10:29.96,main,,0000,0000,0000,,Jakmile jsem doma…
Dialogue: 0,0:10:29.96,0:10:32.08,main,,0000,0000,0000,,…nemůžu se donutit dělat úkoly.
Dialogue: 0,0:10:32.08,0:10:33.40,main,,0000,0000,0000,,Já taky.
Dialogue: 0,0:10:36.60,0:10:38.60,main,,0000,0000,0000,,Nikdo to nebude komentovat?
Dialogue: 0,0:10:41.88,0:10:44.80,main,,0000,0000,0000,,Proč vzdycháš, Miyuki-chan?
Dialogue: 0,0:10:45.52,0:10:47.72,main,,0000,0000,0000,,Je mi trapně to v takovém věku říkat…
Dialogue: 0,0:10:47.72,0:10:50.20,main,,0000,0000,0000,,Stejně jako jsem ohledně kontaktní čoček…
Dialogue: 0,0:10:50.20,0:10:52.60,main,,0000,0000,0000,,…ale vyhýbám se zubaři, protože mě děsí…
Dialogue: 0,0:10:52.60,0:10:54.68,main,,0000,0000,0000,,…ale mám kaz.
Dialogue: 0,0:10:55.28,0:10:58.32,main,,0000,0000,0000,,Bolí to i když piji vodu.
Dialogue: 0,0:11:01.04,0:11:02.48,main,,0000,0000,0000,,Není to…
Dialogue: 0,0:11:02.48,0:11:03.96,main,,0000,0000,0000,,…normální?
Dialogue: 0,0:11:03.96,0:11:05.24,main,,0000,0000,0000,,Sama se dost bojím zubaře.
Dialogue: 0,0:11:05.76,0:11:07.72,main,,0000,0000,0000,,Ale proč se mu vyhýbáš…
Dialogue: 0,0:11:07.72,0:11:09.32,main,,0000,0000,0000,,…dokud tvůj zub začne bolet?
Dialogue: 0,0:11:15.04,0:11:16.40,main,,0000,0000,0000,,Je to zvláštní…
Dialogue: 0,0:11:16.40,0:11:19.40,main,,0000,0000,0000,,I když vím, že můj zub bude bolet…
Dialogue: 0,0:11:19.40,0:11:22.36,main,,0000,0000,0000,,pořád se mi nechce k zubaři.
Dialogue: 0,0:11:23.16,0:11:24.76,main,,0000,0000,0000,,Když to poprvé začalo bolet…
Dialogue: 0,0:11:24.76,0:11:26.80,main,,0000,0000,0000,,…řekla jsem si, že nepůjdu k zubaři…
Dialogue: 0,0:11:26.80,0:11:29.56,main,,0000,0000,0000,,…že budu raději žít s bolestí.
Dialogue: 0,0:11:30.00,0:11:32.00,main,,0000,0000,0000,,To je zubař vážně tak děsivý?
Dialogue: 0,0:11:32.00,0:11:33.76,main,,0000,0000,0000,,Nikdy jsem tam nemusela, takže nevím.
Dialogue: 0,0:11:34.28,0:11:35.76,main,,0000,0000,0000,,Tys nikdy nešla k zubaři?
Dialogue: 0,0:11:35.76,0:11:37.48,main,,0000,0000,0000,,To máš štěstí…
Dialogue: 0,0:11:37.48,0:11:40.16,main,,0000,0000,0000,,Je tam ten zápach nemocnice…
Dialogue: 0,0:11:40.16,0:11:41.96,main,,0000,0000,0000,,Nebo nervy drásající čekání.
Dialogue: 0,0:11:42.56,0:11:43.36,main,,0000,0000,0000,,Ooh! Ooh!
Dialogue: 0,0:11:43.36,0:11:45.72,main,,0000,0000,0000,,A zvuk vrtačky mě pokaždé vyděsí!
Dialogue: 0,0:11:45.72,0:11:46.40,main,,0000,0000,0000,,Vrták!
Dialogue: 0,0:11:46.76,0:11:47.80,main,,0000,0000,0000,,Je to děsivé!
Dialogue: 0,0:11:48.44,0:11:51.16,main,,0000,0000,0000,,Ale kluci tam nejspíš chodí rádi, co?
Dialogue: 0,0:11:51.76,0:11:52.16,main,,0000,0000,0000,,Proč?
Dialogue: 0,0:11:52.16,0:11:54.32,main,,0000,0000,0000,,Kluci přece mají rádi vrtačky ne?
Dialogue: 0,0:11:54.80,0:11:55.80,main,,0000,0000,0000,,Dinosauří království!
Dialogue: 0,0:11:56.16,0:12:00.48,main,,0000,0000,0000,,Slyšela jsem, že mají rádi když roboti bojují s vrtáky!
Dialogue: 0,0:12:03.08,0:12:06.36,main,,0000,0000,0000,,Ale nejspíš se jim nelíbí, když jim někdo vrtá zuby…
Dialogue: 0,0:12:06.84,0:12:08.56,main,,0000,0000,0000,,Zajímalo by mě…
Dialogue: 0,0:12:08.56,0:12:11.24,main,,0000,0000,0000,,Nejsou recepční a asistentky…
Dialogue: 0,0:12:11.24,0:12:12.36,main,,0000,0000,0000,,…obvykle pěkné slečny?
Dialogue: 0,0:12:12.76,0:12:14.76,main,,0000,0000,0000,,Když si to teď zmínila, tak mi to tak přijde…
Dialogue: 0,0:12:15.36,0:12:16.36,main,,0000,0000,0000,,Zajímalo by mě proč…
Dialogue: 0,0:12:16.60,0:12:19.36,main,,0000,0000,0000,,Možná je to jedno z jejich kritérií při pohovoru…
Dialogue: 0,0:12:19.84,0:12:22.16,main,,0000,0000,0000,,Exkluzivně krásné asistentky a vrtačky, heh?
Dialogue: 0,0:12:29.76,0:12:33.32,main,,0000,0000,0000,,To vážně zní jako něco co mají chlapi rádi.
Dialogue: 0,0:12:33.32,0:12:33.84,main,,0000,0000,0000,,Tak potom…
Dialogue: 0,0:12:33.84,0:12:36.44,main,,0000,0000,0000,,Jak by měla vypadat ošetřovna pro ženy?
Dialogue: 0,0:12:37.04,0:12:39.80,main,,0000,0000,0000,,Zaprvé se musí zbavit toho zápachu.
Dialogue: 0,0:12:40.20,0:12:44.92,main,,0000,0000,0000,,A místo židlí mít měkké masážní křesla.
Dialogue: 0,0:12:45.40,0:12:48.00,main,,0000,0000,0000,,A taky by měli začlenit aromaterapii, že?
Dialogue: 0,0:12:48.00,0:12:50.84,main,,0000,0000,0000,,A přidat masáž nohou, zatím co vás ošetřují.
Dialogue: 0,0:12:52.80,0:12:53.96,main,,0000,0000,0000,,To zní dobře.
Dialogue: 0,0:12:53.96,0:12:58.16,main,,0000,0000,0000,,A k utišení bolesti by tě měli nechat hrát hry.
Dialogue: 0,0:12:58.16,0:13:00.24,main,,0000,0000,0000,,Mohlo by být trochu obtížné zároveň hrát hry…
Dialogue: 0,0:13:00.24,0:13:03.32,main,,0000,0000,0000,,Ale je to dobrý způsob jak tě odpoutat od bolesti.
Dialogue: 0,0:13:03.32,0:13:06.44,main,,0000,0000,0000,,Mohli by ti dát sluchátka a pustit ti, oblíbenou písničku.
Dialogue: 0,0:13:07.04,0:13:07.76,main,,0000,0000,0000,,To zní dobře.
Dialogue: 0,0:13:07.76,0:13:09.08,main,,0000,0000,0000,,To by bylo taky fajn.
Dialogue: 0,0:13:13.92,0:13:16.96,main,,0000,0000,0000,,Ale pořád se k zubaři necítím.
Dialogue: 0,0:13:16.96,0:13:18.00,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:13:18.48,0:13:19.04,main,,0000,0000,0000,,Co je tohle?!
Dialogue: 0,0:13:19.56,0:13:20.56,main,,0000,0000,0000,,Co se děje?
Dialogue: 0,0:13:20.56,0:13:21.08,main,,0000,0000,0000,,No…
Dialogue: 0,0:13:21.08,0:13:23.80,main,,0000,0000,0000,,Animují další sérii co mám ráda…
Dialogue: 0,0:13:23.80,0:13:27.36,main,,0000,0000,0000,,…ale používají naprosto jiné obsazení z drama CD.
Dialogue: 0,0:13:28.20,0:13:29.40,main,,0000,0000,0000,,Nechápu.
Dialogue: 0,0:13:30.48,0:13:33.16,main,,0000,0000,0000,,Bylo to dílo vyšší síly?
Dialogue: 0,0:13:33.20,0:13:33.80,main,,0000,0000,0000,,Osobní problémy?
Dialogue: 0,0:13:33.80,0:13:34.24,main,,0000,0000,0000,,Peníze?
Dialogue: 0,0:13:34.68,0:13:37.52,main,,0000,0000,0000,,Nikdy si nekupuji drama CD…
Dialogue: 0,0:13:37.52,0:13:39.16,main,,0000,0000,0000,,…takže dokud hlasy sedí k původním postavám…
Dialogue: 0,0:13:39.16,0:13:41.32,main,,0000,0000,0000,,…nezajímá mě koho použijí.
Dialogue: 0,0:13:41.32,0:13:43.60,main,,0000,0000,0000,,Tak to nefunguje, Kagami.
Dialogue: 0,0:13:43.60,0:13:46.24,main,,0000,0000,0000,,Lidé co si na drama CD zvykli…
Dialogue: 0,0:13:46.24,0:13:49.28,main,,0000,0000,0000,,…cítí, že něco není v pořádku, když se začnou dívat na anime.
Dialogue: 0,0:13:49.28,0:13:52.20,main,,0000,0000,0000,,A chce to nějakou dobu, než si zvykneš.
Dialogue: 0,0:13:53.76,0:13:54.60,main,,0000,0000,0000,,No…
Dialogue: 0,0:13:54.60,0:13:56.64,main,,0000,0000,0000,,…jedna dobrá věc z toho ale plyne.
Dialogue: 0,0:13:56.92,0:13:57.92,main,,0000,0000,0000,,Co je to?
Dialogue: 0,0:13:57.92,0:13:59.52,main,,0000,0000,0000,,Drama CD a anime.
Dialogue: 0,0:13:59.52,0:14:02.32,main,,0000,0000,0000,,Můžeš si skrz ně vychutnat dva odlišné světy.
Dialogue: 0,0:14:02.32,0:14:03.72,main,,0000,0000,0000,,Dva za cenu jedné.
Dialogue: 0,0:14:04.28,0:14:06.12,main,,0000,0000,0000,,Ty jsi hardcore fanda, že?
Dialogue: 0,0:14:06.92,0:14:08.08,main,,0000,0000,0000,,Mockrát děkuji!
Dialogue: 0,0:14:08.44,0:14:08.92,main,,0000,0000,0000,,No…
Dialogue: 0,0:14:08.92,0:14:13.88,main,,0000,0000,0000,,Jeden z nás si musí dát Miyukin nuget…
Dialogue: 0,0:14:14.44,0:14:17.08,main,,0000,0000,0000,,Řekněme, že půjde tomu kdo vyhraje „čumící“ soutež.
Dialogue: 0,0:14:17.08,0:14:17.64,main,,0000,0000,0000,,C-
Dialogue: 0,0:14:17.64,0:14:18.92,main,,0000,0000,0000,,Jsi jako malá.
Dialogue: 0,0:14:18.92,0:14:20.16,main,,0000,0000,0000,,Čumící soutěž?
Dialogue: 0,0:14:20.16,0:14:21.32,main,,0000,0000,0000,,To zní zábavně!
Dialogue: 0,0:14:21.32,0:14:23.28,main,,0000,0000,0000,,Kagami nejspíš prohraje.
Dialogue: 0,0:14:23.28,0:14:24.12,main,,0000,0000,0000,,Řekla jsi něco?
Dialogue: 0,0:14:24.24,0:14:26.40,main,,0000,0000,0000,,Tak jo… začínáme!
Dialogue: 0,0:14:33.52,0:14:35.88,main,,0000,0000,0000,,To bylo… wow.
Dialogue: 0,0:14:37.44,0:14:40.36,main,,0000,0000,0000,,Ani nevypadáš jako člověk!
Dialogue: 0,0:14:39.80,0:14:42.56,main2,,0000,0000,0000,,Hej, to trochu přeháníte.
Dialogue: 0,0:14:42.56,0:14:44.16,main,,0000,0000,0000,,Promiňte, že jste museli čekat.
Dialogue: 0,0:14:44.16,0:14:45.72,main,,0000,0000,0000,,Na toaletách byla dlouhá fronta.
Dialogue: 0,0:14:46.40,0:14:47.56,main,,0000,0000,0000,,Stalo se něco?
Dialogue: 0,0:14:47.64,0:14:50.84,main,,0000,0000,0000,,Hej. Ukaž ten obličej Miyuki!
Dialogue: 0,0:14:50.84,0:14:52.12,main,,0000,0000,0000,,To ne.
Dialogue: 0,0:14:52.12,0:14:56.32,main,,0000,0000,0000,,To bylo neuvěřitelné. Nikdy jsem něco takového neviděla.
Dialogue: 0,0:14:56.80,0:14:57.40,main,,0000,0000,0000,,Tak fajn.
Dialogue: 0,0:15:02.56,0:15:04.20,main,,0000,0000,0000,,Je to skvělé pokaždé to vidím.
Dialogue: 0,0:15:04.80,0:15:08.64,main,,0000,0000,0000,,Tohle se ti může hodit u talentových zkoušek na příjmačkách.
Dialogue: 0,0:15:09.12,0:15:10.24,main,,0000,0000,0000,,To je ono!
Dialogue: 0,0:15:10.48,0:15:11.92,main,,0000,0000,0000,,Dostane tě to na vysokou!
Dialogue: 0,0:15:11.92,0:15:13.12,main,,0000,0000,0000,,Zaručeno!
Dialogue: 0,0:15:13.88,0:15:15.72,main,,0000,0000,0000,,Moc mě to nevzrušuje.
Dialogue: 0,0:15:16.24,0:15:20.40,main,,0000,0000,0000,,V televizi dávali ten pořad o tom kolik se toho za rok průměrně přečte…
Dialogue: 0,0:15:20.44,0:15:24.80,main,,0000,0000,0000,,…no a oni vynechali mangu a počítali jen normální knihy.
Dialogue: 0,0:15:24.80,0:15:26.12,main,,0000,0000,0000,,No, manga…
Dialogue: 0,0:15:26.12,0:15:28.08,main,,0000,0000,0000,,Počítají se light novely?
Dialogue: 0,0:15:29.04,0:15:30.28,main,,0000,0000,0000,,Nejsem si jistá.
Dialogue: 0,0:15:30.28,0:15:32.24,main,,0000,0000,0000,,Myslím, že by měli být v pořádku…
Dialogue: 0,0:15:32.80,0:15:34.16,main,,0000,0000,0000,,A co obrázkové knihy?
Dialogue: 0,0:15:36.36,0:15:38.64,main,,0000,0000,0000,,No říká se jim knihy, takže asi jo…
Dialogue: 0,0:15:39.16,0:15:44.92,main,,0000,0000,0000,,Ostatní fantazi jako Harry Potter je považovaný za normální…
Dialogue: 0,0:15:45.56,0:15:48.20,main,,0000,0000,0000,,Takže co vlastně nazveš normální knihou?
Dialogue: 0,0:15:50.72,0:15:53.04,main,,0000,0000,0000,,A ta show skončila tím, že…
Dialogue: 0,0:15:53.04,0:15:57.12,main,,0000,0000,0000,,…prý mladí lidé stále méně čtou…
Dialogue: 0,0:15:57.56,0:16:00.92,main,,0000,0000,0000,,…ale neprospívá ti vlastně…
Dialogue: 0,0:16:00.92,0:16:05.08,main,,0000,0000,0000,,…procházení webu a blogování?
Dialogue: 0,0:16:06.44,0:16:08.64,main,,0000,0000,0000,,Chápu to… a taky ne…
Dialogue: 0,0:16:09.12,0:16:09.64,main,,0000,0000,0000,,Hoj.
Dialogue: 0,0:16:09.64,0:16:10.92,main,,0000,0000,0000,,Tak vyrazíme.
Dialogue: 0,0:16:10.92,0:16:11.84,main,,0000,0000,0000,,Počkej minutku.
Dialogue: 0,0:16:12.72,0:16:13.36,main,,0000,0000,0000,,Promiň.
Dialogue: 0,0:16:13.36,0:16:15.04,main,,0000,0000,0000,,Nemůžu s vámi.
Dialogue: 0,0:16:15.80,0:16:17.36,main,,0000,0000,0000,,Máš něco na práci?
Dialogue: 0,0:16:17.36,0:16:17.88,main,,0000,0000,0000,,To je neobvyklé.
Dialogue: 0,0:16:18.96,0:16:20.80,main,,0000,0000,0000,,Začala jsem pracovat.
Dialogue: 0,0:16:23.72,0:16:24.60,main,,0000,0000,0000,,Počkat!
Dialogue: 0,0:16:24.60,0:16:26.16,main,,0000,0000,0000,,Ona má skutečně práci!
Dialogue: 0,0:16:26.16,0:16:28.04,main,,0000,0000,0000,,To je počítačový festival vážně tak důležitý?!
Dialogue: 0,0:16:30.32,0:16:31.36,main,,0000,0000,0000,,Nicméně…
Dialogue: 0,0:16:31.36,0:16:33.36,main,,0000,0000,0000,,Konata mě překvapila.
Dialogue: 0,0:16:33.36,0:16:34.68,main,,0000,0000,0000,,To teda jo.
Dialogue: 0,0:16:34.68,0:16:36.96,main,,0000,0000,0000,,Skutečně má práci.
Dialogue: 0,0:16:36.96,0:16:38.44,main,,0000,0000,0000,,Myslela jsem si, že si dělá jen srandu.
Dialogue: 0,0:16:39.44,0:16:40.96,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:16:41.68,0:16:44.84,main,,0000,0000,0000,,Ale zajímalo by mě, jak ji někdo vzal.
Dialogue: 0,0:16:44.92,0:16:46.80,main,,0000,0000,0000,,T-to je docela hrubé…
Dialogue: 0,0:16:46.88,0:16:47.60,main,,0000,0000,0000,,Krom toho…
Dialogue: 0,0:16:47.60,0:16:51.04,main,,0000,0000,0000,,Neumím si ji představit dělat manuální práci.
Dialogue: 0,0:16:51.04,0:16:52.32,main,,0000,0000,0000,,T-to je také pravda…
Dialogue: 0,0:16:52.80,0:16:54.64,main,,0000,0000,0000,,Zajímalo by mne, kde pracuje.
Dialogue: 0,0:16:55.16,0:16:56.68,main,,0000,0000,0000,,Možná, že v knihkupectví?
Dialogue: 0,0:16:56.68,0:16:57.84,main,,0000,0000,0000,,Má přece ráda mangu.
Dialogue: 0,0:16:59.24,0:16:59.96,main,,0000,0000,0000,,Promiňte.
Dialogue: 0,0:16:59.96,0:17:00.44,main,,0000,0000,0000,,Vzal bych si tohle.
Dialogue: 0,0:17:00.44,0:17:01.48,main,,0000,0000,0000,,Zadržte.
Dialogue: 0,0:17:01.48,0:17:03.28,main,,0000,0000,0000,,Mám teď moc práce.
Dialogue: 0,0:17:05.88,0:17:06.88,main,,0000,0000,0000,,Ka-ching!
Dialogue: 0,0:17:07.20,0:17:09.04,main,,0000,0000,0000,,Doufám, že ji nevyhodí…
Dialogue: 0,0:17:09.52,0:17:10.68,main,,0000,0000,0000,,Tak co třeba v samoobsluze?
Dialogue: 0,0:17:11.76,0:17:12.40,main,,0000,0000,0000,,Promiňte.
Dialogue: 0,0:17:12.40,0:17:12.96,main,,0000,0000,0000,,Vzal bych si tohle.
Dialogue: 0,0:17:14.08,0:17:16.04,main,,0000,0000,0000,,Čtyřistadevadesátosumyenů.
Dialogue: 0,0:17:17.52,0:17:19.04,main,,0000,0000,0000,,Amátetopřesně.
Dialogue: 0,0:17:20.80,0:17:21.76,main,,0000,0000,0000,,Děkujemepříjďtezase.
Dialogue: 0,0:17:22.96,0:17:24.12,main,,0000,0000,0000,,Pochybuji.
Dialogue: 0,0:17:24.12,0:17:25.36,main,,0000,0000,0000,,Tak co třeba fast food?
Dialogue: 0,0:17:25.80,0:17:27.48,main,,0000,0000,0000,,Směji se. Je s tím problém?
Dialogue: 0,0:17:27.04,0:17:28.44,main2,,0000,0000,0000,,To asi taky nee…
Dialogue: 0,0:17:28.44,0:17:30.36,main,,0000,0000,0000,,Takže rodinné restaurace asi taky padají.
Dialogue: 0,0:17:30.52,0:17:31.36,main,,0000,0000,0000,,Vítejte.
Dialogue: 0,0:17:32.08,0:17:34.16,main,,0000,0000,0000,,Zajímalo by mne, kde pracuje?
Dialogue: 0,0:17:35.40,0:17:36.92,main,,0000,0000,0000,,Dopravní průzkum?
Dialogue: 0,0:17:36.92,0:17:37.84,main,,0000,0000,0000,,To ne.
Dialogue: 0,0:17:37.84,0:17:39.12,main,,0000,0000,0000,,Videopůjčovna?
Dialogue: 0,0:17:39.12,0:17:40.28,main,,0000,0000,0000,,Tím si nejsem jistá.
Dialogue: 0,0:17:40.28,0:17:41.08,main,,0000,0000,0000,,Bar.
Dialogue: 0,0:17:41.08,0:17:42.16,main,,0000,0000,0000,,Neh.
Dialogue: 0,0:17:42.16,0:17:43.04,main,,0000,0000,0000,,Donáška pizzy?
Dialogue: 0,0:17:43.04,0:17:44.16,main,,0000,0000,0000,,Nemá řidičák.
Dialogue: 0,0:17:44.16,0:17:45.40,main,,0000,0000,0000,,Pokladní v supermarketu.
Dialogue: 0,0:17:45.40,0:17:45.88,main,,0000,0000,0000,,Nemožné.
Dialogue: 0,0:17:45.88,0:17:46.80,main,,0000,0000,0000,,Doučování?
Dialogue: 0,0:17:46.80,0:17:47.64,main,,0000,0000,0000,,Nemyslitelné.
Dialogue: 0,0:17:47.64,0:17:48.52,main,,0000,0000,0000,,Pekárna!
Dialogue: 0,0:17:49.12,0:17:49.96,main,,0000,0000,0000,,Takže zůstává…
Dialogue: 0,0:17:51.80,0:17:53.12,main,,0000,0000,0000,,Zeptáme se jí zítra.
Dialogue: 0,0:17:53.68,0:17:54.68,main,,0000,0000,0000,,Hej, Konato.
Dialogue: 0,0:17:54.72,0:17:56.48,main,,0000,0000,0000,,Kde pracuješ?
Dialogue: 0,0:17:56.96,0:17:57.84,main,,0000,0000,0000,,Řekni nám!
Dialogue: 0,0:18:00.04,0:18:01.88,main,,0000,0000,0000,,Normální costplay kavárna.
Dialogue: 0,0:18:02.68,0:18:04.44,main,,0000,0000,0000,,To k ní sedí perfektně!
Dialogue: 0,0:18:04.44,0:18:05.60,main,,0000,0000,0000,,Ale co tam dělá?
Dialogue: 0,0:18:05.60,0:18:08.64,copyrights,,0000,0000,0000,,{\pos(129,76)}Překlad od paralaXe
Dialogue: 0,0:18:05.60,0:18:08.64,copyrights,,0000,0000,0000,,{\pos(85,50)}Domovská stránka: www.otakogu.cz
Dialogue: 0,0:18:11.52,0:18:13.36,main,,0000,0000,0000,,Hodně štěstí!
Dialogue: 0,0:18:13.36,0:18:14.68,main,,0000,0000,0000,,Já jsem Akira Kogami!
Dialogue: 0,0:18:14.68,0:18:17.08,main,,0000,0000,0000,,A tohle je druhá epizoda Lucky Channelu!
Dialogue: 0,0:18:17.08,0:18:19.56,main,,0000,0000,0000,,Akira jde na to!
Dialogue: 0,0:18:20.00,0:18:20.92,main,,0000,0000,0000,,O-kaj!
Dialogue: 0,0:18:20.92,0:18:22.04,main,,0000,0000,0000,,Vedle mě…
Dialogue: 0,0:18:22.08,0:18:24.80,main,,0000,0000,0000,,…máme mého báječného asistenta!
Dialogue: 0,0:18:24.80,0:18:26.64,main,,0000,0000,0000,,Minoru Shiraishi-san!
Dialogue: 0,0:18:26.64,0:18:28.12,main,,0000,0000,0000,,Zamávejte mu!
Dialogue: 0,0:18:28.56,0:18:29.64,main,,0000,0000,0000,,Eh-eh-eh-eh-eh-
Dialogue: 0,0:18:30.00,0:18:32.76,main,,0000,0000,0000,,Já jsem Akira-samin asistent, Minoru Shiraishi.
Dialogue: 0,0:18:32.76,0:18:34.40,main,,0000,0000,0000,,Jsem teprve začátečník…
Dialogue: 0,0:18:34.40,0:18:36.80,main,,0000,0000,0000,,…takže doufám, že si zapamatujete moje jméno.
Dialogue: 0,0:18:36.80,0:18:37.24,main,,0000,0000,0000,,Ano.
Dialogue: 0,0:18:37.44,0:18:38.64,main,,0000,0000,0000,,Tlesk. Tlesk. Tlesk.
Dialogue: 0,0:18:39.88,0:18:42.24,main,,0000,0000,0000,,Jsme teprve na začátku…
Dialogue: 0,0:18:42.24,0:18:46.40,main,,0000,0000,0000,,ale už teď máme záplavu dopisů!
Dialogue: 0,0:18:46.40,0:18:48.32,main,,0000,0000,0000,,Akira je moc šťastná!
Dialogue: 0,0:18:48.32,0:18:49.00,main,,0000,0000,0000,,Díky!
Dialogue: 0,0:18:49.28,0:18:49.76,main,,0000,0000,0000,,Páni,
Dialogue: 0,0:18:49.76,0:18:51.68,main,,0000,0000,0000,,to je vážně něco.
Dialogue: 0,0:18:51.68,0:18:52.48,main,,0000,0000,0000,,Myslím, že bys tomu mohla říkat…
Dialogue: 0,0:18:52.48,0:18:54.36,main,,0000,0000,0000,,…ukazatel Akiriné popularity.
Dialogue: 0,0:18:55.84,0:18:56.76,main,,0000,0000,0000,,Co-
Dialogue: 0,0:18:56.76,0:18:57.72,main,,0000,0000,0000,,Cože?
Dialogue: 0,0:18:57.72,0:18:59.04,main,,0000,0000,0000,,Viděla jsem dnešní epizodu.
Dialogue: 0,0:18:59.04,0:18:59.68,main,,0000,0000,0000,,Ano?
Dialogue: 0,0:18:59.68,0:19:00.52,main,,0000,0000,0000,,Ukázal ses tam?
Dialogue: 0,0:19:01.00,0:19:02.56,main,,0000,0000,0000,,Ve skutečné show.
Dialogue: 0,0:19:02.56,0:19:03.84,main,,0000,0000,0000,,Byls tam, že jo?
Dialogue: 0,0:19:04.96,0:19:06.20,main,,0000,0000,0000,,Byli tak velkorysí a dali mě tam.
Dialogue: 0,0:19:08.16,0:19:09.96,main,,0000,0000,0000,,Je mi to fuk.
Dialogue: 0,0:19:09.96,0:19:11.36,main,,0000,0000,0000,,Ah, jo…
Dialogue: 0,0:19:11.36,0:19:13.40,main,,0000,0000,0000,,Kde, žes to byl?
Dialogue: 0,0:19:13.96,0:19:15.60,main,,0000,0000,0000,,No, myslím, že…
Dialogue: 0,0:19:15.60,0:19:17.28,main,,0000,0000,0000,,…scéna 45, část A.
Dialogue: 0,0:19:17.28,0:19:18.80,main,,0000,0000,0000,,Scéna ve třídě, hned po přestávce.
Dialogue: 0,0:19:20.68,0:19:21.88,main,,0000,0000,0000,,Jo už vím.
Dialogue: 0,0:19:21.88,0:19:24.36,main,,0000,0000,0000,,Moc si nebyl vidět, takže jsem zapomněla.
Dialogue: 0,0:19:25.12,0:19:27.52,main,,0000,0000,0000,,No, tentokrát jsem měl jen jeden řádek.
Dialogue: 0,0:19:27.80,0:19:28.84,main,,0000,0000,0000,,Cožeto?!
Dialogue: 0,0:19:28.84,0:19:30.04,main,,0000,0000,0000,,To ti nestačí?!
Dialogue: 0,0:19:30.04,0:19:31.24,main,,0000,0000,0000,,Ne! Tak jsem to nemyslel!
Dialogue: 0,0:19:32.12,0:19:34.48,main,,0000,0000,0000,,Nejsem si jistá, jestli by se měl nováček jako ty chovat takhle.
Dialogue: 0,0:19:35.08,0:19:37.84,main,,0000,0000,0000,,Když jsem já začínala…
Dialogue: 0,0:19:39.08,0:19:40.44,main,,0000,0000,0000,,Začínala?
Dialogue: 0,0:19:40.44,0:19:41.72,main,,0000,0000,0000,,Kolik ti bylo?
Dialogue: 0,0:19:42.40,0:19:43.40,main,,0000,0000,0000,,Tři.
Dialogue: 0,0:19:43.40,0:19:46.68,main,,0000,0000,0000,,Byla jsem tehdy ráda, že mám vůbec něco.
Dialogue: 0,0:19:46.68,0:19:48.68,main,,0000,0000,0000,,Kvantita není problém.
Dialogue: 0,0:19:48.68,0:19:49.76,main,,0000,0000,0000,,To máš naprostou pravdu.
Dialogue: 0,0:19:49.76,0:19:53.08,main,,0000,0000,0000,,Moje nervy byly nadračky, po tom co kolem mě bylo tolik veteránů.
Dialogue: 0,0:19:53.08,0:19:54.48,main,,0000,0000,0000,,Bylo to moc poučné!
Dialogue: 0,0:19:54.48,0:19:55.96,main,,0000,0000,0000,,Byl jsi pěkně ztuhlý.
Dialogue: 0,0:19:56.80,0:19:58.04,main,,0000,0000,0000,,Vážně moc.
Dialogue: 0,0:19:58.04,0:19:59.40,main,,0000,0000,0000,,Ooh, podívej! Já jsem amatér.
Dialogue: 0,0:19:59.92,0:20:01.00,main,,0000,0000,0000,,Šak víš.
Dialogue: 0,0:20:01.00,0:20:03.16,main,,0000,0000,0000,,To bylo zajímavé…
Dialogue: 0,0:20:03.16,0:20:04.92,main,,0000,0000,0000,,udělej to znovu.
Dialogue: 0,0:20:06.12,0:20:07.12,main,,0000,0000,0000,,Tady?
Dialogue: 0,0:20:07.12,0:20:09.72,main,,0000,0000,0000,,Je tu šance, že si zahraješ zase, co?
Dialogue: 0,0:20:10.36,0:20:10.96,main,,0000,0000,0000,,No, myslím…
Dialogue: 0,0:20:10.96,0:20:13.24,main,,0000,0000,0000,,Tak bys měl raději začít cvičit.
Dialogue: 0,0:20:13.24,0:20:14.80,main,,0000,0000,0000,,Nabízím ti tady pomoc!
Dialogue: 0,0:20:15.48,0:20:16.28,main,,0000,0000,0000,,Ano, madam!
Dialogue: 0,0:20:16.32,0:20:17.92,main,,0000,0000,0000,,Prosím učte mě!
Dialogue: 0,0:20:17.92,0:20:18.80,main,,0000,0000,0000,,Dej mi ty skripta.
Dialogue: 0,0:20:20.12,0:20:21.72,main,,0000,0000,0000,,Jaká, že to byla scéna?
Dialogue: 0,0:20:21.72,0:20:22.92,main,,0000,0000,0000,,Scéna 45.
Dialogue: 0,0:20:24.40,0:20:24.88,main,,0000,0000,0000,,Tahle?
Dialogue: 0,0:20:25.20,0:20:27.44,main,,0000,0000,0000,,Tak já budu slečna Kuroi.
Dialogue: 0,0:20:27.44,0:20:28.40,main,,0000,0000,0000,,Ano, prosím.
Dialogue: 0,0:20:28.40,0:20:29.56,main,,0000,0000,0000,,Připravit a…
Dialogue: 0,0:20:30.04,0:20:30.60,main,,0000,0000,0000,,Akce!
Dialogue: 0,0:20:31.40,0:20:32.24,main,,0000,0000,0000,,Ahoj.
Dialogue: 0,0:20:32.24,0:20:34.64,main,,0000,0000,0000,,Užili jste si prázdnin?
Dialogue: 0,0:20:34.64,0:20:35.68,main,,0000,0000,0000,,Tak jo…
Dialogue: 0,0:20:35.68,0:20:38.20,main,,0000,0000,0000,,Měli jste s-s-
Dialogue: 0,0:20:37.44,0:20:38.20,main2,,0000,0000,0000,,Spoustu.
Dialogue: 0,0:20:38.20,0:20:39.32,main,,0000,0000,0000,,Spoustu domácích úkolů…
Dialogue: 0,0:20:39.32,0:20:41.52,main,,0000,0000,0000,,…a test hned po prázdninách…
Dialogue: 0,0:20:41.52,0:20:44.88,main,,0000,0000,0000,,…takže jste si nejspíš moc srandy neužili.
Dialogue: 0,0:20:44.88,0:20:47.28,main,,0000,0000,0000,,Sotva se to podobalo prázdninám.
Dialogue: 0,0:20:50.52,0:20:51.04,main,,0000,0000,0000,,Co to bylo?
Dialogue: 0,0:20:52.20,0:20:53.20,main,,0000,0000,0000,,Špatné.
Dialogue: 0,0:20:53.20,0:20:55.64,main,,0000,0000,0000,,Vůbec to nebylo procítěné.
Dialogue: 0,0:20:55.64,0:20:56.12,main,,0000,0000,0000,,Cítit-
Dialogue: 0,0:20:56.12,0:20:56.72,main,,0000,0000,0000,,Tady!
Dialogue: 0,0:20:56.72,0:20:57.16,main,,0000,0000,0000,,Právě tady!
Dialogue: 0,0:20:57.16,0:20:59.04,main,,0000,0000,0000,,Hned teď to procítíš!
Dialogue: 0,0:20:59.04,0:20:59.80,main,,0000,0000,0000,,Omlouvám se.
Dialogue: 0,0:20:59.80,0:21:02.48,main,,0000,0000,0000,,Prostě jen číst řádky nestačí… i opice to dokáže.
Dialogue: 0,0:21:02.92,0:21:04.44,main,,0000,0000,0000,,Opice neumí…
Dialogue: 0,0:21:08.12,0:21:10.20,main,,0000,0000,0000,,Je čas se rozloučit!
Dialogue: 0,0:21:10.20,0:21:11.64,main,,0000,0000,0000,,Ten čas ale letí…
Dialogue: 0,0:21:11.64,0:21:14.00,main,,0000,0000,0000,,…když se dobře bavíte, že?
Dialogue: 0,0:21:14.00,0:21:15.00,main,,0000,0000,0000,,Takže…
Dialogue: 0,0:21:15.00,0:21:20.20,main,,0000,0000,0000,,Posílejte nám vaše komentáře a návrhy!
Dialogue: 0,0:21:20.52,0:21:22.40,main,,0000,0000,0000,,Uvidíme se příště.
Dialogue: 0,0:21:22.44,0:21:24.40,main,,0000,0000,0000,,Ahoj-ahoj!
Dialogue: 0,0:21:25.40,0:21:26.20,main,,0000,0000,0000,,Tak jo.
Dialogue: 0,0:21:26.20,0:21:27.80,main,,0000,0000,0000,,Jaké to bylo?
Dialogue: 0,0:21:27.80,0:21:29.28,main,,0000,0000,0000,,Co myslíš?
Dialogue: 0,0:21:29.28,0:21:30.80,main,,0000,0000,0000,,Šak víš…
Dialogue: 0,0:21:30.80,0:21:32.36,main,,0000,0000,0000,,Vypadalo to, že to někoho omráčilo?
Dialogue: 0,0:21:32.36,0:21:34.36,main,,0000,0000,0000,,Neperou se spolu navzájem?
Dialogue: 0,0:21:34.36,0:21:35.16,main,,0000,0000,0000,,Ne…
Dialogue: 0,0:21:35.16,0:21:36.84,main,,0000,0000,0000,,Tak mi to nepřišlo…
Dialogue: 0,0:21:36.84,0:21:38.48,main,,0000,0000,0000,,Tak bys měl…
Dialogue: 0,0:21:40.92,0:21:42.72,main,,0000,0000,0000,,Jsi úžasná, Kono-chan!
Dialogue: 0,0:21:42.72,0:21:44.16,main,,0000,0000,0000,,Vážně si se do toho vložila.
Dialogue: 0,0:21:44.16,0:21:45.28,main,,0000,0000,0000,,Hej, Konato!
Dialogue: 0,0:21:45.28,0:21:47.24,main,,0000,0000,0000,,Jsou ostatní dvě písně takovéto?
Dialogue: 0,0:21:48.04,0:21:50.24,main,,0000,0000,0000,,Je tu něco z anime a z SFX.
Dialogue: 0,0:21:50.24,0:21:52.08,main,,0000,0000,0000,,To není co jsem myslela!
Dialogue: 0,0:21:52.08,0:21:54.32,main,,0000,0000,0000,,Zazpívej něco co zná i zbytek!
Dialogue: 0,0:21:55.04,0:21:57.04,main,,0000,0000,0000,,Můžu zpívat co chci.
Dialogue: 0,0:21:57.04,0:21:57.68,main,,0000,0000,0000,,Tak se podíváme…
Dialogue: 0,0:21:57.68,0:21:59.04,main,,0000,0000,0000,,Tady je ovladač.
Dialogue: 0,0:21:59.04,0:22:00.12,main,,0000,0000,0000,,No tak, Tsukaso.
Dialogue: 0,0:22:00.12,0:22:01.08,main,,0000,0000,0000,,Pokud si neobjednáš píseň…
Dialogue: 0,0:22:01.08,0:22:04.56,main,,0000,0000,0000,,…budeme pořád poslouchat to její skřehotání a písničky z anime.
Dialogue: 0,0:22:04.64,0:22:06.56,main,,0000,0000,0000,,Mě to nevadí.
Dialogue: 0,0:22:06.56,0:22:08.48,main,,0000,0000,0000,,Je to vlastně docela sranda.
Dialogue: 0,0:22:08.48,0:22:10.24,main,,0000,0000,0000,,I ty, Miyuki?!
Dialogue: 0,0:22:10.68,0:22:11.68,main,,0000,0000,0000,,Promiňte jestli přes mě někdo nevidí.
Dialogue: 0,0:22:11.68,0:22:12.88,main,,0000,0000,0000,,Tentokrát budu stát.
Dialogue: 0,0:22:12.88,0:22:14.56,main,,0000,0000,0000,,Co teď?
Dialogue: 0,0:22:14.56,0:22:15.72,main,,0000,0000,0000,,Pokud se při zpěvu nepostavíš…
Dialogue: 0,0:22:15.72,0:22:17.96,main,,0000,0000,0000,,…tvůj hlas nemůže plně zazářit.
Dialogue: 0,0:22:18.04,0:22:19.92,main,,0000,0000,0000,,To budeš zpívat ještě hlasitěji?!
Dialogue: 0,0:22:22.32,0:22:23.32,main,,0000,0000,0000,,Skutečná píseň ze show!
Dialogue: 0,0:22:23.92,0:22:25.12,main,,0000,0000,0000,,To je docela dobrý!
Dialogue: 0,0:22:25.12,0:22:27.88,main,,0000,0000,0000,,Vypadá to, že to je ještě z doby než jsme se narodili.
Dialogue: 0,0:22:28.56,0:22:29.44,mainup2,,0000,0000,0000,,
Dialogue: 0,0:22:30.20,0:22:31.12,mainup2,,0000,0000,0000,,
