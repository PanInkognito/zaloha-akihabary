Dialogue: 0,0:00:00.00,0:00:05.72,whitebig,,0000,0000,0000,,{\pos(225,126)}
Dialogue: 0,0:00:00.00,0:00:05.72,whitebig,,0000,0000,0000,,{\pos(226,152)}
Dialogue: 0,0:00:00.88,0:00:02.36,main,,0000,0000,0000,,Tak začneme.
Dialogue: 0,0:00:02.36,0:00:03.40,main,,0000,0000,0000,,Jdeme na to.
Dialogue: 0,0:00:03.96,0:00:05.76,main,,0000,0000,0000,,No tak už začněme!
Dialogue: 0,0:00:06.12,0:00:07.28,op1,,0000,0000,0000,,Okolo 3 cm!
Dialogue: 0,0:00:07.28,0:00:08.56,op1,,0000,0000,0000,,Je to snad málo?
Dialogue: 0,0:00:08.56,0:00:08.80,op1,,0000,0000,0000,,Jejda!
Dialogue: 0,0:00:08.80,0:00:10.64,op1,,0000,0000,0000,,Schované pod naší uniformou!
Dialogue: 0,0:00:10.64,0:00:11.68,op1,,0000,0000,0000,,Mám toho nechat?
Dialogue: 0,0:00:11.68,0:00:12.00,op1,,0000,0000,0000,,JEJ!
Dialogue: 0,0:00:12.00,0:00:13.08,op1,,0000,0000,0000,,Jít do toho?
Dialogue: 0,0:00:13.08,0:00:13.88,op1,,0000,0000,0000,,Snažit se?
Dialogue: 0,0:00:13.88,0:00:15.44,op1,,0000,0000,0000,,Vychutnat si každý okamžik. Jo!
Dialogue: 0,0:00:15.44,0:00:16.04,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.04,0:00:16.28,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:16.28,0:00:16.68,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.68,0:00:17.04,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:17.04,0:00:17.64,op1,,0000,0000,0000,,Jen mezi námi dvěma!
Dialogue: 0,0:00:17.64,0:00:20.36,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:00:22.36,0:00:23.16,logo,,0000,0000,0000,,{\move(108,218,202,218,0,0)}LUCKY
Dialogue: 0,0:00:23.16,0:00:24.88,logo,,0000,0000,0000,,{\pos(202,218)}LUCKY
Dialogue: 0,0:00:22.36,0:00:23.16,logo,,0000,0000,0000,,{\move(638,218,459,218,0,0)}STAR
Dialogue: 0,0:00:23.16,0:00:24.88,logo,,0000,0000,0000,,{\pos(459,218)}STAR
Dialogue: 0,0:00:25.28,0:00:25.88,op1,,0000,0000,0000,,Cítím se nějak nesvá.
Dialogue: 0,0:00:25.88,0:00:26.48,op1,,0000,0000,0000,,Co jsem to jedla?
Dialogue: 0,0:00:26.48,0:00:27.08,op1,,0000,0000,0000,,Mám tě hrozně ráda!
Dialogue: 0,0:00:27.08,0:00:27.28,op1,,0000,0000,0000,,Oops!
Dialogue: 0,0:00:27.28,0:00:28.32,op1,,0000,0000,0000,,Jedno z nich je špatně!
Dialogue: 0,0:00:28.32,0:00:29.08,op1,,0000,0000,0000,,Relaxovat!
Dialogue: 0,0:00:29.08,0:00:29.72,op1,,0000,0000,0000,,Nic nedělat!
Dialogue: 0,0:00:29.72,0:00:30.24,op1,,0000,0000,0000,,Flákat se!
Dialogue: 0,0:00:30.24,0:00:31.32,op1,,0000,0000,0000,,Už toho nechej!
Dialogue: 0,0:00:31.32,0:00:33.00,op1,,0000,0000,0000,,Je ta nová holka oblíbená?
Dialogue: 0,0:00:33.00,0:00:34.64,op1,,0000,0000,0000,,Já bych řekla, že je docela normální!
Dialogue: 0,0:00:34.64,0:00:36.12,op1,,0000,0000,0000,,To jsem jediná překvapená?
Dialogue: 0,0:00:36.12,0:00:36.44,op1,,0000,0000,0000,,Vepřové kousky!
Dialogue: 0,0:00:36.44,0:00:36.84,op1,,0000,0000,0000,,Skleněné nudle!
Dialogue: 0,0:00:36.84,0:00:37.16,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:00:37.16,0:00:37.96,op2,,0000,0000,0000,,DA-DA-DA-DA-DA!
Dialogue: 0,0:00:38.52,0:00:39.32,op1,,0000,0000,0000,,Ooh?
Dialogue: 0,0:00:39.32,0:00:40.08,op1,,0000,0000,0000,,Víš co tím myslím?
Dialogue: 0,0:00:39.68,0:00:40.52,op1up,,0000,0000,0000,,Moje brýle…
Dialogue: 0,0:00:40.52,0:00:41.60,op1up,,0000,0000,0000,,Kde mám brýle?
Dialogue: 0,0:00:41.68,0:00:43.64,op1,,0000,0000,0000,,Nedávno jsme hrála tu samou bojovku.
Dialogue: 0,0:00:42.28,0:00:43.64,op1up,,0000,0000,0000,,Bez nich nic nevidím…
Dialogue: 0,0:00:44.68,0:00:45.28,op2,,0000,0000,0000,,POM! POM!
Dialogue: 0,0:00:45.28,0:00:46.32,op1,,0000,0000,0000,,Roztleskávačky!
Dialogue: 0,0:00:46.32,0:00:47.84,op2,,0000,0000,0000,,DÁME SI SLADKOST!
Dialogue: 0,0:00:47.84,0:00:48.44,op2,,0000,0000,0000,,LA! LA!
Dialogue: 0,0:00:48.44,0:00:49.48,op1,,0000,0000,0000,,Párty začíná!
Dialogue: 0,0:00:49.48,0:00:51.08,op2,,0000,0000,0000,,MÁŠ TO V KRVI!
Dialogue: 0,0:00:51.08,0:00:51.64,op2,,0000,0000,0000,,ANO!
Dialogue: 0,0:00:51.64,0:00:52.64,op1,,0000,0000,0000,,Zanechává to dojem.
Dialogue: 0,0:00:52.64,0:00:54.12,op1,,0000,0000,0000,,Malé planetky.
Dialogue: 0,0:00:54.12,0:00:54.64,op1,,0000,0000,0000,,Narazili!
Dialogue: 0,0:00:54.64,0:00:55.28,op1,,0000,0000,0000,,A shořeli!
Dialogue: 0,0:00:55.28,0:00:55.64,op1,,0000,0000,0000,,Bylo to ohromné!
Dialogue: 0,0:00:55.64,0:00:56.40,op1,,0000,0000,0000,,Z plných plic křičíme!
Dialogue: 0,0:00:56.40,0:00:56.96,op1,,0000,0000,0000,,ZPÍVEJ A TANCUJ!
Dialogue: 0,0:00:57.32,0:00:58.60,op1,,0000,0000,0000,,Přiznej si to!
Dialogue: 0,0:00:58.60,0:01:03.08,op1,,0000,0000,0000,,Nakonec se stejně budeme smát!
Dialogue: 0,0:01:03.08,0:01:06.84,op1,,0000,0000,0000,,Protože mám na sobě námořnickou uniformu.
Dialogue: 0,0:01:06.84,0:01:09.72,op2,,0000,0000,0000,,To je ono!
Dialogue: 0,0:01:10.28,0:01:13.84,op1,,0000,0000,0000,,Je teprve pondělí a já už mám špatnou náladu.
Dialogue: 0,0:01:13.84,0:01:15.64,op1,,0000,0000,0000,,Co mám dělat?
Dialogue: 0,0:01:15.84,0:01:19.68,op1,,0000,0000,0000,,Raději si vezmu moji letní uniformu -
Dialogue: 0,0:01:19.68,0:01:22.56,op1,,0000,0000,0000,,- je rozkošná!
Dialogue: 0,0:01:22.80,0:01:23.96,op1,,0000,0000,0000,,Trojdílná uniforma!
Dialogue: 0,0:01:23.96,0:01:25.40,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:01:25.40,0:01:25.84,op1,,0000,0000,0000,,JO!
Dialogue: 0,0:01:25.84,0:01:26.28,op1,,0000,0000,0000,,Dej se do toho!
Dialogue: 0,0:01:26.28,0:01:26.64,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.64,0:01:27.28,op1,,0000,0000,0000,,SNAŽ SE!
Dialogue: 0,0:01:27.28,0:01:27.44,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:27.44,0:01:29.32,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:01:31.84,0:01:32.96,main,,0000,0000,0000,,Připravit!
Dialogue: 0,0:01:33.52,0:01:34.56,main,,0000,0000,0000,,Pozor!
Dialogue: 0,0:01:44.40,0:01:45.64,main,,0000,0000,0000,,Wow, zase jsi to zvládla.
Dialogue: 0,0:01:45.64,0:01:46.76,main,,0000,0000,0000,,To bylo úžasné.
Dialogue: 0,0:01:47.56,0:01:49.32,main,,0000,0000,0000,,Jak to, že nejsi v žádné sportovním klubu,
Dialogue: 0,0:01:49.32,0:01:51.96,main,,0000,0000,0000,,když jsi tak dobrá?
Dialogue: 0,0:01:51.96,0:01:53.96,main,,0000,0000,0000,,Protože, kdybych se do nějakého přidala…
Dialogue: 0,0:01:54.60,0:01:57.24,main,,0000,0000,0000,,…nestíhala bych vysílání anime.
Dialogue: 0,0:01:57.36,0:02:00.28,white,,0000,0000,0000,,{\frz-90\pos(283,142)}Rychlá
Dialogue: 0,0:01:57.36,0:02:00.28,white,,0000,0000,0000,,{\frz-90\pos(380,222)}holka
Dialogue: 0,0:02:07.20,0:02:08.36,main,,0000,0000,0000,,Ty, Tsukaso.
Dialogue: 0,0:02:08.36,0:02:10.32,main,,0000,0000,0000,,Z jakého konce jíš čokoládový kornout?
Dialogue: 0,0:02:12.16,0:02:13.32,main,,0000,0000,0000,,Myslím, že od hlavy.
Dialogue: 0,0:02:14.04,0:02:14.80,main,,0000,0000,0000,,Chápu.
Dialogue: 0,0:02:15.96,0:02:16.96,main,,0000,0000,0000,,Mimochodem…
Dialogue: 0,0:02:16.96,0:02:18.16,main,,0000,0000,0000,,…která strana je hlava?
Dialogue: 0,0:02:18.16,0:02:19.48,main,,0000,0000,0000,,Tenká nebo tlustá strana?
Dialogue: 0,0:02:20.92,0:02:24.60,main,,0000,0000,0000,,Myslím, že ta tenká.
Dialogue: 0,0:02:24.60,0:02:25.64,main,,0000,0000,0000,,Aha.
Dialogue: 0,0:02:25.64,0:02:28.28,main,,0000,0000,0000,,Já myslela, že je to ta tlustá.
Dialogue: 0,0:02:29.20,0:02:31.28,main,,0000,0000,0000,,Proč bys řekla, že je to ta tenká?
Dialogue: 0,0:02:31.28,0:02:32.88,main,,0000,0000,0000,,No, nepřipomíná ti ulitu?
Dialogue: 0,0:02:33.60,0:02:35.80,main,,0000,0000,0000,,Proč sis ty myslela, že je to ta tlustá?
Dialogue: 0,0:02:35.80,0:02:36.76,main,,0000,0000,0000,,No, nepřipomíná ti…
Dialogue: 0,0:02:36.76,0:02:37.96,main,,0000,0000,0000,,…housenku?
Dialogue: 0,0:02:38.92,0:02:39.76,main,,0000,0000,0000,,Housenku?
Dialogue: 0,0:02:39.76,0:02:43.28,main,,0000,0000,0000,,Ale asi je lepší si představit ulitu.
Dialogue: 0,0:02:59.24,0:03:00.12,main,,0000,0000,0000,,U-Um…
Dialogue: 0,0:03:01.24,0:03:02.96,main,,0000,0000,0000,,Mohla bys odloupnout tenký konec…
Dialogue: 0,0:03:02.96,0:03:05.92,main,,0000,0000,0000,,…a sníst ho s přebytečnou čokoládou.
Dialogue: 0,0:03:06.92,0:03:08.32,main,,0000,0000,0000,,Už to chápu.
Dialogue: 0,0:03:11.92,0:03:13.48,main,,0000,0000,0000,,Dobrý postřeh, Miyuki-san.
Dialogue: 0,0:03:13.48,0:03:14.76,main,,0000,0000,0000,,Jsi tak chytrá.
Dialogue: 0,0:03:14.76,0:03:15.56,main,,0000,0000,0000,,Ne, tak zase ne.
Dialogue: 0,0:03:15.56,0:03:17.88,main,,0000,0000,0000,,Každý má svůj vlastní styl.
Dialogue: 0,0:03:18.28,0:03:20.52,main,,0000,0000,0000,,Jak jíš větrník?
Dialogue: 0,0:03:20.92,0:03:22.68,main,,0000,0000,0000,,Větrníky?
Dialogue: 0,0:03:22.72,0:03:23.56,main,,0000,0000,0000,,No…
Dialogue: 0,0:03:23.80,0:03:26.20,main,,0000,0000,0000,,První oddělím spodní a horní poloviny od sebe…
Dialogue: 0,0:03:26.20,0:03:29.44,main,,0000,0000,0000,,…a pak sním vršek, který si namáčím do krému vespodu.
Dialogue: 0,0:03:29.44,0:03:33.20,main,,0000,0000,0000,,A potom sním spodní polovinu.
Dialogue: 0,0:03:33.20,0:03:36.48,main,,0000,0000,0000,,Takto se nemusím obávat, že mi spadne nějaký krém…
Dialogue: 0,0:03:36.48,0:03:39.60,main,,0000,0000,0000,,…a navíc mám v rovnováze množství větrníku a krému.
Dialogue: 0,0:03:41.48,0:03:45.12,main,,0000,0000,0000,,A co ty, co mají uvnitř krém i puding?
Dialogue: 0,0:03:45.12,0:03:46.88,main,,0000,0000,0000,,Jak jíš je?
Dialogue: 0,0:03:46.88,0:03:47.88,main,,0000,0000,0000,,Smícháš je dohromady?
Dialogue: 0,0:03:47.88,0:03:48.48,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:03:48.52,0:03:50.36,main,,0000,0000,0000,,To bych neudělala.
Dialogue: 0,0:03:51.16,0:03:53.08,main,,0000,0000,0000,,Ale, jak bys je chtěla zamíchat?
Dialogue: 0,0:03:53.64,0:03:54.88,main,,0000,0000,0000,,Jen se ptám.
Dialogue: 0,0:03:54.96,0:03:57.04,main,,0000,0000,0000,,Kdy jíš jahody na dortu…
Dialogue: 0,0:03:57.04,0:03:59.52,main,,0000,0000,0000,,…kaštan na vrcholu Mont Blancu?
Dialogue: 0,0:03:59.52,0:04:00.96,main,,0000,0000,0000,,Takže…
Dialogue: 0,0:04:00.96,0:04:05.68,main,,0000,0000,0000,,Jsou tu také jahody mezi jednotlivými vrstvami.
Dialogue: 0,0:04:05.68,0:04:10.92,main,,0000,0000,0000,,Snažím se dodržovat rovnoměrné rozdělení krému, jahod a piškotu.
Dialogue: 0,0:04:11.68,0:04:15.64,main,,0000,0000,0000,,Takže nemám vyhraněný čas na jahody.
Dialogue: 0,0:04:16.40,0:04:17.56,main,,0000,0000,0000,,Ty se vážně vyznáš.
Dialogue: 0,0:04:18.24,0:04:21.76,main,,0000,0000,0000,,Já si vždy nechám jahodu až na konec.
Dialogue: 0,0:04:21.76,0:04:24.24,main,,0000,0000,0000,,Ale někdy jsem tak plná…
Dialogue: 0,0:04:24.24,0:04:25.80,main,,0000,0000,0000,,…že ji ani nesním…
Dialogue: 0,0:04:25.80,0:04:27.76,main,,0000,0000,0000,,…nebo si ji dá někdo z rodiny.
Dialogue: 0,0:04:27.76,0:04:30.88,main,,0000,0000,0000,,Vážně bys měla sníst to nejlepší už na začátku.
Dialogue: 0,0:04:30.88,0:04:34.92,main,,0000,0000,0000,,Když tak o tom přemýšlím, tak jsem nikdy neviděla, že by si někdo vzal jahodu od mé sestry.
Dialogue: 0,0:04:34.92,0:04:36.00,main,,0000,0000,0000,,To je celá Kagami.
Dialogue: 0,0:04:36.00,0:04:37.08,main,,0000,0000,0000,,Pořád obezřetná.
Dialogue: 0,0:04:37.08,0:04:41.52,main,,0000,0000,0000,,Dokážeš sníst poslední kousek nanuku, aniž by ti spadl?
Dialogue: 0,0:04:41.52,0:04:42.60,main,,0000,0000,0000,,Spadl?
Dialogue: 0,0:04:42.96,0:04:44.52,main,,0000,0000,0000,,Pokud nejsi pozorná…
Dialogue: 0,0:04:46.44,0:04:47.76,main,,0000,0000,0000,,…tak ti poslední kousek spadne, že?
Dialogue: 0,0:04:48.92,0:04:50.36,main,,0000,0000,0000,,Jak to děláš ty, Kona-chan?
Dialogue: 0,0:04:50.36,0:04:51.56,main,,0000,0000,0000,,Slížu ho do posledního kousku.
Dialogue: 0,0:04:52.88,0:04:55.80,main,,0000,0000,0000,,Mohla bys s ním otáčet.
Dialogue: 0,0:04:56.16,0:04:57.36,main,,0000,0000,0000,,To je dobrý nápad.
Dialogue: 0,0:04:57.36,0:05:00.36,main,,0000,0000,0000,,Já nemám zrovna v oblibě zmrzlinové kornouty.
Dialogue: 0,0:05:01.00,0:05:02.04,main,,0000,0000,0000,,Proč?
Dialogue: 0,0:05:02.92,0:05:05.52,main,,0000,0000,0000,,Nesnesu, když mi zůstane pouze kornout.
Dialogue: 0,0:05:05.92,0:05:06.96,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:05:06.96,0:05:09.08,main,,0000,0000,0000,,Vždy ti nakonec nějaký zůstane.
Dialogue: 0,0:05:09.08,0:05:13.76,main,,0000,0000,0000,,Já tlačím zmrzlinu dolů, když ho jím.
Dialogue: 0,0:05:14.68,0:05:16.76,main,,0000,0000,0000,,Čím ji tlačíš?
Dialogue: 0,0:05:16.76,0:05:18.80,main,,0000,0000,0000,,Trochu ji tam zašťouchám.
Dialogue: 0,0:05:19.52,0:05:20.92,main,,0000,0000,0000,,Zašťoucháš?
Dialogue: 0,0:05:21.60,0:05:23.84,main,,0000,0000,0000,,Yuki-chan, ty si potrpíš na vyváženost, je to tak?
Dialogue: 0,0:05:23.84,0:05:25.00,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:05:25.00,0:05:27.20,main,,0000,0000,0000,,Tak co děláš s rýží s kari?
Dialogue: 0,0:05:27.20,0:05:28.72,main,,0000,0000,0000,,Smícháš omáčku s rýží?
Dialogue: 0,0:05:29.20,0:05:29.88,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:05:29.88,0:05:31.64,main,,0000,0000,0000,,Nesmíchávám je…
Dialogue: 0,0:05:31.64,0:05:33.28,main,,0000,0000,0000,,…ale dám si záležet, aby byli dobře vyvážené.
Dialogue: 0,0:05:33.64,0:05:35.12,main,,0000,0000,0000,,Já je míchám.
Dialogue: 0,0:05:35.32,0:05:36.36,main,,0000,0000,0000,,Co ty, Kono-chan?
Dialogue: 0,0:05:36.36,0:05:37.76,main,,0000,0000,0000,,To záleží na kari.
Dialogue: 0,0:05:37.76,0:05:40.32,main,,0000,0000,0000,,Pokud je na tom nějaká worcestrová omáčka nebo syrové vejce…
Dialogue: 0,0:05:40.32,0:05:41.48,main,,0000,0000,0000,,…tak je promíchám.
Dialogue: 0,0:05:41.88,0:05:43.08,main,,0000,0000,0000,,Worcestrová omáčka na kari?
Dialogue: 0,0:05:43.56,0:05:44.56,main,,0000,0000,0000,,Je to dobrý.
Dialogue: 0,0:05:44.56,0:05:46.80,main,,0000,0000,0000,,Dáváš ji i na smažená vejce?
Dialogue: 0,0:05:47.44,0:05:49.08,main,,0000,0000,0000,,Na ty dávám sojovou omáčku.
Dialogue: 0,0:05:50.00,0:05:51.92,main,,0000,0000,0000,,To já používám majonézu.
Dialogue: 0,0:05:51.92,0:05:53.32,main,,0000,0000,0000,,Na smažená vejce?
Dialogue: 0,0:05:53.32,0:05:54.52,main,,0000,0000,0000,,Nemyslím vařená.
Dialogue: 0,0:05:55.60,0:05:56.80,main,,0000,0000,0000,,Majonéza…
Dialogue: 0,0:05:56.80,0:05:58.68,main,,0000,0000,0000,,…se hodí k většině jídel z vajec.
Dialogue: 0,0:05:58.68,0:05:59.76,main,,0000,0000,0000,,Dáváš si ji na kari?
Dialogue: 0,0:06:01.40,0:06:03.44,main,,0000,0000,0000,,Musí to, ale být bílá rýže.
Dialogue: 0,0:06:03.44,0:06:06.40,main,,0000,0000,0000,,Ale ta sladkost se hodí i k hovězím miskám.
Dialogue: 0,0:06:06.40,0:06:07.48,main,,0000,0000,0000,,Hodí se i k Oyako-don.
Dialogue: 0,0:06:08.00,0:06:10.48,main,,0000,0000,0000,,Víš jako jsou vejce a kuřecí maso jako děti a rodiče.
Dialogue: 0,0:06:10.72,0:06:13.92,main,,0000,0000,0000,,Takže, když přidám majonézu, která je také z vajec…
Dialogue: 0,0:06:14.16,0:06:16.40,main,,0000,0000,0000,,…říkám tomu vylepšené Oyako-don.
Dialogue: 0,0:06:18.04,0:06:18.84,main,,0000,0000,0000,,A taky…
Dialogue: 0,0:06:18.84,0:06:20.44,main,,0000,0000,0000,,…normálně nesnesu natto…
Dialogue: 0,0:06:20.44,0:06:22.52,main,,0000,0000,0000,,…ale když k tomu přidám majonézu, je to hned lepší.
Dialogue: 0,0:06:23.04,0:06:24.00,main,,0000,0000,0000,,To si nedokážu představit.
Dialogue: 0,0:06:24.64,0:06:25.40,main,,0000,0000,0000,,Ale…
Dialogue: 0,0:06:25.40,0:06:29.56,main,,0000,0000,0000,,…mojonéza na vejcí na hniličku může být nejlepší.
Dialogue: 0,0:06:29.56,0:06:32.08,main,,0000,0000,0000,,Smažená vejce jsou u nás taky tekutá.
Dialogue: 0,0:06:32.92,0:06:35.12,main,,0000,0000,0000,,Můj otec je tak má rád.
Dialogue: 0,0:06:35.12,0:06:38.68,main,,0000,0000,0000,,Rád z nich vycucne žloutek.
Dialogue: 0,0:06:38.68,0:06:42.00,main,,0000,0000,0000,,A když jsou moc uvařená, tak to nejde.
Dialogue: 0,0:06:42.60,0:06:44.48,main,,0000,0000,0000,,A co třeba kuře na špízu?
Dialogue: 0,0:06:44.48,0:06:45.64,main,,0000,0000,0000,,Co je s ním?
Dialogue: 0,0:06:45.64,0:06:47.28,main,,0000,0000,0000,,Necháváte ho na něm?
Dialogue: 0,0:06:47.28,0:06:49.48,main,,0000,0000,0000,,Nebo použijete hůlky a sundáte ho?
Dialogue: 0,0:06:49.48,0:06:50.92,main,,0000,0000,0000,,Hm…
Dialogue: 0,0:06:50.92,0:06:54.08,main,,0000,0000,0000,,Když ho jím sama, tak ho nechám na špízu.
Dialogue: 0,0:06:54.08,0:06:56.60,main,,0000,0000,0000,,Ale když jsem s lidmi, tak ho sundám.
Dialogue: 0,0:06:56.60,0:06:57.72,main,,0000,0000,0000,,Jo, jo.
Dialogue: 0,0:06:57.72,0:07:00.52,main,,0000,0000,0000,,Je to trochu nešikovné, když zůstane jen jedno.
Dialogue: 0,0:07:01.28,0:07:03.88,main,,0000,0000,0000,,Nikdy se nemůžu přimět si ho dát.
Dialogue: 0,0:07:03.88,0:07:06.32,main,,0000,0000,0000,,A jak ubíhá čas a nikdo ho nechce sníst…
Dialogue: 0,0:07:07.76,0:07:09.20,main,,0000,0000,0000,,…začne osychat…
Dialogue: 0,0:07:09.20,0:07:10.72,main,,0000,0000,0000,,…a nevypadá vůbec dobře…
Dialogue: 0,0:07:10.72,0:07:12.80,main,,0000,0000,0000,,…jenže nikdo si ho nechce dát…
Dialogue: 0,0:07:12.80,0:07:13.72,main,,0000,0000,0000,,…ale když přijde číšník tak řekneš…
Dialogue: 0,0:07:13.72,0:07:15.60,main,,0000,0000,0000,,„Moment, ještě jsem nedojedli.“
Dialogue: 0,0:07:15.60,0:07:16.88,main,,0000,0000,0000,,…jaksi ze zvyku.
Dialogue: 0,0:07:17.52,0:07:20.24,main,,0000,0000,0000,,Ale nakonec ho nikdo nesní.
Dialogue: 0,0:07:20.52,0:07:21.84,main,,0000,0000,0000,,Takové plýtvání.
Dialogue: 0,0:07:21.84,0:07:23.44,main,,0000,0000,0000,,Je mi ho trochu líto.
Dialogue: 0,0:07:24.76,0:07:26.00,main,,0000,0000,0000,,A když se griluje…
Dialogue: 0,0:07:26.00,0:07:29.68,main,,0000,0000,0000,,…tak je mi líto masa, které zůstane připečené ke grilu.
Dialogue: 0,0:07:29.68,0:07:33.20,main,,0000,0000,0000,,Měla bys jíst při grilovaní a bavení se…
Dialogue: 0,0:07:33.20,0:07:35.00,main,,0000,0000,0000,,…takže je toho dost.
Dialogue: 0,0:07:35.00,0:07:38.76,main,,0000,0000,0000,,Je to ještě horší s hovězími jazyky, ty jsou hotové skoro hned.
Dialogue: 0,0:07:38.76,0:07:40.12,main,,0000,0000,0000,,To máš pravdu.
Dialogue: 0,0:07:40.12,0:07:43.56,main,,0000,0000,0000,,Jsou hotové už po jednom otočení.
Dialogue: 0,0:07:43.56,0:07:44.60,main,,0000,0000,0000,,Otočení?
Dialogue: 0,0:07:45.56,0:07:46.36,main,,0000,0000,0000,,Hovězí jazyky?
Dialogue: 0,0:07:47.16,0:07:49.28,main,,0000,0000,0000,,Ty se neotáčejí.
Dialogue: 0,0:07:50.32,0:07:50.92,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:07:51.28,0:07:52.60,main,,0000,0000,0000,,Pokud je otočíš…
Dialogue: 0,0:07:52.60,0:07:54.88,main,,0000,0000,0000,,…cibule nahoře spadne dolů.
Dialogue: 0,0:07:55.52,0:07:56.52,main,,0000,0000,0000,,Oh, aha.
Dialogue: 0,0:07:56.56,0:08:01.24,main,,0000,0000,0000,,Udělají se, bez jakéhokoliv otáčení.
Dialogue: 0,0:08:01.52,0:08:02.36,main,,0000,0000,0000,,Rozumím…
Dialogue: 0,0:08:02.36,0:08:04.04,main,,0000,0000,0000,,Od teď to budu tak dělat.
Dialogue: 0,0:08:04.04,0:08:06.36,main,,0000,0000,0000,,Nikdy je neotáčej.
Dialogue: 0,0:08:06.36,0:08:07.64,main,,0000,0000,0000,,Vážně…
Dialogue: 0,0:08:12.96,0:08:16.32,main,,0000,0000,0000,,Mimochodem, co je hlava čokoládového kornoutu? Tenká nebo tlustá strana?
Dialogue: 0,0:08:17.80,0:08:19.12,main,,0000,0000,0000,,To je teda přísné.
Dialogue: 0,0:08:17.40,0:08:19.36,newspaper,,0000,0000,0000,,{\frz-125\pos(518,288)}Chtěl jsem aby byla mým otrokem
Dialogue: 0,0:08:17.40,0:08:19.36,newspaper,,0000,0000,0000,,{\frz-20\pos(522,113)}Uvězněna na dva měsíce
Dialogue: 0,0:08:20.00,0:08:22.56,main,,0000,0000,0000,,Poslední dobou je tu spousta únosů.
Dialogue: 0,0:08:22.56,0:08:26.24,main,,0000,0000,0000,,Chlapi co nutí aby jim holky říkali „Pane“ a tak.
Dialogue: 0,0:08:27.24,0:08:28.28,main,,0000,0000,0000,,Možná to není až tak…
Dialogue: 0,0:08:28.28,0:08:29.32,main,,0000,0000,0000,,…problém morálky…
Dialogue: 0,0:08:29.32,0:08:32.48,main,,0000,0000,0000,,…jako spíš, že hrají moc erotických her?
Dialogue: 0,0:08:32.52,0:08:34.28,main,,0000,0000,0000,,V těch hrách je to docela běžné.
Dialogue: 0,0:08:34.32,0:08:36.20,main,,0000,0000,0000,,To tu mluvíš o sobě?!
Dialogue: 0,0:08:38.20,0:08:39.20,main,,0000,0000,0000,,Hej, počkej moment.
Dialogue: 0,0:08:39.20,0:08:41.24,main,,0000,0000,0000,,Jak to, že znáš tak dobře co je v těch hrách…
Dialogue: 0,0:08:41.24,0:08:42.04,main,,0000,0000,0000,,Slečno středoškolačko?
Dialogue: 0,0:08:42.68,0:08:45.56,copyrights,,0000,0000,0000,,{\pos(85,50)}Překlad od paralaXe
Dialogue: 0,0:08:42.68,0:08:45.56,copyrights,,0000,0000,0000,,{\pos(129,76)}navštivte www.otakogu.cz
Dialogue: 0,0:08:45.76,0:08:47.44,table,,0000,0000,0000,,{\pos(372,70)}První semestr průběžné hodnocení
Dialogue: 0,0:08:45.76,0:08:47.44,table,,0000,0000,0000,,{\frz-90\pos(317,257)}Miyuki Takara
Dialogue: 0,0:08:48.08,0:08:51.08,main,,0000,0000,0000,,Tvoje známky jsou vždy tak dobré.
Dialogue: 0,0:08:52.16,0:08:54.32,main,,0000,0000,0000,,To nemáš žádné neúspěchy?
Dialogue: 0,0:08:54.92,0:08:56.52,main,,0000,0000,0000,,Stydím se to říct…
Dialogue: 0,0:08:56.52,0:08:59.08,main,,0000,0000,0000,,…ale občas se zapomenu…
Dialogue: 0,0:08:59.12,0:09:00.84,main,,0000,0000,0000,,…nebo jen tak hledím do prázdna.
Dialogue: 0,0:09:01.36,0:09:04.20,main,,0000,0000,0000,,Pořezala jsem se, když jsem škrábala brambory.
Dialogue: 0,0:09:03.76,0:09:04.20,main,,0000,0000,0000,,Au!
Dialogue: 0,0:09:04.52,0:09:07.40,main,,0000,0000,0000,,A zakopla jsem, když jsem pospíchala.
Dialogue: 0,0:09:08.08,0:09:11.52,main,,0000,0000,0000,,A taky sem si přivřela tašku do dveří v metru.
Dialogue: 0,0:09:12.68,0:09:13.80,main,,0000,0000,0000,,Miyuki-san.
Dialogue: 0,0:09:13.80,0:09:17.12,main,,0000,0000,0000,,To nejsou neúspěchy, to je to čemu říkáme moé.
Dialogue: 0,0:09:17.76,0:09:18.36,main,,0000,0000,0000,,Moé?
Dialogue: 0,0:09:18.36,0:09:19.64,main,,0000,0000,0000,,Nemotorná.
Dialogue: 0,0:09:19.64,0:09:22.68,main,,0000,0000,0000,,Co se teď děje v Kono-chaniné hlavě!?
Dialogue: 0,0:09:23.16,0:09:26.16,main,,0000,0000,0000,,Miyuki-san, vypadáš vážně pěkně, proč vlastně nosíš brýle?
Dialogue: 0,0:09:26.16,0:09:28.40,main,,0000,0000,0000,,Takto se líbíš jen určitému typu kluků.
Dialogue: 0,0:09:28.40,0:09:29.44,main,,0000,0000,0000,,Určitému typu…
Dialogue: 0,0:09:30.00,0:09:32.40,main,,0000,0000,0000,,Když si odmyslíme oblíbenost u kluků…
Dialogue: 0,0:09:32.40,0:09:34.08,main,,0000,0000,0000,,…proč nenosíš čočky?
Dialogue: 0,0:09:34.08,0:09:36.80,main,,0000,0000,0000,,Slyšela jsem, že brýle zhoršují zrak.
Dialogue: 0,0:09:36.80,0:09:37.76,main,,0000,0000,0000,,Eh…
Dialogue: 0,0:09:37.76,0:09:40.28,main,,0000,0000,0000,,Je mi to trapné říkat, když jsme už na střední…
Dialogue: 0,0:09:41.12,0:09:41.96,main,,0000,0000,0000,,A-Ale…
Dialogue: 0,0:09:42.32,0:09:43.68,main,,0000,0000,0000,,Já se…
Dialogue: 0,0:09:43.68,0:09:44.88,main,,0000,0000,0000,,…jich bojím!
Dialogue: 0,0:09:44.88,0:09:46.36,main,,0000,0000,0000,,Bojím se čoček!
Dialogue: 0,0:09:46.36,0:09:47.60,main,,0000,0000,0000,,Abych byla přesnější…
Dialogue: 0,0:09:47.60,0:09:50.00,main,,0000,0000,0000,,…bojím se dávat si něco do očí.
Dialogue: 0,0:09:50.00,0:09:52.48,main,,0000,0000,0000,,Dokonce ani nedokážu otevřít oči, když jsem pod vodou.
Dialogue: 0,0:09:52.68,0:09:53.40,main,,0000,0000,0000,,Zatraceně!
Dialogue: 0,0:09:53.40,0:09:55.16,main,,0000,0000,0000,,Dokonce je i roztomilá!
Dialogue: 0,0:09:55.16,0:09:56.00,main,,0000,0000,0000,,To musí být přirozené.
Dialogue: 0,0:09:57.20,0:09:58.68,main,,0000,0000,0000,,Není to Miyuki-san?
Dialogue: 0,0:10:00.00,0:10:01.80,main,,0000,0000,0000,,Je tu kvůli očím?
Dialogue: 0,0:10:01.80,0:10:02.80,main,,0000,0000,0000,,Na čočky?
Dialogue: 0,0:10:05.80,0:10:08.88,main,,0000,0000,0000,,Nejspíš je takto lepší.
Dialogue: 0,0:10:08.88,0:10:09.92,main,,0000,0000,0000,,To asi bolelo…
Dialogue: 0,0:10:09.92,0:10:11.44,main,,0000,0000,0000,,Má to vrozené.
Dialogue: 0,0:10:14.12,0:10:16.12,main,,0000,0000,0000,,Kagami je doma s nachlazením?
Dialogue: 0,0:10:16.88,0:10:19.96,main,,0000,0000,0000,,Vypadá to ale, že nemá chřipku.
Dialogue: 0,0:10:19.96,0:10:23.68,main,,0000,0000,0000,,Já jsem hloupá, takže se o chřipku nemusím bát.
Dialogue: 0,0:10:24.56,0:10:26.04,main,,0000,0000,0000,,Slyšela jsem, že to platí o nachlazení…
Dialogue: 0,0:10:26.04,0:10:28.00,main,,0000,0000,0000,,…ale nevím jestli i na chřipku.
Dialogue: 0,0:10:30.60,0:10:32.84,main,,0000,0000,0000,,On je mezi nimi rozdíl?
Dialogue: 0,0:10:32.84,0:10:35.08,main,,0000,0000,0000,,Myslela jsem, že chřipka je něco jako super-nachlazení.
Dialogue: 0,0:10:35.08,0:10:37.12,main,,0000,0000,0000,,M-Myslím že jsou rozdílné…
Dialogue: 0,0:10:37.72,0:10:40.88,main,,0000,0000,0000,,Ale teď si nejsem jistá…
Dialogue: 0,0:10:41.80,0:10:43.92,main,,0000,0000,0000,,Rozdíl mezi chřipkou a nachlazením?
Dialogue: 0,0:10:45.36,0:10:46.60,main,,0000,0000,0000,,No…
Dialogue: 0,0:10:47.04,0:10:49.52,main,,0000,0000,0000,,Chřipka je způsobena virem.
Dialogue: 0,0:10:49.52,0:10:52.80,main,,0000,0000,0000,,Příznaky jsou horečka a bolest celého těla.
Dialogue: 0,0:10:52.80,0:10:55.68,main,,0000,0000,0000,,A je tu riziko komplikací.
Dialogue: 0,0:10:55.68,0:10:56.48,main,,0000,0000,0000,,Na druhé straně…
Dialogue: 0,0:10:56.48,0:10:59.04,main,,0000,0000,0000,,…příznaky nachlazení jsou bolest v krku a rýma…
Dialogue: 0,0:10:59.04,0:11:01.20,main,,0000,0000,0000,,…a horečka není tak vysoká.
Dialogue: 0,0:11:01.20,0:11:03.60,main,,0000,0000,0000,,A je tu navíc spousta menších rozdílů.
Dialogue: 0,0:11:05.32,0:11:06.20,main,,0000,0000,0000,,Takže…
Dialogue: 0,0:11:06.20,0:11:08.24,main,,0000,0000,0000,,to jsou rozdílné nemoci.
Dialogue: 0,0:11:09.08,0:11:10.00,main,,0000,0000,0000,,Chápu…
Dialogue: 0,0:11:11.28,0:11:12.40,main,,0000,0000,0000,,N-Nicméně…
Dialogue: 0,0:11:12.72,0:11:15.20,main,,0000,0000,0000,,No, mají i dost společného…
Dialogue: 0,0:11:15.20,0:11:19.08,main,,0000,0000,0000,,…takže bys možná mohla říct, že je to vylepšené nachlazení…
Dialogue: 0,0:11:19.08,0:11:19.80,main,,0000,0000,0000,,Věděla jsem to!
Dialogue: 0,0:11:22.24,0:11:22.76,main,,0000,0000,0000,,Kono-chan.
Dialogue: 0,0:11:22.76,0:11:25.08,main,,0000,0000,0000,,Co ten zamyšlený výraz?
Dialogue: 0,0:11:25.08,0:11:26.96,main,,0000,0000,0000,,Ale, to nic.
Dialogue: 0,0:11:26.96,0:11:29.08,main,,0000,0000,0000,,Jen jsem přemýšlela o našem ranním rozhovoru.
Dialogue: 0,0:11:29.64,0:11:31.12,main,,0000,0000,0000,,Ohledně nachlazení a chřipky?
Dialogue: 0,0:11:31.44,0:11:35.12,main,,0000,0000,0000,,Ty jsi neřekla že nesouhlasíš, když jsem řekla že jsem hloupá.
Dialogue: 0,0:11:35.12,0:11:35.96,main,,0000,0000,0000,,N-Ne!
Dialogue: 0,0:11:35.96,0:11:37.00,main,,0000,0000,0000,,Tak to není!
Dialogue: 0,0:11:42.72,0:11:43.80,main,,0000,0000,0000,,Konato-chan.
Dialogue: 0,0:11:43.80,0:11:44.76,main,,0000,0000,0000,,Vítej.
Dialogue: 0,0:11:44.76,0:11:46.80,main,,0000,0000,0000,,Přišla navštívit sestru.
Dialogue: 0,0:11:47.68,0:11:49.12,main,,0000,0000,0000,,Děkujeme za návštěvu.
Dialogue: 0,0:11:49.12,0:11:49.72,main,,0000,0000,0000,,To nic.
Dialogue: 0,0:11:49.72,0:11:51.44,main,,0000,0000,0000,,Promiňte, že vyrušuji.
Dialogue: 0,0:11:56.24,0:11:58.64,main,,0000,0000,0000,,Kagama vypadá docela roztomile, když spí.
Dialogue: 0,0:11:59.04,0:12:01.32,main,,0000,0000,0000,,Co to děláš?              Je vzhůru. Je vzhůru.
Dialogue: 0,0:12:02.24,0:12:03.00,main,,0000,0000,0000,,Navštívit?
Dialogue: 0,0:12:03.00,0:12:03.84,main,,0000,0000,0000,,Ty?
Dialogue: 0,0:12:03.84,0:12:05.56,main,,0000,0000,0000,,Přišla jsi celou tu cestu, abys mě viděla?
Dialogue: 0,0:12:05.60,0:12:06.48,main,,0000,0000,0000,,Tak to je.
Dialogue: 0,0:12:06.48,0:12:08.68,main,,0000,0000,0000,,A-Ale já nechci, abys to ode mě chytila.
Dialogue: 0,0:12:08.68,0:12:10.12,main,,0000,0000,0000,,Ale vážně si toho cením.
Dialogue: 0,0:12:10.80,0:12:11.36,main,,0000,0000,0000,,Ah.
Dialogue: 0,0:12:11.36,0:12:13.16,main,,0000,0000,0000,,To je milé.
Dialogue: 0,0:12:13.16,0:12:15.32,main,,0000,0000,0000,,Bála se o mě…
Dialogue: 0,0:12:15.36,0:12:17.56,main,,0000,0000,0000,,…protože jsem nemocná?
Dialogue: 0,0:12:17.56,0:12:21.48,main,,0000,0000,0000,,Myslela jsem si, že má v hlavě jen hry a anime.
Dialogue: 0,0:12:21.48,0:12:23.36,main,,0000,0000,0000,,Trochu se mě to dotklo.
Dialogue: 0,0:12:24.92,0:12:25.40,main,,0000,0000,0000,,No…
Dialogue: 0,0:12:25.40,0:12:27.76,main,,0000,0000,0000,,…taky jsem chtěla vidět ten úkol z minula.
Dialogue: 0,0:12:27.80,0:12:30.24,main,,0000,0000,0000,,A pak viděla jsem jak pěkně spíš…
Dialogue: 0,0:12:30.24,0:12:31.56,main,,0000,0000,0000,,…tak to ani neřeš.
Dialogue: 0,0:12:31.56,0:12:32.04,main,,0000,0000,0000,,Vypadni!
Dialogue: 0,0:12:33.84,0:12:34.84,main,,0000,0000,0000,,Kagami!
Dialogue: 0,0:12:34.88,0:12:37.60,main,,0000,0000,0000,,Miyuki-chan tě přišla navštívit!
Dialogue: 0,0:12:38.12,0:12:38.68,main,,0000,0000,0000,,Fajn!
Dialogue: 0,0:12:39.28,0:12:40.20,main,,0000,0000,0000,,Promiň, že ruším.
Dialogue: 0,0:12:41.12,0:12:42.92,main,,0000,0000,0000,,Je mi líto, že sem jdeš, když máš volno.
Dialogue: 0,0:12:42.92,0:12:44.80,main,,0000,0000,0000,,Je v pořádku, že jsi takto vzhůru?
Dialogue: 0,0:12:45.28,0:12:46.20,main,,0000,0000,0000,,Děkuji.                Tady máš něco ode mě.
Dialogue: 0,0:12:46.72,0:12:48.32,main,,0000,0000,0000,,Už jsem o dost lepší.
Dialogue: 0,0:12:48.32,0:12:50.20,main,,0000,0000,0000,,Po tom co jsem si vzala nějaké léky mi klesla horečka.
Dialogue: 0,0:12:50.20,0:12:50.56,main,,0000,0000,0000,,Tady.
Dialogue: 0,0:12:51.24,0:12:52.92,main,,0000,0000,0000,,Květiny a ovoce! To jsi nemusela!
Dialogue: 0,0:12:53.76,0:12:54.76,main,,0000,0000,0000,,Posaď se.
Dialogue: 0,0:12:54.76,0:12:56.04,main,,0000,0000,0000,,Moc děkuji.
Dialogue: 0,0:12:56.56,0:13:00.36,main,,0000,0000,0000,,Chtěla jsem tě navštívit se Izumi-san a ostatními…
Dialogue: 0,0:13:00.36,0:13:03.00,main,,0000,0000,0000,,…ale bohužel jsem si nemohla udělat čas.
Dialogue: 0,0:13:03.00,0:13:04.12,main,,0000,0000,0000,,To je v pohodě.
Dialogue: 0,0:13:04.68,0:13:08.16,main,,0000,0000,0000,,Mám pro tebe nějaké věci ze studentské rady.
Dialogue: 0,0:13:08.16,0:13:09.32,main,,0000,0000,0000,,Díky!
Dialogue: 0,0:13:09.76,0:13:13.84,main,,0000,0000,0000,,Pokud by jsi se na ně mohla jen mrknout…
Dialogue: 0,0:13:14.68,0:13:18.00,main,,0000,0000,0000,,A tady jsou rozpočty pro každý klub.
Dialogue: 0,0:13:18.00,0:13:20.64,main,,0000,0000,0000,,A co jsem to ještě chtěla?
Dialogue: 0,0:13:20.64,0:13:21.64,main,,0000,0000,0000,,Em…
Dialogue: 0,0:13:22.20,0:13:24.16,main,,0000,0000,0000,,Pokud bys chtěla odpočívat…
Dialogue: 0,0:13:24.16,0:13:25.28,main,,0000,0000,0000,,…tak já hned odejdu.
Dialogue: 0,0:13:26.00,0:13:28.08,main,,0000,0000,0000,,Takto má vypadat správná návštěva!
Dialogue: 0,0:13:28.08,0:13:30.08,main,,0000,0000,0000,,S-Stalo se něco?
Dialogue: 0,0:13:30.56,0:13:32.76,main,,0000,0000,0000,,No, Konata se tu včera ukázala…
Dialogue: 0,0:13:34.48,0:13:36.04,main,,0000,0000,0000,,Jak ti říkám.
Dialogue: 0,0:13:36.04,0:13:40.52,main,,0000,0000,0000,,Možná se takto projevuje.
Dialogue: 0,0:13:40.52,0:13:42.76,main,,0000,0000,0000,,Ten domácí úkol mohl být pokus o vtip…
Dialogue: 0,0:13:42.76,0:13:46.04,main,,0000,0000,0000,,…aby tě rozveselila.
Dialogue: 0,0:13:46.04,0:13:49.80,main,,0000,0000,0000,,A možná, že tě viděla spát…
Dialogue: 0,0:13:49.80,0:13:51.48,main,,0000,0000,0000,,…protože tě nechtěla vzbudit.
Dialogue: 0,0:13:52.04,0:13:53.92,main,,0000,0000,0000,,To je vždy jen pozitivní!?
Dialogue: 0,0:13:53.92,0:13:55.48,main,,0000,0000,0000,,To by se mi nikdy nestalo.
Dialogue: 0,0:13:55.96,0:13:57.08,main,,0000,0000,0000,,Tak mě napadá…
Dialogue: 0,0:13:57.08,0:13:59.48,main,,0000,0000,0000,,Dneska tu Tsukasa-san není?
Dialogue: 0,0:14:00.24,0:14:01.56,main,,0000,0000,0000,,Dobré ráno.
Dialogue: 0,0:14:02.28,0:14:05.64,main,,0000,0000,0000,,Je ti líp, ségra?
Dialogue: 0,0:14:08.96,0:14:09.96,main,,0000,0000,0000,,Jak vidíš…
Dialogue: 0,0:14:09.96,0:14:12.84,main,,0000,0000,0000,,Je víc rozespalá, než nemocný člověk.
Dialogue: 0,0:14:13.56,0:14:14.84,main,,0000,0000,0000,,U-um…
Dialogue: 0,0:14:14.84,0:14:16.88,main,,0000,0000,0000,,Takto to není každé ráno…
Dialogue: 0,0:14:16.88,0:14:19.92,main,,0000,0000,0000,,…jen jsem si ještě nezvykla na změnu času…
Dialogue: 0,0:14:20.48,0:14:21.68,main,,0000,0000,0000,,Je to trochu pozdě, ale vítej.
Dialogue: 0,0:14:22.52,0:14:23.84,main,,0000,0000,0000,,Naprosto ti rozumím.
Dialogue: 0,0:14:23.84,0:14:26.48,main,,0000,0000,0000,,Taky mám problémy se vzbudit.
Dialogue: 0,0:14:26.48,0:14:28.08,main,,0000,0000,0000,,T-To je pravda.
Dialogue: 0,0:14:28.08,0:14:29.68,main,,0000,0000,0000,,Taky se cítíš tak ospalá?
Dialogue: 0,0:14:29.68,0:14:31.16,main,,0000,0000,0000,,Opravdu.
Dialogue: 0,0:14:31.16,0:14:33.32,main,,0000,0000,0000,,A dnes je navíc obzvláště horko.
Dialogue: 0,0:14:34.04,0:14:34.88,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:14:34.88,0:14:36.04,main,,0000,0000,0000,,Jinak to asi nejde.
Dialogue: 0,0:14:36.52,0:14:39.12,main,,0000,0000,0000,,A poslední dny bylo taky horko.
Dialogue: 0,0:14:40.00,0:14:42.84,main,,0000,0000,0000,,Když jsem se probudila, tak už bylo odpoledne.
Dialogue: 0,0:14:42.84,0:14:45.04,main,,0000,0000,0000,,Většinu času jsem prospala.
Dialogue: 0,0:14:45.04,0:14:47.84,main,,0000,0000,0000,,Není na škodu si odpočinout, když máš volno.
Dialogue: 0,0:14:47.84,0:14:50.92,main,,0000,0000,0000,,Takže jsem nakonec na sobě měla moje pyžamo celý den.
Dialogue: 0,0:14:51.44,0:14:52.28,main,,0000,0000,0000,,A teď ho taky mám.
Dialogue: 0,0:14:53.60,0:14:55.92,main,,0000,0000,0000,,To je pyžamo?
Dialogue: 0,0:14:56.64,0:14:57.68,main,,0000,0000,0000,,Ano.
Dialogue: 0,0:14:57.68,0:14:59.36,main,,0000,0000,0000,,Je docela rozkošné.
Dialogue: 0,0:14:59.36,0:15:01.00,main,,0000,0000,0000,,Neřekla bych, že to jsou pyžama.
Dialogue: 0,0:15:01.00,0:15:01.84,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:15:02.68,0:15:04.56,main,,0000,0000,0000,,Zajímalo by mě, jestli bych v něm mohla zůstat.
Dialogue: 0,0:15:05.00,0:15:06.72,main,,0000,0000,0000,,Klidně v něm můžeš i ven.
Dialogue: 0,0:15:07.04,0:15:09.00,main,,0000,0000,0000,,Tenhle rozhovor snad nemá konce.
Dialogue: 0,0:15:08.04,0:15:10.44,main2,,0000,0000,0000,,Nikdo si toho ani nevšimne.
Dialogue: 0,0:15:09.00,0:15:10.44,mainup,,0000,0000,0000,,Mohla bych v něm do obchodu?
Dialogue: 0,0:15:10.36,0:15:11.60,Nurse,,0000,0000,0000,,{\frz15\pos(353,182)}Ošetřovna
Dialogue: 0,0:15:12.44,0:15:14.12,main,,0000,0000,0000,,Přibrala jsem…
Dialogue: 0,0:15:15.24,0:15:15.84,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:15:15.84,0:15:16.92,main,,0000,0000,0000,,Tím to nebude.
Dialogue: 0,0:15:16.92,0:15:19.88,main,,0000,0000,0000,,Dnes mám na sobě podprsenku s drátěnou výztuhou…
Dialogue: 0,0:15:19.92,0:15:21.52,main,,0000,0000,0000,,…takže se to projevilo.
Dialogue: 0,0:15:21.52,0:15:22.48,main,,0000,0000,0000,,To bude ono.
Dialogue: 0,0:15:22.48,0:15:24.56,main,,0000,0000,0000,,Zajímalo by mě, kolik gramů to tak dělá.
Dialogue: 0,0:15:24.56,0:15:25.92,main,,0000,0000,0000,,Když to odečtu…
Dialogue: 0,0:15:25.92,0:15:26.60,Nurse,,0000,0000,0000,,{\frz15\pos(353,182)}Ošetřovna
Dialogue: 0,0:15:28.96,0:15:30.36,main,,0000,0000,0000,,Je to za mnou…
Dialogue: 0,0:15:30.80,0:15:32.08,main,,0000,0000,0000,,To sem tomu dala.
Dialogue: 0,0:15:32.08,0:15:33.92,main,,0000,0000,0000,,Úplně jsem zapomněla, že jsou dneska prohlídky…
Dialogue: 0,0:15:33.92,0:15:36.60,main,,0000,0000,0000,,…a měla jsem svoje kalhotky s potiskem.
Dialogue: 0,0:15:36.60,0:15:38.40,main,,0000,0000,0000,,To je trapas!
Dialogue: 0,0:15:39.76,0:15:41.24,main,,0000,0000,0000,,Vůbec jsem nevyrostla…
Dialogue: 0,0:15:42.44,0:15:43.24,main,,0000,0000,0000,,Zatraceně!
Dialogue: 0,0:15:43.24,0:15:45.60,main,,0000,0000,0000,,Jediná jsi spokojená!
Dialogue: 0,0:15:46.12,0:15:47.40,main,,0000,0000,0000,,Hoj.
Dialogue: 0,0:15:47.40,0:15:48.00,main,,0000,0000,0000,,Jsem tady.
Dialogue: 0,0:15:48.32,0:15:49.52,main,,0000,0000,0000,,Ségra.
Dialogue: 0,0:15:49.52,0:15:50.88,main,,0000,0000,0000,,Napadlo mě…
Dialogue: 0,0:15:51.64,0:15:54.00,main,,0000,0000,0000,,Kagami sem chodí docela často.
Dialogue: 0,0:15:54.00,0:15:56.08,main,,0000,0000,0000,,Máš ve své třídě nějaké kamarády?
Dialogue: 0,0:15:56.08,0:15:57.68,main,,0000,0000,0000,,Nesrovnávej mě s tebou.
Dialogue: 0,0:15:57.68,0:15:58.96,main,,0000,0000,0000,,Chceš se snad dohadovat?
Dialogue: 0,0:15:59.68,0:16:01.32,main,,0000,0000,0000,,Myslela jsem, že nemáš kamarády.
Dialogue: 0,0:16:01.32,0:16:02.40,main,,0000,0000,0000,,Nestarej se o mě…
Dialogue: 0,0:16:02.40,0:16:04.80,main,,0000,0000,0000,,…ale spíš o tvůj nedostatek společenského života.
Dialogue: 0,0:16:05.32,0:16:07.32,main,,0000,0000,0000,,O to se neboj.
Dialogue: 0,0:16:07.32,0:16:10.24,main,,0000,0000,0000,,Věř tomu nebo, ale mám kolem sebe stovky přátel.
Dialogue: 0,0:16:10.24,0:16:13.48,main,,0000,0000,0000,,A jsem dost dobrá v navazování známostí.
Dialogue: 0,0:16:13.48,0:16:15.36,main,,0000,0000,0000,,A každou noc jsem na párty.
Dialogue: 0,0:16:16.20,0:16:20.96,main,,0000,0000,0000,,Já a moji společníci jsem se snažili získat zpět ukradené světlo z krystalu, aby jsme zachránili svět, před záhubou…
Dialogue: 0,0:16:20.96,0:16:21.84,main,,0000,0000,0000,,…a osvobodili unesenou princeznu…
Dialogue: 0,0:16:21.84,0:16:23.36,main,,0000,0000,0000,,To mluvíš o hře?!
Dialogue: 0,0:16:23.40,0:16:25.84,main,,0000,0000,0000,,Dlouho jsem ho neviděla.
Dialogue: 0,0:16:25.84,0:16:26.80,main,,0000,0000,0000,,Koho?
Dialogue: 0,0:16:26.80,0:16:28.92,main,,0000,0000,0000,,Nespíš mluví o někom ze hry.
Dialogue: 0,0:16:30.00,0:16:31.72,main,,0000,0000,0000,,Hrál za válečníka.
Dialogue: 0,0:16:31.72,0:16:37.52,main,,0000,0000,0000,,Šel někde lovit monstra, aby si vylepšil své schopnost a od té doby jsem ho neviděla.
Dialogue: 0,0:16:39.28,0:16:40.88,main,,0000,0000,0000,,Slyšela jsem…
Dialogue: 0,0:16:40.88,0:16:43.48,main,,0000,0000,0000,,…že leží ve stokách.
Dialogue: 0,0:16:43.48,0:16:44.84,main,,0000,0000,0000,,Zajímalo by mě, jestli je v pořádku.
Dialogue: 0,0:16:44.88,0:16:48.12,main,,0000,0000,0000,,Všichni se o něj bojí.
Dialogue: 0,0:16:48.12,0:16:50.04,main,,0000,0000,0000,,To se v té hře izoloval?
Dialogue: 0,0:16:50.04,0:16:51.76,main,,0000,0000,0000,,Nevím podrobnosti…
Dialogue: 0,0:16:51.76,0:16:55.72,main,,0000,0000,0000,,…ale podle jeho výpisu přihlášení to vypadá, že je online po celou dobu.
Dialogue: 0,0:16:55.72,0:16:57.76,main,,0000,0000,0000,,Takže se izoloval ve virtuálním i reálném světě?!
Dialogue: 0,0:16:57.76,0:16:59.92,main,,0000,0000,0000,,No, přece jen je to jeden člověk.
Dialogue: 0,0:17:00.32,0:17:01.16,main,,0000,0000,0000,,Tak mě napadá…
Dialogue: 0,0:17:01.16,0:17:04.12,main,,0000,0000,0000,,Jak jste se vy dvě seznámili?
Dialogue: 0,0:17:04.72,0:17:09.24,main,,0000,0000,0000,,Nějaký hromotluk z ciziny se chystal unést Tsukasu pryč…
Dialogue: 0,0:17:09.24,0:17:11.68,main,,0000,0000,0000,,…když v tom jsem se objevila.
Dialogue: 0,0:17:11.68,0:17:13.08,main,,0000,0000,0000,,Použitím mého běsnícího úderu…
Dialogue: 0,0:17:13.08,0:17:15.24,main,,0000,0000,0000,,…a ukázkového zdrcujícího kopu…
Dialogue: 0,0:17:15.24,0:17:16.56,main,,0000,0000,0000,,…jsem ji zachránila.
Dialogue: 0,0:17:16.56,0:17:19.32,main,,0000,0000,0000,,Proč se vždycky nakonec bavíme o hrách?
Dialogue: 0,0:17:19.32,0:17:23.12,main,,0000,0000,0000,,No, to se stává, když natrefíš na zkušeného bojovníka.
Dialogue: 0,0:17:23.12,0:17:25.36,main,,0000,0000,0000,,Nejsem si jistá, jestli ses měla tak rychle uchýlit k násilí…
Dialogue: 0,0:17:25.36,0:17:26.36,main,,0000,0000,0000,,…ale poradila sis dobře.
Dialogue: 0,0:17:26.80,0:17:30.36,main,,0000,0000,0000,,Ten cizinec se mě vlastně jen ptal na směr…
Dialogue: 0,0:17:30.36,0:17:33.40,main,,0000,0000,0000,,…ale moc jsem mu nerozuměla, když v tom se objevila Kona-chan…
Dialogue: 0,0:17:33.96,0:17:34.96,main,,0000,0000,0000,,To jsi to naprosto zpackala!
Dialogue: 0,0:17:34.96,0:17:36.36,main,,0000,0000,0000,,A proč se tváříš tak hrdě?!
Dialogue: 0,0:17:36.92,0:17:40.08,main,,0000,0000,0000,,Čas znova nadešel…
Dialogue: 0,0:17:40.60,0:17:41.92,main,,0000,0000,0000,,Má se něco stát?
Dialogue: 0,0:17:41.92,0:17:42.56,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:17:42.56,0:17:45.00,main,,0000,0000,0000,,Jen mě napadlo, že zase začíná sezóna baseballu…
Dialogue: 0,0:17:45.00,0:17:48.44,main,,0000,0000,0000,,Ty nemáš ráda sporty?
Dialogue: 0,0:17:48.44,0:17:50.24,main,,0000,0000,0000,,Jsi tak sportovní…
Dialogue: 0,0:17:50.24,0:17:52.40,main,,0000,0000,0000,,…takže jsem myslela, že je máš ráda.
Dialogue: 0,0:17:52.40,0:17:55.00,main,,0000,0000,0000,,Když se protáhnou a hrají přesčas…
Dialogue: 0,0:17:55.00,0:17:58.00,main,,0000,0000,0000,,…vysílání nových dílů anime se naprosto pomíchá.
Dialogue: 0,0:17:58.32,0:18:01.88,main,,0000,0000,0000,,Někdy je dokonce show úplně zrušena.
Dialogue: 0,0:18:01.88,0:18:05.08,main,,0000,0000,0000,,Není příjemné, když přijdeš o něco co máš ráda.
Dialogue: 0,0:18:05.08,0:18:08.04,main,,0000,0000,0000,,Když jíš jahodu na jahodovém dortu, Kagami?
Dialogue: 0,0:18:08.04,0:18:08.72,main,,0000,0000,0000,,Proč se ptáš?
Dialogue: 0,0:18:09.04,0:18:12.36,main,,0000,0000,0000,,Jen mě zajímalo, jestli si ten typ co si nechává nejlepší nakonec.
Dialogue: 0,0:18:12.36,0:18:15.00,main,,0000,0000,0000,,Obvykle jím to nejlepší až nakonec…
Dialogue: 0,0:18:15.00,0:18:17.16,main,,0000,0000,0000,,…ale s jahodami to záleží na situaci.
Dialogue: 0,0:18:17.16,0:18:19.24,main,,0000,0000,0000,,A griluješ obě dvě strany hovězího jazyku?
Dialogue: 0,0:18:19.72,0:18:21.04,main,,0000,0000,0000,,Hovězího jazyku?
Dialogue: 0,0:18:22.16,0:18:23.84,main,,0000,0000,0000,,Je hotový skoro hned…
Dialogue: 0,0:18:23.84,0:18:25.60,main,,0000,0000,0000,,…ale je to ještě rychlejší, když griluješ obě strany.
Dialogue: 0,0:18:25.60,0:18:26.68,main,,0000,0000,0000,,To nesmíš.
Dialogue: 0,0:18:26.68,0:18:28.12,main,,0000,0000,0000,,Cibule by spadla.
Dialogue: 0,0:18:28.12,0:18:30.88,main,,0000,0000,0000,,Nepřidáváš cibuli, až nakonec.
Dialogue: 0,0:18:30.88,0:18:32.64,main,,0000,0000,0000,,Tak by se ale neopekla.
Dialogue: 0,0:18:33.12,0:18:36.12,main,,0000,0000,0000,,Ale nejsou tam jen kvůli chuti?
Dialogue: 0,0:18:36.24,0:18:37.64,main,,0000,0000,0000,,Ty to prostě nechápeš…
Dialogue: 0,0:18:37.64,0:18:40.32,main,,0000,0000,0000,,Nešlo by to tak i tak?
Dialogue: 0,0:18:40.96,0:18:44.68,main,,0000,0000,0000,,Jaká strana čokoládového kornoutu je podle tebe hlava? Tenká nebo Tlustá?
Dialogue: 0,0:18:46.28,0:18:47.12,main,,0000,0000,0000,,Tlustá?
Dialogue: 0,0:18:47.60,0:18:49.12,main,,0000,0000,0000,,Taky si to myslím.
Dialogue: 0,0:18:49.12,0:18:49.64,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:18:50.04,0:18:51.40,main,,0000,0000,0000,,A z jakého konce ho jíš?
Dialogue: 0,0:18:52.44,0:18:53.72,main,,0000,0000,0000,,Nemáš odloupnout tenký konec…
Dialogue: 0,0:18:53.72,0:18:55.60,main,,0000,0000,0000,,…a pomocí něho jíst přebývající čokoládu?
Dialogue: 0,0:19:03.72,0:19:07.64,main,,0000,0000,0000,,Máš kromě náš nějaké jiné reálné kamarády?
Dialogue: 0,0:19:08.84,0:19:12.60,main,,0000,0000,0000,,Ještě na základce jsem měla celkem blízké kamarády.
Dialogue: 0,0:19:13.40,0:19:14.48,main,,0000,0000,0000,,Takže máš.
Dialogue: 0,0:19:15.20,0:19:17.60,main,,0000,0000,0000,,Ale dlouho jsem s ní nebyla…
Dialogue: 0,0:19:17.60,0:19:19.32,main,,0000,0000,0000,,Zajímalo by mě co asi dělá…
Dialogue: 0,0:19:22.16,0:19:24.76,main,,0000,0000,0000,,Na základce napsala…
Dialogue: 0,0:19:24.76,0:19:26.60,main,,0000,0000,0000,,…že její sen je stát se čarodějem.
Dialogue: 0,0:19:26.60,0:19:27.72,main,,0000,0000,0000,,Takže je stejná jako ty?!
