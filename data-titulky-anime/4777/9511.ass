Dialogue: 0,0:00:01.11,0:00:02.56,main,,0000,0000,0000,,Tak začneme.
Dialogue: 0,0:00:02.57,0:00:03.61,main,,0000,0000,0000,,Jdeme na to.
Dialogue: 0,0:00:04.19,0:00:05.98,main,,0000,0000,0000,,No tak už začněme!
Dialogue: 0,0:00:06.32,0:00:07.49,op1,,0000,0000,0000,,Okolo 3 cm!
Dialogue: 0,0:00:07.49,0:00:08.78,op1,,0000,0000,0000,,Je to snad málo?
Dialogue: 0,0:00:08.78,0:00:09.03,op1,,0000,0000,0000,,Jejda!
Dialogue: 0,0:00:09.03,0:00:10.86,op1,,0000,0000,0000,,Schované pod naší uniformou!
Dialogue: 0,0:00:10.87,0:00:11.91,op1,,0000,0000,0000,,Mám toho nechat?
Dialogue: 0,0:00:11.91,0:00:12.20,op1,,0000,0000,0000,,JEJ!
Dialogue: 0,0:00:12.20,0:00:13.28,op1,,0000,0000,0000,,Jít do toho?
Dialogue: 0,0:00:13.28,0:00:14.08,op1,,0000,0000,0000,,Snažit se?
Dialogue: 0,0:00:14.08,0:00:15.66,op1,,0000,0000,0000,,Vychutnat si každý okamžik. Jo!
Dialogue: 0,0:00:15.66,0:00:16.25,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.26,0:00:16.50,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:16.50,0:00:16.90,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.91,0:00:17.25,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:17.25,0:00:17.87,op1,,0000,0000,0000,,Jen mezi námi dvěma!
Dialogue: 0,0:00:17.87,0:00:20.58,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:00:22.56,0:00:23.36,logo,,0000,0000,0000,,{\move(108,218,202,218,0,0)}LUCKY
Dialogue: 0,0:00:23.36,0:00:25.08,logo,,0000,0000,0000,,{\pos(202,218)}LUCKY
Dialogue: 0,0:00:22.56,0:00:23.36,logo,,0000,0000,0000,,{\move(638,218,459,218,0,0)}STAR
Dialogue: 0,0:00:23.36,0:00:25.08,logo,,0000,0000,0000,,{\pos(459,218)}STAR
Dialogue: 0,0:00:25.51,0:00:26.09,op1,,0000,0000,0000,,Cítím se nějak nesvá.
Dialogue: 0,0:00:26.09,0:00:26.71,op1,,0000,0000,0000,,Co jsem to jedla?
Dialogue: 0,0:00:26.71,0:00:27.30,op1,,0000,0000,0000,,Mám tě hrozně ráda!
Dialogue: 0,0:00:27.30,0:00:27.51,op1,,0000,0000,0000,,Oops!
Dialogue: 0,0:00:27.51,0:00:28.55,op1,,0000,0000,0000,,Jedno z nich je špatně!
Dialogue: 0,0:00:28.55,0:00:29.30,op1,,0000,0000,0000,,Relaxovat!
Dialogue: 0,0:00:29.30,0:00:29.93,op1,,0000,0000,0000,,Nic nedělat!
Dialogue: 0,0:00:29.93,0:00:30.47,op1,,0000,0000,0000,,Flákat se!
Dialogue: 0,0:00:30.47,0:00:31.55,op1,,0000,0000,0000,,Už toho nechej!
Dialogue: 0,0:00:31.55,0:00:33.21,op1,,0000,0000,0000,,Je ta nová holka oblíbená?
Dialogue: 0,0:00:33.21,0:00:34.85,op1,,0000,0000,0000,,Já bych řekla, že je docela normální!
Dialogue: 0,0:00:34.85,0:00:36.35,op1,,0000,0000,0000,,To jsem jediná překvapená?
Dialogue: 0,0:00:36.35,0:00:36.66,op1,,0000,0000,0000,,Vepřové kousky!
Dialogue: 0,0:00:36.66,0:00:37.06,op1,,0000,0000,0000,,Skleněné nudle!
Dialogue: 0,0:00:37.06,0:00:37.39,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:00:37.39,0:00:38.18,op2,,0000,0000,0000,,DA-DA-DA-DA-DA!
Dialogue: 0,0:00:38.73,0:00:39.52,op1,,0000,0000,0000,,Ooh?
Dialogue: 0,0:00:39.52,0:00:40.31,op1,,0000,0000,0000,,Víš co tím myslím?
Dialogue: 0,0:00:39.89,0:00:40.73,op1up,,0000,0000,0000,,Moje brýle…
Dialogue: 0,0:00:40.73,0:00:41.81,op1up,,0000,0000,0000,,Kde mám brýle?
Dialogue: 0,0:00:41.90,0:00:43.86,op1,,0000,0000,0000,,Nedávno jsme hrála tu samou bojovku.
Dialogue: 0,0:00:42.48,0:00:43.86,op1up,,0000,0000,0000,,Bez nich nic nevidím…
Dialogue: 0,0:00:44.90,0:00:45.48,op2,,0000,0000,0000,,POM! POM!
Dialogue: 0,0:00:45.48,0:00:46.53,op1,,0000,0000,0000,,Roztleskávačky!
Dialogue: 0,0:00:46.53,0:00:48.07,op2,,0000,0000,0000,,DÁME SI SLADKOST!
Dialogue: 0,0:00:48.07,0:00:48.65,op2,,0000,0000,0000,,LA! LA!
Dialogue: 0,0:00:48.65,0:00:49.70,op1,,0000,0000,0000,,Párty začíná!
Dialogue: 0,0:00:49.70,0:00:51.28,op2,,0000,0000,0000,,MÁŠ TO V KRVI!
Dialogue: 0,0:00:51.28,0:00:51.86,op2,,0000,0000,0000,,ANO!
Dialogue: 0,0:00:51.87,0:00:52.87,op1,,0000,0000,0000,,Zanechává to dojem.
Dialogue: 0,0:00:52.87,0:00:54.32,op1,,0000,0000,0000,,Malé planetky.
Dialogue: 0,0:00:54.33,0:00:54.87,op1,,0000,0000,0000,,Narazili!
Dialogue: 0,0:00:54.87,0:00:55.49,op1,,0000,0000,0000,,A shořeli!
Dialogue: 0,0:00:55.49,0:00:55.87,op1,,0000,0000,0000,,Bylo to ohromné!
Dialogue: 0,0:00:55.87,0:00:56.62,op1,,0000,0000,0000,,Z plných plic křičíme!
Dialogue: 0,0:00:56.62,0:00:57.18,op1,,0000,0000,0000,,ZPÍVEJ A TANCUJ!
Dialogue: 0,0:00:57.54,0:00:58.83,op1,,0000,0000,0000,,Přiznej si to!
Dialogue: 0,0:00:58.83,0:01:03.29,op1,,0000,0000,0000,,Nakonec se stejně budeme smát!
Dialogue: 0,0:01:03.29,0:01:07.05,op1,,0000,0000,0000,,Protože mám na sobě námořnickou uniformu.
Dialogue: 0,0:01:07.05,0:01:09.92,op2,,0000,0000,0000,,To je ono!
Dialogue: 0,0:01:10.51,0:01:14.05,op1,,0000,0000,0000,,Je teprve pondělí a já už mám špatnou náladu.
Dialogue: 0,0:01:14.05,0:01:15.85,op1,,0000,0000,0000,,Co mám dělat?
Dialogue: 0,0:01:16.06,0:01:19.89,op1,,0000,0000,0000,,Raději si vezmu moji letní uniformu -
Dialogue: 0,0:01:19.89,0:01:22.77,op1,,0000,0000,0000,,- je rozkošná!
Dialogue: 0,0:01:23.02,0:01:24.19,op1,,0000,0000,0000,,Trojdílná uniforma!
Dialogue: 0,0:01:24.19,0:01:25.61,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:01:25.61,0:01:26.06,op1,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.07,0:01:26.48,op1,,0000,0000,0000,,Dej se do toho!
Dialogue: 0,0:01:26.48,0:01:26.86,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.86,0:01:27.48,op1,,0000,0000,0000,,SNAŽ SE!
Dialogue: 0,0:01:27.48,0:01:27.65,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:27.65,0:01:29.54,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:01:35.72,0:01:36.34,main,,0000,0000,0000,,Fajn.
Dialogue: 0,0:01:36.34,0:01:37.84,main,,0000,0000,0000,,Kdo půjde vyřešit tento příklad?
Dialogue: 0,0:01:38.64,0:01:39.30,main,,0000,0000,0000,,Shiraishi.
Dialogue: 0,0:01:39.93,0:01:41.80,main,,0000,0000,0000,,Právě jste mne vyvolala!
Dialogue: 0,0:01:41.81,0:01:42.64,main,,0000,0000,0000,,Opravdu?
Dialogue: 0,0:01:43.26,0:01:44.64,main,,0000,0000,0000,,Tak Izumi.
Dialogue: 0,0:01:45.31,0:01:46.14,main,,0000,0000,0000,,Izumi?
Dialogue: 0,0:01:47.69,0:01:49.14,main,,0000,0000,0000,,Ne, nespala jsem.
Dialogue: 0,0:01:50.15,0:01:51.98,main,,0000,0000,0000,,Ještě jsem nic neřekla.
Dialogue: 0,0:01:52.57,0:01:55.57,whitebig,,0000,0000,0000,,Problémy s motivací
Dialogue: 0,0:01:58.20,0:02:00.78,main,,0000,0000,0000,,Zapomněla jsem svůj sešit angličtiny!
Dialogue: 0,0:02:00.78,0:02:02.74,main,,0000,0000,0000,,Proč si nepůjčíš od Kagami?
Dialogue: 0,0:02:02.74,0:02:04.58,main,,0000,0000,0000,,Už měli angličtinu, ne?
Dialogue: 0,0:02:05.08,0:02:06.04,main,,0000,0000,0000,,Vážně…
Dialogue: 0,0:02:06.04,0:02:08.21,main,,0000,0000,0000,,Jsi hrozně zapomětlivá.
Dialogue: 0,0:02:08.21,0:02:09.79,main,,0000,0000,0000,,Děkuji!
Dialogue: 0,0:02:09.79,0:02:11.92,main,,0000,0000,0000,,Den předem si máš vše nachystat a dvakrát zkontrolovat.
Dialogue: 0,0:02:12.71,0:02:15.38,main,,0000,0000,0000,,Tsukasa hodně zapomíná, že?
Dialogue: 0,0:02:16.13,0:02:17.76,main,,0000,0000,0000,,Když o tom přemýšlím…
Dialogue: 0,0:02:17.76,0:02:20.43,main,,0000,0000,0000,,…Konata nezapomíná věci, jako jsou sešity a tak.
Dialogue: 0,0:02:20.50,0:02:24.60,main,,0000,0000,0000,,To proto, že jsem co se těchto věcí týká překvapivě pečlivá.
Dialogue: 0,0:02:24.64,0:02:27.60,main,,0000,0000,0000,,Plus - nikdy si neberu sešity domů.
Dialogue: 0,0:02:28.06,0:02:29.23,main,,0000,0000,0000,,Aha.
Dialogue: 0,0:02:29.23,0:02:30.60,main,,0000,0000,0000,,Tomu se ale neříká pečlivost.
Dialogue: 0,0:02:30.65,0:02:35.19,main,,0000,0000,0000,,Ale bez sešitu si nemůžeš zopakovat, co jsme probírali.
Dialogue: 0,0:02:35.19,0:02:37.74,main,,0000,0000,0000,,Doma se nikdy neučím.
Dialogue: 0,0:02:37.74,0:02:41.74,main,,0000,0000,0000,,A tak se šprtáš noc předem…
Dialogue: 0,0:02:41.74,0:02:44.28,main,,0000,0000,0000,,Ale Kona-chan je v tomto dost dobrá.
Dialogue: 0,0:02:45.70,0:02:48.58,main,,0000,0000,0000,,To by mohl být důvod, proč zapomínám dělat úkoly.
Dialogue: 0,0:02:49.12,0:02:51.67,main,,0000,0000,0000,,Ty na ně nezapomínáš.
Dialogue: 0,0:02:51.67,0:02:52.96,main,,0000,0000,0000,,Ty je prostě nechceš dělat.
Dialogue: 0,0:02:53.46,0:02:56.84,main,,0000,0000,0000,,Tak mi ukaž tvůj úkol do dnešní japonštiny.
Dialogue: 0,0:02:56.84,0:02:57.94,main,,0000,0000,0000,,Takže už zase, co?
Dialogue: 0,0:02:58.42,0:02:59.84,main,,0000,0000,0000,,Víš ty co...
Dialogue: 0,0:02:59.84,0:03:01.34,main,,0000,0000,0000,,Miyuki s tebou chodí do třídy…
Dialogue: 0,0:03:01.34,0:03:02.89,main,,0000,0000,0000,,řekni si o ten úkol jí.
Dialogue: 0,0:03:03.47,0:03:05.26,main,,0000,0000,0000,,Jak bych to řekla…
Dialogue: 0,0:03:05.26,0:03:06.89,main,,0000,0000,0000,,Miyuki-san je tak milý člověk…
Dialogue: 0,0:03:06.89,0:03:09.98,main,,0000,0000,0000,,…že je těžké ji o něco požádat.
Dialogue: 0,0:03:09.98,0:03:11.44,main,,0000,0000,0000,,Víš jak to myslím?
Dialogue: 0,0:03:11.44,0:03:12.64,main,,0000,0000,0000,,Mám z toho takový divný pocit.
Dialogue: 0,0:03:13.19,0:03:14.94,main,,0000,0000,0000,,Rozumím co říkáš…
Dialogue: 0,0:03:14.94,0:03:16.23,main,,0000,0000,0000,,…ale pořád to nemůžu přijmout.
Dialogue: 0,0:03:16.82,0:03:17.69,main,,0000,0000,0000,,Izumi.
Dialogue: 0,0:03:18.23,0:03:19.11,main,,0000,0000,0000,,Izumi?
Dialogue: 0,0:03:20.74,0:03:22.03,main,,0000,0000,0000,,Cože? Opozdila se?
Dialogue: 0,0:03:22.53,0:03:23.20,main,,0000,0000,0000,,Stop!
Dialogue: 0,0:03:23.20,0:03:24.78,main,,0000,0000,0000,,Neopozdila jsem se!
Dialogue: 0,0:03:25.00,0:03:27.70,main,,0000,0000,0000,,Jsem tady pozdě, protože jsem musela někomu pomoct!
Dialogue: 0,0:03:28.37,0:03:30.79,main,,0000,0000,0000,,Normálně odcházím s tím klukem…
Dialogue: 0,0:03:30.79,0:03:33.33,main,,0000,0000,0000,,…ale dneska to prostě nějak nevyšlo!
Dialogue: 0,0:03:34.33,0:03:38.59,main,,0000,0000,0000,,Říkal že se nemůžeme rozejít, že je to takhle fajn…
Dialogue: 0,0:03:38.59,0:03:40.34,main,,0000,0000,0000,,…a celý to bylo divný…
Dialogue: 0,0:03:40.34,0:03:42.17,main,,0000,0000,0000,,…měla jsem z toho špatný pocit.
Dialogue: 0,0:03:42.18,0:03:46.47,main,,0000,0000,0000,,Když jste v jedné partě dlouho, tak k ní máte jisté závazky.
Dialogue: 0,0:03:46.47,0:03:48.31,main,,0000,0000,0000,,Takže jsme mu všichni pomohli.
Dialogue: 0,0:03:48.31,0:03:50.52,main,,0000,0000,0000,,Jo, přátelé jsou skvělá věc.
Dialogue: 0,0:03:50.93,0:03:52.02,main,,0000,0000,0000,,Vážně…
Dialogue: 0,0:03:52.02,0:03:55.02,main,,0000,0000,0000,,Takže jsi byla celou noc vzhůru a hrála jsi online hru.
Dialogue: 0,0:03:55.03,0:03:58.43,main,,0000,0000,0000,,a hrála jsi i po tom co jsem se já odhlásila, co?
Dialogue: 0,0:03:58.44,0:03:59.15,main,,0000,0000,0000,,Izumi.
Dialogue: 0,0:03:59.73,0:04:00.61,main,,0000,0000,0000,,Izumi?
Dialogue: 0,0:04:02.24,0:04:03.53,main,,0000,0000,0000,,Cože? Opozdila se?
Dialogue: 0,0:04:03.86,0:04:04.70,main,,0000,0000,0000,,Stop!
Dialogue: 0,0:04:04.70,0:04:06.20,main,,0000,0000,0000,,Neopozdila jsem se!
Dialogue: 0,0:04:06.66,0:04:09.45,main,,0000,0000,0000,,Potkala jsem se s jednou holkou v obchodní čtvrti.
Dialogue: 0,0:04:09.45,0:04:12.41,main,,0000,0000,0000,,Chytila mě za ruku a začala se mnou utíkat pryč.
Dialogue: 0,0:04:12.41,0:04:15.29,main,,0000,0000,0000,,Později jsem zjistila, že ukradla nějaké taiyaki…
Dialogue: 0,0:04:15.29,0:04:16.67,main,,0000,0000,0000,,…a tak byla na útěku…
Dialogue: 0,0:04:16.67,0:04:19.80,main,,0000,0000,0000,,Zajímala jsem se, co to má co dělat se mnou…
Dialogue: 0,0:04:19.80,0:04:23.01,main,,0000,0000,0000,,…ale neměla jsem jinou možnost, než se schovat v kavárně před pekařem taiyaki.
Dialogue: 0,0:04:24.55,0:04:26.51,main,,0000,0000,0000,,Tolik potenciálních comebacků…
Dialogue: 0,0:04:26.51,0:04:28.51,main,,0000,0000,0000,,musím toho ale nechat!
Dialogue: 0,0:04:28.51,0:04:29.51,main,,0000,0000,0000,,Jsem učitelka!
Dialogue: 0,0:04:29.52,0:04:30.71,main,,0000,0000,0000,,Jsem lidská bytost!
Dialogue: 0,0:04:31.60,0:04:34.18,main,,0000,0000,0000,,Je léto a já jsem nachlazená.
Dialogue: 0,0:04:34.69,0:04:35.56,main,,0000,0000,0000,,Izumi.
Dialogue: 0,0:04:35.56,0:04:36.60,main,,0000,0000,0000,,Co se děje?
Dialogue: 0,0:04:37.52,0:04:39.19,main,,0000,0000,0000,,Paní učitelko, moc se omlouvám…
Dialogue: 0,0:04:39.19,0:04:41.11,main,,0000,0000,0000,,…ale potřebuji dneska zůstat doma…
Dialogue: 0,0:04:41.78,0:04:42.74,main,,0000,0000,0000,,Hej.
Dialogue: 0,0:04:42.74,0:04:44.49,main,,0000,0000,0000,,Zase jsi moc hrála?
Dialogue: 0,0:04:44.49,0:04:46.99,main,,0000,0000,0000,,Ten samý trik nebude fungovat podruhé.
Dialogue: 0,0:04:47.37,0:04:49.32,main,,0000,0000,0000,,Ne, jsem nemocná…
Dialogue: 0,0:04:50.79,0:04:53.75,main,,0000,0000,0000,,Nech mě hádat. Zase máš letní nemoc?
Dialogue: 0,0:04:53.75,0:04:55.54,main,,0000,0000,0000,,Zima skončila a je krásné počasí…
Dialogue: 0,0:04:56.00,0:05:00.47,main,,0000,0000,0000,,tak se nevymlouvej a pojď do školy!
Dialogue: 0,0:05:03.47,0:05:07.76,main,,0000,0000,0000,,Takže se mnou včera jednala jako s nějakým simulantem.
Dialogue: 0,0:05:07.76,0:05:09.85,main,,0000,0000,0000,,Sklidíš co si zaseješ.
Dialogue: 0,0:05:09.85,0:05:12.14,main,,0000,0000,0000,,Už je ti dobře?
Dialogue: 0,0:05:12.14,0:05:13.81,main,,0000,0000,0000,,Vypadá to, že mi klesla horečka…
Dialogue: 0,0:05:13.81,0:05:15.77,main,,0000,0000,0000,,…ale pořád mě bolí v krku.
Dialogue: 0,0:05:16.60,0:05:18.94,main,,0000,0000,0000,,To znamená, že dýcháš skrz pusu.
Dialogue: 0,0:05:21.15,0:05:21.82,main,,0000,0000,0000,,Heh?
Dialogue: 0,0:05:21.82,0:05:22.52,main,,0000,0000,0000,,No…
Dialogue: 0,0:05:22.53,0:05:24.65,main,,0000,0000,0000,,Můžeš dýchat skrz pusu…
Dialogue: 0,0:05:24.65,0:05:26.20,main,,0000,0000,0000,,…ale nedýcháš spíš nosem?
Dialogue: 0,0:05:26.20,0:05:26.86,main,,0000,0000,0000,,Ano…
Dialogue: 0,0:05:26.86,0:05:30.66,main,,0000,0000,0000,,Ale Kona-chan dýchá víc pusou.
Dialogue: 0,0:05:31.20,0:05:31.95,main,,0000,0000,0000,,Protože…
Dialogue: 0,0:05:31.95,0:05:32.37,main,,0000,0000,0000,,Víš, že?
Dialogue: 0,0:05:32.37,0:05:35.79,main,,0000,0000,0000,,Pokud vir vstoupí skrz pusu, bude tě bolet v krku.
Dialogue: 0,0:05:35.79,0:05:39.04,main,,0000,0000,0000,,A pokud skrz nos, tak budeš mít rýmu.
Dialogue: 0,0:05:39.04,0:05:44.13,main,,0000,0000,0000,,Takže lidé s bolavým krkem mají pravděpodobně větší šanci, že virus vstoupil skrz krk…
Dialogue: 0,0:05:46.63,0:05:48.80,main,,0000,0000,0000,,Je z ní Miyuki-san.
Dialogue: 0,0:05:48.80,0:05:49.64,main,,0000,0000,0000,,Jo…
Dialogue: 0,0:05:49.64,0:05:50.30,main,,0000,0000,0000,,Heh?
Dialogue: 0,0:05:52.26,0:05:53.93,main,,0000,0000,0000,,Vedro…
Dialogue: 0,0:05:54.77,0:05:57.18,main,,0000,0000,0000,,Nechce se mi nic dělat…
Dialogue: 0,0:05:59.19,0:06:00.40,main,,0000,0000,0000,,Kagami?
Dialogue: 0,0:06:00.40,0:06:01.81,main,,0000,0000,0000,,Co se té dnešní párty týká…
Dialogue: 0,0:06:01.82,0:06:03.69,main,,0000,0000,0000,,Co kdybychom to udělali u mě?
Dialogue: 0,0:06:03.69,0:06:06.44,main,,0000,0000,0000,,Je takové vedro, že se nemůžu ani hnout.
Dialogue: 0,0:06:07.82,0:06:10.45,main,,0000,0000,0000,,My právě ale pečeme koláčky.
Dialogue: 0,0:06:12.12,0:06:12.87,main,,0000,0000,0000,,Přijdu.
Dialogue: 0,0:06:13.49,0:06:14.45,main,,0000,0000,0000,,Pospěš si.
Dialogue: 0,0:06:15.20,0:06:15.87,main,,0000,0000,0000,,Vítejte!
Dialogue: 0,0:06:16.41,0:06:18.04,main,,0000,0000,0000,,Šťastné narozeniny!
Dialogue: 0,0:06:18.04,0:06:19.33,main,,0000,0000,0000,,Tsukaso a ostatní.
Dialogue: 0,0:06:19.33,0:06:20.66,main,,0000,0000,0000,,Nezestručňuj mě!
Dialogue: 0,0:06:20.67,0:06:23.34,main,,0000,0000,0000,,Přeji Vám, Šťastné narozeniny.
Dialogue: 0,0:06:23.34,0:06:25.84,main,,0000,0000,0000,,Kagami-san a Tsukaso-san.
Dialogue: 0,0:06:26.46,0:06:27.97,main,,0000,0000,0000,,Díky!
Dialogue: 0,0:06:27.97,0:06:28.72,main,,0000,0000,0000,,Děkuji!
Dialogue: 0,0:06:29.76,0:06:32.43,main,,0000,0000,0000,,Není to mnoho, ale…
Dialogue: 0,0:06:32.43,0:06:33.26,main,,0000,0000,0000,,…mám pro vás dárek.
Dialogue: 0,0:06:33.68,0:06:34.81,main,,0000,0000,0000,,Co to asi bude?
Dialogue: 0,0:06:34.81,0:06:36.06,main,,0000,0000,0000,,Otevřete ho.
Dialogue: 0,0:06:38.44,0:06:40.10,main,,0000,0000,0000,,Stejné náušnice!
Dialogue: 0,0:06:41.02,0:06:42.86,main,,0000,0000,0000,,Zvažovala jsem spoustu možností…
Dialogue: 0,0:06:42.86,0:06:44.19,main,,0000,0000,0000,,…ale měla jsem problém se rozhodnout.
Dialogue: 0,0:06:44.73,0:06:47.36,main,,0000,0000,0000,,A tady jsou dárky ode mne!
Dialogue: 0,0:06:48.24,0:06:49.19,main,,0000,0000,0000,,Oh.
Dialogue: 0,0:06:49.20,0:06:49.90,main,,0000,0000,0000,,Otevřete je.
Dialogue: 0,0:06:51.24,0:06:52.66,main,,0000,0000,0000,,Popisek: Vůdce
Dialogue: 0,0:06:53.16,0:06:54.03,main,,0000,0000,0000,,Co je to?
Dialogue: 0,0:06:54.58,0:06:56.04,main,,0000,0000,0000,,Náramek brigádního velitele.
Dialogue: 0,0:06:56.04,0:06:56.95,main,,0000,0000,0000,,Vyzkoušej si ho.
Dialogue: 0,0:06:57.29,0:06:59.96,main,,0000,0000,0000,,Proč bych si ho měla nasazovat?!
Dialogue: 0,0:07:00.83,0:07:04.54,main,,0000,0000,0000,,Napadlo mně, že by ti perfektně sedl.
Dialogue: 0,0:07:04.54,0:07:06.10,main,,0000,0000,0000,,Víc by sedl k tobě.
Dialogue: 0,0:07:06.21,0:07:06.92,main,,0000,0000,0000,,Um…
Dialogue: 0,0:07:06.92,0:07:08.17,main,,0000,0000,0000,,A co je toto…
Dialogue: 0,0:07:08.45,0:07:10.00,main,,0000,0000,0000,,To Heart?!
Dialogue: 0,0:07:08.44,0:07:10.00,white,,0000,0000,0000,,(pozn. - název anime)
Dialogue: 0,0:07:10.13,0:07:12.01,main,,0000,0000,0000,,Stálo to celé jmění…
Dialogue: 0,0:07:12.01,0:07:13.34,main,,0000,0000,0000,,Opatruj ho.
Dialogue: 0,0:07:13.72,0:07:16.47,main,,0000,0000,0000,,Nesnaž se nás dostat do cosplay!
Dialogue: 0,0:07:16.47,0:07:19.06,main,,0000,0000,0000,,Od naší uniformy se skoro neliší.
Dialogue: 0,0:07:20.44,0:07:21.85,main,,0000,0000,0000,,To je docela problém.
Dialogue: 0,0:07:21.85,0:07:23.19,main,,0000,0000,0000,,Pokud je to moc velké…
Dialogue: 0,0:07:23.19,0:07:23.77,main,,0000,0000,0000,,Že?
Dialogue: 0,0:07:23.77,0:07:24.69,main,,0000,0000,0000,,To teda jo…
Dialogue: 0,0:07:24.69,0:07:26.07,main,,0000,0000,0000,,a když to navíc štípe.
Dialogue: 0,0:07:27.36,0:07:28.28,main,,0000,0000,0000,,Hej.
Dialogue: 0,0:07:28.28,0:07:29.65,main,,0000,0000,0000,,To jsou naše narozeniny.
Dialogue: 0,0:07:29.65,0:07:31.15,main,,0000,0000,0000,,Mohla by ses trochu krotit.
Dialogue: 0,0:07:31.61,0:07:34.82,main,,0000,0000,0000,,Tsukasiny koláčky jsou vždycky hrozně dobré.
Dialogue: 0,0:07:35.95,0:07:38.24,main,,0000,0000,0000,,Tentokrát jsem je pekla s ní.
Dialogue: 0,0:07:38.95,0:07:40.94,main,,0000,0000,0000,,Chtěla jsem se to naučit.
Dialogue: 0,0:07:41.83,0:07:42.83,main,,0000,0000,0000,,Co se děje?
Dialogue: 0,0:07:43.00,0:07:44.54,main,,0000,0000,0000,,Když to tak slyším…
Dialogue: 0,0:07:44.54,0:07:47.25,main,,0000,0000,0000,,…některé nechutnají, tak dobře jako jiné.
Dialogue: 0,0:07:47.25,0:07:48.38,main,,0000,0000,0000,,Divné, co?
Dialogue: 0,0:07:48.38,0:07:49.68,main,,0000,0000,0000,,Co tím myslíš?!
Dialogue: 0,0:07:49.88,0:07:51.76,main,,0000,0000,0000,,Jestlipak je tento od Kagami.
Dialogue: 0,0:07:52.51,0:07:54.59,main,,0000,0000,0000,,Můžeme si dát i dort.
Dialogue: 0,0:07:55.10,0:07:58.85,main,,0000,0000,0000,,Takže Kagami jeden den v roce vážně makala.
Dialogue: 0,0:07:58.85,0:07:59.85,main,,0000,0000,0000,,Sklapni.
Dialogue: 0,0:08:01.52,0:08:02.47,main,,0000,0000,0000,,Mimochodem…
Dialogue: 0,0:08:02.48,0:08:05.40,main,,0000,0000,0000,,…nedohadujete se vždy, jak rozkrojit dort?
Dialogue: 0,0:08:05.56,0:08:06.56,main,,0000,0000,0000,,Proč?
Dialogue: 0,0:08:06.61,0:08:07.61,main,,0000,0000,0000,,Však víš.
Dialogue: 0,0:08:07.62,0:08:09.62,main,,0000,0000,0000,,Když rozkrajuješ dort na několik dílů…
Dialogue: 0,0:08:09.63,0:08:11.90,main,,0000,0000,0000,,…jak rozdělíš jahody na vrchu…
Dialogue: 0,0:08:11.90,0:08:14.40,main,,0000,0000,0000,,…nebo rozhodneš, kdo dostane čokoládové figurky a ozdoby?
Dialogue: 0,0:08:14.57,0:08:17.28,main,,0000,0000,0000,,Moje starší sestra miluje čokoládu.
Dialogue: 0,0:08:20.50,0:08:21.50,main,,0000,0000,0000,,Zatraceně.
Dialogue: 0,0:08:21.50,0:08:23.40,main,,0000,0000,0000,,Nerozkrojila jsem ho rovnoměrně.
Dialogue: 0,0:08:23.41,0:08:26.14,main,,0000,0000,0000,,Když Kagami přizná chybu necítím vůbec žádnou lítost.
Dialogue: 0,0:08:26.14,0:08:27.11,main,,0000,0000,0000,,O čem to mluvíš?!
Dialogue: 0,0:08:27.13,0:08:28.13,main,,0000,0000,0000,,Itadakima-su.
Dialogue: 0,0:08:28.63,0:08:29.63,main,,0000,0000,0000,,Itadakimasu!
Dialogue: 0,0:08:37.55,0:08:38.55,main,,0000,0000,0000,,Co?
Dialogue: 0,0:08:41.10,0:08:42.60,main,,0000,0000,0000,,Moje jahoda je tak malá.
Dialogue: 0,0:08:46.81,0:08:48.06,main,,0000,0000,0000,,Co je s tebou?
Dialogue: 0,0:08:48.06,0:08:50.11,main,,0000,0000,0000,,Mám kousek na obličeji?
Dialogue: 0,0:08:50.11,0:08:54.03,main,,0000,0000,0000,,Právě jsem objevila další z Kagaminých půvabů.
Dialogue: 0,0:08:54.03,0:08:56.73,main,,0000,0000,0000,,Zníš přesně jako Shin-chan, tak toho nech.
Dialogue: 0,0:08:56.74,0:08:59.03,main,,0000,0000,0000,,Už jsou skoro letní prázdniny.
Dialogue: 0,0:08:59.53,0:09:01.08,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:09:01.08,0:09:03.54,main,,0000,0000,0000,,Pláž, bazén, letní festivaly…
Dialogue: 0,0:09:03.54,0:09:05.66,main,,0000,0000,0000,,Ještě nějaká typická činnost?
Dialogue: 0,0:09:06.37,0:09:08.50,main,,0000,0000,0000,,Léto je pouze o každoroční události!
Dialogue: 0,0:09:08.88,0:09:11.00,main,,0000,0000,0000,,Velké otevření bojiště!
Dialogue: 0,0:09:11.00,0:09:12.42,main,,0000,0000,0000,,Jediný a jedinečný…
Dialogue: 0,0:09:12.42,0:09:13.63,main,,0000,0000,0000,,Comiket!
Dialogue: 0,0:09:14.42,0:09:15.67,main,,0000,0000,0000,,Ne, ne, ne, ne, ne.
Dialogue: 0,0:09:15.68,0:09:17.09,main,,0000,0000,0000,,To se týká pouze tebe.
Dialogue: 0,0:09:17.09,0:09:18.43,main,,0000,0000,0000,,To je něco jako kriket?
Dialogue: 0,0:09:18.43,0:09:19.60,main,,0000,0000,0000,,N-No…
Dialogue: 0,0:09:19.60,0:09:21.00,main,,0000,0000,0000,,Každý dělá něco jiného.
Dialogue: 0,0:09:22.00,0:09:23.77,main,,0000,0000,0000,,Jako navštěvování příbuzných a tak.
Dialogue: 0,0:09:23.77,0:09:25.02,main,,0000,0000,0000,,Dobré ráno.
Dialogue: 0,0:09:26.44,0:09:27.64,main,,0000,0000,0000,,Miyuki…
Dialogue: 0,0:09:27.65,0:09:30.31,main,,0000,0000,0000,,…děláváš něco ráda v létě, něco zvláštního?
Dialogue: 0,0:09:30.32,0:09:31.57,main,,0000,0000,0000,,No, tak…
Dialogue: 0,0:09:32.48,0:09:35.48,main,,0000,0000,0000,,Letos budu zase na dovolené v cizině.
Dialogue: 0,0:09:36.38,0:09:37.28,main,,0000,0000,0000,,Zase?
Dialogue: 0,0:09:37.28,0:09:38.28,main,,0000,0000,0000,,Řekla zase ?!
Dialogue: 0,0:09:38.28,0:09:40.07,main,,0000,0000,0000,,Zatracená buržoázistka!
Dialogue: 0,0:09:40.08,0:09:41.48,main,,0000,0000,0000,,To je něco jako jogurt?
Dialogue: 0,0:09:42.04,0:09:45.00,main,,0000,0000,0000,,Říkala jsi, že jsi začala dělat v costplay kavárně…
Dialogue: 0,0:09:45.00,0:09:49.00,main,,0000,0000,0000,,Nemusíš na to mít extra dobrou figuru?
Dialogue: 0,0:09:49.00,0:09:49.96,main,,0000,0000,0000,,No…
Dialogue: 0,0:09:49.96,0:09:53.46,main,,0000,0000,0000,,docela jsem se strachovala, protože nemám zrovna velká prsa…
Dialogue: 0,0:09:53.46,0:09:54.96,main,,0000,0000,0000,,…ale v jedné hře bylo kdysi řečeno…
Dialogue: 0,0:09:54.96,0:09:56.76,main,,0000,0000,0000,,„Malá prsa jsou symbol...“
Dialogue: 0,0:09:56.76,0:09:58.34,main,,0000,0000,0000,,„Vzácnost!“
Dialogue: 0,0:09:58.34,0:09:59.47,main,,0000,0000,0000,,Slovo od slova.
Dialogue: 0,0:10:01.00,0:10:02.50,main,,0000,0000,0000,,A když o tom přemýšlíš…
Dialogue: 0,0:10:02.50,0:10:04.85,main,,0000,0000,0000,,…je tu poptávka, že?
Dialogue: 0,0:10:04.85,0:10:06.31,main,,0000,0000,0000,,Takže jsem dost cenná!
Dialogue: 0,0:10:06.77,0:10:10.35,main,,0000,0000,0000,,Zajímalo by mě, jak ji text ze hry může dodat tolik sebevědomí.
Dialogue: 0,0:10:10.36,0:10:12.26,main,,0000,0000,0000,,Takže já jsem asi taky v pořádku.
Dialogue: 0,0:10:12.48,0:10:13.38,main,,0000,0000,0000,,Páni…
Dialogue: 0,0:10:13.38,0:10:15.07,main,,0000,0000,0000,,Blíží se testy.
Dialogue: 0,0:10:15.07,0:10:17.28,main,,0000,0000,0000,,Myslíš, že je dobré tak moc hrát?
Dialogue: 0,0:10:18.49,0:10:21.70,main,,0000,0000,0000,,Kdybys tak použila něco z toho nadšení pro studování.
Dialogue: 0,0:10:21.70,0:10:24.03,main,,0000,0000,0000,,Hned bys měla lepší známky.
Dialogue: 0,0:10:26.25,0:10:28.54,main,,0000,0000,0000,,Když je tak lehce ovlivnitelná…
Dialogue: 0,0:10:28.54,0:10:32.08,main,,0000,0000,0000,,…nestal by se z ní génius, kdyby někdo vydal hru propagující studování?
Dialogue: 0,0:10:32.79,0:10:35.00,main,,0000,0000,0000,,Na DS mají přece Moe Drill.
Dialogue: 0,0:10:32.79,0:10:35.00,comments,,0000,0000,0000,,
Dialogue: 0,0:10:35.30,0:10:35.92,main,,0000,0000,0000,,Co to?
Dialogue: 0,0:10:36.72,0:10:39.30,main,,0000,0000,0000,,Poslední dobou je spousta zločinů spáchaných nezletilými.
Dialogue: 0,0:10:39.80,0:10:42.05,main,,0000,0000,0000,,A vždy když se to stane…
Dialogue: 0,0:10:42.05,0:10:43.30,main,,0000,0000,0000,,…všichni sousedé říkají…
Dialogue: 0,0:10:43.81,0:10:45.14,main,,0000,0000,0000,,Bylo to tak milé dítě.
Dialogue: 0,0:10:45.14,0:10:48.27,main,,0000,0000,0000,,Nikdy bych si nedovedla představit, že by udělal něco takového.
Dialogue: 0,0:10:48.27,0:10:48.98,main,,0000,0000,0000,,No…
Dialogue: 0,0:10:48.98,0:10:53.11,main,,0000,0000,0000,,To jen znamená, že nemůžeš někoho posuzovat podle zevnějšku.
Dialogue: 0,0:10:54.94,0:10:57.40,main,,0000,0000,0000,,Pokud by jsi někdy spáchala zločin…
Dialogue: 0,0:10:57.40,0:11:00.20,main,,0000,0000,0000,,Vždy jsem si byla jistá, že něco takového udělá.
Dialogue: 0,0:11:00.74,0:11:02.61,main,,0000,0000,0000,,Řekla bych to celému světu.
Dialogue: 0,0:11:05.58,0:11:07.08,main,,0000,0000,0000,,Jsem unavená…
Dialogue: 0,0:11:08.00,0:11:10.91,main,,0000,0000,0000,,Je tak vedro, že se mi nechce nic dělat.
Dialogue: 0,0:11:12.00,0:11:14.84,main,,0000,0000,0000,,Chápu, jak se cítíš…
Dialogue: 0,0:11:15.42,0:11:19.26,main,,0000,0000,0000,,…ale tobě se nikdy nechce nic dělat.
Dialogue: 0,0:11:31.27,0:11:33.27,main,,0000,0000,0000,,Ahoj, Kagami.
Dialogue: 0,0:11:33.27,0:11:34.36,main,,0000,0000,0000,,Čao.
Dialogue: 0,0:11:34.36,0:11:36.32,main,,0000,0000,0000,,Tak jak pokračuješ s učením na testy?
Dialogue: 0,0:11:37.78,0:11:39.74,main,,0000,0000,0000,,Uklízela jsem si svůj pokoj.
Dialogue: 0,0:11:40.53,0:11:45.12,main,,0000,0000,0000,,Znáš ten pocit, že musíš začít uklízet, když jsi pod tlakem?
Dialogue: 0,0:11:45.12,0:11:46.20,main,,0000,0000,0000,,Rozumím…
Dialogue: 0,0:11:46.20,0:11:47.30,main,,0000,0000,0000,,…ale raději se uč.
Dialogue: 0,0:11:47.95,0:11:48.85,main,,0000,0000,0000,,Fajn!
Dialogue: 0,0:11:48.91,0:11:50.66,main,,0000,0000,0000,,Tak se do toho dáme!
Dialogue: 0,0:11:56.67,0:11:59.00,main,,0000,0000,0000,,Dám si třicet minut pauzu.
Dialogue: 0,0:11:59.01,0:12:01.11,main,,0000,0000,0000,,Začínám ztrácet koncentraci.
Dialogue: 0,0:12:06.51,0:12:07.64,main,,0000,0000,0000,,Už je čas.
Dialogue: 0,0:12:09.31,0:12:10.81,main,,0000,0000,0000,,Tak ještě dalších třicet minut.
Dialogue: 0,0:12:10.81,0:12:12.18,main,,0000,0000,0000,,Ještě třicet minut…
Dialogue: 0,0:12:19.03,0:12:21.94,main,,0000,0000,0000,,Nemá teď začít Haruhi?
Dialogue: 0,0:12:25.28,0:12:29.33,main,,0000,0000,0000,,Těma dlouhejma scénama s Nagato, to pěkně protahují.
Dialogue: 0,0:12:29.95,0:12:33.58,main,,0000,0000,0000,,Ta Yu** není poslední dobou moc úžasná.
Dialogue: 0,0:12:38.34,0:12:40.96,main,,0000,0000,0000,,Tady taky už nikdo není.
Dialogue: 0,0:12:42.67,0:12:43.80,main,,0000,0000,0000,,To jsou už dvě?
Dialogue: 0,0:12:43.80,0:12:46.05,main,,0000,0000,0000,,Dnes se mi učit vážně nechce.
Dialogue: 0,0:12:46.05,0:12:48.26,main,,0000,0000,0000,,Půjdu spát a učit se budu ráno.
Dialogue: 0,0:12:50.27,0:12:52.31,main,,0000,0000,0000,,Když vstanu v pět hodin…
Dialogue: 0,0:12:52.31,0:12:54.73,main,,0000,0000,0000,,…tak můžu spát tři hodiny.
Dialogue: 0,0:13:11.04,0:13:16.00,main,,0000,0000,0000,,Včera jsem nakonec zabíjela čas čtením mangy a sledováním anime.
Dialogue: 0,0:13:16.00,0:13:18.03,main,,0000,0000,0000,,To zní, jako něco co děláš normálně.
Dialogue: 0,0:13:18.04,0:13:19.14,main,,0000,0000,0000,,Tak mě napadá…
Dialogue: 0,0:13:19.14,0:13:22.71,main,,0000,0000,0000,,Máš docela zvláštní vkus na mangu a hry.
Dialogue: 0,0:13:22.71,0:13:24.30,main,,0000,0000,0000,,Čteš mangu zaměřenou na kluky…
Dialogue: 0,0:13:24.30,0:13:25.93,main,,0000,0000,0000,,…a hraješ hlavně seznamovací hry pro kluky.
Dialogue: 0,0:13:25.93,0:13:29.51,main,,0000,0000,0000,,Můj otec hodně četl mangu a hrál hry…
Dialogue: 0,0:13:29.51,0:13:31.72,main,,0000,0000,0000,,…tak to mám asi po něm.
Dialogue: 0,0:13:31.72,0:13:33.68,main,,0000,0000,0000,,Už od mala jsem ho sledovala.
Dialogue: 0,0:13:34.56,0:13:36.39,main,,0000,0000,0000,,Rozumím té manze…
Dialogue: 0,0:13:36.39,0:13:39.06,main,,0000,0000,0000,,…ale hrát seznamovací hry před svou dcerou?
Dialogue: 0,0:13:39.06,0:13:40.61,main,,0000,0000,0000,,Co je to za otce?
Dialogue: 0,0:13:41.78,0:13:43.36,main,,0000,0000,0000,,To vám máma nic neřekne,
Dialogue: 0,0:13:43.36,0:13:45.57,main,,0000,0000,0000,,když tak pořád hrajete nebo sledujete anime?
Dialogue: 0,0:13:46.15,0:13:48.15,main,,0000,0000,0000,,Já nemám mámu.
Dialogue: 0,0:13:49.28,0:13:52.16,main,,0000,0000,0000,,Zemřela, když jsem byla malá.
Dialogue: 0,0:13:53.29,0:13:54.62,main,,0000,0000,0000,,Tak to je…
Dialogue: 0,0:13:55.16,0:13:57.46,main,,0000,0000,0000,,Takže doma dělám domácí práce…
Dialogue: 0,0:13:57.46,0:13:59.29,main,,0000,0000,0000,,…a proto jsem vnich lepší než Kagami.
Dialogue: 0,0:13:59.71,0:14:01.54,main,,0000,0000,0000,,Neříkej takové věci…
Dialogue: 0,0:14:01.54,0:14:02.88,main,,0000,0000,0000,,…když není způsob jak to dokázat.
Dialogue: 0,0:14:03.55,0:14:05.42,main,,0000,0000,0000,,Tsukasa je také dobrá v pečení, že?
Dialogue: 0,0:14:05.97,0:14:08.18,main,,0000,0000,0000,,Nevím, jestli bych řekla, že jsem dobrá…
Dialogue: 0,0:14:08.18,0:14:08.72,main,,0000,0000,0000,,…ale zbožňuji to!
Dialogue: 0,0:14:09.22,0:14:11.68,main,,0000,0000,0000,,Zajímalo by mě, jak dobrá je Miyuki-san.
Dialogue: 0,0:14:11.68,0:14:12.97,main,,0000,0000,0000,,Vypadá bohatě…
Dialogue: 0,0:14:12.97,0:14:14.93,main,,0000,0000,0000,,…takže možná není na domácí práce dobrá.
Dialogue: 0,0:14:16.31,0:14:17.35,main,,0000,0000,0000,,Vaření?
Dialogue: 0,0:14:18.14,0:14:19.35,main,,0000,0000,0000,,Abych řekla pravdu…
Dialogue: 0,0:14:19.35,0:14:22.15,main,,0000,0000,0000,,Pomáhám pouze čas od času…
Dialogue: 0,0:14:22.15,0:14:25.28,main,,0000,0000,0000,,…takže v tom nejsem moc dobrá.
Dialogue: 0,0:14:25.28,0:14:26.78,main,,0000,0000,0000,,Je to trochu trapné.
Dialogue: 0,0:14:26.78,0:14:27.74,main,,0000,0000,0000,,Oh! Já to věděla.
Dialogue: 0,0:14:28.91,0:14:30.81,main,,0000,0000,0000,,Ale jsi tak dobrá ve všem jiném…
Dialogue: 0,0:14:30.81,0:14:32.91,main,,0000,0000,0000,,…takže se dá předpokládat, že tvoje standarty jsou trochu jinde.
Dialogue: 0,0:14:32.91,0:14:36.04,main,,0000,0000,0000,,Nemůžeš si myslet, že je v tom špatná jen proto, že to říká ona.
Dialogue: 0,0:14:37.16,0:14:38.08,main,,0000,0000,0000,,Chápu…
Dialogue: 0,0:14:38.08,0:14:39.25,main,,0000,0000,0000,,Ne, to není pravda…
Dialogue: 0,0:14:39.79,0:14:42.38,main,,0000,0000,0000,,A co pak naše učitelka?
Dialogue: 0,0:14:42.88,0:14:44.46,main,,0000,0000,0000,,Slyšela jsem, že žije sama…
Dialogue: 0,0:14:44.46,0:14:46.67,main,,0000,0000,0000,,…ale vypadá, že by mohla být flákač.
Dialogue: 0,0:14:48.47,0:14:49.76,main,,0000,0000,0000,,Dneska…
Dialogue: 0,0:14:49.76,0:14:52.80,main,,0000,0000,0000,,…můžeš v klidu přežít i bez vaření!
Dialogue: 0,0:14:52.80,0:14:53.85,main,,0000,0000,0000,,Máme tu obchody a tak…
Dialogue: 0,0:14:55.85,0:14:57.22,main,,0000,0000,0000,,Jen žertuji!
Dialogue: 0,0:14:57.23,0:14:58.81,main,,0000,0000,0000,,Umím vařit.
Dialogue: 0,0:14:58.81,0:15:01.10,main,,0000,0000,0000,,Asi tak jako průměrný člověk.
Dialogue: 0,0:15:01.10,0:15:03.90,main,,0000,0000,0000,,Prosím nedělejte vtipy, které znějí jako skutečnost.
Dialogue: 0,0:15:07.32,0:15:09.53,main,,0000,0000,0000,,Tsukasa má pořád můj slovník.
Dialogue: 0,0:15:10.24,0:15:11.16,main,,0000,0000,0000,,Tsukaso.
Dialogue: 0,0:15:11.16,0:15:12.87,main,,0000,0000,0000,,Promiň. Mohla bys mi vrátit můj slovník?
Dialogue: 0,0:15:15.79,0:15:19.20,main,,0000,0000,0000,,Trošku jsem pro rozptýlení uklízela…
Dialogue: 0,0:15:19.21,0:15:19.91,main,,0000,0000,0000,,…jej…
Dialogue: 0,0:15:19.91,0:15:20.71,main,,0000,0000,0000,,Ty, taky?
Dialogue: 0,0:15:22.79,0:15:23.63,main,,0000,0000,0000,,Čao.
Dialogue: 0,0:15:23.63,0:15:25.04,main,,0000,0000,0000,,Co je?
Dialogue: 0,0:15:25.05,0:15:30.09,main,,0000,0000,0000,,Lidi z práce se snaží na léto vymyslet nové menu.
Dialogue: 0,0:15:30.76,0:15:32.43,main,,0000,0000,0000,,Takže ty děláš i takové věci.
Dialogue: 0,0:15:32.43,0:15:34.85,main,,0000,0000,0000,,Počkat, ty ses přestala učit na test?
Dialogue: 0,0:15:34.85,0:15:37.40,main,,0000,0000,0000,,Ostré jídlo je dobré, když je venku horko…
Dialogue: 0,0:15:37.43,0:15:40.56,main,,0000,0000,0000,,…tak co třeba super pálivý ramen?!
Dialogue: 0,0:15:37.42,0:15:40.56,white,,0000,0000,0000,,(Pozn. - druh nudlí)
Dialogue: 0,0:15:40.73,0:15:43.83,main,,0000,0000,0000,,Spíš něco, na co normálně narazíš v kavárně.
Dialogue: 0,0:15:43.90,0:15:46.40,main,,0000,0000,0000,,Tak potom super pálivý parfait!
Dialogue: 0,0:15:43.89,0:15:46.40,white,,0000,0000,0000,,(Pozn. - druh zmrzliny)
Dialogue: 0,0:15:46.40,0:15:49.44,main,,0000,0000,0000,,Přestaň vypouštět tyhle impulzivní nápady.
Dialogue: 0,0:15:53.95,0:15:55.87,main,,0000,0000,0000,,Nemůžu stát…
Dialogue: 0,0:15:59.45,0:16:03.42,main,,0000,0000,0000,,Proč nemáme na škole hodiny plavání?
Dialogue: 0,0:16:03.42,0:16:05.17,main,,0000,0000,0000,,Máme bazén…
Dialogue: 0,0:16:05.17,0:16:08.05,main,,0000,0000,0000,,Ve skutečnosti, jsem docela ráda.
Dialogue: 0,0:16:09.05,0:16:10.09,main,,0000,0000,0000,,Proč?
Dialogue: 0,0:16:10.09,0:16:11.26,main,,0000,0000,0000,,Abych byla upřímná…
Dialogue: 0,0:16:11.26,0:16:13.59,main,,0000,0000,0000,,Pod vodou nedokážu otevřít oči…
Dialogue: 0,0:16:13.59,0:16:15.59,main,,0000,0000,0000,,…takže mi plavání moc nejde.
Dialogue: 0,0:16:16.14,0:16:16.97,main,,0000,0000,0000,,Je to trapné.
Dialogue: 0,0:16:17.51,0:16:20.02,main,,0000,0000,0000,,Jak já bych si přála mít tvé „slabosti“.
Dialogue: 0,0:16:23.48,0:16:26.15,main,,0000,0000,0000,,Konečně začne Letní počítačový festival!
Dialogue: 0,0:16:27.57,0:16:28.19,main,,0000,0000,0000,,To je pravda…
Dialogue: 0,0:16:29.03,0:16:32.11,main,,0000,0000,0000,,To proto jsem začala pracovat…
Dialogue: 0,0:16:33.15,0:16:34.74,main,,0000,0000,0000,,Festival…
Dialogue: 0,0:16:34.74,0:16:37.49,main,,0000,0000,0000,,Tolik nostalgie!
Dialogue: 0,0:16:37.49,0:16:39.33,main,,0000,0000,0000,,Co, že vytahuješ staré vzpomínky?
Dialogue: 0,0:16:39.83,0:16:40.74,main,,0000,0000,0000,,No…
Dialogue: 0,0:16:40.75,0:16:43.66,main,,0000,0000,0000,,Když si s tebou začnou zákazníci povídat…
Dialogue: 0,0:16:43.67,0:16:47.42,main,,0000,0000,0000,,nemáš taky pocit, že jsou všichni najednou natěšení na festival?
Dialogue: 0,0:16:47.42,0:16:51.21,main,,0000,0000,0000,,Použij trochu té vášně k přípravě na zkoušky.
Dialogue: 0,0:16:56.64,0:17:00.72,main,,0000,0000,0000,,Nemůžu usnout.
Dialogue: 0,0:17:01.35,0:17:03.77,main,,0000,0000,0000,,Asi si přečtu knihu, kterou jsem si koupila.
Dialogue: 0,0:17:15.53,0:17:16.86,main,,0000,0000,0000,,Nejen, že jsem neusnula…
Dialogue: 0,0:17:16.87,0:17:17.91,main,,0000,0000,0000,,…už je dokonce ráno…
Dialogue: 0,0:17:18.99,0:17:20.20,main,,0000,0000,0000,,…a teprve teď…
Dialogue: 0,0:17:20.20,0:17:22.24,main,,0000,0000,0000,,…začínám usínat…
Dialogue: 0,0:17:22.25,0:17:24.75,phone,,0000,0000,0000,,Přepněte prosím Váš mobilní telefon do tichého režimu a snažte se vyvarovat hovorům.
Dialogue: 0,0:17:24.75,0:17:26.04,main,,0000,0000,0000,,Crrrrr…
Dialogue: 0,0:17:26.54,0:17:30.63,main,,0000,0000,0000,,Dokážu pochopit, proč mám vypnout mobil…
Dialogue: 0,0:17:30.63,0:17:32.34,main,,0000,0000,0000,,například v nemocnici a tak.
Dialogue: 0,0:17:33.55,0:17:37.55,main,,0000,0000,0000,,Ale proč ho nemůžu používat ve vlaku?
Dialogue: 0,0:17:37.55,0:17:38.51,main,,0000,0000,0000,,Protože…
Dialogue: 0,0:17:38.51,0:17:41.97,main,,0000,0000,0000,,…by jsi tím rušila ostatní lidi?
Dialogue: 0,0:17:42.77,0:17:43.93,main,,0000,0000,0000,,Ale…
Dialogue: 0,0:17:43.93,0:17:47.85,main,,0000,0000,0000,,Ve vlaku se spolu lidé normálně baví, takže už hlučné jsou.
Dialogue: 0,0:17:47.85,0:17:49.86,main,,0000,0000,0000,,A to zahrnuje muže a ženy všeho věku.
Dialogue: 0,0:17:49.86,0:17:51.61,main,,0000,0000,0000,,To je asi pravda…
Dialogue: 0,0:17:53.74,0:17:56.95,main,,0000,0000,0000,,Budeš během letního festivalu pracovat ve svatyni, jako na Nový rok?
Dialogue: 0,0:17:57.36,0:17:58.16,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:17:58.16,0:18:00.20,main,,0000,0000,0000,,V létě nepracuji.
Dialogue: 0,0:18:02.49,0:18:03.33,main,,0000,0000,0000,,Ah, jo…
Dialogue: 0,0:18:03.33,0:18:05.54,main,,0000,0000,0000,,už bude zase to období, co?
Dialogue: 0,0:18:05.54,0:18:06.29,main,,0000,0000,0000,,Heh?
Dialogue: 0,0:18:06.29,0:18:08.37,main,,0000,0000,0000,,Nedržela jsi dietu?
Dialogue: 0,0:18:08.63,0:18:09.38,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:18:09.38,0:18:11.50,main,,0000,0000,0000,,Možná je to kvůli všemu tomu studování…
Dialogue: 0,0:18:11.50,0:18:14.46,main,,0000,0000,0000,,ale když jsem se včera vážila tak jsem měla méně.
Dialogue: 0,0:18:14.46,0:18:16.55,main,,0000,0000,0000,,Takže je to nakonec asi dobrý.
Dialogue: 0,0:18:16.55,0:18:17.50,main,,0000,0000,0000,,To bylo...
Dialogue: 0,0:18:17.50,0:18:18.76,main,,0000,0000,0000,,spíš jako výmluva..!
Dialogue: 0,0:18:20.30,0:18:21.39,main,,0000,0000,0000,,Dobré ráno!
Dialogue: 0,0:18:22.14,0:18:23.56,main,,0000,0000,0000,,Dobré ráno, Kona-chan!
Dialogue: 0,0:18:24.27,0:18:25.10,main,,0000,0000,0000,,Heh?
Dialogue: 0,0:18:25.10,0:18:26.77,main,,0000,0000,0000,,Co je s Kagami?
Dialogue: 0,0:18:26.77,0:18:27.94,main,,0000,0000,0000,,Konato!
Dialogue: 0,0:18:28.85,0:18:29.81,main,,0000,0000,0000,,Moje váha
Dialogue: 0,0:18:30.36,0:18:31.98,main,,0000,0000,0000,,zase jsem přibrala, jako mrknutím oka…
Dialogue: 0,0:18:32.48,0:18:35.86,main,,0000,0000,0000,,To proto, že jsi se přestala hlídat a začala jsi mlsat.
Dialogue: 0,0:18:35.86,0:18:37.11,main,,0000,0000,0000,,Počkat, to byl verš?
Dialogue: 0,0:18:38.00,0:18:42.00,newspaper,,0000,0000,0000,,{\pos(342,194)}Moje váha
Dialogue: 0,0:18:39.30,0:18:42.00,newspaper,,0000,0000,0000,,{\pos(342,247)}zase jsem přibrala
Dialogue: 0,0:18:41.00,0:18:42.00,newspaper,,0000,0000,0000,,{\pos(342,298)}jako mrknutím oka
Dialogue: 0,0:18:42.70,0:18:44.60,main,,0000,0000,0000,,Jedla… jedla jsem jen trochu…
Dialogue: 0,0:18:44.60,0:18:48.79,main,,0000,0000,0000,,Kilogram nebo dva stejně nemají vliv na to jak vypadáš.
Dialogue: 0,0:18:48.79,0:18:52.08,main,,0000,0000,0000,,Proč jsi tak nadšená nebo rozzuřená nad každým kilogramem?
Dialogue: 0,0:18:54.92,0:18:57.34,main,,0000,0000,0000,,To proto jsou ženy tak zoufalé.
Dialogue: 0,0:18:57.34,0:18:59.76,main,,0000,0000,0000,,Myslela jsem, že ty jsi taky žena, ne?
Dialogue: 0,0:18:59.76,0:19:02.76,copyrights,,0000,0000,0000,,Korektury - Xenootaku
Dialogue: 0,0:18:59.76,0:19:02.76,copyrights,,0000,0000,0000,,Překlad - paralaX
Dialogue: 0,0:18:59.76,0:19:02.76,copyrights,,0000,0000,0000,,Domovská stránka - www.otakogu.cz
Dialogue: 0,0:19:05.81,0:19:07.22,main,,0000,0000,0000,,Hodně štěstí!
Dialogue: 0,0:19:07.68,0:19:10.94,main,,0000,0000,0000,,Vítejte u další epizody Lucky Channelu.
Dialogue: 0,0:19:10.94,0:19:13.06,main,,0000,0000,0000,,Měli jste se dobře?
Dialogue: 0,0:19:13.06,0:19:17.15,main,,0000,0000,0000,,Já jsem Akira Kogami, vždy na 120%!
Dialogue: 0,0:19:17.15,0:19:17.61,main,,0000,0000,0000,,Dnes…
Dialogue: 0,0:19:17.61,0:19:18.32,main,,0000,0000,0000,,Nyoron!
Dialogue: 0,0:19:18.32,0:19:18.94,main,,0000,0000,0000,,Jen žertuji!
Dialogue: 0,0:19:18.95,0:19:20.90,main,,0000,0000,0000,,Jsem asistent, Minoru Shiraishi.
Dialogue: 0,0:19:22.37,0:19:22.78,main,,0000,0000,0000,,Hej.
Dialogue: 0,0:19:23.58,0:19:24.03,main,,0000,0000,0000,,Co?
Dialogue: 0,0:19:24.03,0:19:25.91,main,,0000,0000,0000,,Nepřerušuj mě!
Dialogue: 0,0:19:26.45,0:19:27.83,main,,0000,0000,0000,,Omlouvám se!
Dialogue: 0,0:19:27.83,0:19:29.29,main,,0000,0000,0000,,Tohle je už trochu moc.
Dialogue: 0,0:19:29.29,0:19:31.67,main,,0000,0000,0000,,Snaž se trochu víc.
Dialogue: 0,0:19:31.67,0:19:33.63,main,,0000,0000,0000,,Chceš snad, aby tě nahradili?
Dialogue: 0,0:19:34.17,0:19:34.96,main,,0000,0000,0000,,Mockrát se omlouvám!
Dialogue: 0,0:19:34.96,0:19:35.46,main,,0000,0000,0000,,Budu se moc snažit!
Dialogue: 0,0:19:35.46,0:19:37.76,main,,0000,0000,0000,,Dám do toho vše!
Dialogue: 0,0:19:38.42,0:19:40.10,main,,0000,0000,0000,,Fajn! Dnes…
Dialogue: 0,0:19:40.18,0:19:41.34,main,,0000,0000,0000,,Ta-Dá!
Dialogue: 0,0:19:41.34,0:19:44.85,main,,0000,0000,0000,,Máme pro všechny velkou zprávu!
Dialogue: 0,0:19:45.43,0:19:47.68,main,,0000,0000,0000,,Publikováno v listopadovém vydání Comptiqu…
Dialogue: 0,0:19:47.68,0:19:49.73,main,,0000,0000,0000,,Projekt Módní Akira!
Dialogue: 0,0:19:50.19,0:19:54.19,main,,0000,0000,0000,,Už jsem od všech obdržela takovou spoustu polednic!
Dialogue: 0,0:19:54.19,0:19:54.86,main,,0000,0000,0000,,Eh.
Dialogue: 0,0:19:54.86,0:19:58.78,main,,0000,0000,0000,,Myslím, že v tom projektu byla zapojena i další osoba…
Dialogue: 0,0:19:59.49,0:20:01.74,main,,0000,0000,0000,,Děkuji za všechnu odezvu!
Dialogue: 0,0:20:01.74,0:20:04.41,main,,0000,0000,0000,,Jako Nori-P!
Dialogue: 0,0:20:04.41,0:20:06.74,main,,0000,0000,0000,,Nemyslím si, že všichni budou rozumět vtipu z osmdesátých let…
Dialogue: 0,0:20:06.74,0:20:11.83,main,,0000,0000,0000,,Tak a teď vám představím nápad, který byl vybrán, před natáčením.
Dialogue: 0,0:20:11.83,0:20:12.59,main,,0000,0000,0000,,Třeba tento.
Dialogue: 0,0:20:12.92,0:20:13.62,main,,0000,0000,0000,,Um…
Dialogue: 0,0:20:13.63,0:20:16.88,main,,0000,0000,0000,,Tento je od Miss Cherry ze Satte, Saitama.
Dialogue: 0,0:20:16.88,0:20:18.42,main,,0000,0000,0000,,Projekt Módní Akira!
Dialogue: 0,0:20:20.67,0:20:23.80,main,,0000,0000,0000,,Kagaminy culíky vypadají skvěle.
Dialogue: 0,0:20:23.80,0:20:26.89,main,,0000,0000,0000,,Co kdyby i Akira-san měla culíky?
Dialogue: 0,0:20:26.89,0:20:28.01,main,,0000,0000,0000,,Culíky, jo?
Dialogue: 0,0:20:28.01,0:20:28.35,main,,0000,0000,0000,,To zní!
Dialogue: 0,0:20:29.02,0:20:30.10,main,,0000,0000,0000,,Báječně!
Dialogue: 0,0:20:30.60,0:20:31.98,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:20:31.98,0:20:33.85,main,,0000,0000,0000,,Vlasy jsou důležitá část ženského života!
Dialogue: 0,0:20:33.85,0:20:35.35,main,,0000,0000,0000,,Když je rozdělím na dva…
Dialogue: 0,0:20:35.36,0:20:38.90,main,,0000,0000,0000,,…mohly by vypadat úžasně…
Dialogue: 0,0:20:38.90,0:20:40.11,main,,0000,0000,0000,,O tom není pochyb.
Dialogue: 0,0:20:40.11,0:20:43.32,main,,0000,0000,0000,,Myslím si ale, že s tvojí délkou vlasů to nepůjde…
Dialogue: 0,0:20:43.32,0:20:44.49,main,,0000,0000,0000,,Víš ale…
Dialogue: 0,0:20:44.49,0:20:46.98,main,,0000,0000,0000,,…ne že by dlouhé vlasy byly nějak zvlášť dobrá věc.
Dialogue: 0,0:20:46.78,0:20:47.57,main2,,0000,0000,0000,,Akira-samo.
Dialogue: 0,0:20:47.58,0:20:48.33,main,,0000,0000,0000,,Je to vidět.
Dialogue: 0,0:20:48.45,0:20:50.45,main,,0000,0000,0000,,Váží to spoustu…
Dialogue: 0,0:20:49.04,0:20:50.16,main2,,0000,0000,0000,,Vážně to jde vidět.
Dialogue: 0,0:20:50.16,0:20:50.95,main2,,0000,0000,0000,,Akira-san, jen trochu…
Dialogue: 0,0:20:50.45,0:20:53.04,main,,0000,0000,0000,,Je práce se o ně starat.
Dialogue: 0,0:20:53.04,0:20:55.54,main,,0000,0000,0000,,A krom toho, teď nejsou v módě.
Dialogue: 0,0:20:55.54,0:20:58.54,main,,0000,0000,0000,,Kluci pořád mluví o tom, jak se jim líbí na ženách dlouhé vlasy…
Dialogue: 0,0:20:58.55,0:21:03.59,main,,0000,0000,0000,,…ale vůbec netuší kolik času snahy a peněz stojí o ně pečovat.
Dialogue: 0,0:20:59.05,0:21:01.13,main2,,0000,0000,0000,,Teď mluvíte o sobě.
Dialogue: 0,0:21:01.13,0:21:01.71,main2,,0000,0000,0000,,Eh…
Dialogue: 0,0:21:01.72,0:21:03.59,main2,,0000,0000,0000,,Vypadla jste z role.
Dialogue: 0,0:21:03.59,0:21:06.97,main,,0000,0000,0000,,A tu snahu nikdo, nikdy neocení…
Dialogue: 0,0:21:03.59,0:21:04.72,main2,,0000,0000,0000,,Už nejste Akira-san.
Dialogue: 0,0:21:04.72,0:21:05.68,main2,,0000,0000,0000,,Posloucháte mě?
Dialogue: 0,0:21:05.68,0:21:06.47,main2,,0000,0000,0000,,Haló?
Dialogue: 0,0:21:08.47,0:21:09.97,main,,0000,0000,0000,,A přišel čas se rozloučit!
Dialogue: 0,0:21:10.64,0:21:14.39,main,,0000,0000,0000,,A co se týká Akiri s culíky…
Dialogue: 0,0:21:14.39,0:21:18.40,main,,0000,0000,0000,,víc se dozvíte na naší webové stránce anebo v rádiu!
Dialogue: 0,0:21:18.40,0:21:19.48,main,,0000,0000,0000,,Na viděnou!
Dialogue: 0,0:21:19.98,0:21:22.19,main,,0000,0000,0000,,Budu se na vás těšit!
Dialogue: 0,0:21:22.19,0:21:22.99,main,,0000,0000,0000,,Uvidíme se příště!
Dialogue: 0,0:21:22.49,0:21:24.99,main2,,0000,0000,0000,,Tak se prosím těšte na příští díl.
Dialogue: 0,0:21:26.28,0:21:27.61,main,,0000,0000,0000,,Cos to před tím předváděl?
Dialogue: 0,0:21:28.16,0:21:28.99,main,,0000,0000,0000,,Co tím myslíte…
Dialogue: 0,0:21:28.99,0:21:31.04,main,,0000,0000,0000,,Pořád jsi mě přerušoval.
Dialogue: 0,0:21:31.04,0:21:33.45,main,,0000,0000,0000,,To tě tak naštvalo, že jsem tě na začátku nezmínila?
Dialogue: 0,0:21:33.46,0:21:34.50,main,,0000,0000,0000,,Ne! Já jen…
Dialogue: 0,0:21:34.50,0:21:37.30,main,,0000,0000,0000,,…jen mne napadlo, že dnes nikdo nebude vědět co Nori-P je…
Dialogue: 0,0:21:39.09,0:21:39.50,main,,0000,0000,0000,,Ahh…
Dialogue: 0,0:21:39.50,0:21:39.88,main,,0000,0000,0000,,Odpusťte mi.
Dialogue: 0,0:21:39.88,0:21:40.13,main,,0000,0000,0000,,Uh…
Dialogue: 0,0:21:40.13,0:21:41.42,main,,0000,0000,0000,,Proč nezačneme znovu?
Dialogue: 0,0:21:41.42,0:21:42.13,main,,0000,0000,0000,,Mimochodem…
Dialogue: 0,0:21:42.13,0:21:42.63,main,,0000,0000,0000,,Ano.
Dialogue: 0,0:21:42.63,0:21:44.60,main,,0000,0000,0000,,Slyšela jsem, že dělají konkurz na Minoru Shiraishiho.
Dialogue: 0,0:21:44.60,0:21:45.26,main,,0000,0000,0000,,Vážně?!
Dialogue: 0,0:21:45.26,0:21:46.05,main,,0000,0000,0000,,To slyším poprvé!
Dialogue: 0,0:21:46.05,0:21:47.01,main,,0000,0000,0000,,Můžeš to zkusit, jestli chceš.
Dialogue: 0,0:21:47.01,0:21:47.97,main,,0000,0000,0000,,Zkusit? Já?
Dialogue: 0,0:21:47.47,0:21:48.51,main2,,0000,0000,0000,,Nejsi rád?
Dialogue: 0,0:21:48.51,0:21:49.89,main,,0000,0000,0000,,Ale já jsem on!
Dialogue: 0,0:21:51.35,0:21:51.97,main,,0000,0000,0000,,Cože?
Dialogue: 0,0:21:55.56,0:21:57.14,main,,0000,0000,0000,,Jsi úžasná, Kona-chan!
Dialogue: 0,0:21:57.15,0:21:58.73,main,,0000,0000,0000,,Dokážeš zazpívat vše!
Dialogue: 0,0:21:58.73,0:22:00.69,main,,0000,0000,0000,,Pokud se jedná o píseň z anime.
Dialogue: 0,0:22:00.69,0:22:02.73,main,,0000,0000,0000,,Zpíváš dobře…
Dialogue: 0,0:22:02.73,0:22:04.53,main,,0000,0000,0000,,…a tolika různými hlasy.
Dialogue: 0,0:22:04.53,0:22:07.49,main,,0000,0000,0000,,Při zpívání písní z anime…
Dialogue: 0,0:22:07.49,0:22:09.45,main,,0000,0000,0000,,…je naprosto zásadní držet se originálu.
Dialogue: 0,0:22:10.87,0:22:11.74,main,,0000,0000,0000,,Fajn.
Dialogue: 0,0:22:11.74,0:22:13.04,main,,0000,0000,0000,,Tak já budu zpívat.
Dialogue: 0,0:22:13.04,0:22:14.83,main,,0000,0000,0000,,Co budeš zpívat, ségra?
Dialogue: 0,0:22:14.83,0:22:16.79,main,,0000,0000,0000,,Před chvílí jsem viděla název…
Dialogue: 0,0:22:16.79,0:22:18.21,main,,0000,0000,0000,,…ale už si ho nepamatuji.
Dialogue: 0,0:22:18.88,0:22:20.63,main,,0000,0000,0000,,Miyuki, ty to neznáš?
Dialogue: 0,0:22:21.29,0:22:23.38,main,,0000,0000,0000,,Hádám, že nejsi o nic lepší.
Dialogue: 0,0:22:28.30,0:22:29.64,main,,0000,0000,0000,,Tohle jsem už slyšela!
Dialogue: 0,0:22:29.64,0:22:31.97,main,,0000,0000,0000,,Nesledovali jsme to nedávno společně?
Dialogue: 0,0:22:32.60,0:22:33.68,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:22:33.68,0:22:35.77,main,,0000,0000,0000,,Dávám přednost filmu…
Dialogue: 0,0:22:35.98,0:22:37.56,main,,0000,0000,0000,,Jsi snad nějaký starý podivín?
Dialogue: 0,0:22:41.02,0:22:46.53,mainup2,,0000,0000,0000,,Sbohem neznamená odloučení.
Dialogue: 0,0:22:46.99,0:22:47.70,main,,0000,0000,0000,,Jsi dobrá.
Dialogue: 0,0:22:47.70,0:22:51.12,main,,0000,0000,0000,,Ségra zpívá z celé rodiny nejlíp.
Dialogue: 0,0:22:48.49,0:22:55.54,mainup2,,0000,0000,0000,,Pouze slib, že se znovu uvidíme.
Dialogue: 0,0:22:55.54,0:23:02.33,mainup2,,0000,0000,0000,,To místo jsem viděla ve svých snech…
Dialogue: 0,0:23:02.71,0:23:09.72,mainup2,,0000,0000,0000,,zanechalo pouze chladný pocit v mém srdci.
Dialogue: 0,0:23:09.72,0:23:13.35,mainup2,,0000,0000,0000,,Přála bych si…
Dialogue: 0,0:23:13.35,0:23:17.39,mainup2,,0000,0000,0000,,…abych tě mohla obejmout.
Dialogue: 0,0:23:17.39,0:23:23.44,mainup2,,0000,0000,0000,,Hodiny a hodiny.
Dialogue: 0,0:23:23.44,0:23:25.44,mainup2,,0000,0000,0000,,Příští epizoda
Dialogue: 0,0:23:25.44,0:23:27.44,mainup2,,0000,0000,0000,,
