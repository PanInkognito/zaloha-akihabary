Dialogue: 0,0:00:01.11,0:00:02.56,main,,0000,0000,0000,,Tak začneme.
Dialogue: 0,0:00:02.57,0:00:03.61,main,,0000,0000,0000,,Jdeme na to.
Dialogue: 0,0:00:04.19,0:00:05.98,main,,0000,0000,0000,,No tak už začněme!
Dialogue: 0,0:00:06.32,0:00:07.49,op1,,0000,0000,0000,,Okolo 3 cm!
Dialogue: 0,0:00:07.49,0:00:08.78,op1,,0000,0000,0000,,Je to snad málo?
Dialogue: 0,0:00:08.78,0:00:09.03,op1,,0000,0000,0000,,Jejda!
Dialogue: 0,0:00:09.03,0:00:10.86,op1,,0000,0000,0000,,Schované pod naší uniformou!
Dialogue: 0,0:00:10.87,0:00:11.91,op1,,0000,0000,0000,,Mám toho nechat?
Dialogue: 0,0:00:11.91,0:00:12.20,op1,,0000,0000,0000,,JEJ!
Dialogue: 0,0:00:12.20,0:00:13.28,op1,,0000,0000,0000,,Jít do toho?
Dialogue: 0,0:00:13.28,0:00:14.08,op1,,0000,0000,0000,,Snažit se?
Dialogue: 0,0:00:14.08,0:00:15.66,op1,,0000,0000,0000,,Vychutnat si každý okamžik. Jo!
Dialogue: 0,0:00:15.66,0:00:16.25,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.26,0:00:16.50,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:16.50,0:00:16.90,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.91,0:00:17.25,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:17.25,0:00:17.87,op1,,0000,0000,0000,,Jen mezi námi dvěma!
Dialogue: 0,0:00:17.87,0:00:20.58,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:00:22.56,0:00:23.36,logo,,0000,0000,0000,,{\move(108,218,202,218,0,0)}LUCKY
Dialogue: 0,0:00:23.36,0:00:25.08,logo,,0000,0000,0000,,{\pos(202,218)}LUCKY
Dialogue: 0,0:00:22.56,0:00:23.36,logo,,0000,0000,0000,,{\move(638,218,459,218,0,0)}STAR
Dialogue: 0,0:00:23.36,0:00:25.08,logo,,0000,0000,0000,,{\pos(459,218)}STAR
Dialogue: 0,0:00:25.51,0:00:26.09,op1,,0000,0000,0000,,Cítím se nějak nesvá.
Dialogue: 0,0:00:26.09,0:00:26.71,op1,,0000,0000,0000,,Co jsem to jedla?
Dialogue: 0,0:00:26.71,0:00:27.30,op1,,0000,0000,0000,,Mám tě hrozně ráda!
Dialogue: 0,0:00:27.30,0:00:27.51,op1,,0000,0000,0000,,Oops!
Dialogue: 0,0:00:27.51,0:00:28.55,op1,,0000,0000,0000,,Jedno z nich je špatně!
Dialogue: 0,0:00:28.55,0:00:29.30,op1,,0000,0000,0000,,Relaxovat!
Dialogue: 0,0:00:29.30,0:00:29.93,op1,,0000,0000,0000,,Nic nedělat!
Dialogue: 0,0:00:29.93,0:00:30.47,op1,,0000,0000,0000,,Flákat se!
Dialogue: 0,0:00:30.47,0:00:31.55,op1,,0000,0000,0000,,Už toho nechej!
Dialogue: 0,0:00:31.55,0:00:33.21,op1,,0000,0000,0000,,Je ta nová holka oblíbená?
Dialogue: 0,0:00:33.21,0:00:34.85,op1,,0000,0000,0000,,Já bych řekla, že je docela normální!
Dialogue: 0,0:00:34.85,0:00:36.35,op1,,0000,0000,0000,,To jsem jediná překvapená?
Dialogue: 0,0:00:36.35,0:00:36.66,op1,,0000,0000,0000,,Vepřové kousky!
Dialogue: 0,0:00:36.66,0:00:37.06,op1,,0000,0000,0000,,Skleněné nudle!
Dialogue: 0,0:00:37.06,0:00:37.39,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:00:37.39,0:00:38.18,op2,,0000,0000,0000,,DA-DA-DA-DA-DA!
Dialogue: 0,0:00:38.73,0:00:39.52,op1,,0000,0000,0000,,Ooh?
Dialogue: 0,0:00:39.52,0:00:40.31,op1,,0000,0000,0000,,Víš co tím myslím?
Dialogue: 0,0:00:39.89,0:00:40.73,op1up,,0000,0000,0000,,Moje brýle…
Dialogue: 0,0:00:40.73,0:00:41.81,op1up,,0000,0000,0000,,Kde mám brýle?
Dialogue: 0,0:00:41.90,0:00:43.86,op1,,0000,0000,0000,,Nedávno jsme hrála tu samou bojovku.
Dialogue: 0,0:00:42.48,0:00:43.86,op1up,,0000,0000,0000,,Bez nich nic nevidím…
Dialogue: 0,0:00:44.90,0:00:45.48,op2,,0000,0000,0000,,POM! POM!
Dialogue: 0,0:00:45.48,0:00:46.53,op1,,0000,0000,0000,,Roztleskávačky!
Dialogue: 0,0:00:46.53,0:00:48.07,op2,,0000,0000,0000,,DÁME SI SLADKOST!
Dialogue: 0,0:00:48.07,0:00:48.65,op2,,0000,0000,0000,,LA! LA!
Dialogue: 0,0:00:48.65,0:00:49.70,op1,,0000,0000,0000,,Párty začíná!
Dialogue: 0,0:00:49.70,0:00:51.28,op2,,0000,0000,0000,,MÁŠ TO V KRVI!
Dialogue: 0,0:00:51.28,0:00:51.86,op2,,0000,0000,0000,,ANO!
Dialogue: 0,0:00:51.87,0:00:52.87,op1,,0000,0000,0000,,Zanechává to dojem.
Dialogue: 0,0:00:52.87,0:00:54.32,op1,,0000,0000,0000,,Malé planetky.
Dialogue: 0,0:00:54.33,0:00:54.87,op1,,0000,0000,0000,,Narazili!
Dialogue: 0,0:00:54.87,0:00:55.49,op1,,0000,0000,0000,,A shořeli!
Dialogue: 0,0:00:55.49,0:00:55.87,op1,,0000,0000,0000,,Bylo to ohromné!
Dialogue: 0,0:00:55.87,0:00:56.62,op1,,0000,0000,0000,,Z plných plic křičíme!
Dialogue: 0,0:00:56.62,0:00:57.18,op1,,0000,0000,0000,,ZPÍVEJ A TANCUJ!
Dialogue: 0,0:00:57.54,0:00:58.83,op1,,0000,0000,0000,,Přiznej si to!
Dialogue: 0,0:00:58.83,0:01:03.29,op1,,0000,0000,0000,,Nakonec se stejně budeme smát!
Dialogue: 0,0:01:03.29,0:01:07.05,op1,,0000,0000,0000,,Protože mám na sobě námořnickou uniformu.
Dialogue: 0,0:01:07.05,0:01:09.92,op2,,0000,0000,0000,,To je ono!
Dialogue: 0,0:01:10.51,0:01:14.05,op1,,0000,0000,0000,,Je teprve pondělí a já už mám špatnou náladu.
Dialogue: 0,0:01:14.05,0:01:15.85,op1,,0000,0000,0000,,Co mám dělat?
Dialogue: 0,0:01:16.06,0:01:19.89,op1,,0000,0000,0000,,Raději si vezmu moji letní uniformu -
Dialogue: 0,0:01:19.89,0:01:22.77,op1,,0000,0000,0000,,- je rozkošná!
Dialogue: 0,0:01:23.02,0:01:24.19,op1,,0000,0000,0000,,Trojdílná uniforma!
Dialogue: 0,0:01:24.19,0:01:25.61,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:01:25.61,0:01:26.06,op1,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.07,0:01:26.48,op1,,0000,0000,0000,,Dej se do toho!
Dialogue: 0,0:01:26.48,0:01:26.86,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.86,0:01:27.48,op1,,0000,0000,0000,,SNAŽ SE!
Dialogue: 0,0:01:27.48,0:01:27.65,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:27.65,0:01:29.54,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:01:30.84,0:01:36.05,good_weather,,0000,0000,0000,,{\pos(586,278)}Perfektní počasí \Npro sportovní\N festivaly
Dialogue: 0,0:01:30.84,0:01:31.67,main,,0000,0000,0000,,Dnes tu máme nejteplejší den…
Dialogue: 0,0:01:31.67,0:01:33.59,main,,0000,0000,0000,,…celého podzimu…
Dialogue: 0,0:01:33.59,0:01:36.05,main,,0000,0000,0000,,…a po celé zemi se konají sportovní festivaly.
Dialogue: 0,0:01:36.47,0:01:41.97,good_weather,,0000,0000,0000,,{\pos(505,365)}Otcové a další příbuzní si \N užívají festivalů
Dialogue: 0,0:01:36.47,0:01:37.13,main,,0000,0000,0000,,No…
Dialogue: 0,0:01:37.13,0:01:38.84,main,,0000,0000,0000,,Přišel jsem, abych povzbudil mou dceru…
Dialogue: 0,0:01:38.84,0:01:40.55,main,,0000,0000,0000,,…ale nečekal jsem jaké to tu bude.
Dialogue: 0,0:01:40.55,0:01:41.97,main,,0000,0000,0000,,Ta cesta se vážně vyplatila.
Dialogue: 0,0:01:43.14,0:01:44.97,main,,0000,0000,0000,,Mám několik skvělých fotek.
Dialogue: 0,0:01:45.10,0:01:45.43,mainup,,0000,0000,0000,,Tady.
Dialogue: 0,0:01:45.52,0:01:46.93,main,,0000,0000,0000,,Tati.
Dialogue: 0,0:01:45.52,0:01:46.93,mainup,,0000,0000,0000,,Není to skvělá fotka?
Dialogue: 0,0:01:46.98,0:01:48.81,main,,0000,0000,0000,,O jaké dceři to mluvíš?
Dialogue: 0,0:01:48.81,0:01:51.81,white,,0000,0000,0000,,{\frz-90\pos(332,137)}I beze mne
Dialogue: 0,0:01:48.81,0:01:51.81,white,,0000,0000,0000,,{\frz-90\pos(262,234)}to bude poprask
Dialogue: 0,0:01:53.19,0:01:55.78,main,,0000,0000,0000,,Možná bych si na sportovní festival měla nechat ostříhat vlasy.
Dialogue: 0,0:01:55.78,0:01:57.03,main,,0000,0000,0000,,Možná si je nechám hodně zkrátit.
Dialogue: 0,0:01:58.03,0:01:59.78,main,,0000,0000,0000,,Necháš se ostříhat?
Dialogue: 0,0:01:59.78,0:02:01.53,main,,0000,0000,0000,,Ale trvá věčnost než narostou do takové délky.
Dialogue: 0,0:02:04.37,0:02:06.20,main,,0000,0000,0000,,Jen jsem to tak řekla…
Dialogue: 0,0:02:06.21,0:02:08.41,main,,0000,0000,0000,,To je ale odpověď…
Dialogue: 0,0:02:08.42,0:02:09.67,main,,0000,0000,0000,,Určitě je to kvůli klukům!
Dialogue: 0,0:02:09.67,0:02:11.67,main,,0000,0000,0000,,Přestaň je spojovat se vším co udělám!
Dialogue: 0,0:02:11.67,0:02:13.04,main,,0000,0000,0000,,A ty toho moc namluvíš Tsukaso!
Dialogue: 0,0:02:13.46,0:02:15.25,main,,0000,0000,0000,,Ale kdyby se nechala Kagami ostříhat…
Dialogue: 0,0:02:15.42,0:02:17.34,main,,0000,0000,0000,,Jednou za čas…
Dialogue: 0,0:02:17.34,0:02:18.72,main,,0000,0000,0000,,je změna normální, ne?
Dialogue: 0,0:02:18.97,0:02:19.76,main,,0000,0000,0000,,Ne, nemůžeš!
Dialogue: 0,0:02:19.76,0:02:22.18,main,,0000,0000,0000,,Holky jako ty musí mít copánky!
Dialogue: 0,0:02:22.18,0:02:24.26,main,,0000,0000,0000,,Promiň, že mám nudný obličej!
Dialogue: 0,0:02:24.85,0:02:25.43,main,,0000,0000,0000,,No jo.
Dialogue: 0,0:02:25.43,0:02:27.22,main,,0000,0000,0000,,On se vlastně blíží sportovní festival.
Dialogue: 0,0:02:27.23,0:02:28.52,main,,0000,0000,0000,,To je ale otrava.
Dialogue: 0,0:02:28.52,0:02:30.77,main,,0000,0000,0000,,To není zrovna odpověď, která by se očekávala od někoho mladého.
Dialogue: 0,0:02:30.98,0:02:33.44,main,,0000,0000,0000,,Ty jsi ve sportech dobrá Kona-chan, pro tebe je to hračka…
Dialogue: 0,0:02:33.44,0:02:35.19,main,,0000,0000,0000,,…ale pro někoho jako jsem já…
Dialogue: 0,0:02:35.19,0:02:36.53,main,,0000,0000,0000,,…jen všechny stahuji dolů.
Dialogue: 0,0:02:36.53,0:02:37.74,main,,0000,0000,0000,,Neboj se.
Dialogue: 0,0:02:39.49,0:02:42.53,main,,0000,0000,0000,,A čeho se účastníš ségra?
Dialogue: 0,0:02:42.53,0:02:44.58,main,,0000,0000,0000,,No znáš přeci Kagami…
Dialogue: 0,0:02:44.95,0:02:46.08,main,,0000,0000,0000,,Určitě půjde na soutěž v pojídání chleba.
Dialogue: 0,0:02:46.45,0:02:48.00,main,,0000,0000,0000,,Vždy si říkám…
Dialogue: 0,0:02:48.00,0:02:49.75,main,,0000,0000,0000,,jestli se mě snažíš vyprovokovat, nebo tak něco?
Dialogue: 0,0:02:50.29,0:02:53.08,main,,0000,0000,0000,,Tak co budeš dělat, Kagami?
Dialogue: 0,0:02:57.34,0:02:58.38,main,,0000,0000,0000,,Uhodla jsem, že?
Dialogue: 0,0:02:58.38,0:02:59.51,main,,0000,0000,0000,,Uhodla?
Dialogue: 0,0:02:59.51,0:03:00.42,main,,0000,0000,0000,,Ta mě ale štve!
Dialogue: 0,0:03:01.05,0:03:04.43,main,,0000,0000,0000,,Trochu jsem na festival trénovala.
Dialogue: 0,0:03:04.47,0:03:05.72,main,,0000,0000,0000,,Přece jen podzim je období sportů.
Dialogue: 0,0:03:07.52,0:03:10.48,main,,0000,0000,0000,,Ale děláš to opravdu pro festival?
Dialogue: 0,0:03:10.48,0:03:13.35,main,,0000,0000,0000,,Jsi si jistá, že za tím něco není?
Dialogue: 0,0:03:14.36,0:03:15.23,main,,0000,0000,0000,,Jako co?
Dialogue: 0,0:03:19.99,0:03:20.69,main,,0000,0000,0000,,Vážně chceš, abych to řekla?
Dialogue: 0,0:03:20.70,0:03:22.15,main,,0000,0000,0000,,Vážně mi leze na nervy!
Dialogue: 0,0:03:23.37,0:03:25.87,main,,0000,0000,0000,,Jo? Takže Miyuki se účastní štafety?
Dialogue: 0,0:03:25.87,0:03:27.16,main,,0000,0000,0000,,No, docela rozumím.
Dialogue: 0,0:03:27.16,0:03:31.12,main,,0000,0000,0000,,První měla dělat překážkový běh, ale…
Dialogue: 0,0:03:31.62,0:03:32.33,main,,0000,0000,0000,,Fajn.
Dialogue: 0,0:03:32.33,0:03:34.04,main,,0000,0000,0000,,Takže, žádní další kandidáti?
Dialogue: 0,0:03:34.67,0:03:36.92,main,,0000,0000,0000,,Nikdy před tím jsem to nezkusila.
Dialogue: 0,0:03:36.92,0:03:38.88,main,,0000,0000,0000,,To není dobré, Miyuki!
Dialogue: 0,0:03:39.96,0:03:43.84,main,,0000,0000,0000,,Některé části tvého těla moc trčí ven, takže se na překážkový běh nehodíš.
Dialogue: 0,0:03:43.84,0:03:44.93,main,,0000,0000,0000,,Musíš se věcem vyhýbat.
Dialogue: 0,0:03:46.14,0:03:48.22,main,,0000,0000,0000,,No a po tom co jsem jí to řekla dělá štafetu.
Dialogue: 0,0:03:48.22,0:03:50.93,main,,0000,0000,0000,,To zní jako něco co by řekl nějaký chlápek ve středním věku.
Dialogue: 0,0:03:51.69,0:03:52.18,main,,0000,0000,0000,,A jo.
Dialogue: 0,0:03:52.19,0:03:55.69,main,,0000,0000,0000,,Chtěla jsem se zeptat, jestli jste u vás na střední měli soutěž ve sbírání věcí?
Dialogue: 0,0:03:56.23,0:03:56.94,main,,0000,0000,0000,,Jasně, že jo.
Dialogue: 0,0:03:57.40,0:04:00.36,main,,0000,0000,0000,,Jednou mě vybrali…
Dialogue: 0,0:04:00.61,0:04:01.69,pink,,0000,0000,0000,,{\frz-90\pos(278,163)}Proč?
Dialogue: 0,0:04:01.86,0:04:06.99,main,,0000,0000,0000,,Ale nikdy mi neřekli proč vlastně.
Dialogue: 0,0:04:06.99,0:04:09.49,main,,0000,0000,0000,,Napadá mě tolik nevyslovitelných věcí.
Dialogue: 0,0:04:09.87,0:04:13.04,main,,0000,0000,0000,,No, když něco nevíš tak není nic lehčího, než se zeptat Miyuki-san.
Dialogue: 0,0:04:13.04,0:04:14.42,main,,0000,0000,0000,,Na něco takového se jí neptej.
Dialogue: 0,0:04:14.42,0:04:16.04,main,,0000,0000,0000,,Je nemožné odpovědět.
Dialogue: 0,0:04:16.42,0:04:18.13,main,,0000,0000,0000,,Takže ona se vážně zeptala…
Dialogue: 0,0:04:18.13,0:04:20.05,main,,0000,0000,0000,,Nějaká tvá vlastnost pro kterou si tě mohli vybrat…
Dialogue: 0,0:04:20.38,0:04:23.01,main,,0000,0000,0000,,ale nechtěli ti ji říct?
Dialogue: 0,0:04:25.89,0:04:29.05,main,,0000,0000,0000,,Třeba chtěli někoho se znamínkem na obličeji.
Dialogue: 0,0:04:29.06,0:04:31.27,main,,0000,0000,0000,,Někdo je na toto citlivý.
Dialogue: 0,0:04:31.56,0:04:32.98,main,,0000,0000,0000,,Asi se to čekalo…
Dialogue: 0,0:04:32.98,0:04:35.06,main,,0000,0000,0000,,ale stejně je to umění se takto vykroutit.
Dialogue: 0,0:04:35.06,0:04:36.52,main,,0000,0000,0000,,Ale jsem si naprosto jistá, že se plete.
Dialogue: 0,0:04:39.48,0:04:41.73,main,,0000,0000,0000,,Už se na sportovní festival těším…
Dialogue: 0,0:04:41.74,0:04:43.82,main,,0000,0000,0000,,ale hned po něm máme testy.
Dialogue: 0,0:04:44.20,0:04:45.65,main,,0000,0000,0000,,Po tomto festivalu…
Dialogue: 0,0:04:45.66,0:04:48.95,main,,0000,0000,0000,,…už nejsou žádné další události, což je trochu škoda.
Dialogue: 0,0:04:48.95,0:04:49.70,main,,0000,0000,0000,,Pololetní testy.
Dialogue: 0,0:04:49.70,0:04:50.45,main,,0000,0000,0000,,Standardizované testy.
Dialogue: 0,0:04:50.45,0:04:51.29,main,,0000,0000,0000,,A závěrečné zkoušky.
Dialogue: 0,0:04:51.29,0:04:52.91,main,,0000,0000,0000,,Vše co pak následuje jsou jen testy.
Dialogue: 0,0:04:52.91,0:04:57.88,main,,0000,0000,0000,,Druhý semestr je díky sportovním a kulturním festivalům vážně vysilující.
Dialogue: 0,0:04:58.59,0:05:01.09,main,,0000,0000,0000,,Zvláště pak pololetní testy.
Dialogue: 0,0:05:02.96,0:05:05.05,main,,0000,0000,0000,,Taky se říká že podzim je dobré období na čtení knih…
Dialogue: 0,0:05:05.05,0:05:06.80,main,,0000,0000,0000,,…takže se možná do něčeho pustím.
Dialogue: 0,0:05:06.80,0:05:09.93,main,,0000,0000,0000,,Je to také dobrá doba pro studování na testy.
Dialogue: 0,0:05:09.93,0:05:13.31,main,,0000,0000,0000,,A těsně před Vánoci vyjdou všechny nové hry!
Dialogue: 0,0:05:13.31,0:05:15.05,main,,0000,0000,0000,,Ale první máme závěrečné testy.
Dialogue: 0,0:05:19.15,0:05:19.52,main,,0000,0000,0000,,Jo!
Dialogue: 0,0:05:19.52,0:05:20.19,main,,0000,0000,0000,,Vy dvě.
Dialogue: 0,0:05:20.61,0:05:23.15,main,,0000,0000,0000,,Mohli by jste se přestat pokoušet vyhnout realitě?
Dialogue: 0,0:05:26.78,0:05:27.74,main,,0000,0000,0000,,Chápu.
Dialogue: 0,0:05:27.95,0:05:29.70,main,,0000,0000,0000,,Už je vlastně skoro čas sportovních festivalů.
Dialogue: 0,0:05:29.70,0:05:31.78,main,,0000,0000,0000,,Víš jak jsou ty pistole co se používají pro odstartování závodu?
Dialogue: 0,0:05:31.79,0:05:34.83,main,,0000,0000,0000,,Do střední jsem si myslela, že to jsou opravdové zbraně.
Dialogue: 0,0:05:34.83,0:05:35.79,main,,0000,0000,0000,,To ne…
Dialogue: 0,0:05:35.79,0:05:39.63,main,,0000,0000,0000,,Už tehdy jsem věděla, že chci být policistkou.
Dialogue: 0,0:05:39.63,0:05:42.21,main,,0000,0000,0000,,Ten omyl vedl k něčemu úplně nesovisejícímu… úžasné.
Dialogue: 0,0:05:43.21,0:05:44.71,main,,0000,0000,0000,,To je pravda, Konato.
Dialogue: 0,0:05:45.17,0:05:47.47,main,,0000,0000,0000,,Kdy, že u vás bude sportovní festival?
Dialogue: 0,0:05:47.93,0:05:49.18,main,,0000,0000,0000,,Tuto neděli.
Dialogue: 0,0:05:50.97,0:05:52.64,main,,0000,0000,0000,,T-Tuto neděli?!
Dialogue: 0,0:05:52.64,0:05:55.22,main,,0000,0000,0000,,Proč jsi mi tak důležitou věc neoznámila dřív?
Dialogue: 0,0:05:55.23,0:05:57.31,main,,0000,0000,0000,,Vím to teprve ode dneška.
Dialogue: 0,0:05:57.64,0:05:59.27,main,,0000,0000,0000,,Není času na zbytí!
Dialogue: 0,0:06:01.06,0:06:02.56,main,,0000,0000,0000,,Proč tak pospíchá?
Dialogue: 0,0:06:03.02,0:06:03.69,main,,0000,0000,0000,,Tak jo!
Dialogue: 0,0:06:03.69,0:06:05.82,main,,0000,0000,0000,,Dneska je významný den!
Dialogue: 0,0:06:05.82,0:06:08.45,main,,0000,0000,0000,,Chci vidět plody vaší přípravy!
Dialogue: 0,0:06:08.66,0:06:09.53,main,,0000,0000,0000,,Sensei!
Dialogue: 0,0:06:09.53,0:06:13.33,main,,0000,0000,0000,,Já jsem ten typ co si vede dobře i bez tréninku!
Dialogue: 0,0:06:13.33,0:06:14.33,main,,0000,0000,0000,,To si nech pro sebe!
Dialogue: 0,0:06:14.83,0:06:17.66,main,,0000,0000,0000,,Každopádně si dejte záležet aby jste do „kuřecích zápasů“ dali vše co máte!
Dialogue: 0,0:06:17.66,0:06:20.29,main,,0000,0000,0000,,Musíme je obklíčit a zničit, když budou v nejnevýhodnější pozici!
Dialogue: 0,0:06:20.71,0:06:24.04,main,,0000,0000,0000,,Takže útok na jejich slabiny?
Dialogue: 0,0:06:24.05,0:06:26.09,main,,0000,0000,0000,,Ale jediné co potřebujeme je získat jejich klobouky…
Dialogue: 0,0:06:26.09,0:06:27.34,main,,0000,0000,0000,,Mlácení lidí je docela…
Dialogue: 0,0:06:27.34,0:06:28.88,main,,0000,0000,0000,,O takové detaily se nestarej.
Dialogue: 0,0:06:28.88,0:06:31.34,main,,0000,0000,0000,,Mladí vždy řeší takové prkotiny.
Dialogue: 0,0:06:31.34,0:06:32.80,main,,0000,0000,0000,,Takže jsme malí skrčci, hm?
Dialogue: 0,0:06:37.93,0:06:38.89,main,,0000,0000,0000,,Sestři!
Dialogue: 0,0:06:38.89,0:06:39.89,main,,0000,0000,0000,,Do toho!
Dialogue: 0,0:06:39.89,0:06:41.40,main,,0000,0000,0000,,Hodně štěstí!
Dialogue: 0,0:06:43.65,0:06:44.27,main,,0000,0000,0000,,Hej.
Dialogue: 0,0:06:44.27,0:06:47.82,main,,0000,0000,0000,,Jak je chléb přichycený ke šňůrce?
Dialogue: 0,0:06:47.82,0:06:51.49,main,,0000,0000,0000,,Nejspíš je nějak připnutý.
Dialogue: 0,0:06:52.28,0:06:54.91,main,,0000,0000,0000,,Takže nejde sundat moc lehce?
Dialogue: 0,0:06:54.91,0:06:56.54,main,,0000,0000,0000,,Přines nám vítězství!
Dialogue: 0,0:06:56.87,0:06:58.79,main,,0000,0000,0000,,Elegantně jak se sluší a patří na dívku!
Dialogue: 0,0:06:58.79,0:07:01.08,main,,0000,0000,0000,,Jako by soutěž v pojídání chleba mohla být elegantní!
Dialogue: 0,0:07:01.08,0:07:02.00,main,,0000,0000,0000,,Připravit!
Dialogue: 0,0:07:11.80,0:07:12.68,main,,0000,0000,0000,,Dokázala to!
Dialogue: 0,0:07:18.56,0:07:20.27,main,,0000,0000,0000,,To je naše Kagami!
Dialogue: 0,0:07:25.02,0:07:27.23,main,,0000,0000,0000,,Tam ten člověk má úžasný foťák.
Dialogue: 0,0:07:33.11,0:07:35.16,main,,0000,0000,0000,,Je tak průhledný.
Dialogue: 0,0:07:35.66,0:07:36.49,main,,0000,0000,0000,,Hej.
Dialogue: 0,0:07:36.49,0:07:38.79,main,,0000,0000,0000,,Nejdeš teď na běh na 100 metrů?
Dialogue: 0,0:07:38.79,0:07:40.00,main,,0000,0000,0000,,Asi jo.
Dialogue: 0,0:07:40.00,0:07:43.00,main,,0000,0000,0000,,Jak se můžu naučit běhat tak rychle jako ty?
Dialogue: 0,0:07:43.00,0:07:45.33,main,,0000,0000,0000,,Je důležité si to umět představit.
Dialogue: 0,0:07:45.34,0:07:46.50,main,,0000,0000,0000,,Představit.
Dialogue: 0,0:07:49.63,0:07:50.17,main,,0000,0000,0000,,Připravit!
Dialogue: 0,0:07:51.13,0:07:51.92,main,,0000,0000,0000,,Pozor!
Dialogue: 0,0:07:54.50,0:07:57.02,main,,0000,0000,0000,,To je ale pěkně stará představa.
Dialogue: 0,0:08:02.77,0:08:03.64,main,,0000,0000,0000,,Cíl!
Dialogue: 0,0:08:04.15,0:08:05.10,main,,0000,0000,0000,,Tak nějak?
Dialogue: 0,0:08:05.11,0:08:05.85,main,,0000,0000,0000,,To myslíš vážně?
Dialogue: 0,0:08:09.03,0:08:10.90,main,,0000,0000,0000,,3 metry 25 centimetrů.
Dialogue: 0,0:08:11.36,0:08:13.74,main,,0000,0000,0000,,Takže se měří od nejkratší vzdálenosti.
Dialogue: 0,0:08:18.83,0:08:19.70,main,,0000,0000,0000,,Jdu na to!
Dialogue: 0,0:08:22.25,0:08:23.58,main,,0000,0000,0000,,To je naše Kagami.
Dialogue: 0,0:08:23.58,0:08:25.04,main,,0000,0000,0000,,Do všeho jde po hlavě.
Dialogue: 0,0:08:27.38,0:08:29.04,main,,0000,0000,0000,,Jedna, dvě, tři.
Dialogue: 0,0:08:29.42,0:08:30.96,main,,0000,0000,0000,,Jedna, dvě, tři.
Dialogue: 0,0:08:30.96,0:08:34.76,main,,0000,0000,0000,,Slyšela jsem, že pro překážkový běh je důležité načasování kroků.
Dialogue: 0,0:08:34.76,0:08:36.39,main,,0000,0000,0000,,Kroků?
Dialogue: 0,0:08:36.39,0:08:37.39,main,,0000,0000,0000,,Jedna.
Dialogue: 0,0:08:37.39,0:08:38.35,main,,0000,0000,0000,,Dvě.
Dialogue: 0,0:08:38.35,0:08:39.56,main,,0000,0000,0000,,Tři.
Dialogue: 0,0:08:39.56,0:08:40.72,main,,0000,0000,0000,,Aha!
Dialogue: 0,0:08:40.72,0:08:42.27,main,,0000,0000,0000,,Jedna, dvě, tři.
Dialogue: 0,0:08:42.60,0:08:44.44,main,,0000,0000,0000,,Jedna, dvě, tři.
Dialogue: 0,0:08:43.40,0:08:44.44,main2,,0000,0000,0000,,Připravit!
Dialogue: 0,0:08:44.44,0:08:44.94,main,,0000,0000,0000,,Jedna…
Dialogue: 0,0:08:45.81,0:08:46.73,main,,0000,0000,0000,,Um…
Dialogue: 0,0:08:46.73,0:08:48.11,main,,0000,0000,0000,,Tři, dvě, tři!
Dialogue: 0,0:08:48.40,0:08:49.94,main,,0000,0000,0000,,Jedna, tři, heh?
Dialogue: 0,0:08:50.90,0:08:51.40,main,,0000,0000,0000,,Um…
Dialogue: 0,0:08:51.40,0:08:53.07,main,,0000,0000,0000,,Začala jsem mou pravou nohou, takže…
Dialogue: 0,0:08:55.95,0:08:57.24,main,,0000,0000,0000,,Spletla jsem to!
Dialogue: 0,0:08:57.24,0:08:58.49,main,,0000,0000,0000,,To byla levá noha, takže…
Dialogue: 0,0:08:59.91,0:09:02.29,main,,0000,0000,0000,,Tři, tři, tři.
Dialogue: 0,0:09:03.58,0:09:05.87,main,,0000,0000,0000,,Zatím srazila každou překážku.
Dialogue: 0,0:09:05.87,0:09:07.54,main,,0000,0000,0000,,Dalo by se říct, že má talent.
Dialogue: 0,0:09:11.25,0:09:12.88,main,,0000,0000,0000,,To jsem zkazila.
Dialogue: 0,0:09:13.76,0:09:14.55,main,,0000,0000,0000,,Jak jsem vám říkal.
Dialogue: 0,0:09:14.55,0:09:17.26,main,,0000,0000,0000,,Myslel jsem si, že to běží má dcera.
Dialogue: 0,0:09:17.26,0:09:18.80,main,,0000,0000,0000,,Jsem si jistá, že to se stává často.
Dialogue: 0,0:09:19.80,0:09:21.68,main,,0000,0000,0000,,Konečně je tu štafeta.
Dialogue: 0,0:09:21.68,0:09:22.56,main,,0000,0000,0000,,Yuki-chan!
Dialogue: 0,0:09:22.56,0:09:23.64,main,,0000,0000,0000,,Do toho!
Dialogue: 0,0:09:23.64,0:09:24.27,main,,0000,0000,0000,,Ne…
Dialogue: 0,0:09:24.43,0:09:25.64,main,,0000,0000,0000,,Řekl jsem vám, že moje dcera je…
Dialogue: 0,0:09:25.77,0:09:27.14,main,,0000,0000,0000,,Počkejte chvilku!
Dialogue: 0,0:09:25.77,0:09:26.85,main2,,0000,0000,0000,,Budu se snažit!
Dialogue: 0,0:09:27.15,0:09:28.85,main,,0000,0000,0000,,Ještě jsem nic neudělal!
Dialogue: 0,0:09:29.73,0:09:30.56,main,,0000,0000,0000,,Do toho!
Dialogue: 0,0:09:33.07,0:09:33.90,main,,0000,0000,0000,,Úžasné!
Dialogue: 0,0:09:33.90,0:09:35.24,main,,0000,0000,0000,,Je druhá!
Dialogue: 0,0:09:35.24,0:09:36.15,main,,0000,0000,0000,,To jde.
Dialogue: 0,0:09:36.15,0:09:37.61,main,,0000,0000,0000,,Sachi, tady!
Dialogue: 0,0:09:41.53,0:09:41.83,main,,0000,0000,0000,,Dobře!
Dialogue: 0,0:09:42.04,0:09:42.53,main,,0000,0000,0000,,Tady!
Dialogue: 0,0:09:45.87,0:09:47.29,main,,0000,0000,0000,,Propadli jsme se na třetí.
Dialogue: 0,0:09:47.29,0:09:49.12,main,,0000,0000,0000,,To je ale závod.
Dialogue: 0,0:09:53.59,0:09:55.09,main,,0000,0000,0000,,Dávají do toho vše.
Dialogue: 0,0:09:55.09,0:09:56.63,main,,0000,0000,0000,,Začíná to být zajímavé.
Dialogue: 0,0:09:58.22,0:10:00.76,main,,0000,0000,0000,,Zvládne to Yuki-chan?
Dialogue: 0,0:10:24.58,0:10:26.00,main,,0000,0000,0000,,Do toho, Miyuki!
Dialogue: 0,0:10:32.92,0:10:33.96,main,,0000,0000,0000,,Jo!
Dialogue: 0,0:10:33.96,0:10:35.42,main,,0000,0000,0000,,Yuki-chan je skvělá!
Dialogue: 0,0:10:35.42,0:10:37.01,main,,0000,0000,0000,,To je naše Miyuki-san.
Dialogue: 0,0:10:37.51,0:10:40.22,main,,0000,0000,0000,,Dokonce nezapomněla ani na diváky.
Dialogue: 0,0:10:50.23,0:10:51.73,main,,0000,0000,0000,,To musí být Tsukasa.
Dialogue: 0,0:10:52.36,0:10:53.48,main,,0000,0000,0000,,Páni.
Dialogue: 0,0:10:54.40,0:11:00.49,mainup2,,0000,0000,0000,,H, He, Li, Be, B, C, N, O, F, Ne…
Dialogue: 0,0:11:20.22,0:11:21.01,main,,0000,0000,0000,,Hej!
Dialogue: 0,0:11:21.01,0:11:23.14,main,,0000,0000,0000,,Mohla by jsi s tím něco udělat?
Dialogue: 0,0:11:23.14,0:11:24.26,main,,0000,0000,0000,,Promiň.
Dialogue: 0,0:11:24.26,0:11:28.14,main,,0000,0000,0000,,Mám hrát tuto část v naší hudební třídě…
Dialogue: 0,0:11:28.14,0:11:30.27,main,,0000,0000,0000,,…ale už mě začínají bolet prsty.
Dialogue: 0,0:11:31.94,0:11:33.52,main,,0000,0000,0000,,Proč jsi si vybrala toto?
Dialogue: 0,0:11:33.52,0:11:37.27,main,,0000,0000,0000,,Myslela jsem si, že by to mohlo být jednodušší.
Dialogue: 0,0:11:38.40,0:11:41.61,main,,0000,0000,0000,,Proč nezačneš na něčem jednodušším?
Dialogue: 0,0:11:57.00,0:11:58.80,main,,0000,0000,0000,,Spletla jsi většinu not.
Dialogue: 0,0:11:59.76,0:12:01.92,main,,0000,0000,0000,,Zkusme to spolu.
Dialogue: 0,0:12:01.93,0:12:02.80,main,,0000,0000,0000,,Připravit a…
Dialogue: 0,0:12:07.18,0:12:08.51,main,,0000,0000,0000,,Z-Zadrž!
Dialogue: 0,0:12:08.52,0:12:10.02,main,,0000,0000,0000,,Hraj sama.
Dialogue: 0,0:12:19.32,0:12:21.32,main,,0000,0000,0000,,Něco je špatně…
Dialogue: 0,0:12:21.32,0:12:23.40,main,,0000,0000,0000,,Zkus foukat jemněji.
Dialogue: 0,0:12:23.95,0:12:26.82,main,,0000,0000,0000,,Když se uvolníš tak to půjde mnohem lépe.
Dialogue: 0,0:12:27.49,0:12:28.70,main,,0000,0000,0000,,Rozumím.
Dialogue: 0,0:12:28.70,0:12:29.66,main,,0000,0000,0000,,Jemněji…
Dialogue: 0,0:12:29.66,0:12:30.58,main,,0000,0000,0000,,Přesně tak.
Dialogue: 0,0:12:31.12,0:12:32.04,main,,0000,0000,0000,,Zkusím to.
Dialogue: 0,0:13:01.03,0:13:04.07,main,,0000,0000,0000,,Zdá se mi to, nebo vážně víc dýchá než hraje?
Dialogue: 0,0:13:07.12,0:13:09.70,main,,0000,0000,0000,,Napadlo mě, že by jsme se jednou pro změnu mohli najíst v kantýně…
Dialogue: 0,0:13:09.70,0:13:10.91,main,,0000,0000,0000,,ale je tu hrozně narváno.
Dialogue: 0,0:13:11.33,0:13:12.87,main,,0000,0000,0000,,To je hrůza.
Dialogue: 0,0:13:12.87,0:13:15.66,main,,0000,0000,0000,,Čekání v tak dlouhých frontách je strašně únavné.
Dialogue: 0,0:13:15.67,0:13:16.96,main,,0000,0000,0000,,Nemám je ráda.
Dialogue: 0,0:13:16.96,0:13:19.92,main,,0000,0000,0000,,Nejsi na ně z různých anime akcí zvyklá?
Dialogue: 0,0:13:20.21,0:13:23.92,main,,0000,0000,0000,,Já ale nemám ráda fronty na jejíž konci mě nic zajímavého nečeká.
Dialogue: 0,0:13:24.30,0:13:26.97,main,,0000,0000,0000,,Jsi dost upřímná co se tvých zájmů týká.
Dialogue: 0,0:13:26.97,0:13:27.67,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:13:28.09,0:13:30.22,main,,0000,0000,0000,,Když jsem naposledy byla v zábavním parku…
Dialogue: 0,0:13:30.22,0:13:32.89,main,,0000,0000,0000,,…tak jsem se si myslela že stojím ve frontě na atrakci…
Dialogue: 0,0:13:32.89,0:13:35.06,main,,0000,0000,0000,,…ale ve skutečnosti to byla fronta na toalety.
Dialogue: 0,0:13:35.06,0:13:38.98,main,,0000,0000,0000,,Základní pravidlo je vždy dvakrát zkontrolovat jestli stojíš ve správné frontě.
Dialogue: 0,0:13:39.31,0:13:41.32,main,,0000,0000,0000,,A čekala jsem tam celé dvě hodiny…
Dialogue: 0,0:13:41.90,0:13:46.15,main,,0000,0000,0000,,I já jsem několikrát čekala ve frontě…
Dialogue: 0,0:13:46.15,0:13:49.41,main,,0000,0000,0000,,a když jsem se konečně dostala na řadu, tak si člověk co stál přede mnou koupil poslední kus.
Dialogue: 0,0:13:49.41,0:13:50.45,main,,0000,0000,0000,,Bylo to hrozné.
Dialogue: 0,0:13:50.45,0:13:51.74,main,,0000,0000,0000,,Ani si to nechci představovat.
Dialogue: 0,0:13:52.08,0:13:53.87,main,,0000,0000,0000,,Prostě nemáš štěstí.
Dialogue: 0,0:13:56.58,0:13:58.00,main,,0000,0000,0000,,Co si dnes dáte?
Dialogue: 0,0:13:58.58,0:14:00.33,main,,0000,0000,0000,,No, já si dám asi kary.
Dialogue: 0,0:14:00.34,0:14:01.50,main,,0000,0000,0000,,S tím se nikdy nespálím.
Dialogue: 0,0:14:01.50,0:14:03.13,main,,0000,0000,0000,,Já si dám ramen.
Dialogue: 0,0:14:03.13,0:14:04.25,main,,0000,0000,0000,,I já.
Dialogue: 0,0:14:04.26,0:14:06.88,main,,0000,0000,0000,,Přemýšlela jsem nad miso ramenem.
Dialogue: 0,0:14:06.88,0:14:08.26,main,,0000,0000,0000,,Se spoustou kukuřice…
Dialogue: 0,0:14:08.26,0:14:09.93,main,,0000,0000,0000,,…a mořskou řasou!
Dialogue: 0,0:14:10.22,0:14:12.55,main,,0000,0000,0000,,Ty si asi dáš puding a ramen, co?
Dialogue: 0,0:14:13.26,0:14:15.02,main,,0000,0000,0000,,Když mi ještě zůstane nějaké místo…
Dialogue: 0,0:14:18.23,0:14:19.81,main,,0000,0000,0000,,Tobě vždy nějaké zbude…
Dialogue: 0,0:14:20.94,0:14:22.19,main,,0000,0000,0000,,A co ty, Yuki-chan?
Dialogue: 0,0:14:22.19,0:14:23.94,main,,0000,0000,0000,,Já si dám sendvič.
Dialogue: 0,0:14:23.94,0:14:25.23,main,,0000,0000,0000,,Asi jahodový.
Dialogue: 0,0:14:26.11,0:14:27.70,main,,0000,0000,0000,,Přesně tohle mi k tobě sedí.
Dialogue: 0,0:14:28.24,0:14:29.03,main,,0000,0000,0000,,Že?
Dialogue: 0,0:14:29.03,0:14:30.28,main,,0000,0000,0000,,Mimochodem…
Dialogue: 0,0:14:30.28,0:14:33.45,main,,0000,0000,0000,,Není jahodový sendvič považovaný za dezert?
Dialogue: 0,0:14:33.45,0:14:35.54,main,,0000,0000,0000,,Dá se to počítat jako oběd?
Dialogue: 0,0:14:35.54,0:14:36.91,main,,0000,0000,0000,,To je pravda…
Dialogue: 0,0:14:36.91,0:14:39.12,main,,0000,0000,0000,,Ty vždy blábolíš o takových podivných problémech.
Dialogue: 0,0:14:39.92,0:14:42.46,main,,0000,0000,0000,,A co ty čokoládové kornoutky co vždy jíš?
Dialogue: 0,0:14:42.46,0:14:44.67,main,,0000,0000,0000,,Ty se počítají jako dezert?
Dialogue: 0,0:14:44.67,0:14:47.01,main,,0000,0000,0000,,Tsukaso, jak se opovažuješ zasahovat do mých věcí…
Dialogue: 0,0:14:47.84,0:14:49.43,main,,0000,0000,0000,,No, to je jedno…
Dialogue: 0,0:14:49.93,0:14:52.22,main,,0000,0000,0000,,Bohužel pro tebe, Miyuki-san…
Dialogue: 0,0:14:52.22,0:14:54.30,main,,0000,0000,0000,,Řada na sendvič je tamhle.
Dialogue: 0,0:14:55.43,0:14:56.68,main,,0000,0000,0000,,To je celá ona.
Dialogue: 0,0:14:56.68,0:14:58.60,main,,0000,0000,0000,,No, chůze je zdravá.
Dialogue: 0,0:14:59.31,0:14:59.81,main,,0000,0000,0000,,Hej.
Dialogue: 0,0:14:59.81,0:15:01.10,main,,0000,0000,0000,,Si na řadě, Kona-chan.
Dialogue: 0,0:15:01.81,0:15:02.86,main,,0000,0000,0000,,Kary prosím.
Dialogue: 0,0:15:02.86,0:15:03.94,main,,0000,0000,0000,,S omáčkou navíc.
Dialogue: 0,0:15:03.94,0:15:04.82,main,,0000,0000,0000,,Dobře.
Dialogue: 0,0:15:04.82,0:15:06.23,main,,0000,0000,0000,,Jdeš na to pěkně zhurta.
Dialogue: 0,0:15:06.23,0:15:08.15,main,,0000,0000,0000,,Musíš si vzít co můžeš.
Dialogue: 0,0:15:10.86,0:15:11.16,main,,0000,0000,0000,,Dobře.
Dialogue: 0,0:15:11.16,0:15:12.32,main,,0000,0000,0000,,Další?
Dialogue: 0,0:15:12.32,0:15:13.82,main,,0000,0000,0000,,Ramen se sojovou omáčkou.
Dialogue: 0,0:15:13.83,0:15:15.12,main,,0000,0000,0000,,A s tvrdými nudlemi.
Dialogue: 0,0:15:15.12,0:15:16.70,main,,0000,0000,0000,,Dobře, moment prosím.
Dialogue: 0,0:15:17.91,0:15:18.33,main,,0000,0000,0000,,Fajn.
Dialogue: 0,0:15:18.33,0:15:19.83,main,,0000,0000,0000,,Tady je vaše kary.
Dialogue: 0,0:15:21.67,0:15:24.08,main,,0000,0000,0000,,Půjdu zabrat stůl.
Dialogue: 0,0:15:24.09,0:15:25.50,main2,,0000,0000,0000,,Dobře.
Dialogue: 0,0:15:24.38,0:15:25.50,main,,0000,0000,0000,,Díky.
Dialogue: 0,0:15:27.13,0:15:27.92,main,,0000,0000,0000,,Další prosím.
Dialogue: 0,0:15:28.63,0:15:29.34,main,,0000,0000,0000,,Ano…
Dialogue: 0,0:15:29.34,0:15:31.43,main,,0000,0000,0000,,Dala bych si miso ramen, prosím.
Dialogue: 0,0:15:31.43,0:15:32.43,main,,0000,0000,0000,,Omlouvám se.
Dialogue: 0,0:15:32.43,0:15:34.47,main,,0000,0000,0000,,Minulý zákazník si objednal naše poslední nudle.
Dialogue: 0,0:15:34.47,0:15:36.18,main,,0000,0000,0000,,Už jsme je vyprodali.
Dialogue: 0,0:15:37.60,0:15:38.39,main,,0000,0000,0000,,No.
Dialogue: 0,0:15:38.39,0:15:40.64,main,,0000,0000,0000,,Pokud by ti stačila sojová omáčka, tak můžeš mít mé.
Dialogue: 0,0:15:40.64,0:15:42.98,main,,0000,0000,0000,,Já si dám kitsune udon.
Dialogue: 0,0:15:42.98,0:15:44.94,main,,0000,0000,0000,,Díky, setři.
Dialogue: 0,0:15:51.49,0:15:52.61,main,,0000,0000,0000,,Hej, ségra.
Dialogue: 0,0:15:53.87,0:15:57.62,main,,0000,0000,0000,,Jsi starší sestra protože ses narodila dřív, že?
Dialogue: 0,0:15:57.62,0:15:58.20,main,,0000,0000,0000,,Jo.
Dialogue: 0,0:15:58.62,0:16:01.04,main,,0000,0000,0000,,Přemýšlela jsem, že je dobré, že jsem mladší.
Dialogue: 0,0:16:01.79,0:16:02.46,main,,0000,0000,0000,,Proč?
Dialogue: 0,0:16:02.87,0:16:08.75,main,,0000,0000,0000,,Protože by bylo trapné, kdyby si starší setra chodila k mladší pro radu s úkoly.
Dialogue: 0,0:16:08.76,0:16:12.93,main,,0000,0000,0000,,Nejsem si jistá, že to s tím má hodně co dělat.
Dialogue: 0,0:16:13.34,0:16:15.09,main,,0000,0000,0000,,Krom toho jsme dvojčata.
Dialogue: 0,0:16:15.10,0:16:17.01,main,,0000,0000,0000,,Ani jedna z nás není skutečně starší nebo mladší.
Dialogue: 0,0:16:17.01,0:16:17.93,main,,0000,0000,0000,,Neboj se…
Dialogue: 0,0:16:17.93,0:16:19.43,main,,0000,0000,0000,,…a dodělej si úkoly.
Dialogue: 0,0:16:21.94,0:16:24.85,main,,0000,0000,0000,,Ale kdybych já byla straší sestra…
Dialogue: 0,0:16:24.85,0:16:27.40,main,,0000,0000,0000,,možná bych byla chytřejší jako Onee-chan.
Dialogue: 0,0:16:28.27,0:16:29.61,main,,0000,0000,0000,,Sestři!
Dialogue: 0,0:16:29.61,0:16:31.19,main,,0000,0000,0000,,Ukaž mi tvůj úkol!
Dialogue: 0,0:16:31.19,0:16:32.32,main,,0000,0000,0000,,Zase?
Dialogue: 0,0:16:32.32,0:16:34.78,main,,0000,0000,0000,,Jsi nemožná, Kagami.
Dialogue: 0,0:16:35.92,0:16:39.28,main,,0000,0000,0000,,- To by bylo něco!\N- Hej, jsi v pořádku?
Dialogue: 0,0:16:39.37,0:16:40.45,main,,0000,0000,0000,,Dokázala jsem to!
Dialogue: 0,0:16:40.45,0:16:41.62,main,,0000,0000,0000,,Newtype…
Dialogue: 0,0:16:41.62,0:16:42.37,main,,0000,0000,0000,,…publikovali moji…
Dialogue: 0,0:16:42.37,0:16:43.96,main,,0000,0000,0000,,…vlastní…
Dialogue: 0,0:16:43.96,0:16:45.37,main,,0000,0000,0000,,…pohlednici!
Dialogue: 0,0:16:45.79,0:16:48.09,main,,0000,0000,0000,,V květnovém čísle…
Dialogue: 0,0:16:48.50,0:16:49.29,main,,0000,0000,0000,,Provincie Saitama.
Dialogue: 0,0:16:48.75,0:16:52.17,main2,,0000,0000,0000,,Ta s Shiraishim ležícím pod Akirou.
Dialogue: 0,0:16:49.30,0:16:50.84,main,,0000,0000,0000,,Izumi Konata.
Dialogue: 0,0:16:51.80,0:16:52.76,main,,0000,0000,0000,,To je dobrý…
Dialogue: 0,0:16:52.17,0:16:53.72,main2,,0000,0000,0000,,Podívej s na ní!
Dialogue: 0,0:16:52.76,0:16:53.72,main,,0000,0000,0000,,Dobrá práce, že ti ji otiskli.
Dialogue: 0,0:16:55.97,0:16:57.05,main,,0000,0000,0000,,Úžasné!
Dialogue: 0,0:16:57.05,0:16:59.64,main,,0000,0000,0000,,Jaké překvapení!
Dialogue: 0,0:16:59.64,0:17:01.31,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:17:01.31,0:17:04.27,main,,0000,0000,0000,,Teď mám další věc na kterou se můžu těšit!
Dialogue: 0,0:17:04.27,0:17:05.60,main,,0000,0000,0000,,Moc mě nepřechval!
Dialogue: 0,0:17:08.31,0:17:10.27,main,,0000,0000,0000,,1 260 yenů…
Dialogue: 0,0:17:10.28,0:17:13.07,main,,0000,0000,0000,,a 567 yenů, heh?
Dialogue: 0,0:17:13.49,0:17:16.57,main,,0000,0000,0000,,Ještě jednu a budu mít dost bodů.
Dialogue: 0,0:17:17.16,0:17:19.45,main,,0000,0000,0000,,Potom si vezmu něco jiného…
Dialogue: 0,0:17:19.62,0:17:20.20,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:17:20.20,0:17:22.16,main,,0000,0000,0000,,Potom bych spadla do jejich pasti.
Dialogue: 0,0:17:22.16,0:17:24.83,main,,0000,0000,0000,,Mě tak lehce neošálí.
Dialogue: 0,0:17:24.83,0:17:27.92,main,,0000,0000,0000,,Nekoupím si něco co nepotřebuji jen kvůli bonusovým bodům.
Dialogue: 0,0:17:27.92,0:17:29.67,main,,0000,0000,0000,,To je jejich strategie.
Dialogue: 0,0:17:30.84,0:17:34.38,main,,0000,0000,0000,,Nejsem tak hloupá, abych naletěla.
Dialogue: 0,0:17:35.13,0:17:35.80,main,,0000,0000,0000,,Oh!
Dialogue: 0,0:17:36.89,0:17:38.59,main,,0000,0000,0000,,Zapoměla jsem, že vyšel pátý díl.
Dialogue: 0,0:17:38.60,0:17:38.97,main,,0000,0000,0000,,Fajn!
Dialogue: 0,0:17:38.97,0:17:40.01,main,,0000,0000,0000,,Jsem rozhodnuta!
Dialogue: 0,0:17:41.81,0:17:44.74,genius,,0000,0000,0000,,{\frz10\pos(110,282)}Génius
Dialogue: 0,0:17:41.81,0:17:44.74,genius,,0000,0000,0000,,{\frz-30\pos(598,293)}Génius
Dialogue: 0,0:17:42.31,0:17:44.64,main,,0000,0000,0000,,Celkově to dělá 2 587 yenů.
Dialogue: 0,0:17:53.07,0:17:54.57,main,,0000,0000,0000,,No dobře…
Dialogue: 0,0:17:54.17,0:17:55.49,mmm,,0000,0000,0000,,{\pos(288,47)}Jedna extra…
Dialogue: 0,0:18:00.23,0:18:01.08,main,,0000,0000,0000,,Nějak to nejde.
Dialogue: 0,0:18:00.53,0:18:01.08,main2,,0000,0000,0000,,Omlouvám se.
Dialogue: 0,0:18:02.33,0:18:03.45,main,,0000,0000,0000,,Možná že se nechtějí oddělit.
Dialogue: 0,0:18:03.54,0:18:04.29,main2,,0000,0000,0000,,Jo…
Dialogue: 0,0:18:03.54,0:18:06.96,main,,0000,0000,0000,,Myslela jsem, že se tohle děje pouze v mange a anime…
Dialogue: 0,0:18:06.96,0:18:08.96,main,,0000,0000,0000,,…ale jak vidím tak se stává i v běžném životě.
Dialogue: 0,0:18:08.96,0:18:10.25,main,,0000,0000,0000,,Asi jo.
Dialogue: 0,0:18:10.25,0:18:11.54,main,,0000,0000,0000,,Což znamená…
Dialogue: 0,0:18:12.38,0:18:14.80,main,,0000,0000,0000,,Že mezi těma určitě zajiskřilo.
Dialogue: 0,0:18:14.80,0:18:15.88,main,,0000,0000,0000,,Konato…
Dialogue: 0,0:18:15.88,0:18:18.88,main,,0000,0000,0000,,Fantazie je něco smyšleného a nereálného.
Dialogue: 0,0:18:18.89,0:18:21.14,main,,0000,0000,0000,,Nedokážeš rozeznat mezi sny a realitou?
Dialogue: 0,0:18:21.17,0:18:24.18,copyrights,,0000,0000,0000,,{\pos(85,50)}Překlad: paralaX
Dialogue: 0,0:18:21.17,0:18:24.18,copyrights,,0000,0000,0000,,{\pos(129,76)}Domovská stránka: www.otakogu.cz
Dialogue: 0,0:18:26.98,0:18:28.81,main,,0000,0000,0000,,Oha-Lucky!
Dialogue: 0,0:18:28.81,0:18:31.52,main,,0000,0000,0000,,S radostí připravena šířit lásku!
Dialogue: 0,0:18:31.52,0:18:35.03,main,,0000,0000,0000,,Je tu vaše průvodkyně, Akira Kogami!
Dialogue: 0,0:18:35.32,0:18:36.82,main,,0000,0000,0000,,A já jsem její asistent.
Dialogue: 0,0:18:36.82,0:18:38.40,main,,0000,0000,0000,,Minoru Shiraishi.
Dialogue: 0,0:18:38.41,0:18:42.41,main,,0000,0000,0000,,Nemůžu uvěřit, že už je tu osmá epizoda.
Dialogue: 0,0:18:42.41,0:18:43.91,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:18:43.91,0:18:45.91,main,,0000,0000,0000,,Pokud nepřijdeme s nějakým čerstvým materiálem…
Dialogue: 0,0:18:45.91,0:18:47.45,main,,0000,0000,0000,,…lidé si začnou stěžovat.
Dialogue: 0,0:18:47.46,0:18:48.29,main,,0000,0000,0000,,Co, že to?
Dialogue: 0,0:18:49.33,0:18:50.67,main,,0000,0000,0000,,Co mám dělat?!
Dialogue: 0,0:18:51.25,0:18:51.88,main,,0000,0000,0000,,Um…
Dialogue: 0,0:18:51.88,0:18:54.96,main,,0000,0000,0000,,Jen jsem myslel, že by bylo dobré…
Dialogue: 0,0:18:53.48,0:18:54.16,main2,,0000,0000,0000,,Řekl jsi to…
Dialogue: 0,0:18:54.96,0:18:55.71,main,,0000,0000,0000,,Kdyby jsme zkusili něco nového…
Dialogue: 0,0:18:55.71,0:18:56.05,main,,0000,0000,0000,,Tak do toho.
Dialogue: 0,0:18:56.76,0:18:57.55,main,,0000,0000,0000,,To je pravda!
Dialogue: 0,0:18:57.55,0:18:59.17,main,,0000,0000,0000,,Nedávno jsem šla po městě…
Dialogue: 0,0:18:59.18,0:19:00.30,main,,0000,0000,0000,,…a někdo za mnou přišel a začal se mnou mluvit.
Dialogue: 0,0:19:00.72,0:19:01.89,main,,0000,0000,0000,,Nějaký průzkum?
Dialogue: 0,0:19:02.26,0:19:04.43,main,,0000,0000,0000,,No tak, Shiraishi…
Dialogue: 0,0:19:04.43,0:19:06.14,main,,0000,0000,0000,,To myslíš vážně?
Dialogue: 0,0:19:07.06,0:19:09.10,main,,0000,0000,0000,,Samozřejmě to byl fanoušek.
Dialogue: 0,0:19:09.10,0:19:10.89,main,,0000,0000,0000,,To by se tobě nikdy nestalo…
Dialogue: 0,0:19:10.90,0:19:11.81,main,,0000,0000,0000,,Že?!
Dialogue: 0,0:19:11.81,0:19:13.19,main,,0000,0000,0000,,T-T-T-To ne!
Dialogue: 0,0:19:16.65,0:19:17.48,main,,0000,0000,0000,,To je pravda…
Dialogue: 0,0:19:17.49,0:19:18.32,main,,0000,0000,0000,,Že?!
Dialogue: 0,0:19:18.32,0:19:19.70,main,,0000,0000,0000,,Ty jsi jen asistent…
Dialogue: 0,0:19:19.70,0:19:20.53,main,,0000,0000,0000,,Že?!
Dialogue: 0,0:19:20.53,0:19:22.61,main,,0000,0000,0000,,No tak se dál snaž.
Dialogue: 0,0:19:23.91,0:19:24.74,main,,0000,0000,0000,,A-Ale…
Dialogue: 0,0:19:24.74,0:19:26.04,main,,0000,0000,0000,,Už jsem dostal tolik…
Dialogue: 0,0:19:26.04,0:19:27.65,main,,0000,0000,0000,,…dopisů od fanoušků!
Dialogue: 0,0:19:28.46,0:19:30.55,main,,0000,0000,0000,,Cože?
Dialogue: 0,0:19:30.56,0:19:32.99,main,,0000,0000,0000,,Hej Itou! Dostala jsem nějaké i já?!
Dialogue: 0,0:19:31.99,0:19:32.99,main2,,0000,0000,0000,,Jo, cítím se jako celebrita.
Dialogue: 0,0:19:34.79,0:19:35.84,main,,0000,0000,0000,,Takže mám nějaké.
Dialogue: 0,0:19:35.84,0:19:37.42,main,,0000,0000,0000,,Jen jeden?
Dialogue: 0,0:19:38.51,0:19:41.93,main,,0000,0000,0000,,Vaše členství je už téměř u konce…
Dialogue: 0,0:19:42.02,0:19:42.80,main,,0000,0000,0000,,Hej!
Dialogue: 0,0:19:42.80,0:19:45.10,main,,0000,0000,0000,,To je jen nějaká stupidní online služba!
Dialogue: 0,0:19:45.10,0:19:46.76,main,,0000,0000,0000,,Všichni co se díváte!
Dialogue: 0,0:19:46.77,0:19:50.18,main,,0000,0000,0000,,Pošlete nějaké dopisy i mě, prosím!
Dialogue: 0,0:19:50.64,0:19:52.94,main,,0000,0000,0000,,Začala jsem je sbírat!
Dialogue: 0,0:19:54.52,0:19:55.52,main,,0000,0000,0000,,Akiro-sama.
Dialogue: 0,0:19:55.52,0:19:56.86,main,,0000,0000,0000,,Neměli by jsme se dostat k tématu…
Dialogue: 0,0:19:56.86,0:19:58.40,main,,0000,0000,0000,,Moc si nevyskakuj.
Dialogue: 0,0:19:58.40,0:20:00.44,main,,0000,0000,0000,,Jsi lehce nahraditelný.
Dialogue: 0,0:20:01.20,0:20:02.24,main,,0000,0000,0000,,Omlouvám se.
Dialogue: 0,0:20:03.37,0:20:04.20,main,,0000,0000,0000,,Ta-dá!
Dialogue: 0,0:20:04.20,0:20:06.08,main,,0000,0000,0000,,Mám tu jedno oznámení!
Dialogue: 0,0:20:06.08,0:20:06.70,main,,0000,0000,0000,,Wow!
Dialogue: 0,0:20:06.70,0:20:10.91,main,,0000,0000,0000,,Budeme mít v naší show figury!
Dialogue: 0,0:20:08.58,0:20:09.24,main2,,0000,0000,0000,,Akiro-sama!
Dialogue: 0,0:20:09.25,0:20:09.79,main2,,0000,0000,0000,,Tabulka!
Dialogue: 0,0:20:09.79,0:20:10.91,main2,,0000,0000,0000,,Je vzhůru nohama!
Dialogue: 0,0:20:11.96,0:20:13.79,main,,0000,0000,0000,,Oh! To je naschvál!
Dialogue: 0,0:20:13.79,0:20:15.42,main,,0000,0000,0000,,Jste génius!
Dialogue: 0,0:20:15.42,0:20:16.54,main,,0000,0000,0000,,Popravdě…
Dialogue: 0,0:20:16.54,0:20:18.21,main,,0000,0000,0000,,…tentokrát je plán…
Dialogue: 0,0:20:20.01,0:20:21.60,main2,,0000,0000,0000,,Nedotýkej se mě.
Dialogue: 0,0:20:22.30,0:20:22.68,main,,0000,0000,0000,,Heh?
Dialogue: 0,0:20:22.68,0:20:23.76,main,,0000,0000,0000,,To je divné.
Dialogue: 0,0:20:24.80,0:20:27.30,main,,0000,0000,0000,,Tento projekt je…
Dialogue: 0,0:20:27.97,0:20:29.97,main,,0000,0000,0000,,Nedotýkej se mě.
Dialogue: 0,0:20:31.23,0:20:32.52,main,,0000,0000,0000,,Tohle je…
Dialogue: 0,0:20:37.94,0:20:39.61,main,,0000,0000,0000,,Co je s tím?!
Dialogue: 0,0:20:41.36,0:20:43.20,main,,0000,0000,0000,,H-H-Hey, Seishi!
Dialogue: 0,0:20:43.20,0:20:45.32,main,,0000,0000,0000,,Mám to!
Dialogue: 0,0:20:43.20,0:20:44.49,main2,,0000,0000,0000,,Uklidněte se Akiro-sama!
Dialogue: 0,0:20:44.49,0:20:46.99,main2,,0000,0000,0000,,To je jméno vašeho skutečného managera!
Dialogue: 0,0:20:45.32,0:20:47.69,main,,0000,0000,0000,,To je moje!
Dialogue: 0,0:20:47.69,0:20:49.74,main,,0000,0000,0000,,Proč…
Dialogue: 0,0:20:49.74,0:20:50.41,main,,0000,0000,0000,,Moc se omlouvám!
Dialogue: 0,0:20:50.41,0:20:50.95,main,,0000,0000,0000,,Fajn!
Dialogue: 0,0:20:50.95,0:20:57.42,main,,0000,0000,0000,,Potom se rozhodneme koho vyrobíme na základě vašich hlasů.
Dialogue: 0,0:20:57.42,0:21:01.26,main,,0000,0000,0000,,Posílejte nám vaše hlasy!
Dialogue: 0,0:21:01.72,0:21:04.01,main,,0000,0000,0000,,Všichni prosím hlasujte!
Dialogue: 0,0:21:04.30,0:21:04.76,main,,0000,0000,0000,,Fajn.
Dialogue: 0,0:21:04.76,0:21:06.55,main,,0000,0000,0000,,To je pro dnešek vše.
Dialogue: 0,0:21:06.55,0:21:09.18,main,,0000,0000,0000,,Těšte se na další díl Lucky Channelu!
Dialogue: 0,0:21:09.18,0:21:10.64,main,,0000,0000,0000,,Ahoj-ahoj!
Dialogue: 0,0:21:13.18,0:21:16.77,main,,0000,0000,0000,,Je jasný, že to vyhraje někdo z hlavní show.
Dialogue: 0,0:21:16.77,0:21:17.77,main,,0000,0000,0000,,Proč?
Dialogue: 0,0:21:17.77,0:21:18.90,main,,0000,0000,0000,,Jako bych měla nějakou šanci.
Dialogue: 0,0:21:18.90,0:21:21.03,main,,0000,0000,0000,,Nemůžete si být jistá.
Dialogue: 0,0:21:21.40,0:21:21.73,main,,0000,0000,0000,,Ah…
Dialogue: 0,0:21:21.74,0:21:22.94,main,,0000,0000,0000,,Konečně budu mít vlastní figuru.
Dialogue: 0,0:21:22.94,0:21:23.82,main,,0000,0000,0000,,O čem to mluvíš?
Dialogue: 0,0:21:23.82,0:21:24.65,main,,0000,0000,0000,,Tohle je…
Dialogue: 0,0:21:26.41,0:21:28.28,main,,0000,0000,0000,,To bylo úžasné, Yuki-chan!
Dialogue: 0,0:21:28.28,0:21:29.95,main,,0000,0000,0000,,Děkuji.
Dialogue: 0,0:21:29.95,0:21:32.29,main,,0000,0000,0000,,Tohle jsem od tebe nečekala.
Dialogue: 0,0:21:32.41,0:21:34.71,main,,0000,0000,0000,,To ne, je mi trapně.
Dialogue: 0,0:21:34.71,0:21:37.37,main,,0000,0000,0000,,Hej, Yuki-chan. Zpívejme společně.
Dialogue: 0,0:21:38.00,0:21:38.96,main,,0000,0000,0000,,Dobře.
Dialogue: 0,0:21:39.54,0:21:40.63,main,,0000,0000,0000,,Do toho! Do toho!
Dialogue: 0,0:21:40.63,0:21:41.80,main,,0000,0000,0000,,Duet?
Dialogue: 0,0:21:41.80,0:21:43.13,main,,0000,0000,0000,,Kterou znáš?
Dialogue: 0,0:21:43.13,0:21:44.26,main,,0000,0000,0000,,No…
Dialogue: 0,0:21:44.26,0:21:46.55,main,,0000,0000,0000,,Než se rozhodnou tak budu zpívat já.
Dialogue: 0,0:21:46.55,0:21:47.97,main,,0000,0000,0000,,To budeš zpívat nonstop?
Dialogue: 0,0:21:47.97,0:21:50.26,main,,0000,0000,0000,,Kagami, i ty bys měla zpívat!
Dialogue: 0,0:21:52.60,0:21:53.64,main,,0000,0000,0000,,Co je to?
Dialogue: 0,0:21:53.64,0:21:55.14,main,,0000,0000,0000,,Cesta na západ.
Dialogue: 0,0:21:55.14,0:21:57.73,main,,0000,0000,0000,,Machaaki jako Son Goku byl výborný!
Dialogue: 0,0:21:57.73,0:21:58.52,main,,0000,0000,0000,,Heh?
Dialogue: 0,0:21:58.52,0:21:59.98,main,,0000,0000,0000,,Nebyl to Shingo-chan?
Dialogue: 0,0:21:59.98,0:22:01.98,main,,0000,0000,0000,,To byl třetí remake.
Dialogue: 0,0:22:01.98,0:22:06.24,main,,0000,0000,0000,,Tohle je ze série která běžela od roku 1978 do roku 1980.
Dialogue: 0,0:22:06.24,0:22:08.66,main,,0000,0000,0000,,Původní série!
Dialogue: 0,0:22:08.66,0:22:09.78,main,,0000,0000,0000,,Kolik ti je?!
Dialogue: 0,0:22:12.24,0:22:13.79,main,,0000,0000,0000,,To je dlouhé intro…
Dialogue: 0,0:22:13.79,0:22:17.92,main,,0000,0000,0000,,Teď začíná vyprávění.
Dialogue: 0,0:22:17.92,0:22:19.21,main,,0000,0000,0000,,Jak to mám vědět?
Dialogue: 0,0:22:20.79,0:22:21.42,main,,0000,0000,0000,,Je to tady!
Dialogue: 0,0:22:23.89,0:22:25.32,main,,0000,0000,0000,,Zase řveš?!
Dialogue: 0,0:22:47.65,0:22:50.41,mainup2,,0000,0000,0000,,Zrozen z vejce na vrcholu hory.
Dialogue: 0,0:22:50.41,0:22:52.37,main,,0000,0000,0000,,Je to v angličtině. Kašlu na to.
Dialogue: 0,0:22:52.37,0:22:54.54,main,,0000,0000,0000,,To je po tom všem napínání vše?
