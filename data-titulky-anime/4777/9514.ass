Dialogue: 0,0:00:01.11,0:00:02.56,main,,0000,0000,0000,,Tak začneme.
Dialogue: 0,0:00:02.57,0:00:03.61,main,,0000,0000,0000,,Jdeme na to.
Dialogue: 0,0:00:04.19,0:00:05.98,main,,0000,0000,0000,,No tak už začněme!
Dialogue: 0,0:00:06.32,0:00:07.49,op1,,0000,0000,0000,,Okolo 3 cm!
Dialogue: 0,0:00:07.49,0:00:08.78,op1,,0000,0000,0000,,Je to snad málo?
Dialogue: 0,0:00:08.78,0:00:09.03,op1,,0000,0000,0000,,Jejda!
Dialogue: 0,0:00:09.03,0:00:10.86,op1,,0000,0000,0000,,Schované pod naší uniformou!
Dialogue: 0,0:00:10.87,0:00:11.91,op1,,0000,0000,0000,,Mám toho nechat?
Dialogue: 0,0:00:11.91,0:00:12.20,op1,,0000,0000,0000,,JEJ!
Dialogue: 0,0:00:12.20,0:00:13.28,op1,,0000,0000,0000,,Jít do toho?
Dialogue: 0,0:00:13.28,0:00:14.08,op1,,0000,0000,0000,,Snažit se?
Dialogue: 0,0:00:14.08,0:00:15.66,op1,,0000,0000,0000,,Vychutnat si každý okamžik. Jo!
Dialogue: 0,0:00:15.66,0:00:16.25,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.26,0:00:16.50,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:16.50,0:00:16.90,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.91,0:00:17.25,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:17.25,0:00:17.87,op1,,0000,0000,0000,,Jen mezi námi dvěma!
Dialogue: 0,0:00:17.87,0:00:20.58,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:00:22.56,0:00:23.36,logo,,0000,0000,0000,,{\move(108,218,202,218,0,0)}LUCKY
Dialogue: 0,0:00:23.36,0:00:25.08,logo,,0000,0000,0000,,{\pos(202,218)}LUCKY
Dialogue: 0,0:00:22.56,0:00:23.36,logo,,0000,0000,0000,,{\move(638,218,459,218,0,0)}STAR
Dialogue: 0,0:00:23.36,0:00:25.08,logo,,0000,0000,0000,,{\pos(459,218)}STAR
Dialogue: 0,0:00:25.51,0:00:26.09,op1,,0000,0000,0000,,Cítím se nějak nesvá.
Dialogue: 0,0:00:26.09,0:00:26.71,op1,,0000,0000,0000,,Co jsem to jedla?
Dialogue: 0,0:00:26.71,0:00:27.30,op1,,0000,0000,0000,,Mám tě hrozně ráda!
Dialogue: 0,0:00:27.30,0:00:27.51,op1,,0000,0000,0000,,Oops!
Dialogue: 0,0:00:27.51,0:00:28.55,op1,,0000,0000,0000,,Jedno z nich je špatně!
Dialogue: 0,0:00:28.55,0:00:29.30,op1,,0000,0000,0000,,Relaxovat!
Dialogue: 0,0:00:29.30,0:00:29.93,op1,,0000,0000,0000,,Nic nedělat!
Dialogue: 0,0:00:29.93,0:00:30.47,op1,,0000,0000,0000,,Flákat se!
Dialogue: 0,0:00:30.47,0:00:31.55,op1,,0000,0000,0000,,Už toho nechej!
Dialogue: 0,0:00:31.55,0:00:33.21,op1,,0000,0000,0000,,Je ta nová holka oblíbená?
Dialogue: 0,0:00:33.21,0:00:34.85,op1,,0000,0000,0000,,Já bych řekla, že je docela normální!
Dialogue: 0,0:00:34.85,0:00:36.35,op1,,0000,0000,0000,,To jsem jediná překvapená?
Dialogue: 0,0:00:36.35,0:00:36.66,op1,,0000,0000,0000,,Vepřové kousky!
Dialogue: 0,0:00:36.66,0:00:37.06,op1,,0000,0000,0000,,Skleněné nudle!
Dialogue: 0,0:00:37.06,0:00:37.39,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:00:37.39,0:00:38.18,op2,,0000,0000,0000,,DA-DA-DA-DA-DA!
Dialogue: 0,0:00:38.73,0:00:39.52,op1,,0000,0000,0000,,Ooh?
Dialogue: 0,0:00:39.52,0:00:40.31,op1,,0000,0000,0000,,Víš co tím myslím?
Dialogue: 0,0:00:39.89,0:00:40.73,op1up,,0000,0000,0000,,Moje brýle…
Dialogue: 0,0:00:40.73,0:00:41.81,op1up,,0000,0000,0000,,Kde mám brýle?
Dialogue: 0,0:00:41.90,0:00:43.86,op1,,0000,0000,0000,,Nedávno jsme hrála tu samou bojovku.
Dialogue: 0,0:00:42.48,0:00:43.86,op1up,,0000,0000,0000,,Bez nich nic nevidím…
Dialogue: 0,0:00:44.90,0:00:45.48,op2,,0000,0000,0000,,POM! POM!
Dialogue: 0,0:00:45.48,0:00:46.53,op1,,0000,0000,0000,,Roztleskávačky!
Dialogue: 0,0:00:46.53,0:00:48.07,op2,,0000,0000,0000,,DÁME SI SLADKOST!
Dialogue: 0,0:00:48.07,0:00:48.65,op2,,0000,0000,0000,,LA! LA!
Dialogue: 0,0:00:48.65,0:00:49.70,op1,,0000,0000,0000,,Párty začíná!
Dialogue: 0,0:00:49.70,0:00:51.28,op2,,0000,0000,0000,,MÁŠ TO V KRVI!
Dialogue: 0,0:00:51.28,0:00:51.86,op2,,0000,0000,0000,,ANO!
Dialogue: 0,0:00:51.87,0:00:52.87,op1,,0000,0000,0000,,Zanechává to dojem.
Dialogue: 0,0:00:52.87,0:00:54.32,op1,,0000,0000,0000,,Malé planetky.
Dialogue: 0,0:00:54.33,0:00:54.87,op1,,0000,0000,0000,,Narazili!
Dialogue: 0,0:00:54.87,0:00:55.49,op1,,0000,0000,0000,,A shořeli!
Dialogue: 0,0:00:55.49,0:00:55.87,op1,,0000,0000,0000,,Bylo to ohromné!
Dialogue: 0,0:00:55.87,0:00:56.62,op1,,0000,0000,0000,,Z plných plic křičíme!
Dialogue: 0,0:00:56.62,0:00:57.18,op1,,0000,0000,0000,,ZPÍVEJ A TANCUJ!
Dialogue: 0,0:00:57.54,0:00:58.83,op1,,0000,0000,0000,,Přiznej si to!
Dialogue: 0,0:00:58.83,0:01:03.29,op1,,0000,0000,0000,,Nakonec se stejně budeme smát!
Dialogue: 0,0:01:03.29,0:01:07.05,op1,,0000,0000,0000,,Protože mám na sobě námořnickou uniformu.
Dialogue: 0,0:01:07.05,0:01:09.92,op2,,0000,0000,0000,,To je ono!
Dialogue: 0,0:01:10.51,0:01:14.05,op1,,0000,0000,0000,,Je teprve pondělí a já už mám špatnou náladu.
Dialogue: 0,0:01:14.05,0:01:15.85,op1,,0000,0000,0000,,Co mám dělat?
Dialogue: 0,0:01:16.06,0:01:19.89,op1,,0000,0000,0000,,Raději si vezmu moji letní uniformu -
Dialogue: 0,0:01:19.89,0:01:22.77,op1,,0000,0000,0000,,- je rozkošná!
Dialogue: 0,0:01:23.02,0:01:24.19,op1,,0000,0000,0000,,Trojdílná uniforma!
Dialogue: 0,0:01:24.19,0:01:25.61,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:01:25.61,0:01:26.06,op1,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.07,0:01:26.48,op1,,0000,0000,0000,,Dej se do toho!
Dialogue: 0,0:01:26.48,0:01:26.86,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.86,0:01:27.48,op1,,0000,0000,0000,,SNAŽ SE!
Dialogue: 0,0:01:27.48,0:01:27.65,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:27.65,0:01:29.54,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:01:33.34,0:01:36.84,main,,0000,0000,0000,,Už jsem na sobě nějakou dobu neměla mou uniformu.
Dialogue: 0,0:01:37.68,0:01:38.93,main,,0000,0000,0000,,To je Kona-chan.
Dialogue: 0,0:01:38.93,0:01:39.76,main,,0000,0000,0000,,Kona-chan!
Dialogue: 0,0:01:39.76,0:01:40.80,main,,0000,0000,0000,,Dobré ráno!
Dialogue: 0,0:01:42.60,0:01:43.64,main,,0000,0000,0000,,Páni…
Dialogue: 0,0:01:43.64,0:01:46.14,main,,0000,0000,0000,,Kagami-san.
Dialogue: 0,0:01:46.14,0:01:47.31,main,,0000,0000,0000,,Přeji ti moc krásný den.
Dialogue: 0,0:01:49.98,0:01:52.65,main,,0000,0000,0000,,Ále, Tsukaso…
Dialogue: 0,0:01:52.65,0:01:54.48,main,,0000,0000,0000,,Máš špatně uvázanou mašli.
Dialogue: 0,0:01:55.24,0:01:56.15,main,,0000,0000,0000,,Díky.
Dialogue: 0,0:01:56.15,0:01:57.15,main,,0000,0000,0000,,Ew.
Dialogue: 0,0:01:57.15,0:01:59.49,main,,0000,0000,0000,,Co to s tebou tak najednou je, Kono-chan?
Dialogue: 0,0:01:59.49,0:02:00.07,main,,0000,0000,0000,,No…
Dialogue: 0,0:02:00.41,0:02:04.08,main,,0000,0000,0000,,Stala jsem se závislou na Marimite.
Dialogue: 0,0:02:04.08,0:02:05.04,main,,0000,0000,0000,,Marimite?
Dialogue: 0,0:02:05.04,0:02:06.33,main,,0000,0000,0000,,Jseš snad dítě?
Dialogue: 0,0:02:07.08,0:02:10.08,whitebig,,0000,0000,0000,,{\frz-90\pos(389,189)}Vzhled
Dialogue: 0,0:02:10.46,0:02:11.54,main,,0000,0000,0000,,Hej…
Dialogue: 0,0:02:11.54,0:02:14.67,main,,0000,0000,0000,,Doufám že nezačneš kopírovat i jejich kostýmy.
Dialogue: 0,0:02:14.67,0:02:15.38,main,,0000,0000,0000,,Um…
Dialogue: 0,0:02:15.38,0:02:16.76,main,,0000,0000,0000,,Říká se tomu cosplay?
Dialogue: 0,0:02:16.76,0:02:18.88,main,,0000,0000,0000,,Tak daleko bych nešla.
Dialogue: 0,0:02:18.88,0:02:19.80,main,,0000,0000,0000,,To doufám.
Dialogue: 0,0:02:20.18,0:02:23.05,main,,0000,0000,0000,,Toho mám dost v práci.
Dialogue: 0,0:02:23.85,0:02:24.47,main,,0000,0000,0000,,Ah, jo…
Dialogue: 0,0:02:24.47,0:02:26.52,main,,0000,0000,0000,,Ty vlastně děláš na jednom z těch míst…
Dialogue: 0,0:02:26.93,0:02:29.64,main,,0000,0000,0000,,Ale pokud by se Kona-chan oblékala jako dáma…
Dialogue: 0,0:02:34.94,0:02:37.23,main,,0000,0000,0000,,Je to tak divný, že si to ani nedokážu představit.
Dialogue: 0,0:02:37.57,0:02:40.65,main,,0000,0000,0000,,Poslední dobou dávají nějak moc baseballu.
Dialogue: 0,0:02:40.66,0:02:43.95,main,,0000,0000,0000,,Proč musí být fanoušci anime ti kterým přesunou program?
Dialogue: 0,0:02:44.28,0:02:46.08,main,,0000,0000,0000,,Pokud se chceš na anime dívat tak moc…
Dialogue: 0,0:02:46.08,0:02:48.25,main,,0000,0000,0000,,…mohla bys mít kabelovku nebo tak něco.
Dialogue: 0,0:02:48.25,0:02:49.83,main,,0000,0000,0000,,Nejsou tam specializovaný kanály právě na anime?
Dialogue: 0,0:02:50.96,0:02:52.12,main,,0000,0000,0000,,Kdybych mohla…
Dialogue: 0,0:02:52.13,0:02:53.67,main,,0000,0000,0000,,Bůh ví, že bych chtěla…
Dialogue: 0,0:02:53.67,0:02:56.00,main,,0000,0000,0000,,Ale ti od kabelovky řekli…
Dialogue: 0,0:02:56.00,0:02:57.55,main,,0000,0000,0000,,že k nám ho natáhnout nejde.
Dialogue: 0,0:03:00.51,0:03:01.34,main,,0000,0000,0000,,Yuki-chan.
Dialogue: 0,0:03:01.34,0:03:02.13,main,,0000,0000,0000,,Dobré ráno.
Dialogue: 0,0:03:02.64,0:03:04.93,main,,0000,0000,0000,,Dobré ráno, Tsukaso.
Dialogue: 0,0:03:06.06,0:03:06.72,main,,0000,0000,0000,,Vypadáš nějak unaveně.
Dialogue: 0,0:03:06.72,0:03:08.14,main,,0000,0000,0000,,Stalo se něco?
Dialogue: 0,0:03:08.48,0:03:11.69,main,,0000,0000,0000,,No, byla jsem u zubaře aby mi udělal korunku.
Dialogue: 0,0:03:12.60,0:03:14.36,main,,0000,0000,0000,,Aha, u zubaře.
Dialogue: 0,0:03:14.36,0:03:15.15,main,,0000,0000,0000,,Dobrý!
Dialogue: 0,0:03:15.48,0:03:20.74,main,,0000,0000,0000,,Proč se pak k němu musí tak často vracet?
Dialogue: 0,0:03:21.53,0:03:24.03,main,,0000,0000,0000,,Úplně chápu co myslíš!
Dialogue: 0,0:03:24.03,0:03:26.24,main,,0000,0000,0000,,A není to jen jednou nebo dvakrát!
Dialogue: 0,0:03:26.79,0:03:27.95,main,,0000,0000,0000,,Miyuki-san.
Dialogue: 0,0:03:27.95,0:03:29.20,main,,0000,0000,0000,,Miyuki-san.
Dialogue: 0,0:03:30.00,0:03:31.08,main,,0000,0000,0000,,Konato-san.
Dialogue: 0,0:03:31.08,0:03:32.25,main,,0000,0000,0000,,Dobrý den.
Dialogue: 0,0:03:32.25,0:03:33.54,main,,0000,0000,0000,,Jak se máš?
Dialogue: 0,0:03:34.67,0:03:35.79,main,,0000,0000,0000,,Díky za optání.
Dialogue: 0,0:03:36.09,0:03:38.80,main,,0000,0000,0000,,Těší mě, že se ti daří.
Dialogue: 0,0:03:36.84,0:03:38.38,main2,,0000,0000,0000,,Yuki-chan je úžasná.
Dialogue: 0,0:03:38.38,0:03:39.76,main2,,0000,0000,0000,,Vůbec to s ní nehlo…
Dialogue: 0,0:03:39.76,0:03:41.80,main,,0000,0000,0000,,… a navíc to ani nevypadá hloupě.
Dialogue: 0,0:03:41.80,0:03:44.93,main,,0000,0000,0000,,A ani ji nezaráží jak mluví
Dialogue: 0,0:03:45.60,0:03:49.52,main,,0000,0000,0000,,Tohle je poprvé co jsem slyšela někoho říct „Jak si se poslední dobou měla?“!
Dialogue: 0,0:03:50.48,0:03:51.68,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:03:51.69,0:03:54.23,main,,0000,0000,0000,,V dívčích školách do kterých chodily všechny moje kamarádky…
Dialogue: 0,0:03:54.23,0:03:57.23,main,,0000,0000,0000,,…se to běžně používá.
Dialogue: 0,0:03:58.82,0:04:01.78,main,,0000,0000,0000,,Škola pro vysoce postavené dívky je něco jiného, co říkáš?
Dialogue: 0,0:04:01.78,0:04:03.11,main,,0000,0000,0000,,Jak vytříbené.
Dialogue: 0,0:04:03.11,0:04:04.11,main,,0000,0000,0000,,Kuroi…
Dialogue: 0,0:04:04.11,0:04:06.74,main,,0000,0000,0000,,…mi řekla abych vybral úkoly.
Dialogue: 0,0:04:06.74,0:04:08.78,main,,0000,0000,0000,,Excelentní práce Sebastiane.
Dialogue: 0,0:04:08.79,0:04:09.28,main,,0000,0000,0000,,S-Seba-
Dialogue: 0,0:04:09.74,0:04:12.79,main,,0000,0000,0000,,Sluhové se musí jmenovat Sebastian, co myslíš?
Dialogue: 0,0:04:13.54,0:04:15.17,main,,0000,0000,0000,,Jo to sedí.
Dialogue: 0,0:04:15.32,0:04:17.13,main,,0000,0000,0000,,Řekni nám veselou historku, Sebastiane.
Dialogue: 0,0:04:16.42,0:04:17.13,main2,,0000,0000,0000,,To ne!
Dialogue: 0,0:04:17.46,0:04:18.21,main,,0000,0000,0000,,Fajn.
Dialogue: 0,0:04:18.21,0:04:19.84,main,,0000,0000,0000,,Prázdniny jsou u konce…
Dialogue: 0,0:04:19.84,0:04:21.46,main,,0000,0000,0000,,…takže pokračujeme dál.
Dialogue: 0,0:04:21.88,0:04:23.92,main,,0000,0000,0000,,Začněte přemýšlet o své budoucnosti…
Dialogue: 0,0:04:23.93,0:04:25.59,main,,0000,0000,0000,,…a vložte snahu do studií.
Dialogue: 0,0:04:26.43,0:04:29.30,main,,0000,0000,0000,,Tím myslí kam půjdeme po střední?
Dialogue: 0,0:04:29.31,0:04:31.85,main,,0000,0000,0000,,Přece jen tohle je pouze drobný úsek v našich životech.
Dialogue: 0,0:04:31.85,0:04:35.69,main,,0000,0000,0000,,Ty jsi ale nejspíš o budoucnosti moc nepřemýšlela, že?
Dialogue: 0,0:04:36.35,0:04:36.81,main,,0000,0000,0000,,Cože?
Dialogue: 0,0:04:36.81,0:04:37.86,main,,0000,0000,0000,,Nenavážej se do mě!
Dialogue: 0,0:04:37.86,0:04:38.52,main,,0000,0000,0000,,Protože…
Dialogue: 0,0:04:38.52,0:04:42.53,main,,0000,0000,0000,,jsi si vybrala humanitní předměty jen proto, aby jsi mohla být ve stejné třídě s námi, že?
Dialogue: 0,0:04:42.53,0:04:43.65,main,,0000,0000,0000,,Tsu-Tsukaso!
Dialogue: 0,0:04:43.65,0:04:45.74,main,,0000,0000,0000,,Tys jim to vykecala!
Dialogue: 0,0:04:44.65,0:04:45.74,main2,,0000,0000,0000,,Ah, promiň.
Dialogue: 0,0:04:45.74,0:04:47.20,main,,0000,0000,0000,,A zrovna jí…
Dialogue: 0,0:04:47.20,0:04:48.32,main,,0000,0000,0000,,…ze všech možných lidí!
Dialogue: 0,0:04:48.32,0:04:49.12,main,,0000,0000,0000,,No tak…
Dialogue: 0,0:04:49.12,0:04:53.58,main,,0000,0000,0000,,buď k sobě upřímná.
Dialogue: 0,0:04:53.58,0:04:54.66,main,,0000,0000,0000,,Tady, tady.
Dialogue: 0,0:04:55.12,0:04:56.08,main,,0000,0000,0000,,Sklapni!
Dialogue: 0,0:05:04.05,0:05:05.17,main,,0000,0000,0000,,Hej!
Dialogue: 0,0:05:05.18,0:05:06.55,main,,0000,0000,0000,,Jsem zpět!
Dialogue: 0,0:05:07.47,0:05:08.47,main,,0000,0000,0000,,Sestřenko Yui…
Dialogue: 0,0:05:08.93,0:05:10.60,main,,0000,0000,0000,,Můj letní úkol…
Dialogue: 0,0:05:10.60,0:05:16.44,main,,0000,0000,0000,,Udělala jsem, jaks mi řekla a napsala jsem náhodný věci, když jsem nevěděla odpověď…
Dialogue: 0,0:05:16.44,0:05:18.40,main,,0000,0000,0000,,…a teď to musím celý udělat znova!
Dialogue: 0,0:05:18.40,0:05:20.31,main,,0000,0000,0000,,Zachraň mě!
Dialogue: 0,0:05:20.32,0:05:22.07,main,,0000,0000,0000,,F-fajn…
Dialogue: 0,0:05:22.07,0:05:23.32,main,,0000,0000,0000,,Ale…
Dialogue: 0,0:05:23.32,0:05:26.45,main,,0000,0000,0000,,Pokud ti pomůžu já, tak je možný, že to budeš muset udělat zase znova…
Dialogue: 0,0:05:26.45,0:05:27.24,main,,0000,0000,0000,,…pokud víš co tím myslím…
Dialogue: 0,0:05:27.24,0:05:27.95,main,,0000,0000,0000,,Vykecala se z toho…
Dialogue: 0,0:05:28.49,0:05:32.66,main,,0000,0000,0000,,Prodloužení úkolu bez prodloužení prázdnin…
Dialogue: 0,0:05:32.66,0:05:34.91,main,,0000,0000,0000,,Možná bych si měla půjčit Kagaminy poznámky.
Dialogue: 0,0:05:54.52,0:05:57.64,main,,0000,0000,0000,,Zvuk cikád mě uspal.
Dialogue: 0,0:06:01.82,0:06:02.31,main,,0000,0000,0000,,Heh?
Dialogue: 0,0:06:10.20,0:06:11.45,main,,0000,0000,0000,,Chápu…
Dialogue: 0,0:06:11.45,0:06:14.58,main,,0000,0000,0000,,Program je rozházený protože baseball náhle skončil.
Dialogue: 0,0:06:15.50,0:06:17.71,main,,0000,0000,0000,,Někdy se to hodí.
Dialogue: 0,0:06:18.92,0:06:20.54,main,,0000,0000,0000,,Video je vyplé!
Dialogue: 0,0:06:21.17,0:06:21.88,main,,0000,0000,0000,,Heh?
Dialogue: 0,0:06:21.88,0:06:23.59,main,,0000,0000,0000,,Kde máš brýle, Yuki-chan?
Dialogue: 0,0:06:23.59,0:06:24.50,main,,0000,0000,0000,,Vlastně…
Dialogue: 0,0:06:24.50,0:06:26.05,main,,0000,0000,0000,,Dnes ráno jsem je omylem rozbila…
Dialogue: 0,0:06:26.80,0:06:29.76,main,,0000,0000,0000,,Zapoměla jsem, že jsem je nechala na židli…
Dialogue: 0,0:06:29.76,0:06:30.76,main,,0000,0000,0000,,…a omylem jsem…
Dialogue: 0,0:06:31.18,0:06:32.22,main,,0000,0000,0000,,Omluvám se…
Dialogue: 0,0:06:32.22,0:06:34.97,main,,0000,0000,0000,,…ale mohla bych si dnes půjčit tvé poznámky?
Dialogue: 0,0:06:35.47,0:06:36.31,main,,0000,0000,0000,,Jasně.
Dialogue: 0,0:06:36.31,0:06:38.56,main,,0000,0000,0000,,Pořád si tak nešikovná.
Dialogue: 0,0:06:39.45,0:06:43.15,newspaper,,0000,0000,0000,,{\pos(620,47)}Konatiny poznámky
Dialogue: 0,0:06:40.31,0:06:41.44,main,,0000,0000,0000,,Konata-san…
Dialogue: 0,0:06:40.71,0:06:43.15,newspaper,,0000,0000,0000,,{\pos(135,52)}Špatné písmo
Dialogue: 0,0:06:41.44,0:06:43.15,main,,0000,0000,0000,,Tvůj rukopis je mi záhadou.
Dialogue: 0,0:06:44.07,0:06:47.54,newspaper,,0000,0000,0000,,{\pos(620,47)}Tsukasiny poznámky
Dialogue: 0,0:06:45.07,0:06:46.48,main,,0000,0000,0000,,Tsukaso…
Dialogue: 0,0:06:45.32,0:06:47.54,newspaper,,0000,0000,0000,,{\pos(175,52)}Tak podrobné, že nestihla zapsat vše
Dialogue: 0,0:06:46.48,0:06:47.44,main,,0000,0000,0000,,Počkat!
Dialogue: 0,0:06:48.15,0:06:50.99,main,,0000,0000,0000,,Jsou vážně k ničemu.
Dialogue: 0,0:06:50.99,0:06:51.78,main,,0000,0000,0000,,Tady.
Dialogue: 0,0:06:51.78,0:06:53.41,main,,0000,0000,0000,,Mockrát děkuji.
Dialogue: 0,0:06:53.41,0:06:56.58,main,,0000,0000,0000,,Je dobrý, že máme stejného učitele.
Dialogue: 0,0:06:56.58,0:06:57.37,main,,0000,0000,0000,,Um…
Dialogue: 0,0:06:57.37,0:07:00.21,main,,0000,0000,0000,,Mohla by sis prosím nechat toto pro sebe…
Dialogue: 0,0:07:00.21,0:07:03.58,main,,0000,0000,0000,,Nechci je zranit…
Dialogue: 0,0:07:05.13,0:07:06.67,main,,0000,0000,0000,,Mám hlad!
Dialogue: 0,0:07:07.01,0:07:08.01,main,,0000,0000,0000,,Yuki-chan!
Dialogue: 0,0:07:08.01,0:07:09.21,main,,0000,0000,0000,,Pojďme na oběd!
Dialogue: 0,0:07:10.18,0:07:11.09,main,,0000,0000,0000,,Tsukaso.
Dialogue: 0,0:07:11.09,0:07:11.93,main,,0000,0000,0000,,Konato.
Dialogue: 0,0:07:12.34,0:07:14.18,main,,0000,0000,0000,,Děkuji, že jste mi půjčili vaše poznámky.
Dialogue: 0,0:07:14.43,0:07:15.60,main,,0000,0000,0000,,Nah.
Dialogue: 0,0:07:15.60,0:07:17.60,main,,0000,0000,0000,,To nic.
Dialogue: 0,0:07:17.60,0:07:21.52,main,,0000,0000,0000,,Nikdy bych neřekla, že si ode mne Yuki-chan půjčí poznámky.
Dialogue: 0,0:07:21.52,0:07:22.02,main,,0000,0000,0000,,No…
Dialogue: 0,0:07:22.02,0:07:22.98,main,,0000,0000,0000,,Znova se to už nestane.
Dialogue: 0,0:07:25.62,0:07:27.73,green,,0000,0000,0000,,{\pos(358,91)}Hledí
Dialogue: 0,0:07:27.98,0:07:29.07,main,,0000,0000,0000,,U-Um…
Dialogue: 0,0:07:29.07,0:07:30.49,main,,0000,0000,0000,,Mám něco na obličeji?
Dialogue: 0,0:07:31.53,0:07:34.62,main,,0000,0000,0000,,Když se ve hrách nějaká postava nosící brýle ukáže ve škole bez nich…
Dialogue: 0,0:07:34.62,0:07:36.74,main,,0000,0000,0000,,…tak se z ničeho nic stane velice populární…
Dialogue: 0,0:07:36.74,0:07:38.83,main,,0000,0000,0000,,Ale hádám, že to ve skutečnosti neplatí.
Dialogue: 0,0:07:38.83,0:07:40.70,main,,0000,0000,0000,,T-To vážně ne.
Dialogue: 0,0:07:40.71,0:07:41.91,main,,0000,0000,0000,,Bohužel.
Dialogue: 0,0:07:42.33,0:07:45.04,main,,0000,0000,0000,,Nepřipomínají vám čokoládové kornouty vrtačky?
Dialogue: 0,0:07:45.59,0:07:46.46,main,,0000,0000,0000,,V-Vrtačky…
Dialogue: 0,0:07:47.55,0:07:52.13,main,,0000,0000,0000,,Nebavili jsme se před tím, která strana je hlava?
Dialogue: 0,0:07:52.13,0:07:53.97,main,,0000,0000,0000,,Že ta tenká vypadá jako spodek.
Dialogue: 0,0:07:55.43,0:07:56.64,main,,0000,0000,0000,,Na druhou stranu…
Dialogue: 0,0:07:56.64,0:07:58.76,main,,0000,0000,0000,,…ta tlustá vypadá víc jako zadek.
Dialogue: 0,0:07:58.77,0:07:59.39,main,,0000,0000,0000,,Zadek?
Dialogue: 0,0:08:02.02,0:08:03.35,main,,0000,0000,0000,,To je pořádnej zadek…
Dialogue: 0,0:08:03.35,0:08:04.52,main,,0000,0000,0000,,A dost tučnej.
Dialogue: 0,0:08:04.90,0:08:06.81,main,,0000,0000,0000,,Když člověk ztloustne…
Dialogue: 0,0:08:06.82,0:08:09.73,main,,0000,0000,0000,,…tak je pak těžké si koupit sukni a tak.
Dialogue: 0,0:08:09.73,0:08:10.86,main,,0000,0000,0000,,To je pravda!
Dialogue: 0,0:08:10.86,0:08:13.49,main,,0000,0000,0000,,Pokud použiješ obvod pasu tak ji nedostaneš přes stehna.
Dialogue: 0,0:08:13.49,0:08:16.12,main,,0000,0000,0000,,A pokud ji dostaneš přes stehna tak ti bude volná v pase.
Dialogue: 0,0:08:16.12,0:08:19.37,main,,0000,0000,0000,,Nemáš jinou možnost, než si koupit tu která je dost velká pro tvůj pas…
Dialogue: 0,0:08:19.37,0:08:22.96,main,,0000,0000,0000,,…a když pak jdeš tak se ti protáčí.
Dialogue: 0,0:08:22.96,0:08:23.91,main,,0000,0000,0000,,To je pravda!
Dialogue: 0,0:08:23.92,0:08:26.33,main,,0000,0000,0000,,Tělo se špeky vypadá jako kornout…
Dialogue: 0,0:08:26.33,0:08:27.63,main,,0000,0000,0000,,Pokud ho nahneš…
Dialogue: 0,0:08:27.63,0:08:28.96,main,,0000,0000,0000,,…vypadá jako spící tělo.
Dialogue: 0,0:08:28.96,0:08:31.09,main,,0000,0000,0000,,Pokud ho zase nahneš…
Dialogue: 0,0:08:31.09,0:08:32.26,main,,0000,0000,0000,,…mužeš si ho dát na hlavu jako kornout.
Dialogue: 0,0:08:32.26,0:08:34.17,main,,0000,0000,0000,,To má být vtipné?
Dialogue: 0,0:08:34.59,0:08:39.18,main,,0000,0000,0000,,Nedržela jsi dietu aby jsi na léto zhubla, ségro?
Dialogue: 0,0:08:39.97,0:08:41.68,main,,0000,0000,0000,,Už jsem něco zhodila…
Dialogue: 0,0:08:41.68,0:08:44.06,main,,0000,0000,0000,,…ale obvod pasu se mi nezměnil.
Dialogue: 0,0:08:44.06,0:08:45.44,main,,0000,0000,0000,,Ne, Kagami.
Dialogue: 0,0:08:45.44,0:08:48.56,main,,0000,0000,0000,,Já vidím plody tvé diety.
Dialogue: 0,0:08:48.57,0:08:49.31,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:08:49.82,0:08:51.15,main,,0000,0000,0000,,Je to jasně vidět.
Dialogue: 0,0:08:52.49,0:08:57.78,main,,0000,0000,0000,,Říká se, že když držíš dietu, tak první začneš ztrácet váhu na místech na kterých ani nechceš.
Dialogue: 0,0:08:57.78,0:08:58.87,main,,0000,0000,0000,,Už dost!
Dialogue: 0,0:08:58.87,0:08:59.99,main,,0000,0000,0000,,To by stačilo!
Dialogue: 0,0:09:02.41,0:09:04.37,main,,0000,0000,0000,,Při čtení knihy…
Dialogue: 0,0:09:04.37,0:09:07.08,main,,0000,0000,0000,,…je zásadní držet v paměti téma…
Dialogue: 0,0:09:07.08,0:09:09.88,main,,0000,0000,0000,,…a porozumět autorovu záměru.
Dialogue: 0,0:09:07.58,0:09:09.88,main2,,0000,0000,0000,,Je mi to jasný…
Dialogue: 0,0:09:10.17,0:09:14.51,main,,0000,0000,0000,,Slyšela jsem, že lidé se špatným zrakem hodně civí a dělají u toho různé grimasy…
Dialogue: 0,0:09:14.51,0:09:17.47,main,,0000,0000,0000,,…ale myslím, že to je individuální.
Dialogue: 0,0:09:17.47,0:09:18.80,main,,0000,0000,0000,,Hej, ty!
Dialogue: 0,0:09:18.80,0:09:20.26,main,,0000,0000,0000,,Když jsi řekla, že je to individuální…
Dialogue: 0,0:09:20.26,0:09:21.64,main,,0000,0000,0000,,…tak ses na mě podívala, že jo?!
Dialogue: 0,0:09:22.02,0:09:25.85,main,,0000,0000,0000,,Slyšela jsem, že když uděláš aby oči vypadali užší tak je to přitažlivější.
Dialogue: 0,0:09:25.85,0:09:28.81,main,,0000,0000,0000,,Vypadáš pak prý vyspěleji.
Dialogue: 0,0:09:31.32,0:09:31.94,main,,0000,0000,0000,,Hej!
Dialogue: 0,0:09:31.94,0:09:32.73,main,,0000,0000,0000,,Přestaňte!
Dialogue: 0,0:09:35.07,0:09:36.45,main,,0000,0000,0000,,Jde pozdě!
Dialogue: 0,0:09:36.45,0:09:37.53,main,,0000,0000,0000,,Pitomá Konata…
Dialogue: 0,0:09:37.53,0:09:38.95,main,,0000,0000,0000,,Co dělá?
Dialogue: 0,0:09:39.62,0:09:40.53,main,,0000,0000,0000,,Tady je!
Dialogue: 0,0:09:40.53,0:09:41.83,main,,0000,0000,0000,,Promiňte!
Dialogue: 0,0:09:41.83,0:09:42.70,main,,0000,0000,0000,,Jdu pozdě.
Dialogue: 0,0:09:42.70,0:09:43.74,main,,0000,0000,0000,,Dost pozdě!
Dialogue: 0,0:09:43.75,0:09:46.54,main,,0000,0000,0000,,Pořád si stěžuješ jak ti vadí když dávají anime později…
Dialogue: 0,0:09:46.58,0:09:48.29,main,,0000,0000,0000,,…i a ty děláš to samé!
Dialogue: 0,0:09:48.29,0:09:49.38,main,,0000,0000,0000,,No, víš…
Dialogue: 0,0:09:49.38,0:09:52.54,main,,0000,0000,0000,,Poslední dobou jsou všechny show mimo program…
Dialogue: 0,0:09:52.88,0:09:54.42,main,,0000,0000,0000,,Myslím, že mě to postihlo.
Dialogue: 0,0:09:54.42,0:09:55.09,main,,0000,0000,0000,,To není nakažlivé!
Dialogue: 0,0:09:55.38,0:09:55.84,main,,0000,0000,0000,,Takže?
Dialogue: 0,0:09:55.84,0:09:57.17,main,,0000,0000,0000,,Proč jdeš pozdě?
Dialogue: 0,0:09:57.18,0:09:57.93,main,,0000,0000,0000,,Zaspala jsi?
Dialogue: 0,0:09:57.93,0:09:58.72,main,,0000,0000,0000,,To ne.
Dialogue: 0,0:09:58.72,0:10:00.47,main,,0000,0000,0000,,Nejsem malá.
Dialogue: 0,0:10:00.47,0:10:02.93,main,,0000,0000,0000,,Tak mi dej nějakej dospěláckej důvod proč jdeš pozdě.
Dialogue: 0,0:10:03.52,0:10:04.52,main,,0000,0000,0000,,No…
Dialogue: 0,0:10:04.52,0:10:10.06,main,,0000,0000,0000,,Když jsem se připravila tak mi pořád zůstalo spoustu času…
Dialogue: 0,0:10:10.06,0:10:11.44,main,,0000,0000,0000,,Takže abych ho nějak zaplnila…
Dialogue: 0,0:10:11.44,0:10:13.48,main,,0000,0000,0000,,…jsem začala hrát hry a nemohla jsem přestat…
Dialogue: 0,0:10:13.48,0:10:14.53,main,,0000,0000,0000,,Vážně mě štveš!
Dialogue: 0,0:10:14.53,0:10:15.44,main,,0000,0000,0000,,Jsi horší než dítě.
Dialogue: 0,0:10:15.99,0:10:16.94,main,,0000,0000,0000,,Ah, jo.
Dialogue: 0,0:10:17.32,0:10:21.66,main,,0000,0000,0000,,Během druhého semestru je spousta kulturních a sportovních festivalů, že?
Dialogue: 0,0:10:21.66,0:10:22.95,main,,0000,0000,0000,,A za chvíli máme testy.
Dialogue: 0,0:10:23.66,0:10:27.45,main,,0000,0000,0000,,Kona-chan je velice sportovní, takže by se ji mohlo dobře dařit.
Dialogue: 0,0:10:28.12,0:10:30.17,main,,0000,0000,0000,,Nemám ráda sportovní festivaly, protože jsem po nich unavená.
Dialogue: 0,0:10:30.17,0:10:34.17,main,,0000,0000,0000,,Je docela vzácnost, že takovej televizní povaleč je sportovní typ.
Dialogue: 0,0:10:34.55,0:10:37.51,main,,0000,0000,0000,,Zajímalo by mne, co bude naše třída dělat pro na kulturním festivalu.
Dialogue: 0,0:10:37.51,0:10:40.30,main,,0000,0000,0000,,Není co bych chtěla dělat.
Dialogue: 0,0:10:40.30,0:10:41.39,main,,0000,0000,0000,,Tyhle věci…
Dialogue: 0,0:10:41.39,0:10:43.60,main,,0000,0000,0000,,…se nikdy nedají rozhodnout…
Dialogue: 0,0:10:43.60,0:10:44.89,main,,0000,0000,0000,,…leda, že by někdo chtěl něco dělat. 
Dialogue: 0,0:10:44.89,0:10:47.52,main,,0000,0000,0000,,A ten koho něco napadne se toho většinou ujme…
Dialogue: 0,0:10:47.52,0:10:49.56,main,,0000,0000,0000,,a nikdo se mu do toho neplete.
Dialogue: 0,0:10:49.56,0:10:52.02,main,,0000,0000,0000,,A musíš zůstal po škole a připravovat to.
Dialogue: 0,0:10:52.73,0:10:55.07,main,,0000,0000,0000,,Nebudeš se moc večer dívat na anime.
Dialogue: 0,0:10:55.07,0:10:56.61,main,,0000,0000,0000,,Věděla jsem, že to řekneš.
Dialogue: 0,0:10:57.12,0:10:59.99,copyrights,,0000,0000,0000,,{\pos(85,50)}Překlad: paralaX
Dialogue: 0,0:10:57.12,0:10:59.99,copyrights,,0000,0000,0000,,{\pos(129,76)}Domovská stránka: www.otakogu.cz
Dialogue: 0,0:11:02.41,0:11:05.12,main,,0000,0000,0000,,Dneska večer je na mě řada s vařením večeře.
Dialogue: 0,0:11:05.12,0:11:07.08,main,,0000,0000,0000,,Musím se cestou domů zastavit v obchodě.
Dialogue: 0,0:11:07.95,0:11:09.33,main,,0000,0000,0000,,Co budeš dělat?
Dialogue: 0,0:11:09.33,0:11:12.04,main,,0000,0000,0000,,Všemi oblíbené kuřecí kari!
Dialogue: 0,0:11:12.04,0:11:14.42,main,,0000,0000,0000,,Každý v naší rodině zbožňuje kuřata.
Dialogue: 0,0:11:15.17,0:11:16.30,main,,0000,0000,0000,,Kari, heh?
Dialogue: 0,0:11:16.30,0:11:17.21,main,,0000,0000,0000,,To zní dobře.
Dialogue: 0,0:11:17.21,0:11:21.38,main,,0000,0000,0000,,Slyšela jsem, že když necháš kari přes noc sednout tak pak chutná líp.
Dialogue: 0,0:11:21.38,0:11:23.14,main,,0000,0000,0000,,To jsem také slyšela.
Dialogue: 0,0:11:23.14,0:11:27.18,main,,0000,0000,0000,,Tak já udělám nějaké navíc a zítra ho přinesu na oběd.
Dialogue: 0,0:11:27.18,0:11:28.39,main,,0000,0000,0000,,Stejně by něco zůstalo.
Dialogue: 0,0:11:30.98,0:11:31.56,main,,0000,0000,0000,,Ano.
Dialogue: 0,0:11:31.56,0:11:32.35,main,,0000,0000,0000,,Tohle je residence Hiiragi.
Dialogue: 0,0:11:33.15,0:11:34.15,main,,0000,0000,0000,,Tsukaso?
Dialogue: 0,0:11:34.77,0:11:35.65,main,,0000,0000,0000,,Kona-chan!
Dialogue: 0,0:11:35.65,0:11:37.15,main,,0000,0000,0000,,Proč voláš tak brzo?
Dialogue: 0,0:11:38.03,0:11:39.44,main,,0000,0000,0000,,To kari…
Dialogue: 0,0:11:39.44,0:11:40.44,main,,0000,0000,0000,,Kari?
Dialogue: 0,0:11:40.45,0:11:42.40,main,,0000,0000,0000,,To kari co jsem ho udělala včera…
Dialogue: 0,0:11:42.41,0:11:44.45,main,,0000,0000,0000,,Nechala jsem ho přes noc sednou a je zkažené.
Dialogue: 0,0:11:45.24,0:11:46.12,main,,0000,0000,0000,,To ne.
Dialogue: 0,0:11:46.12,0:11:46.98,main,,0000,0000,0000,,Vážně.
Dialogue: 0,0:11:46.98,0:11:48.08,main,,0000,0000,0000,,Jakto?
Dialogue: 0,0:11:48.08,0:11:50.54,main,,0000,0000,0000,,Kari se většinou za den nezkazí.
Dialogue: 0,0:11:50.54,0:11:51.62,main,,0000,0000,0000,,Že?
Dialogue: 0,0:11:51.62,0:11:53.29,main,,0000,0000,0000,,Nedala jsi do něj něco divného?
Dialogue: 0,0:11:53.29,0:11:54.00,main,,0000,0000,0000,,Nedala.
Dialogue: 0,0:11:54.00,0:11:55.04,main,,0000,0000,0000,,Ale…
Dialogue: 0,0:11:55.04,0:11:56.38,main,,0000,0000,0000,,…dnes ráno v něm byla plíseň!
Dialogue: 0,0:11:56.96,0:11:57.25,main,,0000,0000,0000,,Takže?
Dialogue: 0,0:11:57.25,0:11:58.46,main,,0000,0000,0000,,Dneska další šátek?
Dialogue: 0,0:11:58.46,0:12:00.30,main,,0000,0000,0000,,Chutnají dobře.
Dialogue: 0,0:12:00.30,0:12:01.72,main,,0000,0000,0000,,Když to říkáš…
Dialogue: 0,0:12:02.76,0:12:04.13,main,,0000,0000,0000,,Ah, jo…
Dialogue: 0,0:12:04.14,0:12:05.80,main,,0000,0000,0000,,Právě jsem si všimla…
Dialogue: 0,0:12:05.80,0:12:07.72,main,,0000,0000,0000,,…že vaše obědy jsou někdy…
Dialogue: 0,0:12:07.73,0:12:10.06,goodjob,,0000,0000,0000,,{\pos(562,31)}Dobře připravený
Dialogue: 0,0:12:07.73,0:12:10.06,justblack,,0000,0000,0000,,{\pos(100,128)}Mini-hamburger
Dialogue: 0,0:12:07.73,0:12:10.06,justblack,,0000,0000,0000,,{\pos(61,195)}Smažená vejce
Dialogue: 0,0:12:07.73,0:12:10.06,justblack,,0000,0000,0000,,{\pos(108,292)}Slanina s cibulí
Dialogue: 0,0:12:07.73,0:12:10.06,justblack,,0000,0000,0000,,{\pos(94,367)}Mrkev
Dialogue: 0,0:12:07.73,0:12:10.06,justblack,,0000,0000,0000,,{\pos(218,382)}A tak dál…
Dialogue: 0,0:12:08.14,0:12:10.06,main,,0000,0000,0000,,…velice hezky připravené…
Dialogue: 0,0:12:10.07,0:12:11.93,simplefood,,0000,0000,0000,,{\pos(127,32)}Nic moc
Dialogue: 0,0:12:10.07,0:12:11.93,justblack,,0000,0000,0000,,{\pos(615,159)}Tuňák
Dialogue: 0,0:12:10.07,0:12:11.93,justblack,,0000,0000,0000,,{\pos(582,267)}Ibišek
Dialogue: 0,0:12:10.07,0:12:11.93,justblack,,0000,0000,0000,,{\pos(583,305)}Zelenina
Dialogue: 0,0:12:10.07,0:12:11.93,justblack,,0000,0000,0000,,{\pos(595,360)}A to je vše…
Dialogue: 0,0:12:10.48,0:12:11.93,main,,0000,0000,0000,,…a někdy za moc nestojí…
Dialogue: 0,0:12:12.35,0:12:14.31,main,,0000,0000,0000,,…myslím, že je to jasné.
Dialogue: 0,0:12:14.31,0:12:18.57,main,,0000,0000,0000,,Ts-Tsukasa a já se střídáme v přípravě obědů…
Dialogue: 0,0:12:19.15,0:12:21.32,main,,0000,0000,0000,,Domácí práce mi moc nejdou…
Dialogue: 0,0:12:21.32,0:12:23.65,main,,0000,0000,0000,,Vždy udělám jen něco jednoduchého!
Dialogue: 0,0:12:24.16,0:12:25.61,main,,0000,0000,0000,,Tak promiň, ale jinak to neumím!
Dialogue: 0,0:12:26.07,0:12:26.62,main,,0000,0000,0000,,Co?!
Dialogue: 0,0:12:26.62,0:12:28.12,main,,0000,0000,0000,,Takže jsi ten typ…
Dialogue: 0,0:12:28.12,0:12:30.37,main,,0000,0000,0000,,…co dokáže zapálit pánev když vaří?!
Dialogue: 0,0:12:30.37,0:12:31.87,main,,0000,0000,0000,,To nedokáže nikdo.
Dialogue: 0,0:12:33.21,0:12:35.87,main,,0000,0000,0000,,Yuki-chanin oběd dnes zase vypadá báječně.
Dialogue: 0,0:12:35.88,0:12:36.50,main,,0000,0000,0000,,Ale ne.
Dialogue: 0,0:12:37.00,0:12:39.96,main,,0000,0000,0000,,To jsou pouze zbytky ze včerejška.
Dialogue: 0,0:12:41.76,0:12:43.76,main,,0000,0000,0000,,Večeře u Miyuki…
Dialogue: 0,0:12:43.76,0:12:45.38,main,,0000,0000,0000,,Musí být vážně něco…
Dialogue: 0,0:12:45.39,0:12:47.05,main,,0000,0000,0000,,A tohle jsou pouze zbytky.
Dialogue: 0,0:12:47.60,0:12:48.51,main,,0000,0000,0000,,Um…
Dialogue: 0,0:12:48.51,0:12:52.02,main,,0000,0000,0000,,Co je uvnitř těch vajec?
Dialogue: 0,0:12:52.68,0:12:56.15,main,,0000,0000,0000,,Zůstali nám nějací úhoři, tak jsem je tam přihodila.
Dialogue: 0,0:12:57.65,0:12:59.86,main,,0000,0000,0000,,Většinou se se zbytky úhořů nesetkávám.
Dialogue: 0,0:12:59.86,0:13:02.82,main,,0000,0000,0000,,Jediné co zůstává u nás je plesnivé kari.
Dialogue: 0,0:13:02.82,0:13:05.61,main,,0000,0000,0000,,U vás to není normální.
Dialogue: 0,0:13:06.28,0:13:07.28,main,,0000,0000,0000,,Dobré ráno.
Dialogue: 0,0:13:07.28,0:13:07.99,main,,0000,0000,0000,,Dobré ráno!
Dialogue: 0,0:13:08.16,0:13:08.87,main,,0000,0000,0000,,Čao!
Dialogue: 0,0:13:09.45,0:13:10.74,main,,0000,0000,0000,,Dívej! Dívej!
Dialogue: 0,0:13:10.74,0:13:12.45,main,,0000,0000,0000,,Koupili mi telefon!
Dialogue: 0,0:13:13.25,0:13:14.62,main,,0000,0000,0000,,To máš dobrý.
Dialogue: 0,0:13:14.62,0:13:18.17,main,,0000,0000,0000,,To proto, že jsem dobře zvládla testy.
Dialogue: 0,0:13:18.17,0:13:19.71,main,,0000,0000,0000,,Dej mi tvé číslo.
Dialogue: 0,0:13:19.71,0:13:21.21,main,,0000,0000,0000,,Já ti zase dám své.
Dialogue: 0,0:13:22.34,0:13:23.84,main,,0000,0000,0000,,Jak že to bylo?
Dialogue: 0,0:13:25.63,0:13:26.34,main,,0000,0000,0000,,Um…
Dialogue: 0,0:13:38.44,0:13:39.98,main,,0000,0000,0000,,Počkej moment!
Dialogue: 0,0:13:54.45,0:13:57.12,main,,0000,0000,0000,,Konato. Ty máš telefon?
Dialogue: 0,0:13:58.04,0:14:00.29,main,,0000,0000,0000,,Jo, ale pokaždé ho zapomenu doma.
Dialogue: 0,0:14:00.59,0:14:03.05,main,,0000,0000,0000,,Jediný kdo mi volá je stejně jen můj otec.
Dialogue: 0,0:14:03.05,0:14:05.80,main,,0000,0000,0000,,Není to trochu v rozporu se smyslem vlastnit telefon?
Dialogue: 0,0:14:05.80,0:14:06.38,main,,0000,0000,0000,,Myslíš?
Dialogue: 0,0:14:06.76,0:14:07.42,main,,0000,0000,0000,,Keroro!
Dialogue: 0,0:14:08.01,0:14:08.93,main,,0000,0000,0000,,Není roztomilý?
Dialogue: 0,0:14:09.51,0:14:12.18,main,,0000,0000,0000,,Koupili mi ho s telefonem.
Dialogue: 0,0:14:12.18,0:14:13.35,main,,0000,0000,0000,,Oh, jo…
Dialogue: 0,0:14:13.35,0:14:16.89,main,,0000,0000,0000,,Ve třídě jsme měli holku která měla na mobilu spoustu přívěsků.
Dialogue: 0,0:14:16.89,0:14:18.31,main,,0000,0000,0000,,Celkově vážily…
Dialogue: 0,0:14:18.31,0:14:21.15,main,,0000,0000,0000,,celých 800 gramů.
Dialogue: 0,0:14:21.86,0:14:22.94,main,,0000,0000,0000,,Tolik?
Dialogue: 0,0:14:22.94,0:14:24.15,main,,0000,0000,0000,,To je dost těžké.
Dialogue: 0,0:14:24.15,0:14:25.36,main,,0000,0000,0000,,Když je pak sundala…
Dialogue: 0,0:14:25.36,0:14:27.28,main,,0000,0000,0000,,její bolavá ramena byla vyléčena.
Dialogue: 0,0:14:27.28,0:14:30.82,main,,0000,0000,0000,,To zní jako že jeden hovor se vyrovnal posilování.
Dialogue: 0,0:14:32.24,0:14:36.66,main,,0000,0000,0000,,Je to zajímavý pocit, když víte, že vám každou chvíli někdo může zavolat.
Dialogue: 0,0:14:36.66,0:14:37.54,main,,0000,0000,0000,,Jo?
Dialogue: 0,0:14:37.54,0:14:40.08,main,,0000,0000,0000,,Doufám, že mi brzo někdo zavolá!
Dialogue: 0,0:14:40.08,0:14:43.17,main,,0000,0000,0000,,Dneska už je mobil běžná věc, není z čeho dělat vědu.
Dialogue: 0,0:14:44.92,0:14:46.51,main,,0000,0000,0000,,A mimochodem, už jsi našla svoje číslo?
Dialogue: 0,0:14:47.51,0:14:48.47,main,,0000,0000,0000,,Ještě ne!
Dialogue: 0,0:14:49.05,0:14:49.97,main,,0000,0000,0000,,V Anglii…
Dialogue: 0,0:14:49.97,0:14:53.97,main,,0000,0000,0000,,byla velká občanská válka mezi Lancastrovci a Yorkšírci.
Dialogue: 0,0:14:54.26,0:14:58.68,main,,0000,0000,0000,,Když bojovali tak nosili červené a bílé růže.
Dialogue: 0,0:14:58.69,0:15:01.19,main,,0000,0000,0000,,Proto tato válka je známá jako Válka růží.
Dialogue: 0,0:15:01.19,0:15:05.61,main,,0000,0000,0000,,Trvala od roku 1455 do roku 1485.
Dialogue: 0,0:15:05.61,0:15:06.03,main,,0000,0000,0000,,Toto…
Dialogue: 0,0:15:07.65,0:15:09.32,main,,0000,0000,0000,,Můj?!
Dialogue: 0,0:15:09.74,0:15:11.03,main,,0000,0000,0000,,To je můj telefon!
Dialogue: 0,0:15:11.87,0:15:13.49,main,,0000,0000,0000,,Někdo mi konečně zavolal!
Dialogue: 0,0:15:20.33,0:15:21.08,main,,0000,0000,0000,,Kdo je to?
Dialogue: 0,0:15:21.29,0:15:22.08,main,,0000,0000,0000,,Fajn.
Dialogue: 0,0:15:22.08,0:15:23.00,main,,0000,0000,0000,,Zabaveno.
Dialogue: 0,0:15:24.84,0:15:26.92,main,,0000,0000,0000,,Vyzvedni si ho ve sborovně po škole.
Dialogue: 0,0:15:27.26,0:15:28.42,main,,0000,0000,0000,,To je ale úleva!
Dialogue: 0,0:15:28.42,0:15:30.01,main,,0000,0000,0000,,Vrátila mi ho!
Dialogue: 0,0:15:30.01,0:15:32.89,main,,0000,0000,0000,,Koho by napadlo, že ti tak brzo začne chodit spam?
Dialogue: 0,0:15:36.47,0:15:37.35,main,,0000,0000,0000,,Tati?
Dialogue: 0,0:15:38.43,0:15:39.68,main,,0000,0000,0000,,Pořád ve škole.
Dialogue: 0,0:15:39.68,0:15:42.98,main,,0000,0000,0000,,Povídala jsem si s Konatou a Miyuki a ztratila jsem pojem o čase.
Dialogue: 0,0:15:43.48,0:15:44.44,main,,0000,0000,0000,,V pořádku.
Dialogue: 0,0:15:44.44,0:15:45.98,main,,0000,0000,0000,,Budeme brzo doma.
Dialogue: 0,0:15:45.98,0:15:46.77,main,,0000,0000,0000,,Ahoj.
Dialogue: 0,0:15:47.90,0:15:50.24,main,,0000,0000,0000,,Jen trochu pozdě a podívej co se děje.
Dialogue: 0,0:15:53.11,0:15:53.82,main,,0000,0000,0000,,Ano?
Dialogue: 0,0:15:54.51,0:15:55.52,main,,0000,0000,0000,,Tati?
Dialogue: 0,0:15:56.99,0:15:58.16,main,,0000,0000,0000,,Pořád ve škole.
Dialogue: 0,0:15:58.16,0:15:59.83,main,,0000,0000,0000,,Jsem s Kona-chan a s ostatními.
Dialogue: 0,0:16:01.29,0:16:03.79,main,,0000,0000,0000,,Pomyslela bych si, že ho to po rozhovoru se mnou napadne.
Dialogue: 0,0:16:03.79,0:16:06.71,main,,0000,0000,0000,,To je běžné. Každý otec má strach o své dcery.
Dialogue: 0,0:16:11.80,0:16:14.80,main,,0000,0000,0000,,Dneska opravdu chodí spousta nevyžádané pošty.
Dialogue: 0,0:16:15.47,0:16:19.01,main,,0000,0000,0000,,Lepší je, aby tvá adresa nebyla jednoduchá k uhádnutí.
Dialogue: 0,0:16:19.02,0:16:20.22,main,,0000,0000,0000,,Tak to dělám já.
Dialogue: 0,0:16:20.56,0:16:21.98,main,,0000,0000,0000,,Těžká k uhádnutí?
Dialogue: 0,0:16:22.00,0:16:23.19,main,,0000,0000,0000,,Jak jako?
Dialogue: 0,0:16:23.31,0:16:25.44,main,,0000,0000,0000,,Moje adresa je psaná latinkou.
Dialogue: 0,0:16:25.44,0:16:27.15,main,,0000,0000,0000,,milujidivkysbrylemi.
Dialogue: 0,0:16:27.69,0:16:30.44,main,,0000,0000,0000,,Chtěla jsem něco se služebnými…
Dialogue: 0,0:16:30.44,0:16:33.53,main,,0000,0000,0000,,Ale ty jsou populární a lehce…
Dialogue: 0,0:16:33.57,0:16:34.78,main,,0000,0000,0000,,Přestaň!
Dialogue: 0,0:16:34.78,0:16:35.49,main,,0000,0000,0000,,Už to chápeme!
Dialogue: 0,0:16:35.49,0:16:38.49,main,,0000,0000,0000,,Takže na veřejnosti mluv raději potichu.
Dialogue: 0,0:16:38.49,0:16:41.04,main,,0000,0000,0000,,To co říkáš je mnohem větší problém než nevyžádaná pošta!
Dialogue: 0,0:16:44.46,0:16:45.79,main,,0000,0000,0000,,Dobré ráno!
Dialogue: 0,0:16:45.79,0:16:46.96,main,,0000,0000,0000,,Čao!
Dialogue: 0,0:16:50.21,0:16:52.42,main,,0000,0000,0000,,Co je Tsukaso?
Dialogue: 0,0:16:52.42,0:16:53.92,main,,0000,0000,0000,,Můj mobil…
Dialogue: 0,0:16:54.18,0:16:55.63,main,,0000,0000,0000,,Můj mobil…!
Dialogue: 0,0:16:55.64,0:16:57.43,main,,0000,0000,0000,,Nechala ho v kapse…
Dialogue: 0,0:16:59.43,0:17:01.27,main,,0000,0000,0000,,Je na lepším místě, co?
Dialogue: 0,0:17:03.69,0:17:06.19,main,,0000,0000,0000,,Jestli mi asi koupí nový…
Dialogue: 0,0:17:07.81,0:17:10.27,main,,0000,0000,0000,,Tenhle to přežil.
Dialogue: 0,0:17:10.28,0:17:11.86,main,,0000,0000,0000,,Houževnatý chlapík.
Dialogue: 0,0:17:13.74,0:17:15.78,main,,0000,0000,0000,,Takže ten email minulou noc…
Dialogue: 0,0:17:16.11,0:17:20.66,main,,0000,0000,0000,,Ten ve kterém bylo, že Kagami chce své poznámky zpátky…
Dialogue: 0,0:17:20.66,0:17:22.49,main,,0000,0000,0000,,To bylo poslední cos poslala?
Dialogue: 0,0:17:22.50,0:17:23.50,main,,0000,0000,0000,,Jop.
Dialogue: 0,0:17:23.50,0:17:24.96,main,,0000,0000,0000,,Odpověděla jsi?
Dialogue: 0,0:17:26.42,0:17:29.79,main,,0000,0000,0000,,Když dostaneš mail tak nikdy hned neodpovíš, že jo?!
Dialogue: 0,0:17:29.79,0:17:33.34,main,,0000,0000,0000,,Neodpovídám lidem se kterýma si můžu kdykoliv promluvit.
Dialogue: 0,0:17:33.34,0:17:34.76,main,,0000,0000,0000,,Odpovídej na maily přes email!
Dialogue: 0,0:17:34.76,0:17:36.18,main,,0000,0000,0000,,Vidíme se každý den.
Dialogue: 0,0:17:36.18,0:17:38.14,main,,0000,0000,0000,,Takže jaký to má smysl?
Dialogue: 0,0:17:38.14,0:17:39.80,main,,0000,0000,0000,,Přijdeš o své kamarády!
Dialogue: 0,0:17:39.80,0:17:40.43,main,,0000,0000,0000,,No jo.
Dialogue: 0,0:17:40.72,0:17:42.35,main,,0000,0000,0000,,Šak o nic nejde.
Dialogue: 0,0:17:42.35,0:17:43.27,main,,0000,0000,0000,,O tom ty nerozhoduješ!
Dialogue: 0,0:17:46.98,0:17:49.44,main,,0000,0000,0000,,A nevrátila mi poznámky.
Dialogue: 0,0:17:56.78,0:17:57.66,main,,0000,0000,0000,,Tsukaso!
Dialogue: 0,0:17:57.66,0:17:58.70,main,,0000,0000,0000,,Jdu dovnitř!
Dialogue: 0,0:18:00.03,0:18:01.03,main,,0000,0000,0000,,Ségra.
Dialogue: 0,0:18:02.49,0:18:03.66,main,,0000,0000,0000,,Co je to?
Dialogue: 0,0:18:03.66,0:18:04.29,main,,0000,0000,0000,,Co se děje?
Dialogue: 0,0:18:04.83,0:18:08.29,main,,0000,0000,0000,,Tahle manga co mi ji půjčila Kona-chan mě dohnala k slzám.
Dialogue: 0,0:18:08.67,0:18:09.63,main,,0000,0000,0000,,Dobré ráno.
Dialogue: 0,0:18:09.63,0:18:11.34,main,,0000,0000,0000,,Díky za tu mangu, Kona-chan!
Dialogue: 0,0:18:11.38,0:18:12.54,main,,0000,0000,0000,,Byla úžasná!
Dialogue: 0,0:18:12.75,0:18:14.71,main,,0000,0000,0000,,Že byla přihloupě k popukání?
Dialogue: 0,0:18:15.72,0:18:18.34,main,,0000,0000,0000,,Dohnala mě k slzám!
Dialogue: 0,0:18:18.80,0:18:21.43,main,,0000,0000,0000,,Nemůžu uvěřit, že mluvíš o té samé mange.
Dialogue: 0,0:18:21.68,0:18:22.26,main,,0000,0000,0000,,Tady.
Dialogue: 0,0:18:22.26,0:18:23.97,main,,0000,0000,0000,,Pokud budeš brečet si s Tsukasou.
Dialogue: 0,0:18:23.97,0:18:25.60,main,,0000,0000,0000,,A pokud se budeš smát tak si se mnou.
Dialogue: 0,0:18:25.60,0:18:26.85,main,,0000,0000,0000,,Nechci to číst!
Dialogue: 0,0:18:26.85,0:18:29.02,main,,0000,0000,0000,,Nechci zjistit, že jsem s tebou v jedné skupině.
Dialogue: 0,0:18:29.06,0:18:31.94,main,,0000,0000,0000,,Ale zajímalo by mě jak bys reagovala.
Dialogue: 0,0:18:31.94,0:18:35.86,main,,0000,0000,0000,,Je špatně chtít vědět víc o svých kamarádech?
Dialogue: 0,0:18:36.78,0:18:37.61,main,,0000,0000,0000,,V tom případě…
Dialogue: 0,0:18:37.61,0:18:40.32,main,,0000,0000,0000,,U Attack No. 1 jsi se smála nebo jsi brečela?
Dialogue: 0,0:18:40.32,0:18:41.66,main,,0000,0000,0000,,Brečela.
Dialogue: 0,0:18:40.32,0:18:41.66,main2,,0000,0000,0000,,Smála.
Dialogue: 0,0:18:41.66,0:18:43.41,main,,0000,0000,0000,,Co je tvá nejoblíbenější manga?
Dialogue: 0,0:18:43.41,0:18:44.83,main,,0000,0000,0000,,Sgt. Frog.
Dialogue: 0,0:18:43.41,0:18:44.83,main2,,0000,0000,0000,,Sgt. Frog.
Dialogue: 0,0:18:46.58,0:18:49.75,main,,0000,0000,0000,,Nedokážu říct jestli si jsou podobné nebo ne.
Dialogue: 0,0:18:50.38,0:18:53.25,main,,0000,0000,0000,,Proč se nepřidáš do manga clubu, Kona-chan?
Dialogue: 0,0:18:53.25,0:18:54.71,main,,0000,0000,0000,,Když tak moc miluješ mangu.
Dialogue: 0,0:18:54.71,0:18:57.59,main,,0000,0000,0000,,No, přidala bych se…
Dialogue: 0,0:18:57.59,0:18:59.93,main,,0000,0000,0000,,A bylo by to dobré využití času…
Dialogue: 0,0:18:59.93,0:19:01.43,main,,0000,0000,0000,,
Dialogue: 0,0:19:01.72,0:19:04.18,main,,0000,0000,0000,,Co to má být?!
Dialogue: 0,0:19:05.22,0:19:06.85,main,,0000,0000,0000,,Konato…
Dialogue: 0,0:19:06.85,0:19:08.56,main,,0000,0000,0000,,co jsi se snažila nakreslit?
Dialogue: 0,0:19:08.81,0:19:09.89,main,,0000,0000,0000,,Tohle je Tsukasa.
Dialogue: 0,0:19:10.44,0:19:11.81,main,,0000,0000,0000,,To jsem já!
Dialogue: 0,0:19:11.81,0:19:13.15,main,,0000,0000,0000,,Tohle je Miyuki-san.
Dialogue: 0,0:19:13.15,0:19:15.73,main,,0000,0000,0000,,No mohla bych i říct, že jsi zachytila jejího ducha…
Dialogue: 0,0:19:15.73,0:19:17.03,main,,0000,0000,0000,,Tohle je Kagami.
Dialogue: 0,0:19:17.78,0:19:19.40,main,,0000,0000,0000,,Takže já dýchám oheň.
Dialogue: 0,0:19:19.82,0:19:20.95,main,,0000,0000,0000,,Páni…
Dialogue: 0,0:19:20.95,0:19:24.28,main,,0000,0000,0000,,Přesně to vystihuje Kagami.
Dialogue: 0,0:19:24.28,0:19:25.24,main,,0000,0000,0000,,Je tak hrůzostrašná.
Dialogue: 0,0:19:25.24,0:19:26.99,main,,0000,0000,0000,,Nech toho!
Dialogue: 0,0:19:28.37,0:19:30.29,main,,0000,0000,0000,,Vyšel nový díl FMP.
Dialogue: 0,0:19:31.04,0:19:33.54,main,,0000,0000,0000,,Jj, už bude vycházet.
Dialogue: 0,0:19:33.54,0:19:35.04,main,,0000,0000,0000,,Zastavím se v Gamers.
Dialogue: 0,0:19:35.05,0:19:36.00,main,,0000,0000,0000,,Chceš jít se mnou?
Dialogue: 0,0:19:36.42,0:19:37.25,main,,0000,0000,0000,,Promiň.
Dialogue: 0,0:19:37.26,0:19:40.38,main,,0000,0000,0000,,Dneska se jen stavím v knihkupectví.
Dialogue: 0,0:19:40.38,0:19:44.93,main,,0000,0000,0000,,Měla bys se mnou jít jako na ukázku našeho přátelství!
Dialogue: 0,0:19:44.93,0:19:45.97,main,,0000,0000,0000,,Co je to s tebou?
Dialogue: 0,0:19:45.97,0:19:47.56,main,,0000,0000,0000,,Vůbec tě nechápu.
Dialogue: 0,0:19:47.56,0:19:50.64,main,,0000,0000,0000,,A mimo to bys neměla používat slova jako přátelství.
Dialogue: 0,0:19:50.64,0:19:51.60,main,,0000,0000,0000,,Ale…
Dialogue: 0,0:19:51.60,0:19:55.77,main,,0000,0000,0000,,Jako znak našeho přátelství jsem chtěla tvé nákupní body.
Dialogue: 0,0:19:56.07,0:19:59.07,violet,,0000,0000,0000,,{\pos(698,0)}Skoro můžu mít to co chci
Dialogue: 0,0:19:56.07,0:19:58.28,main,,0000,0000,0000,,Takže tohle je tvá idea přátelství.
Dialogue: 0,0:19:56.87,0:19:59.07,main2,,0000,0000,0000,,Tak dlouho si na mě čekala!
Dialogue: 0,0:19:59.12,0:20:02.06,copyrights,,0000,0000,0000,,{\pos(159,76)}Domovská stránka: www.otakogu.cz
Dialogue: 0,0:19:59.12,0:20:02.06,copyrights,,0000,0000,0000,,{\pos(85,50)}Překlad: paralaX
Dialogue: 0,0:20:07.33,0:20:08.95,main,,0000,0000,0000,,Je ticho…
Dialogue: 0,0:20:08.95,0:20:13.75,main,,0000,0000,0000,,Je naštvaná, že jsem zamíchal mé mašle se slunečnicemi?
Dialogue: 0,0:20:15.38,0:20:16.00,main,,0000,0000,0000,,Já?
Dialogue: 0,0:20:18.96,0:20:19.88,main,,0000,0000,0000,,Já?!
Dialogue: 0,0:20:19.88,0:20:20.80,main,,0000,0000,0000,,Akira-samě?
Dialogue: 0,0:20:20.80,0:20:21.38,main,,0000,0000,0000,,Jste si jistý?
Dialogue: 0,0:20:22.72,0:20:23.68,main,,0000,0000,0000,,Rozumím.
Dialogue: 0,0:20:24.30,0:20:24.84,main,,0000,0000,0000,,Eh.
Dialogue: 0,0:20:27.43,0:20:27.89,main,,0000,0000,0000,,Eh…
Dialogue: 0,0:20:27.89,0:20:29.52,main,,0000,0000,0000,,Už jsme začali…
Dialogue: 0,0:20:31.06,0:20:31.52,main,,0000,0000,0000,,Nemám jinou možnost!
Dialogue: 0,0:20:31.52,0:20:32.56,main,,0000,0000,0000,,Než náš přeladí…
Dialogue: 0,0:20:33.98,0:20:35.69,main,,0000,0000,0000,,O-Oha lucky!
Dialogue: 0,0:20:35.69,0:20:36.77,main,,0000,0000,0000,,Žertuji!
Dialogue: 0,0:20:36.77,0:20:38.19,main,,0000,0000,0000,,Jen jsem to chtěl říct!
Dialogue: 0,0:20:38.19,0:20:41.11,main,,0000,0000,0000,,N-No, tak tu máme další epizodu Lucky Chanelu!
Dialogue: 0,0:20:41.11,0:20:42.95,main,,0000,0000,0000,,Já jsem Minoru Shiraishi!
Dialogue: 0,0:20:42.95,0:20:44.28,main,,0000,0000,0000,,Akira-sama má špatnou náladu, takže…
Dialogue: 0,0:20:47.70,0:20:51.79,main,,0000,0000,0000,,Řekl jsi své jméno před Akira Kogami.
Dialogue: 0,0:20:51.79,0:20:54.50,main,,0000,0000,0000,,Nerozumíš snad co znamená hierarchie?
Dialogue: 0,0:20:54.92,0:20:56.17,main,,0000,0000,0000,,Ne, to ne…
Dialogue: 0,0:20:57.50,0:21:02.80,main,,0000,0000,0000,,Ty tvé copánky máš poslední dobou docela často, co?
Dialogue: 0,0:21:02.80,0:21:04.59,main,,0000,0000,0000,,Docela často?
Dialogue: 0,0:21:04.59,0:21:06.55,main,,0000,0000,0000,,Jen si to moc neužívej!
Dialogue: 0,0:21:06.55,0:21:07.80,main,,0000,0000,0000,,Moc se omlouvám!
Dialogue: 0,0:21:08.14,0:21:09.05,main,,0000,0000,0000,,Krom toho…
Dialogue: 0,0:21:09.06,0:21:14.39,main,,0000,0000,0000,,Lidé čekali, že se v projektu Módní Akira objevím s copánky já, že?
Dialogue: 0,0:21:14.39,0:21:15.10,main,,0000,0000,0000,,Tak co to je?
Dialogue: 0,0:21:15.40,0:21:16.81,main,,0000,0000,0000,,Je to jak jste řekla!
Dialogue: 0,0:21:18.15,0:21:20.11,main,,0000,0000,0000,,Tak já půjdu s dvěma vrtačkama!
Dialogue: 0,0:21:20.11,0:21:21.44,main,,0000,0000,0000,,Vrtačkama?!
Dialogue: 0,0:21:22.36,0:21:23.78,main,,0000,0000,0000,,Nepodceňujte Getterovu rychlost.
Dialogue: 0,0:21:23.78,0:21:24.74,main,,0000,0000,0000,,Číslo dvě.
Dialogue: 0,0:21:24.74,0:21:27.20,main,,0000,0000,0000,,Věk copánků je u konce!
Dialogue: 0,0:21:27.20,0:21:29.16,main,,0000,0000,0000,,Nastává věk vrtaček!
Dialogue: 0,0:21:29.16,0:21:31.08,main,,0000,0000,0000,,Neřekla to Konata ve druhé epizodě?
Dialogue: 0,0:21:31.08,0:21:33.45,main,,0000,0000,0000,,"Vrtačky jsou fantazie každého muže."
Dialogue: 0,0:21:33.46,0:21:34.29,main,,0000,0000,0000,,Fantazie?
Dialogue: 0,0:21:34.66,0:21:35.91,main,,0000,0000,0000,,Tak všichni!
Dialogue: 0,0:21:35.92,0:21:38.63,main,,0000,0000,0000,,Zbytek uvidíte v naší radio show nebo na naší stránce!
Dialogue: 0,0:21:38.96,0:21:41.13,main,,0000,0000,0000,,Těšte se na příště.
Dialogue: 0,0:21:41.42,0:21:42.84,main,,0000,0000,0000,,Ahoj!
Dialogue: 0,0:21:44.72,0:21:45.34,main,,0000,0000,0000,,Řekni…
Dialogue: 0,0:21:45.34,0:21:45.84,main,,0000,0000,0000,,Ano?
Dialogue: 0,0:21:45.84,0:21:47.26,main,,0000,0000,0000,,Co se těch copánků týká…
Dialogue: 0,0:21:47.26,0:21:48.47,main,,0000,0000,0000,,Těch co mám?
Dialogue: 0,0:21:49.01,0:21:50.14,main,,0000,0000,0000,,Ty cos měl ve skutečnosti.
Dialogue: 0,0:21:50.14,0:21:50.64,main,,0000,0000,0000,,Ano.
Dialogue: 0,0:21:50.64,0:21:51.85,main,,0000,0000,0000,,Byly se slunečnicemi, že?
Dialogue: 0,0:21:51.85,0:21:52.72,main,,0000,0000,0000,,Ano.
Dialogue: 0,0:21:52.72,0:21:54.64,main,,0000,0000,0000,,To vypadalo hrozně povědomě…
Dialogue: 0,0:21:54.64,0:21:55.48,main,,0000,0000,0000,,Nemyslíte snad…
Dialogue: 0,0:21:55.48,0:21:57.77,main,,0000,0000,0000,,Před kolika lety, že to bylo? Byl to jeden z…
Dialogue: 0,0:21:56.38,0:21:57.77,main2,,0000,0000,0000,,Zadržte…
Dialogue: 0,0:21:57.77,0:21:58.69,main,,0000,0000,0000,,
Dialogue: 0,0:22:02.57,0:22:04.15,main,,0000,0000,0000,,Byla jsem tak nervózní.
Dialogue: 0,0:22:04.15,0:22:06.40,main,,0000,0000,0000,,Bylo to roztomilé a báječné.
Dialogue: 0,0:22:06.40,0:22:10.20,main,,0000,0000,0000,,Že je to stresující, když zpíváš první písničku?
Dialogue: 0,0:22:10.20,0:22:11.99,main,,0000,0000,0000,,Zajímáš se jestli vše bude v pořádku a tak.
Dialogue: 0,0:22:11.99,0:22:13.49,main,,0000,0000,0000,,Miyuki je další, že?
Dialogue: 0,0:22:13.50,0:22:16.29,main,,0000,0000,0000,,Zazpívej nám něco plné vzpomínek.
Dialogue: 0,0:22:16.29,0:22:17.41,main,,0000,0000,0000,,Proč?
Dialogue: 0,0:22:17.92,0:22:19.58,main,,0000,0000,0000,,Můžu si půjčit mikrofón?
Dialogue: 0,0:22:19.58,0:22:20.25,main,,0000,0000,0000,,Tady.
Dialogue: 0,0:22:20.25,0:22:21.04,main,,0000,0000,0000,,Do toho!
Dialogue: 0,0:22:22.09,0:22:25.05,main,,0000,0000,0000,,Um, na toto nejsem zvyklá.
Dialogue: 0,0:22:25.05,0:22:27.68,main,,0000,0000,0000,,Promiňte mi jestli se to nebude dát poslouchat.
Dialogue: 0,0:22:27.68,0:22:28.76,main,,0000,0000,0000,,Neboj.
Dialogue: 0,0:22:28.76,0:22:29.68,main,,0000,0000,0000,,Do toho!
Dialogue: 0,0:22:33.68,0:22:35.14,main,,0000,0000,0000,,Mužné.
Dialogue: 0,0:22:35.14,0:22:36.23,main,,0000,0000,0000,,Mám to.
Dialogue: 0,0:22:36.23,0:22:38.31,main,,0000,0000,0000,,Tohle se váže k druhé Miyuki.
Dialogue: 0,0:22:43.32,0:22:47.82,mainup2,,0000,0000,0000,,Ve větru Plejád.
Dialogue: 0,0:22:47.99,0:22:52.87,mainup2,,0000,0000,0000,,V prachu mléčné dráhy.
Dialogue: 0,0:22:47.99,0:22:49.61,main,,0000,0000,0000,,I její zpěv je mužný.
Dialogue: 0,0:22:49.61,0:22:52.49,main,,0000,0000,0000,,Tohle je nová forma moé.
Dialogue: 0,0:22:52.49,0:22:54.79,main,,0000,0000,0000,,Jsi dobrá, Yuki-chan!
Dialogue: 0,0:22:52.87,0:22:56.75,mainup2,,0000,0000,0000,,Kam se všichni poděli?
Dialogue: 0,0:22:56.75,0:23:02.46,mainup2,,0000,0000,0000,,Nikdo se nerozloučil.
Dialogue: 0,0:23:02.46,0:23:07.21,mainup2,,0000,0000,0000,,V Pegasových savanách.
Dialogue: 0,0:23:07.22,0:23:12.09,mainup2,,0000,0000,0000,,Na rohu Venuše.
Dialogue: 0,0:23:12.10,0:23:15.85,mainup2,,0000,0000,0000,,Kam se všichni poděli?
Dialogue: 0,0:23:15.85,0:23:21.35,mainup2,,0000,0000,0000,,Nikoho jsem si nevšimla.
Dialogue: 0,0:23:21.35,0:23:26.11,mainup2,,0000,0000,0000,,Nikdo si nepamatuje hvězdy Země.
Dialogue: 0,0:23:26.11,0:23:29.70,mainup2,,0000,0000,0000,,Lidé nevidí nic než oblohu.
Dialogue: 0,0:23:29.70,0:23:31.70,mainup2,,0000,0000,0000,,
