Dialogue: 0,0:00:00.09,0:00:05.92,whitebig,,0000,0000,0000,,{\pos(225,126)}
Dialogue: 0,0:00:00.09,0:00:05.92,whitebig,,0000,0000,0000,,{\pos(226,152)}
Dialogue: 0,0:00:01.11,0:00:02.56,main,,0000,0000,0000,,Tak začneme.
Dialogue: 0,0:00:02.57,0:00:03.61,main,,0000,0000,0000,,Jdeme na to.
Dialogue: 0,0:00:04.19,0:00:05.98,main,,0000,0000,0000,,No tak už začněme!
Dialogue: 0,0:00:06.32,0:00:07.49,op1,,0000,0000,0000,,Okolo 3 cm!
Dialogue: 0,0:00:07.49,0:00:08.78,op1,,0000,0000,0000,,Je to snad málo?
Dialogue: 0,0:00:08.78,0:00:09.03,op1,,0000,0000,0000,,Jejda!
Dialogue: 0,0:00:09.03,0:00:10.86,op1,,0000,0000,0000,,Schované pod naší uniformou!
Dialogue: 0,0:00:10.87,0:00:11.91,op1,,0000,0000,0000,,Mám toho nechat?
Dialogue: 0,0:00:11.91,0:00:12.20,op1,,0000,0000,0000,,JEJ!
Dialogue: 0,0:00:12.20,0:00:13.28,op1,,0000,0000,0000,,Jít do toho?
Dialogue: 0,0:00:13.28,0:00:14.08,op1,,0000,0000,0000,,Snažit se?
Dialogue: 0,0:00:14.08,0:00:15.66,op1,,0000,0000,0000,,Vychutnat si každý okamžik. Jo!
Dialogue: 0,0:00:15.66,0:00:16.25,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.26,0:00:16.50,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:16.50,0:00:16.90,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.91,0:00:17.25,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:17.25,0:00:17.87,op1,,0000,0000,0000,,Jen mezi námi dvěma!
Dialogue: 0,0:00:17.87,0:00:20.58,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:00:22.56,0:00:23.36,logo,,0000,0000,0000,,{\move(108,218,202,218,0,0)}LUCKY
Dialogue: 0,0:00:23.36,0:00:25.08,logo,,0000,0000,0000,,{\pos(202,218)}LUCKY
Dialogue: 0,0:00:22.56,0:00:23.36,logo,,0000,0000,0000,,{\move(638,218,459,218,0,0)}STAR
Dialogue: 0,0:00:23.36,0:00:25.08,logo,,0000,0000,0000,,{\pos(459,218)}STAR
Dialogue: 0,0:00:25.51,0:00:26.09,op1,,0000,0000,0000,,Cítím se nějak nesvá.
Dialogue: 0,0:00:26.09,0:00:26.71,op1,,0000,0000,0000,,Co jsem to jedla?
Dialogue: 0,0:00:26.71,0:00:27.30,op1,,0000,0000,0000,,Mám tě hrozně ráda!
Dialogue: 0,0:00:27.30,0:00:27.51,op1,,0000,0000,0000,,Oops!
Dialogue: 0,0:00:27.51,0:00:28.55,op1,,0000,0000,0000,,Jedno z nich je špatně!
Dialogue: 0,0:00:28.55,0:00:29.30,op1,,0000,0000,0000,,Relaxovat!
Dialogue: 0,0:00:29.30,0:00:29.93,op1,,0000,0000,0000,,Nic nedělat!
Dialogue: 0,0:00:29.93,0:00:30.47,op1,,0000,0000,0000,,Flákat se!
Dialogue: 0,0:00:30.47,0:00:31.55,op1,,0000,0000,0000,,Už toho nechej!
Dialogue: 0,0:00:31.55,0:00:33.21,op1,,0000,0000,0000,,Je ta nová holka oblíbená?
Dialogue: 0,0:00:33.21,0:00:34.85,op1,,0000,0000,0000,,Já bych řekla, že je docela normální!
Dialogue: 0,0:00:34.85,0:00:36.35,op1,,0000,0000,0000,,To jsem jediná překvapená?
Dialogue: 0,0:00:36.35,0:00:36.66,op1,,0000,0000,0000,,Vepřové kousky!
Dialogue: 0,0:00:36.66,0:00:37.06,op1,,0000,0000,0000,,Skleněné nudle!
Dialogue: 0,0:00:37.06,0:00:37.39,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:00:37.39,0:00:38.18,op2,,0000,0000,0000,,DA-DA-DA-DA-DA!
Dialogue: 0,0:00:38.73,0:00:39.52,op1,,0000,0000,0000,,Ooh?
Dialogue: 0,0:00:39.52,0:00:40.31,op1,,0000,0000,0000,,Víš co tím myslím?
Dialogue: 0,0:00:39.89,0:00:40.73,op1up,,0000,0000,0000,,Moje brýle…
Dialogue: 0,0:00:40.73,0:00:41.81,op1up,,0000,0000,0000,,Kde mám brýle?
Dialogue: 0,0:00:41.90,0:00:43.86,op1,,0000,0000,0000,,Nedávno jsme hrála tu samou bojovku.
Dialogue: 0,0:00:42.48,0:00:43.86,op1up,,0000,0000,0000,,Bez nich nic nevidím…
Dialogue: 0,0:00:44.90,0:00:45.48,op2,,0000,0000,0000,,POM! POM!
Dialogue: 0,0:00:45.48,0:00:46.53,op1,,0000,0000,0000,,Roztleskávačky!
Dialogue: 0,0:00:46.53,0:00:48.07,op2,,0000,0000,0000,,DÁME SI SLADKOST!
Dialogue: 0,0:00:48.07,0:00:48.65,op2,,0000,0000,0000,,LA! LA!
Dialogue: 0,0:00:48.65,0:00:49.70,op1,,0000,0000,0000,,Párty začíná!
Dialogue: 0,0:00:49.70,0:00:51.28,op2,,0000,0000,0000,,MÁŠ TO V KRVI!
Dialogue: 0,0:00:51.28,0:00:51.86,op2,,0000,0000,0000,,ANO!
Dialogue: 0,0:00:51.87,0:00:52.87,op1,,0000,0000,0000,,Zanechává to dojem.
Dialogue: 0,0:00:52.87,0:00:54.32,op1,,0000,0000,0000,,Malé planetky.
Dialogue: 0,0:00:54.33,0:00:54.87,op1,,0000,0000,0000,,Narazili!
Dialogue: 0,0:00:54.87,0:00:55.49,op1,,0000,0000,0000,,A shořeli!
Dialogue: 0,0:00:55.49,0:00:55.87,op1,,0000,0000,0000,,Bylo to ohromné!
Dialogue: 0,0:00:55.87,0:00:56.62,op1,,0000,0000,0000,,Z plných plic křičíme!
Dialogue: 0,0:00:56.62,0:00:57.18,op1,,0000,0000,0000,,ZPÍVEJ A TANCUJ!
Dialogue: 0,0:00:57.54,0:00:58.83,op1,,0000,0000,0000,,Přiznej si to!
Dialogue: 0,0:00:58.83,0:01:03.29,op1,,0000,0000,0000,,Nakonec se stejně budeme smát!
Dialogue: 0,0:01:03.29,0:01:07.05,op1,,0000,0000,0000,,Protože mám na sobě námořnickou uniformu.
Dialogue: 0,0:01:07.05,0:01:09.92,op2,,0000,0000,0000,,To je ono!
Dialogue: 0,0:01:10.51,0:01:14.05,op1,,0000,0000,0000,,Je teprve pondělí a já už mám špatnou náladu.
Dialogue: 0,0:01:14.05,0:01:15.85,op1,,0000,0000,0000,,Co mám dělat?
Dialogue: 0,0:01:16.06,0:01:19.89,op1,,0000,0000,0000,,Raději si vezmu moji letní uniformu -
Dialogue: 0,0:01:19.89,0:01:22.77,op1,,0000,0000,0000,,- je rozkošná!
Dialogue: 0,0:01:23.02,0:01:24.19,op1,,0000,0000,0000,,Trojdílná uniforma!
Dialogue: 0,0:01:24.19,0:01:25.61,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:01:25.61,0:01:26.06,op1,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.07,0:01:26.48,op1,,0000,0000,0000,,Dej se do toho!
Dialogue: 0,0:01:26.48,0:01:26.86,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.86,0:01:27.48,op1,,0000,0000,0000,,SNAŽ SE!
Dialogue: 0,0:01:27.48,0:01:27.65,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:27.65,0:01:29.54,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:01:35.30,0:01:35.88,main,,0000,0000,0000,,A je tu léto!
Dialogue: 0,0:01:35.88,0:01:36.51,main,,0000,0000,0000,,Pláž!
Dialogue: 0,0:01:37.13,0:01:42.26,main,,0000,0000,0000,,Díky, že budete řídit a dohlédnete na nás.
Dialogue: 0,0:01:42.26,0:01:43.39,main,,0000,0000,0000,,Žádný problém!
Dialogue: 0,0:01:43.39,0:01:44.39,main,,0000,0000,0000,,Ale fakt je…
Dialogue: 0,0:01:45.18,0:01:46.98,main,,0000,0000,0000,,…že vy dvě se musíte hrozně nudit.
Dialogue: 0,0:01:47.73,0:01:50.77,white,,0000,0000,0000,,{\frz-90\pos(398,195)}Letní zábava
Dialogue: 0,0:01:52.69,0:01:55.32,main,,0000,0000,0000,,Promiňte, že vás o prázdninách obtěžujeme.
Dialogue: 0,0:01:55.32,0:01:55.90,main,,0000,0000,0000,,Ne, to nic.
Dialogue: 0,0:01:55.90,0:01:57.53,main,,0000,0000,0000,,To je vážně v pořádku.
Dialogue: 0,0:01:57.53,0:01:58.65,main,,0000,0000,0000,,Narumi-san…
Dialogue: 0,0:01:58.66,0:02:01.49,main,,0000,0000,0000,,Vy pracuje v dopravním oddělení, že?
Dialogue: 0,0:02:01.49,0:02:02.62,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:02:02.62,0:02:05.41,main,,0000,0000,0000,,Dopravní předpisy, to je moje!
Dialogue: 0,0:02:06.37,0:02:07.04,main,,0000,0000,0000,,Předjel mě.
Dialogue: 0,0:02:07.87,0:02:08.83,main,,0000,0000,0000,,Bastard…
Dialogue: 0,0:02:08.83,0:02:09.16,main,,0000,0000,0000,,Hej-
Dialogue: 0,0:02:09.17,0:02:09.83,main,,0000,0000,0000,,Hej!
Dialogue: 0,0:02:09.83,0:02:11.04,main,,0000,0000,0000,,A co dopravní předpisy?
Dialogue: 0,0:02:11.29,0:02:14.13,main,,0000,0000,0000,,Sestřenka je za volantem pěkný ďábel.
Dialogue: 0,0:02:14.50,0:02:17.42,main,,0000,0000,0000,,Takže Kagami a Tsukasa jsou naše oběti.
Dialogue: 0,0:02:17.92,0:02:19.09,main,,0000,0000,0000,,Aha…
Dialogue: 0,0:02:19.09,0:02:20.76,main,,0000,0000,0000,,I když je policistka?
Dialogue: 0,0:02:21.34,0:02:21.97,main,,0000,0000,0000,,Heh?
Dialogue: 0,0:02:21.97,0:02:22.47,main,,0000,0000,0000,,To je divné.
Dialogue: 0,0:02:22.80,0:02:24.22,main,,0000,0000,0000,,Ztratili se mi.
Dialogue: 0,0:02:26.18,0:02:27.73,main,,0000,0000,0000,,Sensei, tohle je špatný cesta!
Dialogue: 0,0:02:28.06,0:02:29.18,main,,0000,0000,0000,,Tahle ulice je slepá!
Dialogue: 0,0:02:31.06,0:02:32.27,main,,0000,0000,0000,,Neuvěřitelné!
Dialogue: 0,0:02:32.27,0:02:34.40,main,,0000,0000,0000,,Nečekala jsem, že budou obě dvě špatná volba!
Dialogue: 0,0:02:55.88,0:02:57.25,main,,0000,0000,0000,,To auto je úžasné!
Dialogue: 0,0:02:57.26,0:02:58.05,main,,0000,0000,0000,,Kdo je to?!
Dialogue: 0,0:02:58.34,0:03:00.47,main,,0000,0000,0000,,Ty brzdy a driftování! Perfektní!
Dialogue: 0,0:03:00.47,0:03:04.14,main,,0000,0000,0000,,Je přímo za FD! A ta akcelerace je neuvěřitelná!
Dialogue: 0,0:03:04.68,0:03:06.47,main,,0000,0000,0000,,Skoro se dotýká svodidel!
Dialogue: 0,0:03:07.18,0:03:08.64,main,,0000,0000,0000,,Moje chvíle teprve přijde…
Dialogue: 0,0:03:09.98,0:03:12.02,main,,0000,0000,0000,,…jen jestě chvíli počkat!
Dialogue: 0,0:03:17.28,0:03:18.69,main,,0000,0000,0000,,Zrychluje!
Dialogue: 0,0:03:18.90,0:03:20.28,main,,0000,0000,0000,,Pokazily se jí brzdy?!
Dialogue: 0,0:03:25.24,0:03:26.83,main,,0000,0000,0000,,Neuvěřitelné!
Dialogue: 0,0:03:28.54,0:03:31.29,main,,0000,0000,0000,,Co to je s tím (píp) D-jako stylem řízení?!
Dialogue: 0,0:03:33.83,0:03:35.96,main,,0000,0000,0000,,Nanako-san…
Dialogue: 0,0:03:35.96,0:03:37.54,main,,0000,0000,0000,,jste vážně dobrá…
Dialogue: 0,0:03:37.55,0:03:38.59,main,,0000,0000,0000,,…v řízení…
Dialogue: 0,0:03:39.96,0:03:41.38,main,,0000,0000,0000,,Jo?
Dialogue: 0,0:03:41.38,0:03:42.42,main,,0000,0000,0000,,No…
Dialogue: 0,0:03:42.43,0:03:45.30,main,,0000,0000,0000,,obvykle mi je v autě špatně.
Dialogue: 0,0:03:45.80,0:03:49.60,main,,0000,0000,0000,,Proto jsem si vzala prášek,
Dialogue: 0,0:03:49.60,0:03:54.06,main,,0000,0000,0000,,ale s vámi za volantem mi vůbec nic není.
Dialogue: 0,0:03:56.77,0:03:57.06,main,,0000,0000,0000,,Zatraceně!
Dialogue: 0,0:03:57.57,0:04:00.28,main,,0000,0000,0000,,Tohle není vhodná doba na vtipy!
Dialogue: 0,0:04:00.28,0:04:03.78,warning,,0000,0000,0000,,{\be1\pos(550,248)}Z důvodu udržení přiměřené hladiny hluku
Dialogue: 0,0:04:00.28,0:04:03.78,warning,,0000,0000,0000,,{\be1\pos(550,262)\c&H000000& }jsou mezi 10 a 7 večer
Dialogue: 0,0:04:00.28,0:04:03.78,warning,,0000,0000,0000,,{\be1\pos(550,276)}zakázány ohňostroje.
Dialogue: 0,0:04:16.00,0:04:17.29,main,,0000,0000,0000,,No…
Dialogue: 0,0:04:17.29,0:04:18.67,main,,0000,0000,0000,,Co si tak najít nějaký hotel?
Dialogue: 0,0:04:27.60,0:04:30.56,main,,0000,0000,0000,,Dneska je vážně perfektní počasí pro den na pláži!
Dialogue: 0,0:04:30.56,0:04:33.85,main,,0000,0000,0000,,Vážně jsem včera netušila jestli dojedeme.
Dialogue: 0,0:04:33.85,0:04:36.40,main,,0000,0000,0000,,Hned si obleču plavky!
Dialogue: 0,0:04:36.40,0:04:39.19,main,,0000,0000,0000,,Musíš být vždy tak dětinská?
Dialogue: 0,0:04:39.57,0:04:40.27,main,,0000,0000,0000,,Konato-san!
Dialogue: 0,0:04:40.28,0:04:41.90,main,,0000,0000,0000,,To myslíš vážně?
Dialogue: 0,0:04:42.15,0:04:42.86,main,,0000,0000,0000,,Musím říct,
Dialogue: 0,0:04:42.86,0:04:46.07,main,,0000,0000,0000,,že jsi znás poslední která nosí školní plavky.
Dialogue: 0,0:04:46.07,0:04:48.41,main,,0000,0000,0000,,Někteří lidé mají speciální potřeby.
Dialogue: 0,0:04:49.58,0:04:52.12,main,,0000,0000,0000,,J-j-jak jsou ty plavky staré?!
Dialogue: 0,0:04:54.12,0:04:55.33,main,,0000,0000,0000,,Mimochodem…
Dialogue: 0,0:04:55.33,0:04:58.75,main,,0000,0000,0000,,Proč máš plavecké kolo, když dobře plaveš?
Dialogue: 0,0:04:58.75,0:05:02.84,main,,0000,0000,0000,,V oceánu přece můžeš dělat spoustu věcí krom plavání.
Dialogue: 0,0:05:02.84,0:05:04.72,main,,0000,0000,0000,,Takže se budu jen tak poflakovat.
Dialogue: 0,0:05:04.72,0:05:06.38,main,,0000,0000,0000,,V k-l-í-d-k-u.
Dialogue: 0,0:05:06.38,0:05:08.43,main,,0000,0000,0000,,A abych uspokojila tyto potřeby…
Dialogue: 0,0:05:08.43,0:05:11.76,main,,0000,0000,0000,,…musím mít spolu s duší i školní plavky.
Dialogue: 0,0:05:11.77,0:05:13.22,main,,0000,0000,0000,,Chodí tito lidé
Dialogue: 0,0:05:13.22,0:05:14.81,main,,0000,0000,0000,,vůbec do moře?
Dialogue: 0,0:05:19.11,0:05:22.44,main,,0000,0000,0000,,V manze a anime obvykle rozbíjí melouny…
Dialogue: 0,0:05:22.44,0:05:24.36,main,,0000,0000,0000,,…ale ve skutečnosti se to často nedělá.
Dialogue: 0,0:05:24.36,0:05:26.70,main,,0000,0000,0000,,Už jsem to viděla…
Dialogue: 0,0:05:26.70,0:05:28.41,main,,0000,0000,0000,,…ale nejspíš to lidem přišlo jako plýtvání.
Dialogue: 0,0:05:28.70,0:05:29.99,main,,0000,0000,0000,,To je pravda…
Dialogue: 0,0:05:29.99,0:05:32.37,main,,0000,0000,0000,,…pokud rozstřelíš meloun puškou…
Dialogue: 0,0:05:32.37,0:05:34.50,main,,0000,0000,0000,,…tak ho pak máš úplně všude.
Dialogue: 0,0:05:34.50,0:05:35.62,main,,0000,0000,0000,,Díváš se moc na anime.
Dialogue: 0,0:05:36.08,0:05:37.83,main,,0000,0000,0000,,Už jsi to někdy zkoušela, Miyuki-san?
Dialogue: 0,0:05:37.83,0:05:39.17,main,,0000,0000,0000,,Jsi dost bohatá.
Dialogue: 0,0:05:39.17,0:05:41.09,main,,0000,0000,0000,,Ne ještě ne.
Dialogue: 0,0:05:41.09,0:05:42.88,main,,0000,0000,0000,,No když o tom přemýšlím…
Dialogue: 0,0:05:42.88,0:05:46.13,main,,0000,0000,0000,,…není to něco co by dělali bohatí lidé.
Dialogue: 0,0:05:46.13,0:05:47.59,main,,0000,0000,0000,,To máš pravdu.
Dialogue: 0,0:05:55.48,0:05:56.68,main,,0000,0000,0000,,Bylo to jasné…
Dialogue: 0,0:05:56.69,0:05:59.19,main,,0000,0000,0000,,…ale zajímalo mě jestli tvoje oči nebudou vypadat jako v manze.
Dialogue: 0,0:05:59.19,0:06:01.31,main,,0000,0000,0000,,Promiň, že jsem tě zklamala.
Dialogue: 0,0:06:01.86,0:06:03.86,main,,0000,0000,0000,,Ale víš…
Dialogue: 0,0:06:03.86,0:06:07.36,main,,0000,0000,0000,,když holky v našem věku dělají dozor na pláži…
Dialogue: 0,0:06:07.36,0:06:09.95,main,,0000,0000,0000,,tak je to docela smutné, ne?
Dialogue: 0,0:06:09.95,0:06:11.00,main,,0000,0000,0000,,Pleteš se-
Dialogue: 0,0:06:10.45,0:06:11.53,main2,,0000,0000,0000,,Ale jo…
Dialogue: 0,0:06:11.53,0:06:14.70,main,,0000,0000,0000,,Pokud bych měla být s někým takovým…
Dialogue: 0,0:06:14.70,0:06:16.83,main,,0000,0000,0000,,…tak jsem ráda, že jsme sami.
Dialogue: 0,0:06:17.03,0:06:17.63,main2,,0000,0000,0000,,Ne, já…
Dialogue: 0,0:06:17.33,0:06:20.08,main,,0000,0000,0000,,Dneska se lidi berou pozdě.
Dialogue: 0,0:06:20.08,0:06:23.04,main,,0000,0000,0000,,Takže si dám na čas a dobře si vyberu.
Dialogue: 0,0:06:23.25,0:06:24.42,main,,0000,0000,0000,,Tak to není.
Dialogue: 0,0:06:24.42,0:06:27.01,main,,0000,0000,0000,,Můj muž zrovna pracuje daleko.
Dialogue: 0,0:06:27.01,0:06:28.59,main,,0000,0000,0000,,Už jsem vdaná!
Dialogue: 0,0:06:29.43,0:06:33.51,main,,0000,0000,0000,,Pláž je Mekka flirtování…
Dialogue: 0,0:06:33.51,0:06:36.43,main,,0000,0000,0000,,No, takto je to méně otravné.
Dialogue: 0,0:06:36.43,0:06:37.39,main,,0000,0000,0000,,A krom toho…
Dialogue: 0,0:06:37.39,0:06:40.69,main,,0000,0000,0000,,to vypadá, že jsem sestry a jsme tu s rodiči.
Dialogue: 0,0:06:45.40,0:06:46.90,main,,0000,0000,0000,,A šup.
Dialogue: 0,0:06:54.91,0:06:55.66,main,,0000,0000,0000,,Co je?
Dialogue: 0,0:06:56.45,0:06:57.45,main,,0000,0000,0000,,No…
Dialogue: 0,0:06:57.45,0:07:00.71,main,,0000,0000,0000,,Je to zvláštní sednout si když ještě nejsou suché.
Dialogue: 0,0:07:01.21,0:07:01.92,main,,0000,0000,0000,,Chápu.
Dialogue: 0,0:07:02.38,0:07:04.75,main,,0000,0000,0000,,Tak co použít vzdušnou židli.
Dialogue: 0,0:07:04.75,0:07:05.96,main,,0000,0000,0000,,To je docela extrém.
Dialogue: 0,0:07:11.89,0:07:12.76,main,,0000,0000,0000,,Co je?
Dialogue: 0,0:07:12.76,0:07:13.89,main,,0000,0000,0000,,Bolí mě krk.
Dialogue: 0,0:07:13.89,0:07:14.76,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:07:16.60,0:07:17.51,main,,0000,0000,0000,,Jak jsem čekala!
Dialogue: 0,0:07:17.85,0:07:19.73,main,,0000,0000,0000,,Nad čím se tak raduješ?
Dialogue: 0,0:07:19.73,0:07:23.02,main,,0000,0000,0000,,Protože tohle vypadá naprosto přesně jak by mělo.
Dialogue: 0,0:07:23.02,0:07:24.73,main,,0000,0000,0000,,Podívej se na to kary - jen omáčka a nic jiného.
Dialogue: 0,0:07:25.11,0:07:26.48,main,,0000,0000,0000,,Předčilo to mé očekávání!
Dialogue: 0,0:07:26.48,0:07:27.65,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:07:27.65,0:07:30.40,main,,0000,0000,0000,,Normálně by jsi něco takového nejedla…
Dialogue: 0,0:07:30.40,0:07:33.66,main,,0000,0000,0000,,…ale tady to chutná docela dobře!
Dialogue: 0,0:07:38.50,0:07:41.16,main,,0000,0000,0000,,Pivo na pláži je to nejlepší!
Dialogue: 0,0:07:41.16,0:07:43.79,main,,0000,0000,0000,,Jednou za čas pivo neuškodí.
Dialogue: 0,0:07:43.79,0:07:48.17,main,,0000,0000,0000,,Je ale divný, že hotdog stojí 300 yenů.
Dialogue: 0,0:07:48.17,0:07:49.21,main,,0000,0000,0000,,Jo!
Dialogue: 0,0:07:49.21,0:07:50.76,main,,0000,0000,0000,,Možná jsou nedovařené.
Dialogue: 0,0:07:50.76,0:07:51.88,main,,0000,0000,0000,,Ale stejně si je dáme.
Dialogue: 0,0:07:52.26,0:07:54.80,main,,0000,0000,0000,,Mám ráda to jejich kuře…
Dialogue: 0,0:07:54.80,0:07:58.26,main,,0000,0000,0000,,…mastné a hodně ostré.
Dialogue: 0,0:07:58.52,0:08:00.47,main,,0000,0000,0000,,I ta neochucená yakisoba…
Dialogue: 0,0:08:00.48,0:08:01.85,main,,0000,0000,0000,,…chutná docela dobře.
Dialogue: 0,0:08:01.85,0:08:03.23,main,,0000,0000,0000,,Ale stejně…
Dialogue: 0,0:08:03.23,0:08:07.06,main,,0000,0000,0000,,byste tyhle věci doma neměli jíst!
Dialogue: 0,0:08:08.15,0:08:09.53,main,,0000,0000,0000,,Konata je naštvaná.
Dialogue: 0,0:08:12.57,0:08:13.45,main,,0000,0000,0000,,Hej.
Dialogue: 0,0:08:13.99,0:08:15.86,main,,0000,0000,0000,,To je to jídlo tak dobré, že je potřeba dělat tolik rozruchu?
Dialogue: 0,0:08:16.12,0:08:17.03,main,,0000,0000,0000,,Nee.
Dialogue: 0,0:08:17.03,0:08:19.62,main,,0000,0000,0000,,Ne tak abys vyběhla na pláž vstříc oceánu.
Dialogue: 0,0:08:20.04,0:08:22.08,main,,0000,0000,0000,,To má být další odkaz na anime?
Dialogue: 0,0:08:30.63,0:08:31.84,main,,0000,0000,0000,,Moc jsem pila…
Dialogue: 0,0:08:32.13,0:08:32.88,main,,0000,0000,0000,,No myslím…
Dialogue: 0,0:08:32.88,0:08:34.13,main,,0000,0000,0000,,…že tu zůstaneme ještě jednu noc.
Dialogue: 0,0:08:34.13,0:08:36.01,main,,0000,0000,0000,,A co práce, sestřenko?
Dialogue: 0,0:08:36.01,0:08:37.76,main,,0000,0000,0000,,Mám volno i zítra.
Dialogue: 0,0:08:37.76,0:08:38.72,main,,0000,0000,0000,,Neboj se!
Dialogue: 0,0:08:44.85,0:08:46.10,main,,0000,0000,0000,,M-Mrznu…
Dialogue: 0,0:08:47.15,0:08:49.36,main,,0000,0000,0000,,Jste taková padavka.
Dialogue: 0,0:09:20.72,0:09:25.02,main,,0000,0000,0000,,Proč po koupání v moři vlasy vždy vržou…
Dialogue: 0,0:09:25.02,0:09:25.77,main,,0000,0000,0000,,Nevím.
Dialogue: 0,0:09:25.77,0:09:28.48,main,,0000,0000,0000,,Možná to má něco společného se solí?
Dialogue: 0,0:09:28.48,0:09:29.56,main,,0000,0000,0000,,Že, Miyuki?
Dialogue: 0,0:09:30.40,0:09:31.61,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:09:31.61,0:09:34.90,main,,0000,0000,0000,,Hlavní důvod je, že sůl přitahuje UV záření.
Dialogue: 0,0:09:34.90,0:09:37.11,main,,0000,0000,0000,,Co je na tom špatného?
Dialogue: 0,0:09:37.11,0:09:40.16,main,,0000,0000,0000,,Nedopadají na vlasy UV paprsky pořád?
Dialogue: 0,0:09:40.16,0:09:41.12,main,,0000,0000,0000,,No…
Dialogue: 0,0:09:41.12,0:09:43.33,main,,0000,0000,0000,,Vysvětlení…
Dialogue: 0,0:09:43.33,0:09:45.33,main,,0000,0000,0000,,…bude chvilku trvat.
Dialogue: 0,0:09:46.71,0:09:47.46,main,,0000,0000,0000,,Tak jo…
Dialogue: 0,0:09:50.67,0:09:52.84,main,,0000,0000,0000,,Důvod proč jsou vlasy po odpoledni na pláži tak poškozené je
Dialogue: 0,0:09:52.84,0:09:56.13,main,,0000,0000,0000,,že se na vlasy přichytí sůl z moře a částečky naváté větrem…
Dialogue: 0,0:09:56.13,0:09:58.34,main,,0000,0000,0000,,…které absorbují vlhkost z vlasů…
Dialogue: 0,0:09:58.34,0:10:01.35,main,,0000,0000,0000,,…a ty jsou díky tomu vystaveny přímému UV záření.
Dialogue: 0,0:10:01.60,0:10:02.26,main,,0000,0000,0000,,Také…
Dialogue: 0,0:10:02.26,0:10:04.47,main,,0000,0000,0000,,kdykoli jsou vystaveny UV záření…
Dialogue: 0,0:10:04.47,0:10:06.35,main,,0000,0000,0000,,…hodnoty melaninu klesnou…
Dialogue: 0,0:10:06.35,0:10:08.35,main,,0000,0000,0000,,…a vlasy zesvětlají.
Dialogue: 0,0:10:08.94,0:10:12.48,main,,0000,0000,0000,,A pokud ve vlasech zůstane zbytkové množství chlóru…
Dialogue: 0,0:10:12.48,0:10:16.50,main,,0000,0000,0000,,…vlasy můžou nadále měnit barvu.
Dialogue: 0,0:10:16.50,0:10:18.93,main,,0000,0000,0000,,Takže obvyklým důvodem proč jsou vlasy v létě poškozovány…
Dialogue: 0,0:10:18.93,0:10:21.90,main,,0000,0000,0000,,…je UV záření a chlór.
Dialogue: 0,0:10:21.90,0:10:26.20,main,,0000,0000,0000,,Tyto faktory způsobují poškození folikul.
Dialogue: 0,0:10:26.20,0:10:27.91,main,,0000,0000,0000,,Pokud jste vystaveny intenzivnímu UV záření…
Dialogue: 0,0:10:27.91,0:10:32.04,main,,0000,0000,0000,,…molekuly cysteinu jsou ničeny…
Dialogue: 0,0:10:33.88,0:10:35.00,main,,0000,0000,0000,,Dívejte! Dívejte!
Dialogue: 0,0:10:35.21,0:10:36.60,main,,0000,0000,0000,,Timotei!
Dialogue: 0,0:10:35.20,0:10:37.60,white,,0000,0000,0000,,Pozn: Z reklamy na značku šampónu
Dialogue: 0,0:10:37.01,0:10:37.97,main,,0000,0000,0000,,Timotei!
Dialogue: 0,0:10:38.51,0:10:39.88,main,,0000,0000,0000,,Timotei!
Dialogue: 0,0:10:40.14,0:10:41.39,main,,0000,0000,0000,,Co to je?
Dialogue: 0,0:10:47.60,0:10:50.98,main,,0000,0000,0000,,Zajímalo by mě co si o nás myslí druzí.
Dialogue: 0,0:10:51.77,0:10:52.81,main,,0000,0000,0000,,To nevím…
Dialogue: 0,0:10:56.69,0:10:58.90,main,,0000,0000,0000,,Nemáme si co říct…
Dialogue: 0,0:11:00.20,0:11:04.33,main,,0000,0000,0000,,Nejspíš, že jsme rodina nebo příbuzní?
Dialogue: 0,0:11:04.33,0:11:05.53,main,,0000,0000,0000,,Nejspíš.
Dialogue: 0,0:11:07.20,0:11:08.70,main,,0000,0000,0000,,To je vše co k tomu řekneš?
Dialogue: 0,0:11:08.71,0:11:10.25,main,,0000,0000,0000,,Sestřenko!
Dialogue: 0,0:11:10.25,0:11:11.33,main,,0000,0000,0000,,Timotei!
Dialogue: 0,0:11:11.75,0:11:12.67,main,,0000,0000,0000,,Timotei!
Dialogue: 0,0:11:13.29,0:11:14.75,main,,0000,0000,0000,,Timotei!
Dialogue: 0,0:11:15.34,0:11:16.13,main,,0000,0000,0000,,Na to si vzpomínám!
Dialogue: 0,0:11:16.13,0:11:17.00,main,,0000,0000,0000,,Viděla jsem to!
Dialogue: 0,0:11:17.01,0:11:17.76,main,,0000,0000,0000,,Ano!
Dialogue: 0,0:11:17.76,0:11:20.01,main,,0000,0000,0000,,To je reakce kterou jsem chtěla!
Dialogue: 0,0:11:20.01,0:11:21.47,main,,0000,0000,0000,,Kolik ti je?
Dialogue: 0,0:11:24.72,0:11:25.55,main,,0000,0000,0000,,Mám to!
Dialogue: 0,0:11:25.56,0:11:27.47,main,,0000,0000,0000,,Nech toho s těmahle starejma praštěnejma vtipama.
Dialogue: 0,0:11:27.47,0:11:29.10,main,,0000,0000,0000,,Co je Kona-chan?
Dialogue: 0,0:11:29.10,0:11:30.93,main,,0000,0000,0000,,Právě mě něco napadlo.
Dialogue: 0,0:11:30.94,0:11:32.10,main,,0000,0000,0000,,O čem to mluvíš?
Dialogue: 0,0:11:32.10,0:11:34.23,main,,0000,0000,0000,,Tady to začíná!
Dialogue: 0,0:11:34.23,0:11:36.82,main,,0000,0000,0000,,Chci vidět co se stane...
Dialogue: 0,0:11:36.82,0:11:38.53,main,,0000,0000,0000,,a musím si to důkladně zapamatovat!
Dialogue: 0,0:11:38.53,0:11:40.94,main,,0000,0000,0000,,Nenech si utéct žádnou příležitost!
Dialogue: 0,0:11:42.74,0:11:43.78,main,,0000,0000,0000,,Nastraženo.
Dialogue: 0,0:11:47.29,0:11:48.16,main,,0000,0000,0000,,Perfektní!
Dialogue: 0,0:11:48.16,0:11:48.74,main,,0000,0000,0000,,Tak jo!
Dialogue: 0,0:11:48.75,0:11:49.91,main,,0000,0000,0000,,Vůbec netuším co se děje.
Dialogue: 0,0:11:50.79,0:11:51.83,main,,0000,0000,0000,,Nicméně…
Dialogue: 0,0:11:51.83,0:11:54.08,main,,0000,0000,0000,,…protože jsi taková celý den…
Dialogue: 0,0:11:54.08,0:11:55.17,main,,0000,0000,0000,,Jaká…?
Dialogue: 0,0:11:55.17,0:11:57.09,main,,0000,0000,0000,,Nikdo se s námi nesnaží sblížit.
Dialogue: 0,0:11:59.05,0:12:02.47,main,,0000,0000,0000,,To ty jsi s tím první začala!
Dialogue: 0,0:12:02.47,0:12:06.18,main,,0000,0000,0000,,Mě to bylo jedno.
Dialogue: 0,0:12:06.89,0:12:10.31,main,,0000,0000,0000,,Takže Kagami po někom touží?
Dialogue: 0,0:12:10.31,0:12:11.48,main,,0000,0000,0000,,N-Ne…
Dialogue: 0,0:12:11.48,0:12:12.56,main,,0000,0000,0000,,ÁÁa!
Dialogue: 0,0:12:13.94,0:12:14.73,main,,0000,0000,0000,,Sakra!
Dialogue: 0,0:12:14.73,0:12:16.15,main,,0000,0000,0000,,Prošvihla jsem to!
Dialogue: 0,0:12:21.19,0:12:22.03,main,,0000,0000,0000,,Co je?
Dialogue: 0,0:12:23.86,0:12:27.20,main,,0000,0000,0000,,Zajímalo mě jak pečuješ o své vlasy.
Dialogue: 0,0:12:27.20,0:12:30.83,main,,0000,0000,0000,,Moje jsou velice jemné tak nedělám nic.
Dialogue: 0,0:12:32.08,0:12:33.08,main,,0000,0000,0000,,A co ty?
Dialogue: 0,0:12:33.08,0:12:34.42,main,,0000,0000,0000,,Já?!
Dialogue: 0,0:12:34.42,0:12:36.50,main,,0000,0000,0000,,Jen tak normálně.
Dialogue: 0,0:12:45.89,0:12:49.18,main,,0000,0000,0000,,Dneska to vlastně docela vyšlo.
Dialogue: 0,0:12:49.64,0:12:50.56,main,,0000,0000,0000,,Co?
Dialogue: 0,0:12:50.56,0:12:52.22,main,,0000,0000,0000,,No…
Dialogue: 0,0:12:52.23,0:12:53.56,main,,0000,0000,0000,,Však víš…
Dialogue: 0,0:12:53.56,0:12:54.98,main,,0000,0000,0000,,Skoro to přišlo…
Dialogue: 0,0:12:55.90,0:12:56.94,main,,0000,0000,0000,,Jo, to?
Dialogue: 0,0:12:56.94,0:12:57.35,main,,0000,0000,0000,,Chápu.
Dialogue: 0,0:12:57.36,0:12:58.95,main,,0000,0000,0000,,Tak to je dobře.
Dialogue: 0,0:12:58.96,0:13:00.27,main,,0000,0000,0000,,Ty jsi byla v pohodě?
Dialogue: 0,0:13:00.86,0:13:02.19,main,,0000,0000,0000,,Bez problému.
Dialogue: 0,0:13:02.19,0:13:06.03,main,,0000,0000,0000,,U mě to skončilo už před týdnem.
Dialogue: 0,0:13:06.53,0:13:08.57,main,,0000,0000,0000,,Je vážně blbý když se to stane.
Dialogue: 0,0:13:09.20,0:13:10.03,main,,0000,0000,0000,,To máš pravdu.
Dialogue: 0,0:13:10.41,0:13:13.12,main,,0000,0000,0000,,Bylo by pěkně blbý, kdybys byla jediná kdo by nemohl do vody…
Dialogue: 0,0:13:13.12,0:13:15.41,main,,0000,0000,0000,,a muset TO použít by bylo otravný…
Dialogue: 0,0:13:15.42,0:13:16.33,main,,0000,0000,0000,,To jo.
Dialogue: 0,0:13:16.33,0:13:17.58,main,,0000,0000,0000,,Donutí tě to dvakrát přemýšlet.
Dialogue: 0,0:13:17.58,0:13:19.38,main,,0000,0000,0000,,Nikdy jsem to nepoužila.
Dialogue: 0,0:13:21.17,0:13:23.38,main,,0000,0000,0000,,To byla otevřená konverzace.
Dialogue: 0,0:13:23.38,0:13:25.51,main,,0000,0000,0000,,A co ty?
Dialogue: 0,0:13:26.47,0:13:27.76,main,,0000,0000,0000,,Neříkej mi…
Dialogue: 0,0:13:27.76,0:13:29.14,main,,0000,0000,0000,,Co je to za pohled?
Dialogue: 0,0:13:29.14,0:13:29.85,main,,0000,0000,0000,,Nic.
Dialogue: 0,0:13:30.76,0:13:34.18,main,,0000,0000,0000,,Toto je pravdivý příběh…
Dialogue: 0,0:13:35.35,0:13:38.23,main,,0000,0000,0000,,Prodavač počítačového softwaru na úpravu fotografií…
Dialogue: 0,0:13:38.23,0:13:40.40,main,,0000,0000,0000,,…byl na jeho cestě domů…
Dialogue: 0,0:13:40.40,0:13:43.78,main,,0000,0000,0000,,…a jako obvykle nastoupil v noci na autobus…
Dialogue: 0,0:13:44.57,0:13:46.15,main,,0000,0000,0000,,Jeho dům byl…
Dialogue: 0,0:13:46.15,0:13:49.16,main,,0000,0000,0000,,…na konci trasy.
Dialogue: 0,0:13:49.87,0:13:53.54,main,,0000,0000,0000,,Pasažéři postupně vystupovali…
Dialogue: 0,0:13:53.54,0:13:57.79,main,,0000,0000,0000,,…až nakonec zůstal v autobuse sám…
Dialogue: 0,0:13:59.75,0:14:04.71,main,,0000,0000,0000,,Řidič si myslel, že už všichni vystoupili…
Dialogue: 0,0:14:05.76,0:14:06.42,main,,0000,0000,0000,,…a…
Dialogue: 0,0:14:08.68,0:14:12.89,main,,0000,0000,0000,,…z plných plic zazpíval „DANZEN! Futari wa ***Cure“!
Dialogue: 0,0:14:15.39,0:14:17.52,main,,0000,0000,0000,,Jen povídačka co jsem slyšela.
Dialogue: 0,0:14:17.52,0:14:19.35,main,,0000,0000,0000,,Nic to nebylo…
Dialogue: 0,0:14:44.84,0:14:45.88,main,,0000,0000,0000,,To je úleva…
Dialogue: 0,0:14:45.92,0:14:47.76,main,,0000,0000,0000,,Vážně jsem se bála.
Dialogue: 0,0:14:48.72,0:14:49.72,main,,0000,0000,0000,,Já také…
Dialogue: 0,0:14:49.72,0:14:50.99,main,,0000,0000,0000,,Já jsem Sukekiyo.
Dialogue: 0,0:14:54.51,0:14:56.22,main,,0000,0000,0000,,Kde jsi vzala tu masku?!
Dialogue: 0,0:14:56.60,0:14:57.98,main,,0000,0000,0000,,Tak dobrou.
Dialogue: 0,0:14:57.98,0:14:58.39,main,,0000,0000,0000,,Jo.
Dialogue: 0,0:14:58.39,0:14:59.69,main,,0000,0000,0000,,Dobrou noc.
Dialogue: 0,0:14:59.69,0:15:00.52,main,,0000,0000,0000,,Rodina Inugami!
Dialogue: 0,0:15:03.02,0:15:05.02,main,,0000,0000,0000,,Strašit tě je děsná sranda.
Dialogue: 0,0:15:05.03,0:15:06.28,main,,0000,0000,0000,,Už jdi spát.
Dialogue: 0,0:15:12.66,0:15:13.37,main,,0000,0000,0000,,Hej!
Dialogue: 0,0:15:13.37,0:15:14.24,main,,0000,0000,0000,,Jdeme!
Dialogue: 0,0:15:14.95,0:15:15.87,main,,0000,0000,0000,,Dobře!
Dialogue: 0,0:15:50.20,0:15:53.41,main,,0000,0000,0000,,Nenapadá mě nic legračního.
Dialogue: 0,0:15:56.74,0:15:57.74,main,,0000,0000,0000,,Jsem Tsukasa.
Dialogue: 0,0:15:58.33,0:15:59.66,main,,0000,0000,0000,,Jsem Tsukasa.
Dialogue: 0,0:16:10.76,0:16:13.43,main,,0000,0000,0000,,Ségra!
Dialogue: 0,0:16:13.43,0:16:14.43,main,,0000,0000,0000,,Co to?
Dialogue: 0,0:16:14.64,0:16:15.61,main2,,0000,0000,0000,,Bála jsem se!
Dialogue: 0,0:16:14.84,0:16:19.31,main,,0000,0000,0000,,Právě je o krok blíž dospělosti.
Dialogue: 0,0:16:22.60,0:16:25.35,main,,0000,0000,0000,,Počítačový festival je velký hit.
Dialogue: 0,0:16:25.36,0:16:28.11,main,,0000,0000,0000,,Protože Japonci zbožňují limitované edice.
Dialogue: 0,0:16:28.53,0:16:31.15,main,,0000,0000,0000,,Když je slovo „limitovaná edice“ dost ke zvýšení prodejů…
Dialogue: 0,0:16:31.15,0:16:33.11,main,,0000,0000,0000,,…měli by to dávat na vše.
Dialogue: 0,0:16:33.49,0:16:35.82,main,,0000,0000,0000,,Tím by se vytratila jeho podstata.
Dialogue: 0,0:16:36.12,0:16:38.45,main,,0000,0000,0000,,A tak jsem se jí zeptal.
Dialogue: 0,0:16:38.45,0:16:39.03,main,,0000,0000,0000,,Ano.
Dialogue: 0,0:16:39.04,0:16:42.83,main,,0000,0000,0000,,Do konce prázdnin si ještě ukliďte pokoje.
Dialogue: 0,0:16:46.00,0:16:47.84,main,,0000,0000,0000,,Jéé, vzpomínám si!
Dialogue: 0,0:16:47.84,0:16:49.50,main,,0000,0000,0000,,Tyhle sešity jsou pěkně staré.
Dialogue: 0,0:16:49.50,0:16:50.55,main,,0000,0000,0000,,Ségra!
Dialogue: 0,0:16:50.55,0:16:52.38,main,,0000,0000,0000,,Podívej se na toto album!
Dialogue: 0,0:16:52.92,0:16:54.68,main,,0000,0000,0000,,Tady na té fotce vypadá táta zajímavě.
Dialogue: 0,0:16:54.68,0:16:55.34,main,,0000,0000,0000,,To jo.
Dialogue: 0,0:16:55.34,0:16:58.35,main,,0000,0000,0000,,Páni, je to až neuvěřitelné.
Dialogue: 0,0:16:58.76,0:16:59.76,main,,0000,0000,0000,,Vypadá to, že se bavíte…
Dialogue: 0,0:16:59.76,0:17:01.35,main,,0000,0000,0000,,…ale už máte hotovo?
Dialogue: 0,0:17:02.56,0:17:04.94,main,,0000,0000,0000,,Um…
Dialogue: 0,0:17:04.94,0:17:05.52,main,,0000,0000,0000,,Hej.
Dialogue: 0,0:17:05.52,0:17:07.10,main,,0000,0000,0000,,Chceš tohle oblečení?
Dialogue: 0,0:17:07.50,0:17:09.27,main,,0000,0000,0000,,Pokud ho nepotřebuješ, tak si ho vezmu.
Dialogue: 0,0:17:09.27,0:17:10.57,main,,0000,0000,0000,,Tak já ho tady nechám.
Dialogue: 0,0:17:11.19,0:17:12.15,main,,0000,0000,0000,,Dík.
Dialogue: 0,0:17:14.40,0:17:15.03,main,,0000,0000,0000,,Hej.
Dialogue: 0,0:17:15.03,0:17:15.82,main,,0000,0000,0000,,Chceš toto?
Dialogue: 0,0:17:16.45,0:17:17.70,main,,0000,0000,0000,,Tak já si to vezmu.
Dialogue: 0,0:17:17.99,0:17:19.12,main,,0000,0000,0000,,A co toto?
Dialogue: 0,0:17:19.41,0:17:19.95,main,,0000,0000,0000,,Toto?
Dialogue: 0,0:17:20.33,0:17:20.95,main,,0000,0000,0000,,Počkat!
Dialogue: 0,0:17:20.95,0:17:23.79,main,,0000,0000,0000,,Neříkej mi, že si ze mě děláš odpadkový koš…
Dialogue: 0,0:17:24.25,0:17:27.12,main,,0000,0000,0000,,A místo toho se nakonec díváš na fotky.
Dialogue: 0,0:17:27.54,0:17:28.63,main,,0000,0000,0000,,Vím co myslíš.
Dialogue: 0,0:17:28.63,0:17:31.34,main,,0000,0000,0000,,Když narazím na starý časopis, začnu si ho hned číst.
Dialogue: 0,0:17:31.34,0:17:33.05,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:17:33.05,0:17:34.51,main,,0000,0000,0000,,Jo a co Tsukasa?
Dialogue: 0,0:17:34.51,0:17:38.34,main,,0000,0000,0000,,Nic nevyhodila, takže to zabere chvilku.
Dialogue: 0,0:17:38.35,0:17:39.30,main,,0000,0000,0000,,Ještě pořád na tom dělá.
Dialogue: 0,0:17:40.10,0:17:42.68,main,,0000,0000,0000,,Vypadá to, že by si Tsukasa nejraději vše nechala.
Dialogue: 0,0:17:42.68,0:17:45.52,main,,0000,0000,0000,,Na druhou stranu, ty Kagami vypadáš, že vyhodíš úplně vše.
Dialogue: 0,0:17:45.52,0:17:48.15,main,,0000,0000,0000,,Jak to, že o mě máš…
Dialogue: 0,0:17:48.15,0:17:49.81,main,,0000,0000,0000,,…takové mínění?
Dialogue: 0,0:17:50.94,0:17:53.32,main,,0000,0000,0000,,Nechala sis sešity ze základky?
Dialogue: 0,0:17:53.82,0:17:55.32,main,,0000,0000,0000,,Ty sis je nenechala?
Dialogue: 0,0:17:55.74,0:17:59.41,main,,0000,0000,0000,,Spolu s mými vzpomínkami jsem je nechala ve staré škole.
Dialogue: 0,0:17:59.70,0:18:01.12,main,,0000,0000,0000,,Měla jsi si je vzít domů.
Dialogue: 0,0:18:01.12,0:18:02.66,main,,0000,0000,0000,,Způsobuješ tím škole potíže.
Dialogue: 0,0:18:03.62,0:18:05.62,main,,0000,0000,0000,,Konečně jsem hotova.
Dialogue: 0,0:18:06.33,0:18:08.46,main,,0000,0000,0000,,I když pravděpodobně nepotřebuji vše…
Dialogue: 0,0:18:08.46,0:18:10.63,main,,0000,0000,0000,,…pamatuju si kdy jsem to používala…
Dialogue: 0,0:18:10.63,0:18:12.34,main,,0000,0000,0000,,…takže to nemůžu vyhodit.
Dialogue: 0,0:18:12.34,0:18:12.88,main,,0000,0000,0000,,Jo.
Dialogue: 0,0:18:12.92,0:18:15.38,main,,0000,0000,0000,,Někteří lidé si vážně nechávají vše.
Dialogue: 0,0:18:15.72,0:18:17.51,main,,0000,0000,0000,,Vypadá to, že Tsukasa je ten typ.
Dialogue: 0,0:18:17.88,0:18:20.97,main,,0000,0000,0000,,Ty vypadáš jako někdo kdo vyhazuje i užitečné věci.
Dialogue: 0,0:18:20.97,0:18:22.81,main,,0000,0000,0000,,Když si představím jak jsi uklidila své sešity.
Dialogue: 0,0:18:24.14,0:18:25.18,main,,0000,0000,0000,,Miyuki.
Dialogue: 0,0:18:25.18,0:18:26.77,main,,0000,0000,0000,,Ještě než začne škola…
Dialogue: 0,0:18:26.77,0:18:28.48,main,,0000,0000,0000,,…bys měla jít k zubaři.
Dialogue: 0,0:18:28.98,0:18:31.02,main,,0000,0000,0000,,Rozumím…
Dialogue: 0,0:18:31.02,0:18:32.31,main,,0000,0000,0000,,…ale já prostě nemohu.
Dialogue: 0,0:18:34.15,0:18:37.00,main,,0000,0000,0000,,No, cítila bych se špatně kdybych tě nutila.
Dialogue: 0,0:18:38.00,0:18:39.20,main,,0000,0000,0000,,Pokud nemůžeš jít…
Dialogue: 0,0:18:39.20,0:18:40.66,main,,0000,0000,0000,,…tak bude muset doktor přijít za tebou.
Dialogue: 0,0:18:40.66,0:18:41.66,main,,0000,0000,0000,,Domácí prohlídka.
Dialogue: 0,0:18:41.66,0:18:43.12,main,,0000,0000,0000,,P-Počkej prosím!
Dialogue: 0,0:18:43.12,0:18:46.12,copyrights,,0000,0000,0000,,{\pos(129,45)}  Překlad: paralaX
Dialogue: 0,0:18:43.12,0:18:46.12,copyrights,,0000,0000,0000,,{\pos(130,30)}www.otakogu.cz
Dialogue: 0,0:18:49.07,0:18:50.54,main,,0000,0000,0000,,Hodně štěstí!
Dialogue: 0,0:18:50.79,0:18:54.55,main,,0000,0000,0000,,A už tu máme šestou epizodu Lucky Star!
Dialogue: 0,0:18:54.55,0:18:58.51,main,,0000,0000,0000,,Doufám, že je vám všem již jasné co nás teď čeká.
Dialogue: 0,0:18:58.51,0:18:59.63,main,,0000,0000,0000,,Já jsem Akira Kogami!
Dialogue: 0,0:19:01.09,0:19:01.59,main,,0000,0000,0000,,Slabý!
Dialogue: 0,0:19:02.26,0:19:03.43,main,,0000,0000,0000,,Řekl jsi něco?
Dialogue: 0,0:19:05.27,0:19:09.02,main,,0000,0000,0000,,Vypadá to, že můj asistent Shirashi Minoru si ještě nezvykl na svou práci.
Dialogue: 0,0:19:07.68,0:19:08.43,main2,,0000,0000,0000,,Bojím se…
Dialogue: 0,0:19:08.44,0:19:09.35,main2,,0000,0000,0000,,Moc se bojím…
Dialogue: 0,0:19:09.02,0:19:11.90,main,,0000,0000,0000,,To jak se na něj díváte ho dělá nervózním.
Dialogue: 0,0:19:09.35,0:19:11.52,main2,,0000,0000,0000,,Co jsem udělal špatně?
Dialogue: 0,0:19:11.52,0:19:13.48,main2,,0000,0000,0000,,Kvůli Akira-same jsme šli na hory.
Dialogue: 0,0:19:11.90,0:19:14.06,main,,0000,0000,0000,,Doufám, že si na svou práci brzo zvykne.
Dialogue: 0,0:19:13.48,0:19:14.06,main2,,0000,0000,0000,,Jo.
Dialogue: 0,0:19:14.27,0:19:20.24,main,,0000,0000,0000,,A měl by si začít dávat pozor na to jak mluví s místními veterány, nebo se mu může něco přihodit.
Dialogue: 0,0:19:14.28,0:19:15.15,main2,,0000,0000,0000,,Dokonce i na pláž.
Dialogue: 0,0:19:15.15,0:19:19.07,main2,,0000,0000,0000,,Dělám vše co můžu aby byla tato show zábavnější!
Dialogue: 0,0:19:19.07,0:19:19.82,main2,,0000,0000,0000,,Jo…
Dialogue: 0,0:19:19.82,0:19:21.28,main2,,0000,0000,0000,,A ta žvýkačka byla těžká…
Dialogue: 0,0:19:20.24,0:19:21.28,main,,0000,0000,0000,,Tohle prostě nesnáším…
Dialogue: 0,0:19:21.49,0:19:22.24,main,,0000,0000,0000,,Všech 54 kusů…
Dialogue: 0,0:19:22.24,0:19:24.95,main,,0000,0000,0000,,Dnes si představíme hlavní postavu Lucky Star…
Dialogue: 0,0:19:22.53,0:19:24.53,main2,,0000,0000,0000,,54 žvýkaček v mé puse!
Dialogue: 0,0:19:24.53,0:19:26.33,main2,,0000,0000,0000,,Ale i tak se pořád snažím!
Dialogue: 0,0:19:24.95,0:19:27.95,main,,0000,0000,0000,,Konatu Izumi-san!
Dialogue: 0,0:19:26.33,0:19:27.95,main2,,0000,0000,0000,,Co jiného mám dělat?
Dialogue: 0,0:19:29.91,0:19:31.37,main,,0000,0000,0000,,Přestaň blábolit.
Dialogue: 0,0:19:30.37,0:19:32.21,main2,,0000,0000,0000,,Proč já?!
Dialogue: 0,0:19:33.00,0:19:33.88,main,,0000,0000,0000,,Tak se podíváme…
Dialogue: 0,0:19:33.88,0:19:34.84,main,,0000,0000,0000,,Konata Izumi-san.
Dialogue: 0,0:19:34.84,0:19:35.67,main,,0000,0000,0000,,Sedmnáct let.
Dialogue: 0,0:19:35.67,0:19:37.67,main,,0000,0000,0000,,Její záliby jsou hraní, sledování anime a čtení mangy.
Dialogue: 0,0:19:38.80,0:19:39.97,main,,0000,0000,0000,,Podle těchto dat…
Dialogue: 0,0:19:39.97,0:19:42.51,main,,0000,0000,0000,,…sleduje vydání každé nové show a mangy.
Dialogue: 0,0:19:42.51,0:19:45.39,main,,0000,0000,0000,,A o víkendech hraje nonstop MMORPG…
Dialogue: 0,0:19:45.39,0:19:46.93,main,,0000,0000,0000,,Jinými slovy je otaku?
Dialogue: 0,0:19:47.32,0:19:49.14,main,,0000,0000,0000,,Uh… vypadá to tak…
Dialogue: 0,0:19:50.39,0:19:51.48,main,,0000,0000,0000,,Rychle něco řekni!
Dialogue: 0,0:19:51.81,0:19:53.31,main,,0000,0000,0000,,Tím myslíte…
Dialogue: 0,0:19:53.31,0:19:54.61,main,,0000,0000,0000,,…ty kteří se dívají na House Classics?
Dialogue: 0,0:19:54.61,0:19:57.32,main,,0000,0000,0000,,a ty kteří se dívají na Dog of Flanders a brečí?
Dialogue: 0,0:19:58.23,0:20:00.94,main,,0000,0000,0000,,Z jakého století máš tuhle představu o otaku?
Dialogue: 0,0:20:00.95,0:20:02.66,main,,0000,0000,0000,,Jdeš vůbec s dobou?
Dialogue: 0,0:20:02.66,0:20:04.53,main,,0000,0000,0000,,Přestaň mlet hlouposti!
Dialogue: 0,0:20:04.53,0:20:05.74,main,,0000,0000,0000,,U-mmm…
Dialogue: 0,0:20:06.28,0:20:12.21,main,,0000,0000,0000,,No, myslím, že Dog of Fanders má roztomile uklidňující charaktery…
Dialogue: 0,0:20:12.21,0:20:15.21,main,,0000,0000,0000,,Ale dneska lidé preferují dívky jako jsem já!
Dialogue: 0,0:20:15.21,0:20:17.84,main,,0000,0000,0000,,Roztomilé jako koťátko a nevinné jako čerstvě napadaný sníh.
Dialogue: 0,0:20:17.84,0:20:19.09,main,,0000,0000,0000,,Víš co myslím?
Dialogue: 0,0:20:19.09,0:20:21.13,main,,0000,0000,0000,,Pokud se podívám vpravo, oni udělají to samé!
Dialogue: 0,0:20:21.13,0:20:22.93,main,,0000,0000,0000,,Pokud se podívám vlevo tak se tam podívají také!
Dialogue: 0,0:20:22.93,0:20:26.10,main,,0000,0000,0000,,Čekají na můj koncert celou noc předem a ani nemají možnost se umýt!
Dialogue: 0,0:20:26.10,0:20:28.06,main,,0000,0000,0000,,To může způsobit určité hygienické problémy…
Dialogue: 0,0:20:28.06,0:20:29.60,main,,0000,0000,0000,,…ale stejně mě chtějí vidět!
Dialogue: 0,0:20:29.60,0:20:34.69,main,,0000,0000,0000,,Celý natěšení stojí v houfech pod paprsky žhavého slunce a čekají na můj příchod!
Dialogue: 0,0:20:34.69,0:20:38.61,main,,0000,0000,0000,,Dokud není šátek na jejich hlavách celý promáčený!
Dialogue: 0,0:20:42.36,0:20:43.40,main,,0000,0000,0000,,No…
Dialogue: 0,0:20:43.41,0:20:45.78,main,,0000,0000,0000,,Jsou to zákazníci.
Dialogue: 0,0:20:45.78,0:20:49.70,main,,0000,0000,0000,,Když si pak podáváme ruce, nepřestanou mluvit.
Dialogue: 0,0:20:49.70,0:20:52.62,main,,0000,0000,0000,,I na WC se jen hrnou a nedbají na své okolí.
Dialogue: 0,0:20:52.62,0:20:55.17,main,,0000,0000,0000,,Není to příšerné?
Dialogue: 0,0:20:55.88,0:20:58.88,main,,0000,0000,0000,,Jsem tu jen kvůli nim.
Dialogue: 0,0:20:58.88,0:21:02.21,main,,0000,0000,0000,,Musím se pro ně moc snažit.
Dialogue: 0,0:21:02.22,0:21:03.22,main,,0000,0000,0000,,Víš?
Dialogue: 0,0:21:03.22,0:21:05.47,main,,0000,0000,0000,,Tak to je…
Dialogue: 0,0:21:05.47,0:21:07.59,main,,0000,0000,0000,,To brzo pochopíš.
Dialogue: 0,0:21:07.60,0:21:09.93,main,,0000,0000,0000,,Jak těžké je být idol…
Dialogue: 0,0:21:09.93,0:21:13.73,main,,0000,0000,0000,,Jakmile si projdeš dobrý i špatným pochopíš o co tu jde.
Dialogue: 0,0:21:13.73,0:21:16.23,main,,0000,0000,0000,,Jako třeba že máš fanoušky nebo klienty které nemáš rád…
Dialogue: 0,0:21:17.19,0:21:18.56,main,,0000,0000,0000,,Perfektní načasování!
Dialogue: 0,0:21:18.57,0:21:19.44,main,,0000,0000,0000,,Tak všichni!
Dialogue: 0,0:21:19.44,0:21:21.86,main,,0000,0000,0000,,Už lépe rozumíte Konatě?
Dialogue: 0,0:21:21.86,0:21:24.99,main,,0000,0000,0000,,Tak se uvidíme za týden u Lucky Channel!
Dialogue: 0,0:21:24.99,0:21:26.16,main,,0000,0000,0000,,Pá-pá!
Dialogue: 0,0:21:27.49,0:21:28.12,main,,0000,0000,0000,,Uh…
Dialogue: 0,0:21:28.12,0:21:29.66,main,,0000,0000,0000,,Ani jste nepředstavila Konatu…
Dialogue: 0,0:21:29.66,0:21:31.41,main,,0000,0000,0000,,Spíš jste představovala otaku.
Dialogue: 0,0:21:32.29,0:21:33.00,main,,0000,0000,0000,,A-Akira-samo?
Dialogue: 0,0:21:33.00,0:21:34.12,main,,0000,0000,0000,,Jste v pořadku?
Dialogue: 0,0:21:34.12,0:21:34.91,main,,0000,0000,0000,,Nech mě být.
Dialogue: 0,0:21:35.67,0:21:36.50,main,,0000,0000,0000,,Rozumím!
Dialogue: 0,0:21:36.50,0:21:38.42,main,,0000,0000,0000,,A dnešní dopis je od…
Dialogue: 0,0:21:38.42,0:21:39.17,main,,0000,0000,0000,,To není vtipné.
Dialogue: 0,0:21:44.09,0:21:46.26,main,,0000,0000,0000,,Kagami je úžasná!
Dialogue: 0,0:21:46.26,0:21:48.09,main,,0000,0000,0000,,Jo, chápeme.
Dialogue: 0,0:21:48.09,0:21:49.68,main,,0000,0000,0000,,Objednala jsi si písničku, Tsukaso?
Dialogue: 0,0:21:50.14,0:21:51.06,main,,0000,0000,0000,,Jo.
Dialogue: 0,0:21:51.06,0:21:53.43,main,,0000,0000,0000,,Tsukasin hlas je velice čistý.
Dialogue: 0,0:21:54.68,0:21:56.39,main,,0000,0000,0000,,Doufám, že to dokážu zazpívat.
Dialogue: 0,0:21:57.10,0:21:58.73,main,,0000,0000,0000,,Co budeš zpívat Miyuki?
Dialogue: 0,0:21:58.73,0:22:01.40,main,,0000,0000,0000,,Moc písniček neznám…
Dialogue: 0,0:22:01.40,0:22:02.61,main,,0000,0000,0000,,Je to tak trochu trapné.
Dialogue: 0,0:22:05.70,0:22:07.45,main,,0000,0000,0000,,Tak toto?
Dialogue: 0,0:22:07.45,0:22:08.86,main,,0000,0000,0000,,Do toho, Tsukaso!
Dialogue: 0,0:22:09.99,0:22:13.58,mainup2,,0000,0000,0000,,Zítřek bude zvláštní, ZVLÁŠTNÍ DEN!
Dialogue: 0,0:22:13.66,0:22:16.87,mainup2,,0000,0000,0000,,Šance která se neopakuje.
Dialogue: 0,0:22:16.87,0:22:18.29,mainup2,,0000,0000,0000,,Drahoušku!
Dialogue: 0,0:22:19.21,0:22:20.13,mainup2,,0000,0000,0000,,Báječné!
Dialogue: 0,0:22:20.13,0:22:20.93,mainup2,,0000,0000,0000,,Tsukasa!
Dialogue: 0,0:22:20.63,0:22:21.63,mainup2,,0000,0000,0000,,Drahoušku!
Dialogue: 0,0:22:20.88,0:22:21.63,main,,0000,0000,0000,,Hej.
Dialogue: 0,0:22:21.63,0:22:22.71,mainup2,,0000,0000,0000,,Miluji tě!
Dialogue: 0,0:22:22.88,0:22:23.75,mainup2,,0000,0000,0000,,Báječné!
Dialogue: 0,0:22:23.76,0:22:24.63,mainup2,,0000,0000,0000,,Sayuri!
Dialogue: 0,0:22:24.63,0:22:28.38,mainup2,,0000,0000,0000,,Všichni se radí nad karnevalem!
Dialogue: 0,0:22:24.63,0:22:25.88,main,,0000,0000,0000,,Kdo je to?
Dialogue: 0,0:22:28.39,0:22:31.68,mainup2,,0000,0000,0000,,Tohle srdce bude mé!
Dialogue: 0,0:22:31.68,0:22:33.06,mainup2,,0000,0000,0000,,OH BABY!
Dialogue: 0,0:22:33.06,0:22:35.35,mainup2,,0000,0000,0000,,Její jméno není Sayuri Kuninama, že?
Dialogue: 0,0:22:35.35,0:22:36.56,mainup2,,0000,0000,0000,,OH BABY!
Dialogue: 0,0:22:35.60,0:22:37.23,main,,0000,0000,0000,,Kdo to je?!
Dialogue: 0,0:22:36.56,0:22:39.52,mainup2,,0000,0000,0000,,Jaké že bylo její klubové číslo?
Dialogue: 0,0:22:39.52,0:22:43.65,mainup2,,0000,0000,0000,,Když ti dám…
Dialogue: 0,0:22:43.65,0:22:47.28,mainup2,,0000,0000,0000,,…sladkou čokoládu lásky.
Dialogue: 0,0:22:47.28,0:22:50.49,mainup2,,0000,0000,0000,,Nebudu vystupovat.
Dialogue: 0,0:22:47.95,0:22:49.28,main,,0000,0000,0000,,Sklapni už!
Dialogue: 0,0:22:50.49,0:22:54.49,mainup2,,0000,0000,0000,,Tak já to zakončím s poslední zprávou!
Dialogue: 0,0:22:54.49,0:22:56.83,mainup2,,0000,0000,0000,,Valentýnský polibek!
Dialogue: 0,0:22:58.21,0:23:00.58,mainup2,,0000,0000,0000,,Valentýnský polibek!
Dialogue: 0,0:23:01.92,0:23:04.67,mainup2,,0000,0000,0000,,Valentýnský polibek!
Dialogue: 0,0:23:05.63,0:23:07.63,mainup2,,0000,0000,0000,,S uvázanou mašlí!
Dialogue: 0,0:23:07.63,0:23:08.80,mainup2,,0000,0000,0000,,Sha-la-la-la!
Dialogue: 0,0:23:08.80,0:23:09.63,mainup2,,0000,0000,0000,,Báječné…
Dialogue: 0,0:23:09.63,0:23:11.63,mainup2,,0000,0000,0000,,
