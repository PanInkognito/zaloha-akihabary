Dialogue: 0,0:00:00.00,0:00:05.44,whitebig,,0000,0000,0000,,{\pos(225,126)}
Dialogue: 0,0:00:00.00,0:00:05.44,whitebig,,0000,0000,0000,,{\pos(226,152)}
Dialogue: 0,0:00:00.60,0:00:02.08,main,,0000,0000,0000,,Tak začneme.
Dialogue: 0,0:00:02.08,0:00:03.12,main,,0000,0000,0000,,Jdeme na to.
Dialogue: 0,0:00:03.68,0:00:05.48,main,,0000,0000,0000,,No tak už začněme!
Dialogue: 0,0:00:05.84,0:00:07.00,op1,,0000,0000,0000,,Okolo 3 cm!
Dialogue: 0,0:00:07.00,0:00:08.28,op1,,0000,0000,0000,,Je to snad málo?
Dialogue: 0,0:00:08.28,0:00:08.52,op1,,0000,0000,0000,,Jejda!
Dialogue: 0,0:00:08.52,0:00:10.36,op1,,0000,0000,0000,,Schované pod naší uniformou!
Dialogue: 0,0:00:10.36,0:00:11.40,op1,,0000,0000,0000,,Mám toho nechat?
Dialogue: 0,0:00:11.40,0:00:11.72,op1,,0000,0000,0000,,JEJ!
Dialogue: 0,0:00:11.72,0:00:12.80,op1,,0000,0000,0000,,Jít do toho?
Dialogue: 0,0:00:12.80,0:00:13.60,op1,,0000,0000,0000,,Snažit se?
Dialogue: 0,0:00:13.60,0:00:15.16,op1,,0000,0000,0000,,Vychutnat si každý okamžik. Jo!
Dialogue: 0,0:00:15.16,0:00:15.76,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:15.76,0:00:16.00,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:16.00,0:00:16.40,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.40,0:00:16.76,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:16.76,0:00:17.36,op1,,0000,0000,0000,,Jen mezi námi dvěma!
Dialogue: 0,0:00:17.36,0:00:20.08,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:00:22.08,0:00:22.88,logo,,0000,0000,0000,,{\move(108,218,202,218,0,0)}LUCKY
Dialogue: 0,0:00:22.88,0:00:24.60,logo,,0000,0000,0000,,{\pos(202,218)}LUCKY
Dialogue: 0,0:00:22.08,0:00:22.88,logo,,0000,0000,0000,,{\move(638,218,459,218,0,0)}STAR
Dialogue: 0,0:00:22.88,0:00:24.60,logo,,0000,0000,0000,,{\pos(459,218)}STAR
Dialogue: 0,0:00:25.00,0:00:25.60,op1,,0000,0000,0000,,Cítím se nějak nesvá.
Dialogue: 0,0:00:25.60,0:00:26.20,op1,,0000,0000,0000,,Co jsem to jedla?
Dialogue: 0,0:00:26.20,0:00:26.80,op1,,0000,0000,0000,,Mám tě hrozně ráda!
Dialogue: 0,0:00:26.80,0:00:27.00,op1,,0000,0000,0000,,Oops!
Dialogue: 0,0:00:27.00,0:00:28.04,op1,,0000,0000,0000,,Jedno z nich je špatně!
Dialogue: 0,0:00:28.04,0:00:28.80,op1,,0000,0000,0000,,Relaxovat!
Dialogue: 0,0:00:28.80,0:00:29.44,op1,,0000,0000,0000,,Nic nedělat!
Dialogue: 0,0:00:29.44,0:00:29.96,op1,,0000,0000,0000,,Flákat se!
Dialogue: 0,0:00:29.96,0:00:31.04,op1,,0000,0000,0000,,Už toho nechej!
Dialogue: 0,0:00:31.04,0:00:32.72,op1,,0000,0000,0000,,Je ta nová holka oblíbená?
Dialogue: 0,0:00:32.72,0:00:34.36,op1,,0000,0000,0000,,Já bych řekla, že je docela normální!
Dialogue: 0,0:00:34.36,0:00:35.84,op1,,0000,0000,0000,,To jsem jediná překvapená?
Dialogue: 0,0:00:35.84,0:00:36.16,op1,,0000,0000,0000,,Vepřové kousky!
Dialogue: 0,0:00:36.16,0:00:36.56,op1,,0000,0000,0000,,Skleněné nudle!
Dialogue: 0,0:00:36.56,0:00:36.88,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:00:36.88,0:00:37.68,op2,,0000,0000,0000,,DA-DA-DA-DA-DA!
Dialogue: 0,0:00:38.24,0:00:39.04,op1,,0000,0000,0000,,Ooh?
Dialogue: 0,0:00:39.04,0:00:39.80,op1,,0000,0000,0000,,Víš co tím myslím?
Dialogue: 0,0:00:39.40,0:00:40.24,op1up,,0000,0000,0000,,Moje brýle…
Dialogue: 0,0:00:40.24,0:00:41.32,op1up,,0000,0000,0000,,Kde mám brýle?
Dialogue: 0,0:00:41.40,0:00:43.36,op1,,0000,0000,0000,,Nedávno jsme hrála tu samou bojovku.
Dialogue: 0,0:00:42.00,0:00:43.36,op1up,,0000,0000,0000,,Bez nich nic nevidím…
Dialogue: 0,0:00:44.40,0:00:45.00,op2,,0000,0000,0000,,POM! POM!
Dialogue: 0,0:00:45.00,0:00:46.04,op1,,0000,0000,0000,,Roztleskávačky!
Dialogue: 0,0:00:46.04,0:00:47.56,op2,,0000,0000,0000,,DÁME SI SLADKOST!
Dialogue: 0,0:00:47.56,0:00:48.16,op2,,0000,0000,0000,,LA! LA!
Dialogue: 0,0:00:48.16,0:00:49.20,op1,,0000,0000,0000,,Párty začíná!
Dialogue: 0,0:00:49.20,0:00:50.80,op2,,0000,0000,0000,,MÁŠ TO V KRVI!
Dialogue: 0,0:00:50.80,0:00:51.36,op2,,0000,0000,0000,,ANO!
Dialogue: 0,0:00:51.36,0:00:52.36,op1,,0000,0000,0000,,Zanechává to dojem.
Dialogue: 0,0:00:52.36,0:00:53.84,op1,,0000,0000,0000,,Malé planetky.
Dialogue: 0,0:00:53.84,0:00:54.36,op1,,0000,0000,0000,,Narazili!
Dialogue: 0,0:00:54.36,0:00:55.00,op1,,0000,0000,0000,,A shořeli!
Dialogue: 0,0:00:55.00,0:00:55.36,op1,,0000,0000,0000,,Bylo to ohromné!
Dialogue: 0,0:00:55.36,0:00:56.12,op1,,0000,0000,0000,,Z plných plic křičíme!
Dialogue: 0,0:00:56.12,0:00:56.68,op1,,0000,0000,0000,,ZPÍVEJ A TANCUJ!
Dialogue: 0,0:00:57.04,0:00:58.32,op1,,0000,0000,0000,,Přiznej si to!
Dialogue: 0,0:00:58.32,0:01:02.80,op1,,0000,0000,0000,,Nakonec se stejně budeme smát!
Dialogue: 0,0:01:02.80,0:01:06.56,op1,,0000,0000,0000,,Protože mám na sobě námořnickou uniformu.
Dialogue: 0,0:01:06.56,0:01:09.44,op2,,0000,0000,0000,,To je ono!
Dialogue: 0,0:01:10.00,0:01:13.56,op1,,0000,0000,0000,,Je teprve pondělí a já už mám špatnou náladu.
Dialogue: 0,0:01:13.56,0:01:15.36,op1,,0000,0000,0000,,Co mám dělat?
Dialogue: 0,0:01:15.56,0:01:19.40,op1,,0000,0000,0000,,Raději si vezmu moji letní uniformu -
Dialogue: 0,0:01:19.40,0:01:22.28,op1,,0000,0000,0000,,- je rozkošná!
Dialogue: 0,0:01:22.52,0:01:23.68,op1,,0000,0000,0000,,Trojdílná uniforma!
Dialogue: 0,0:01:23.68,0:01:25.12,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:01:25.12,0:01:25.56,op1,,0000,0000,0000,,JO!
Dialogue: 0,0:01:25.56,0:01:26.00,op1,,0000,0000,0000,,Dej se do toho!
Dialogue: 0,0:01:26.00,0:01:26.36,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.36,0:01:27.00,op1,,0000,0000,0000,,SNAŽ SE!
Dialogue: 0,0:01:27.00,0:01:27.16,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:27.16,0:01:29.04,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:01:31.56,0:01:32.68,main,,0000,0000,0000,,Připravit!
Dialogue: 0,0:01:33.24,0:01:34.28,main,,0000,0000,0000,,Pozor!
Dialogue: 0,0:01:44.12,0:01:45.36,main,,0000,0000,0000,,Wow, zase jsi to zvládla.
Dialogue: 0,0:01:45.36,0:01:46.48,main,,0000,0000,0000,,To bylo úžasné.
Dialogue: 0,0:01:47.28,0:01:49.04,main,,0000,0000,0000,,Jak to, že nejsi v žádné sportovním klubu,
Dialogue: 0,0:01:49.04,0:01:51.68,main,,0000,0000,0000,,když jsi tak dobrá?
Dialogue: 0,0:01:51.68,0:01:53.68,main,,0000,0000,0000,,Protože, kdybych se do nějakého přidala…
Dialogue: 0,0:01:54.32,0:01:56.96,main,,0000,0000,0000,,…nestíhala bych vysílání anime.
Dialogue: 0,0:01:57.08,0:02:00.00,white,,0000,0000,0000,,{\frz-90\pos(283,142)}Rychlá
Dialogue: 0,0:01:57.08,0:02:00.00,white,,0000,0000,0000,,{\frz-90\pos(380,222)}holka
Dialogue: 0,0:02:06.92,0:02:08.08,main,,0000,0000,0000,,Ty, Tsukaso.
Dialogue: 0,0:02:08.08,0:02:10.04,main,,0000,0000,0000,,Z jakého konce jíš čokoládový kornout?
Dialogue: 0,0:02:11.88,0:02:13.04,main,,0000,0000,0000,,Myslím, že od hlavy.
Dialogue: 0,0:02:13.76,0:02:14.52,main,,0000,0000,0000,,Chápu.
Dialogue: 0,0:02:15.68,0:02:16.68,main,,0000,0000,0000,,Mimochodem…
Dialogue: 0,0:02:16.68,0:02:17.88,main,,0000,0000,0000,,…která strana je hlava?
Dialogue: 0,0:02:17.88,0:02:19.20,main,,0000,0000,0000,,Tenká nebo tlustá strana?
Dialogue: 0,0:02:20.64,0:02:24.32,main,,0000,0000,0000,,Myslím, že ta tenká.
Dialogue: 0,0:02:24.32,0:02:25.36,main,,0000,0000,0000,,Aha.
Dialogue: 0,0:02:25.36,0:02:28.00,main,,0000,0000,0000,,Já myslela, že je to ta tlustá.
Dialogue: 0,0:02:28.92,0:02:31.00,main,,0000,0000,0000,,Proč bys řekla, že je to ta tenká?
Dialogue: 0,0:02:31.00,0:02:32.60,main,,0000,0000,0000,,No, nepřipomíná ti ulitu?
Dialogue: 0,0:02:33.32,0:02:35.52,main,,0000,0000,0000,,Proč sis ty myslela, že je to ta tlustá?
Dialogue: 0,0:02:35.52,0:02:36.48,main,,0000,0000,0000,,No, nepřipomíná ti…
Dialogue: 0,0:02:36.48,0:02:37.68,main,,0000,0000,0000,,…housenku?
Dialogue: 0,0:02:38.64,0:02:39.48,main,,0000,0000,0000,,Housenku?
Dialogue: 0,0:02:39.48,0:02:43.00,main,,0000,0000,0000,,Ale asi je lepší si představit ulitu.
Dialogue: 0,0:02:58.96,0:02:59.84,main,,0000,0000,0000,,U-Um…
Dialogue: 0,0:03:00.96,0:03:02.68,main,,0000,0000,0000,,Mohla bys odloupnout tenký konec…
Dialogue: 0,0:03:02.68,0:03:05.64,main,,0000,0000,0000,,…a sníst ho s přebytečnou čokoládou.
Dialogue: 0,0:03:06.64,0:03:08.04,main,,0000,0000,0000,,Už to chápu.
Dialogue: 0,0:03:11.64,0:03:13.20,main,,0000,0000,0000,,Dobrý postřeh, Miyuki-san.
Dialogue: 0,0:03:13.20,0:03:14.48,main,,0000,0000,0000,,Jsi tak chytrá.
Dialogue: 0,0:03:14.48,0:03:15.28,main,,0000,0000,0000,,Ne, tak zase ne.
Dialogue: 0,0:03:15.28,0:03:17.60,main,,0000,0000,0000,,Každý má svůj vlastní styl.
Dialogue: 0,0:03:18.00,0:03:20.24,main,,0000,0000,0000,,Jak jíš větrník?
Dialogue: 0,0:03:20.64,0:03:22.40,main,,0000,0000,0000,,Větrníky?
Dialogue: 0,0:03:22.44,0:03:23.28,main,,0000,0000,0000,,No…
Dialogue: 0,0:03:23.52,0:03:25.92,main,,0000,0000,0000,,První oddělím spodní a horní poloviny od sebe…
Dialogue: 0,0:03:25.92,0:03:29.16,main,,0000,0000,0000,,…a pak sním vršek, který si namáčím do krému vespodu.
Dialogue: 0,0:03:29.16,0:03:32.92,main,,0000,0000,0000,,A potom sním spodní polovinu.
Dialogue: 0,0:03:32.92,0:03:36.20,main,,0000,0000,0000,,Takto se nemusím obávat, že mi spadne nějaký krém…
Dialogue: 0,0:03:36.20,0:03:39.32,main,,0000,0000,0000,,…a navíc mám v rovnováze množství větrníku a krému.
Dialogue: 0,0:03:41.20,0:03:44.84,main,,0000,0000,0000,,A co ty, co mají uvnitř krém i puding?
Dialogue: 0,0:03:44.84,0:03:46.60,main,,0000,0000,0000,,Jak jíš je?
Dialogue: 0,0:03:46.60,0:03:47.60,main,,0000,0000,0000,,Smícháš je dohromady?
Dialogue: 0,0:03:47.60,0:03:48.20,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:03:48.24,0:03:50.08,main,,0000,0000,0000,,To bych neudělala.
Dialogue: 0,0:03:50.88,0:03:52.80,main,,0000,0000,0000,,Ale, jak bys je chtěla zamíchat?
Dialogue: 0,0:03:53.36,0:03:54.60,main,,0000,0000,0000,,Jen se ptám.
Dialogue: 0,0:03:54.68,0:03:56.76,main,,0000,0000,0000,,Kdy jíš jahody na dortu…
Dialogue: 0,0:03:56.76,0:03:59.24,main,,0000,0000,0000,,…kaštan na vrcholu Mont Blancu?
Dialogue: 0,0:03:59.24,0:04:00.68,main,,0000,0000,0000,,Takže…
Dialogue: 0,0:04:00.68,0:04:05.40,main,,0000,0000,0000,,Jsou tu také jahody mezi jednotlivými vrstvami.
Dialogue: 0,0:04:05.40,0:04:10.64,main,,0000,0000,0000,,Snažím se dodržovat rovnoměrné rozdělení krému, jahod a piškotu.
Dialogue: 0,0:04:11.40,0:04:15.36,main,,0000,0000,0000,,Takže nemám vyhraněný čas na jahody.
Dialogue: 0,0:04:16.12,0:04:17.28,main,,0000,0000,0000,,Ty se vážně vyznáš.
Dialogue: 0,0:04:17.96,0:04:21.48,main,,0000,0000,0000,,Já si vždy nechám jahodu až na konec.
Dialogue: 0,0:04:21.48,0:04:23.96,main,,0000,0000,0000,,Ale někdy jsem tak plná…
Dialogue: 0,0:04:23.96,0:04:25.52,main,,0000,0000,0000,,…že ji ani nesním…
Dialogue: 0,0:04:25.52,0:04:27.48,main,,0000,0000,0000,,…nebo si ji dá někdo z rodiny.
Dialogue: 0,0:04:27.48,0:04:30.60,main,,0000,0000,0000,,Vážně bys měla sníst to nejlepší už na začátku.
Dialogue: 0,0:04:30.60,0:04:34.64,main,,0000,0000,0000,,Když tak o tom přemýšlím, tak jsem nikdy neviděla, že by si někdo vzal jahodu od mé sestry.
Dialogue: 0,0:04:34.64,0:04:35.72,main,,0000,0000,0000,,To je celá Kagami.
Dialogue: 0,0:04:35.72,0:04:36.80,main,,0000,0000,0000,,Pořád obezřetná.
Dialogue: 0,0:04:36.80,0:04:41.24,main,,0000,0000,0000,,Dokážeš sníst poslední kousek nanuku, aniž by ti spadl?
Dialogue: 0,0:04:41.24,0:04:42.32,main,,0000,0000,0000,,Spadl?
Dialogue: 0,0:04:42.68,0:04:44.24,main,,0000,0000,0000,,Pokud nejsi pozorná…
Dialogue: 0,0:04:46.16,0:04:47.48,main,,0000,0000,0000,,…tak ti poslední kousek spadne, že?
Dialogue: 0,0:04:48.64,0:04:50.08,main,,0000,0000,0000,,Jak to děláš ty, Kona-chan?
Dialogue: 0,0:04:50.08,0:04:51.28,main,,0000,0000,0000,,Slížu ho do posledního kousku.
Dialogue: 0,0:04:52.60,0:04:55.52,main,,0000,0000,0000,,Mohla bys s ním otáčet.
Dialogue: 0,0:04:55.88,0:04:57.08,main,,0000,0000,0000,,To je dobrý nápad.
Dialogue: 0,0:04:57.08,0:05:00.08,main,,0000,0000,0000,,Já nemám zrovna v oblibě zmrzlinové kornouty.
Dialogue: 0,0:05:00.72,0:05:01.76,main,,0000,0000,0000,,Proč?
Dialogue: 0,0:05:02.64,0:05:05.24,main,,0000,0000,0000,,Nesnesu, když mi zůstane pouze kornout.
Dialogue: 0,0:05:05.64,0:05:06.68,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:05:06.68,0:05:08.80,main,,0000,0000,0000,,Vždy ti nakonec nějaký zůstane.
Dialogue: 0,0:05:08.80,0:05:13.48,main,,0000,0000,0000,,Já tlačím zmrzlinu dolů, když ho jím.
Dialogue: 0,0:05:14.40,0:05:16.48,main,,0000,0000,0000,,Čím ji tlačíš?
Dialogue: 0,0:05:16.48,0:05:18.52,main,,0000,0000,0000,,Trochu ji tam zašťouchám.
Dialogue: 0,0:05:19.24,0:05:20.64,main,,0000,0000,0000,,Zašťoucháš?
Dialogue: 0,0:05:21.32,0:05:23.56,main,,0000,0000,0000,,Yuki-chan, ty si potrpíš na vyváženost, je to tak?
Dialogue: 0,0:05:23.56,0:05:24.72,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:05:24.72,0:05:26.92,main,,0000,0000,0000,,Tak co děláš s rýží s kari?
Dialogue: 0,0:05:26.92,0:05:28.44,main,,0000,0000,0000,,Smícháš omáčku s rýží?
Dialogue: 0,0:05:28.92,0:05:29.60,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:05:29.60,0:05:31.36,main,,0000,0000,0000,,Nesmíchávám je…
Dialogue: 0,0:05:31.36,0:05:33.00,main,,0000,0000,0000,,…ale dám si záležet, aby byli dobře vyvážené.
Dialogue: 0,0:05:33.36,0:05:34.84,main,,0000,0000,0000,,Já je míchám.
Dialogue: 0,0:05:35.04,0:05:36.08,main,,0000,0000,0000,,Co ty, Kono-chan?
Dialogue: 0,0:05:36.08,0:05:37.48,main,,0000,0000,0000,,To záleží na kari.
Dialogue: 0,0:05:37.48,0:05:40.04,main,,0000,0000,0000,,Pokud je na tom nějaká worcestrová omáčka nebo syrové vejce…
Dialogue: 0,0:05:40.04,0:05:41.20,main,,0000,0000,0000,,…tak je promíchám.
Dialogue: 0,0:05:41.60,0:05:42.80,main,,0000,0000,0000,,Worcestrová omáčka na kari?
Dialogue: 0,0:05:43.28,0:05:44.28,main,,0000,0000,0000,,Je to dobrý.
Dialogue: 0,0:05:44.28,0:05:46.52,main,,0000,0000,0000,,Dáváš ji i na smažená vejce?
Dialogue: 0,0:05:47.16,0:05:48.80,main,,0000,0000,0000,,Na ty dávám sojovou omáčku.
Dialogue: 0,0:05:49.72,0:05:51.64,main,,0000,0000,0000,,To já používám majonézu.
Dialogue: 0,0:05:51.64,0:05:53.04,main,,0000,0000,0000,,Na smažená vejce?
Dialogue: 0,0:05:53.04,0:05:54.24,main,,0000,0000,0000,,Nemyslím vařená.
Dialogue: 0,0:05:55.32,0:05:56.52,main,,0000,0000,0000,,Majonéza…
Dialogue: 0,0:05:56.52,0:05:58.40,main,,0000,0000,0000,,…se hodí k většině jídel z vajec.
Dialogue: 0,0:05:58.40,0:05:59.48,main,,0000,0000,0000,,Dáváš si ji na kari?
Dialogue: 0,0:06:01.12,0:06:03.16,main,,0000,0000,0000,,Musí to, ale být bílá rýže.
Dialogue: 0,0:06:03.16,0:06:06.12,main,,0000,0000,0000,,Ale ta sladkost se hodí i k hovězím miskám.
Dialogue: 0,0:06:06.12,0:06:07.20,main,,0000,0000,0000,,Hodí se i k Oyako-don.
Dialogue: 0,0:06:07.72,0:06:10.20,main,,0000,0000,0000,,Víš jako jsou vejce a kuřecí maso jako děti a rodiče.
Dialogue: 0,0:06:10.44,0:06:13.64,main,,0000,0000,0000,,Takže, když přidám majonézu, která je také z vajec…
Dialogue: 0,0:06:13.88,0:06:16.12,main,,0000,0000,0000,,…říkám tomu vylepšené Oyako-don.
Dialogue: 0,0:06:17.76,0:06:18.56,main,,0000,0000,0000,,A taky…
Dialogue: 0,0:06:18.56,0:06:20.16,main,,0000,0000,0000,,…normálně nesnesu natto…
Dialogue: 0,0:06:20.16,0:06:22.24,main,,0000,0000,0000,,…ale když k tomu přidám majonézu, je to hned lepší.
Dialogue: 0,0:06:22.76,0:06:23.72,main,,0000,0000,0000,,To si nedokážu představit.
Dialogue: 0,0:06:24.36,0:06:25.12,main,,0000,0000,0000,,Ale…
Dialogue: 0,0:06:25.12,0:06:29.28,main,,0000,0000,0000,,…mojonéza na vejcí na hniličku může být nejlepší.
Dialogue: 0,0:06:29.28,0:06:31.80,main,,0000,0000,0000,,Smažená vejce jsou u nás taky tekutá.
Dialogue: 0,0:06:32.64,0:06:34.84,main,,0000,0000,0000,,Můj otec je tak má rád.
Dialogue: 0,0:06:34.84,0:06:38.40,main,,0000,0000,0000,,Rád z nich vycucne žloutek.
Dialogue: 0,0:06:38.40,0:06:41.72,main,,0000,0000,0000,,A když jsou moc uvařená, tak to nejde.
Dialogue: 0,0:06:42.32,0:06:44.20,main,,0000,0000,0000,,A co třeba kuře na špízu?
Dialogue: 0,0:06:44.20,0:06:45.36,main,,0000,0000,0000,,Co je s ním?
Dialogue: 0,0:06:45.36,0:06:47.00,main,,0000,0000,0000,,Necháváte ho na něm?
Dialogue: 0,0:06:47.00,0:06:49.20,main,,0000,0000,0000,,Nebo použijete hůlky a sundáte ho?
Dialogue: 0,0:06:49.20,0:06:50.64,main,,0000,0000,0000,,Hm…
Dialogue: 0,0:06:50.64,0:06:53.80,main,,0000,0000,0000,,Když ho jím sama, tak ho nechám na špízu.
Dialogue: 0,0:06:53.80,0:06:56.32,main,,0000,0000,0000,,Ale když jsem s lidmi, tak ho sundám.
Dialogue: 0,0:06:56.32,0:06:57.44,main,,0000,0000,0000,,Jo, jo.
Dialogue: 0,0:06:57.44,0:07:00.24,main,,0000,0000,0000,,Je to trochu nešikovné, když zůstane jen jedno.
Dialogue: 0,0:07:01.00,0:07:03.60,main,,0000,0000,0000,,Nikdy se nemůžu přimět si ho dát.
Dialogue: 0,0:07:03.60,0:07:06.04,main,,0000,0000,0000,,A jak ubíhá čas a nikdo ho nechce sníst…
Dialogue: 0,0:07:07.48,0:07:08.92,main,,0000,0000,0000,,…začne osychat…
Dialogue: 0,0:07:08.92,0:07:10.44,main,,0000,0000,0000,,…a nevypadá vůbec dobře…
Dialogue: 0,0:07:10.44,0:07:12.52,main,,0000,0000,0000,,…jenže nikdo si ho nechce dát…
Dialogue: 0,0:07:12.52,0:07:13.44,main,,0000,0000,0000,,…ale když přijde číšník tak řekneš…
Dialogue: 0,0:07:13.44,0:07:15.32,main,,0000,0000,0000,,„Moment, ještě jsem nedojedli.“
Dialogue: 0,0:07:15.32,0:07:16.60,main,,0000,0000,0000,,…jaksi ze zvyku.
Dialogue: 0,0:07:17.24,0:07:19.96,main,,0000,0000,0000,,Ale nakonec ho nikdo nesní.
Dialogue: 0,0:07:20.24,0:07:21.56,main,,0000,0000,0000,,Takové plýtvání.
Dialogue: 0,0:07:21.56,0:07:23.16,main,,0000,0000,0000,,Je mi ho trochu líto.
Dialogue: 0,0:07:24.48,0:07:25.72,main,,0000,0000,0000,,A když se griluje…
Dialogue: 0,0:07:25.72,0:07:29.40,main,,0000,0000,0000,,…tak je mi líto masa, které zůstane připečené ke grilu.
Dialogue: 0,0:07:29.40,0:07:32.92,main,,0000,0000,0000,,Měla bys jíst při grilovaní a bavení se…
Dialogue: 0,0:07:32.92,0:07:34.72,main,,0000,0000,0000,,…takže je toho dost.
Dialogue: 0,0:07:34.72,0:07:38.48,main,,0000,0000,0000,,Je to ještě horší s hovězími jazyky, ty jsou hotové skoro hned.
Dialogue: 0,0:07:38.48,0:07:39.84,main,,0000,0000,0000,,To máš pravdu.
Dialogue: 0,0:07:39.84,0:07:43.28,main,,0000,0000,0000,,Jsou hotové už po jednom otočení.
Dialogue: 0,0:07:43.28,0:07:44.32,main,,0000,0000,0000,,Otočení?
Dialogue: 0,0:07:45.28,0:07:46.08,main,,0000,0000,0000,,Hovězí jazyky?
Dialogue: 0,0:07:46.88,0:07:49.00,main,,0000,0000,0000,,Ty se neotáčejí.
Dialogue: 0,0:07:50.04,0:07:50.64,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:07:51.00,0:07:52.32,main,,0000,0000,0000,,Pokud je otočíš…
Dialogue: 0,0:07:52.32,0:07:54.60,main,,0000,0000,0000,,…cibule nahoře spadne dolů.
Dialogue: 0,0:07:55.24,0:07:56.24,main,,0000,0000,0000,,Oh, aha.
Dialogue: 0,0:07:56.28,0:08:00.96,main,,0000,0000,0000,,Udělají se, bez jakéhokoliv otáčení.
Dialogue: 0,0:08:01.24,0:08:02.08,main,,0000,0000,0000,,Rozumím…
Dialogue: 0,0:08:02.08,0:08:03.76,main,,0000,0000,0000,,Od teď to budu tak dělat.
Dialogue: 0,0:08:03.76,0:08:06.08,main,,0000,0000,0000,,Nikdy je neotáčej.
Dialogue: 0,0:08:06.08,0:08:07.36,main,,0000,0000,0000,,Vážně…
Dialogue: 0,0:08:12.68,0:08:16.04,main,,0000,0000,0000,,Mimochodem, co je hlava čokoládového kornoutu? Tenká nebo tlustá strana?
Dialogue: 0,0:08:17.52,0:08:18.84,main,,0000,0000,0000,,To je teda přísné.
Dialogue: 0,0:08:17.12,0:08:19.08,newspaper,,0000,0000,0000,,{\frz-125\pos(518,288)}Chtěl jsem aby byla mým otrokem
Dialogue: 0,0:08:17.12,0:08:19.08,newspaper,,0000,0000,0000,,{\frz-20\pos(522,113)}Uvězněna na dva měsíce
Dialogue: 0,0:08:19.72,0:08:22.28,main,,0000,0000,0000,,Poslední dobou je tu spousta únosů.
Dialogue: 0,0:08:22.28,0:08:25.96,main,,0000,0000,0000,,Chlapi co nutí aby jim holky říkali „Pane“ a tak.
Dialogue: 0,0:08:26.96,0:08:28.00,main,,0000,0000,0000,,Možná to není až tak…
Dialogue: 0,0:08:28.00,0:08:29.04,main,,0000,0000,0000,,…problém morálky…
Dialogue: 0,0:08:29.04,0:08:32.20,main,,0000,0000,0000,,…jako spíš, že hrají moc erotických her?
Dialogue: 0,0:08:32.24,0:08:34.00,main,,0000,0000,0000,,V těch hrách je to docela běžné.
Dialogue: 0,0:08:34.04,0:08:35.92,main,,0000,0000,0000,,To tu mluvíš o sobě?!
Dialogue: 0,0:08:37.92,0:08:38.92,main,,0000,0000,0000,,Hej, počkej moment.
Dialogue: 0,0:08:38.92,0:08:40.96,main,,0000,0000,0000,,Jak to, že znáš tak dobře co je v těch hrách…
Dialogue: 0,0:08:40.96,0:08:41.76,main,,0000,0000,0000,,Slečno středoškolačko?
Dialogue: 0,0:08:42.40,0:08:45.28,copyrights,,0000,0000,0000,,{\pos(85,50)}Překlad od paralaXe
Dialogue: 0,0:08:42.40,0:08:45.28,copyrights,,0000,0000,0000,,{\pos(129,76)}navštivte www.otakogu.cz
Dialogue: 0,0:08:45.48,0:08:47.16,table,,0000,0000,0000,,{\pos(372,70)}První semestr průběžné hodnocení
Dialogue: 0,0:08:45.48,0:08:47.16,table,,0000,0000,0000,,{\frz-90\pos(317,257)}Miyuki Takara
Dialogue: 0,0:08:47.80,0:08:50.80,main,,0000,0000,0000,,Tvoje známky jsou vždy tak dobré.
Dialogue: 0,0:08:51.88,0:08:54.04,main,,0000,0000,0000,,To nemáš žádné neúspěchy?
Dialogue: 0,0:08:54.64,0:08:56.24,main,,0000,0000,0000,,Stydím se to říct…
Dialogue: 0,0:08:56.24,0:08:58.80,main,,0000,0000,0000,,…ale občas se zapomenu…
Dialogue: 0,0:08:58.84,0:09:00.56,main,,0000,0000,0000,,…nebo jen tak hledím do prázdna.
Dialogue: 0,0:09:01.08,0:09:03.92,main,,0000,0000,0000,,Pořezala jsem se, když jsem škrábala brambory.
Dialogue: 0,0:09:03.48,0:09:03.92,main,,0000,0000,0000,,Au!
Dialogue: 0,0:09:04.24,0:09:07.12,main,,0000,0000,0000,,A zakopla jsem, když jsem pospíchala.
Dialogue: 0,0:09:07.80,0:09:11.24,main,,0000,0000,0000,,A taky sem si přivřela tašku do dveří v metru.
Dialogue: 0,0:09:12.40,0:09:13.52,main,,0000,0000,0000,,Miyuki-san.
Dialogue: 0,0:09:13.52,0:09:16.84,main,,0000,0000,0000,,To nejsou neúspěchy, to je to čemu říkáme moé.
Dialogue: 0,0:09:17.48,0:09:18.08,main,,0000,0000,0000,,Moé?
Dialogue: 0,0:09:18.08,0:09:19.36,main,,0000,0000,0000,,Nemotorná.
Dialogue: 0,0:09:19.36,0:09:22.40,main,,0000,0000,0000,,Co se teď děje v Kono-chaniné hlavě!?
Dialogue: 0,0:09:22.88,0:09:25.88,main,,0000,0000,0000,,Miyuki-san, vypadáš vážně pěkně, proč vlastně nosíš brýle?
Dialogue: 0,0:09:25.88,0:09:28.12,main,,0000,0000,0000,,Takto se líbíš jen určitému typu kluků.
Dialogue: 0,0:09:28.12,0:09:29.16,main,,0000,0000,0000,,Určitému typu…
Dialogue: 0,0:09:29.72,0:09:32.12,main,,0000,0000,0000,,Když si odmyslíme oblíbenost u kluků…
Dialogue: 0,0:09:32.12,0:09:33.80,main,,0000,0000,0000,,…proč nenosíš čočky?
Dialogue: 0,0:09:33.80,0:09:36.52,main,,0000,0000,0000,,Slyšela jsem, že brýle zhoršují zrak.
Dialogue: 0,0:09:36.52,0:09:37.48,main,,0000,0000,0000,,Eh…
Dialogue: 0,0:09:37.48,0:09:40.00,main,,0000,0000,0000,,Je mi to trapné říkat, když jsme už na střední…
Dialogue: 0,0:09:40.84,0:09:41.68,main,,0000,0000,0000,,A-Ale…
Dialogue: 0,0:09:42.04,0:09:43.40,main,,0000,0000,0000,,Já se…
Dialogue: 0,0:09:43.40,0:09:44.60,main,,0000,0000,0000,,…jich bojím!
Dialogue: 0,0:09:44.60,0:09:46.08,main,,0000,0000,0000,,Bojím se čoček!
Dialogue: 0,0:09:46.08,0:09:47.32,main,,0000,0000,0000,,Abych byla přesnější…
Dialogue: 0,0:09:47.32,0:09:49.72,main,,0000,0000,0000,,…bojím se dávat si něco do očí.
Dialogue: 0,0:09:49.72,0:09:52.20,main,,0000,0000,0000,,Dokonce ani nedokážu otevřít oči, když jsem pod vodou.
Dialogue: 0,0:09:52.40,0:09:53.12,main,,0000,0000,0000,,Zatraceně!
Dialogue: 0,0:09:53.12,0:09:54.88,main,,0000,0000,0000,,Dokonce je i roztomilá!
Dialogue: 0,0:09:54.88,0:09:55.72,main,,0000,0000,0000,,To musí být přirozené.
Dialogue: 0,0:09:56.92,0:09:58.40,main,,0000,0000,0000,,Není to Miyuki-san?
Dialogue: 0,0:09:59.72,0:10:01.52,main,,0000,0000,0000,,Je tu kvůli očím?
Dialogue: 0,0:10:01.52,0:10:02.52,main,,0000,0000,0000,,Na čočky?
Dialogue: 0,0:10:05.52,0:10:08.60,main,,0000,0000,0000,,Nejspíš je takto lepší.
Dialogue: 0,0:10:08.60,0:10:09.64,main,,0000,0000,0000,,To asi bolelo…
Dialogue: 0,0:10:09.64,0:10:11.16,main,,0000,0000,0000,,Má to vrozené.
Dialogue: 0,0:10:13.84,0:10:15.84,main,,0000,0000,0000,,Kagami je doma s nachlazením?
Dialogue: 0,0:10:16.60,0:10:19.68,main,,0000,0000,0000,,Vypadá to ale, že nemá chřipku.
Dialogue: 0,0:10:19.68,0:10:23.40,main,,0000,0000,0000,,Já jsem hloupá, takže se o chřipku nemusím bát.
Dialogue: 0,0:10:24.28,0:10:25.76,main,,0000,0000,0000,,Slyšela jsem, že to platí o nachlazení…
Dialogue: 0,0:10:25.76,0:10:27.72,main,,0000,0000,0000,,…ale nevím jestli i na chřipku.
Dialogue: 0,0:10:30.32,0:10:32.56,main,,0000,0000,0000,,On je mezi nimi rozdíl?
Dialogue: 0,0:10:32.56,0:10:34.80,main,,0000,0000,0000,,Myslela jsem, že chřipka je něco jako super-nachlazení.
Dialogue: 0,0:10:34.80,0:10:36.84,main,,0000,0000,0000,,M-Myslím že jsou rozdílné…
Dialogue: 0,0:10:37.44,0:10:40.60,main,,0000,0000,0000,,Ale teď si nejsem jistá…
Dialogue: 0,0:10:41.52,0:10:43.64,main,,0000,0000,0000,,Rozdíl mezi chřipkou a nachlazením?
Dialogue: 0,0:10:45.08,0:10:46.32,main,,0000,0000,0000,,No…
Dialogue: 0,0:10:46.76,0:10:49.24,main,,0000,0000,0000,,Chřipka je způsobena virem.
Dialogue: 0,0:10:49.24,0:10:52.52,main,,0000,0000,0000,,Příznaky jsou horečka a bolest celého těla.
Dialogue: 0,0:10:52.52,0:10:55.40,main,,0000,0000,0000,,A je tu riziko komplikací.
Dialogue: 0,0:10:55.40,0:10:56.20,main,,0000,0000,0000,,Na druhé straně…
Dialogue: 0,0:10:56.20,0:10:58.76,main,,0000,0000,0000,,…příznaky nachlazení jsou bolest v krku a rýma…
Dialogue: 0,0:10:58.76,0:11:00.92,main,,0000,0000,0000,,…a horečka není tak vysoká.
Dialogue: 0,0:11:00.92,0:11:03.32,main,,0000,0000,0000,,A je tu navíc spousta menších rozdílů.
Dialogue: 0,0:11:05.04,0:11:05.92,main,,0000,0000,0000,,Takže…
Dialogue: 0,0:11:05.92,0:11:07.96,main,,0000,0000,0000,,to jsou rozdílné nemoci.
Dialogue: 0,0:11:08.80,0:11:09.72,main,,0000,0000,0000,,Chápu…
Dialogue: 0,0:11:11.00,0:11:12.12,main,,0000,0000,0000,,N-Nicméně…
Dialogue: 0,0:11:12.44,0:11:14.92,main,,0000,0000,0000,,No, mají i dost společného…
Dialogue: 0,0:11:14.92,0:11:18.80,main,,0000,0000,0000,,…takže bys možná mohla říct, že je to vylepšené nachlazení…
Dialogue: 0,0:11:18.80,0:11:19.52,main,,0000,0000,0000,,Věděla jsem to!
Dialogue: 0,0:11:21.96,0:11:22.48,main,,0000,0000,0000,,Kono-chan.
Dialogue: 0,0:11:22.48,0:11:24.80,main,,0000,0000,0000,,Co ten zamyšlený výraz?
Dialogue: 0,0:11:24.80,0:11:26.68,main,,0000,0000,0000,,Ale, to nic.
Dialogue: 0,0:11:26.68,0:11:28.80,main,,0000,0000,0000,,Jen jsem přemýšlela o našem ranním rozhovoru.
Dialogue: 0,0:11:29.36,0:11:30.84,main,,0000,0000,0000,,Ohledně nachlazení a chřipky?
Dialogue: 0,0:11:31.16,0:11:34.84,main,,0000,0000,0000,,Ty jsi neřekla že nesouhlasíš, když jsem řekla že jsem hloupá.
Dialogue: 0,0:11:34.84,0:11:35.68,main,,0000,0000,0000,,N-Ne!
Dialogue: 0,0:11:35.68,0:11:36.72,main,,0000,0000,0000,,Tak to není!
Dialogue: 0,0:11:42.40,0:11:43.48,main,,0000,0000,0000,,Konato-chan.
Dialogue: 0,0:11:43.48,0:11:44.44,main,,0000,0000,0000,,Vítej.
Dialogue: 0,0:11:44.44,0:11:46.48,main,,0000,0000,0000,,Přišla navštívit sestru.
Dialogue: 0,0:11:47.36,0:11:48.80,main,,0000,0000,0000,,Děkujeme za návštěvu.
Dialogue: 0,0:11:48.80,0:11:49.40,main,,0000,0000,0000,,To nic.
Dialogue: 0,0:11:49.40,0:11:51.12,main,,0000,0000,0000,,Promiňte, že vyrušuji.
Dialogue: 0,0:11:55.92,0:11:58.32,main,,0000,0000,0000,,Kagama vypadá docela roztomile, když spí.
Dialogue: 0,0:11:58.72,0:12:01.00,main,,0000,0000,0000,,Co to děláš?              Je vzhůru. Je vzhůru.
Dialogue: 0,0:12:01.92,0:12:02.68,main,,0000,0000,0000,,Navštívit?
Dialogue: 0,0:12:02.68,0:12:03.52,main,,0000,0000,0000,,Ty?
Dialogue: 0,0:12:03.52,0:12:05.24,main,,0000,0000,0000,,Přišla jsi celou tu cestu, abys mě viděla?
Dialogue: 0,0:12:05.28,0:12:06.16,main,,0000,0000,0000,,Tak to je.
Dialogue: 0,0:12:06.16,0:12:08.36,main,,0000,0000,0000,,A-Ale já nechci, abys to ode mě chytila.
Dialogue: 0,0:12:08.36,0:12:09.80,main,,0000,0000,0000,,Ale vážně si toho cením.
Dialogue: 0,0:12:10.48,0:12:11.04,main,,0000,0000,0000,,Ah.
Dialogue: 0,0:12:11.04,0:12:12.84,main,,0000,0000,0000,,To je milé.
Dialogue: 0,0:12:12.84,0:12:15.00,main,,0000,0000,0000,,Bála se o mě…
Dialogue: 0,0:12:15.04,0:12:17.24,main,,0000,0000,0000,,…protože jsem nemocná?
Dialogue: 0,0:12:17.24,0:12:21.16,main,,0000,0000,0000,,Myslela jsem si, že má v hlavě jen hry a anime.
Dialogue: 0,0:12:21.16,0:12:23.04,main,,0000,0000,0000,,Trochu se mě to dotklo.
Dialogue: 0,0:12:24.60,0:12:25.08,main,,0000,0000,0000,,No…
Dialogue: 0,0:12:25.08,0:12:27.44,main,,0000,0000,0000,,…taky jsem chtěla vidět ten úkol z minula.
Dialogue: 0,0:12:27.48,0:12:29.92,main,,0000,0000,0000,,A pak viděla jsem jak pěkně spíš…
Dialogue: 0,0:12:29.92,0:12:31.24,main,,0000,0000,0000,,…tak to ani neřeš.
Dialogue: 0,0:12:31.24,0:12:31.72,main,,0000,0000,0000,,Vypadni!
Dialogue: 0,0:12:33.52,0:12:34.52,main,,0000,0000,0000,,Kagami!
Dialogue: 0,0:12:34.56,0:12:37.28,main,,0000,0000,0000,,Miyuki-chan tě přišla navštívit!
Dialogue: 0,0:12:37.80,0:12:38.36,main,,0000,0000,0000,,Fajn!
Dialogue: 0,0:12:38.96,0:12:39.88,main,,0000,0000,0000,,Promiň, že ruším.
Dialogue: 0,0:12:40.80,0:12:42.60,main,,0000,0000,0000,,Je mi líto, že sem jdeš, když máš volno.
Dialogue: 0,0:12:42.60,0:12:44.48,main,,0000,0000,0000,,Je v pořádku, že jsi takto vzhůru?
Dialogue: 0,0:12:44.96,0:12:45.88,main,,0000,0000,0000,,Děkuji.                Tady máš něco ode mě.
Dialogue: 0,0:12:46.40,0:12:48.00,main,,0000,0000,0000,,Už jsem o dost lepší.
Dialogue: 0,0:12:48.00,0:12:49.88,main,,0000,0000,0000,,Po tom co jsem si vzala nějaké léky mi klesla horečka.
Dialogue: 0,0:12:49.88,0:12:50.24,main,,0000,0000,0000,,Tady.
Dialogue: 0,0:12:50.92,0:12:52.60,main,,0000,0000,0000,,Květiny a ovoce! To jsi nemusela!
Dialogue: 0,0:12:53.44,0:12:54.44,main,,0000,0000,0000,,Posaď se.
Dialogue: 0,0:12:54.44,0:12:55.72,main,,0000,0000,0000,,Moc děkuji.
Dialogue: 0,0:12:56.24,0:13:00.04,main,,0000,0000,0000,,Chtěla jsem tě navštívit se Izumi-san a ostatními…
Dialogue: 0,0:13:00.04,0:13:02.68,main,,0000,0000,0000,,…ale bohužel jsem si nemohla udělat čas.
Dialogue: 0,0:13:02.68,0:13:03.80,main,,0000,0000,0000,,To je v pohodě.
Dialogue: 0,0:13:04.36,0:13:07.84,main,,0000,0000,0000,,Mám pro tebe nějaké věci ze studentské rady.
Dialogue: 0,0:13:07.84,0:13:09.00,main,,0000,0000,0000,,Díky!
Dialogue: 0,0:13:09.44,0:13:13.52,main,,0000,0000,0000,,Pokud by jsi se na ně mohla jen mrknout…
Dialogue: 0,0:13:14.36,0:13:17.68,main,,0000,0000,0000,,A tady jsou rozpočty pro každý klub.
Dialogue: 0,0:13:17.68,0:13:20.32,main,,0000,0000,0000,,A co jsem to ještě chtěla?
Dialogue: 0,0:13:20.32,0:13:21.32,main,,0000,0000,0000,,Em…
Dialogue: 0,0:13:21.88,0:13:23.84,main,,0000,0000,0000,,Pokud bys chtěla odpočívat…
Dialogue: 0,0:13:23.84,0:13:24.96,main,,0000,0000,0000,,…tak já hned odejdu.
Dialogue: 0,0:13:25.68,0:13:27.76,main,,0000,0000,0000,,Takto má vypadat správná návštěva!
Dialogue: 0,0:13:27.76,0:13:29.76,main,,0000,0000,0000,,S-Stalo se něco?
Dialogue: 0,0:13:30.24,0:13:32.44,main,,0000,0000,0000,,No, Konata se tu včera ukázala…
Dialogue: 0,0:13:34.16,0:13:35.72,main,,0000,0000,0000,,Jak ti říkám.
Dialogue: 0,0:13:35.72,0:13:40.20,main,,0000,0000,0000,,Možná se takto projevuje.
Dialogue: 0,0:13:40.20,0:13:42.44,main,,0000,0000,0000,,Ten domácí úkol mohl být pokus o vtip…
Dialogue: 0,0:13:42.44,0:13:45.72,main,,0000,0000,0000,,…aby tě rozveselila.
Dialogue: 0,0:13:45.72,0:13:49.48,main,,0000,0000,0000,,A možná, že tě viděla spát…
Dialogue: 0,0:13:49.48,0:13:51.16,main,,0000,0000,0000,,…protože tě nechtěla vzbudit.
Dialogue: 0,0:13:51.72,0:13:53.60,main,,0000,0000,0000,,To je vždy jen pozitivní!?
Dialogue: 0,0:13:53.60,0:13:55.16,main,,0000,0000,0000,,To by se mi nikdy nestalo.
Dialogue: 0,0:13:55.64,0:13:56.76,main,,0000,0000,0000,,Tak mě napadá…
Dialogue: 0,0:13:56.76,0:13:59.16,main,,0000,0000,0000,,Dneska tu Tsukasa-san není?
Dialogue: 0,0:13:59.92,0:14:01.24,main,,0000,0000,0000,,Dobré ráno.
Dialogue: 0,0:14:01.96,0:14:05.32,main,,0000,0000,0000,,Je ti líp, ségra?
Dialogue: 0,0:14:08.64,0:14:09.64,main,,0000,0000,0000,,Jak vidíš…
Dialogue: 0,0:14:09.64,0:14:12.52,main,,0000,0000,0000,,Je víc rozespalá, než nemocný člověk.
Dialogue: 0,0:14:13.24,0:14:14.52,main,,0000,0000,0000,,U-um…
Dialogue: 0,0:14:14.52,0:14:16.56,main,,0000,0000,0000,,Takto to není každé ráno…
Dialogue: 0,0:14:16.56,0:14:19.60,main,,0000,0000,0000,,…jen jsem si ještě nezvykla na změnu času…
Dialogue: 0,0:14:20.16,0:14:21.36,main,,0000,0000,0000,,Je to trochu pozdě, ale vítej.
Dialogue: 0,0:14:22.20,0:14:23.52,main,,0000,0000,0000,,Naprosto ti rozumím.
Dialogue: 0,0:14:23.52,0:14:26.16,main,,0000,0000,0000,,Taky mám problémy se vzbudit.
Dialogue: 0,0:14:26.16,0:14:27.76,main,,0000,0000,0000,,T-To je pravda.
Dialogue: 0,0:14:27.76,0:14:29.36,main,,0000,0000,0000,,Taky se cítíš tak ospalá?
Dialogue: 0,0:14:29.36,0:14:30.84,main,,0000,0000,0000,,Opravdu.
Dialogue: 0,0:14:30.84,0:14:33.00,main,,0000,0000,0000,,A dnes je navíc obzvláště horko.
Dialogue: 0,0:14:33.72,0:14:34.56,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:14:34.56,0:14:35.72,main,,0000,0000,0000,,Jinak to asi nejde.
Dialogue: 0,0:14:36.20,0:14:38.80,main,,0000,0000,0000,,A poslední dny bylo taky horko.
Dialogue: 0,0:14:39.68,0:14:42.52,main,,0000,0000,0000,,Když jsem se probudila, tak už bylo odpoledne.
Dialogue: 0,0:14:42.52,0:14:44.72,main,,0000,0000,0000,,Většinu času jsem prospala.
Dialogue: 0,0:14:44.72,0:14:47.52,main,,0000,0000,0000,,Není na škodu si odpočinout, když máš volno.
Dialogue: 0,0:14:47.52,0:14:50.60,main,,0000,0000,0000,,Takže jsem nakonec na sobě měla moje pyžamo celý den.
Dialogue: 0,0:14:51.12,0:14:51.96,main,,0000,0000,0000,,A teď ho taky mám.
Dialogue: 0,0:14:53.28,0:14:55.60,main,,0000,0000,0000,,To je pyžamo?
Dialogue: 0,0:14:56.32,0:14:57.36,main,,0000,0000,0000,,Ano.
Dialogue: 0,0:14:57.36,0:14:59.04,main,,0000,0000,0000,,Je docela rozkošné.
Dialogue: 0,0:14:59.04,0:15:00.68,main,,0000,0000,0000,,Neřekla bych, že to jsou pyžama.
Dialogue: 0,0:15:00.68,0:15:01.52,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:15:02.36,0:15:04.24,main,,0000,0000,0000,,Zajímalo by mě, jestli bych v něm mohla zůstat.
Dialogue: 0,0:15:04.68,0:15:06.40,main,,0000,0000,0000,,Klidně v něm můžeš i ven.
Dialogue: 0,0:15:06.72,0:15:08.68,main,,0000,0000,0000,,Tenhle rozhovor snad nemá konce.
Dialogue: 0,0:15:07.72,0:15:10.12,main2,,0000,0000,0000,,Nikdo si toho ani nevšimne.
Dialogue: 0,0:15:08.68,0:15:10.12,mainup,,0000,0000,0000,,Mohla bych v něm do obchodu?
Dialogue: 0,0:15:10.04,0:15:11.28,Nurse,,0000,0000,0000,,{\frz15\pos(353,182)}Ošetřovna
Dialogue: 0,0:15:12.12,0:15:13.80,main,,0000,0000,0000,,Přibrala jsem…
Dialogue: 0,0:15:14.92,0:15:15.52,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:15:15.52,0:15:16.60,main,,0000,0000,0000,,Tím to nebude.
Dialogue: 0,0:15:16.60,0:15:19.56,main,,0000,0000,0000,,Dnes mám na sobě podprsenku s drátěnou výztuhou…
Dialogue: 0,0:15:19.60,0:15:21.20,main,,0000,0000,0000,,…takže se to projevilo.
Dialogue: 0,0:15:21.20,0:15:22.16,main,,0000,0000,0000,,To bude ono.
Dialogue: 0,0:15:22.16,0:15:24.24,main,,0000,0000,0000,,Zajímalo by mě, kolik gramů to tak dělá.
Dialogue: 0,0:15:24.24,0:15:25.60,main,,0000,0000,0000,,Když to odečtu…
Dialogue: 0,0:15:25.60,0:15:26.28,Nurse,,0000,0000,0000,,{\frz15\pos(353,182)}Ošetřovna
Dialogue: 0,0:15:28.64,0:15:30.04,main,,0000,0000,0000,,Je to za mnou…
Dialogue: 0,0:15:30.48,0:15:31.76,main,,0000,0000,0000,,To sem tomu dala.
Dialogue: 0,0:15:31.76,0:15:33.60,main,,0000,0000,0000,,Úplně jsem zapomněla, že jsou dneska prohlídky…
Dialogue: 0,0:15:33.60,0:15:36.28,main,,0000,0000,0000,,…a měla jsem svoje kalhotky s potiskem.
Dialogue: 0,0:15:36.28,0:15:38.08,main,,0000,0000,0000,,To je trapas!
Dialogue: 0,0:15:39.44,0:15:40.92,main,,0000,0000,0000,,Vůbec jsem nevyrostla…
Dialogue: 0,0:15:42.12,0:15:42.92,main,,0000,0000,0000,,Zatraceně!
Dialogue: 0,0:15:42.92,0:15:45.28,main,,0000,0000,0000,,Jediná jsi spokojená!
Dialogue: 0,0:15:45.80,0:15:47.08,main,,0000,0000,0000,,Hoj.
Dialogue: 0,0:15:47.08,0:15:47.68,main,,0000,0000,0000,,Jsem tady.
Dialogue: 0,0:15:48.00,0:15:49.20,main,,0000,0000,0000,,Ségra.
Dialogue: 0,0:15:49.20,0:15:50.56,main,,0000,0000,0000,,Napadlo mě…
Dialogue: 0,0:15:51.32,0:15:53.68,main,,0000,0000,0000,,Kagami sem chodí docela často.
Dialogue: 0,0:15:53.68,0:15:55.76,main,,0000,0000,0000,,Máš ve své třídě nějaké kamarády?
Dialogue: 0,0:15:55.76,0:15:57.36,main,,0000,0000,0000,,Nesrovnávej mě s tebou.
Dialogue: 0,0:15:57.36,0:15:58.64,main,,0000,0000,0000,,Chceš se snad dohadovat?
Dialogue: 0,0:15:59.36,0:16:01.00,main,,0000,0000,0000,,Myslela jsem, že nemáš kamarády.
Dialogue: 0,0:16:01.00,0:16:02.08,main,,0000,0000,0000,,Nestarej se o mě…
Dialogue: 0,0:16:02.08,0:16:04.48,main,,0000,0000,0000,,…ale spíš o tvůj nedostatek společenského života.
Dialogue: 0,0:16:05.00,0:16:07.00,main,,0000,0000,0000,,O to se neboj.
Dialogue: 0,0:16:07.00,0:16:09.92,main,,0000,0000,0000,,Věř tomu nebo, ale mám kolem sebe stovky přátel.
Dialogue: 0,0:16:09.92,0:16:13.16,main,,0000,0000,0000,,A jsem dost dobrá v navazování známostí.
Dialogue: 0,0:16:13.16,0:16:15.04,main,,0000,0000,0000,,A každou noc jsem na párty.
Dialogue: 0,0:16:15.88,0:16:20.64,main,,0000,0000,0000,,Já a moji společníci jsem se snažili získat zpět ukradené světlo z krystalu, aby jsme zachránili svět, před záhubou…
Dialogue: 0,0:16:20.64,0:16:21.52,main,,0000,0000,0000,,…a osvobodili unesenou princeznu…
Dialogue: 0,0:16:21.52,0:16:23.04,main,,0000,0000,0000,,To mluvíš o hře?!
Dialogue: 0,0:16:23.08,0:16:25.52,main,,0000,0000,0000,,Dlouho jsem ho neviděla.
Dialogue: 0,0:16:25.52,0:16:26.48,main,,0000,0000,0000,,Koho?
Dialogue: 0,0:16:26.48,0:16:28.60,main,,0000,0000,0000,,Nespíš mluví o někom ze hry.
Dialogue: 0,0:16:29.68,0:16:31.40,main,,0000,0000,0000,,Hrál za válečníka.
Dialogue: 0,0:16:31.40,0:16:37.20,main,,0000,0000,0000,,Šel někde lovit monstra, aby si vylepšil své schopnost a od té doby jsem ho neviděla.
Dialogue: 0,0:16:38.96,0:16:40.56,main,,0000,0000,0000,,Slyšela jsem…
Dialogue: 0,0:16:40.56,0:16:43.16,main,,0000,0000,0000,,…že leží ve stokách.
Dialogue: 0,0:16:43.16,0:16:44.52,main,,0000,0000,0000,,Zajímalo by mě, jestli je v pořádku.
Dialogue: 0,0:16:44.56,0:16:47.80,main,,0000,0000,0000,,Všichni se o něj bojí.
Dialogue: 0,0:16:47.80,0:16:49.72,main,,0000,0000,0000,,To se v té hře izoloval?
Dialogue: 0,0:16:49.72,0:16:51.44,main,,0000,0000,0000,,Nevím podrobnosti…
Dialogue: 0,0:16:51.44,0:16:55.40,main,,0000,0000,0000,,…ale podle jeho výpisu přihlášení to vypadá, že je online po celou dobu.
Dialogue: 0,0:16:55.40,0:16:57.44,main,,0000,0000,0000,,Takže se izoloval ve virtuálním i reálném světě?!
Dialogue: 0,0:16:57.44,0:16:59.60,main,,0000,0000,0000,,No, přece jen je to jeden člověk.
Dialogue: 0,0:17:00.00,0:17:00.84,main,,0000,0000,0000,,Tak mě napadá…
Dialogue: 0,0:17:00.84,0:17:03.80,main,,0000,0000,0000,,Jak jste se vy dvě seznámili?
Dialogue: 0,0:17:04.40,0:17:08.92,main,,0000,0000,0000,,Nějaký hromotluk z ciziny se chystal unést Tsukasu pryč…
Dialogue: 0,0:17:08.92,0:17:11.36,main,,0000,0000,0000,,…když v tom jsem se objevila.
Dialogue: 0,0:17:11.36,0:17:12.76,main,,0000,0000,0000,,Použitím mého běsnícího úderu…
Dialogue: 0,0:17:12.76,0:17:14.92,main,,0000,0000,0000,,…a ukázkového zdrcujícího kopu…
Dialogue: 0,0:17:14.92,0:17:16.24,main,,0000,0000,0000,,…jsem ji zachránila.
Dialogue: 0,0:17:16.24,0:17:19.00,main,,0000,0000,0000,,Proč se vždycky nakonec bavíme o hrách?
Dialogue: 0,0:17:19.00,0:17:22.80,main,,0000,0000,0000,,No, to se stává, když natrefíš na zkušeného bojovníka.
Dialogue: 0,0:17:22.80,0:17:25.04,main,,0000,0000,0000,,Nejsem si jistá, jestli ses měla tak rychle uchýlit k násilí…
Dialogue: 0,0:17:25.04,0:17:26.04,main,,0000,0000,0000,,…ale poradila sis dobře.
Dialogue: 0,0:17:26.48,0:17:30.04,main,,0000,0000,0000,,Ten cizinec se mě vlastně jen ptal na směr…
Dialogue: 0,0:17:30.04,0:17:33.08,main,,0000,0000,0000,,…ale moc jsem mu nerozuměla, když v tom se objevila Kona-chan…
Dialogue: 0,0:17:33.64,0:17:34.64,main,,0000,0000,0000,,To jsi to naprosto zpackala!
Dialogue: 0,0:17:34.64,0:17:36.04,main,,0000,0000,0000,,A proč se tváříš tak hrdě?!
Dialogue: 0,0:17:36.60,0:17:39.76,main,,0000,0000,0000,,Čas znova nadešel…
Dialogue: 0,0:17:40.28,0:17:41.60,main,,0000,0000,0000,,Má se něco stát?
Dialogue: 0,0:17:41.60,0:17:42.24,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:17:42.24,0:17:44.68,main,,0000,0000,0000,,Jen mě napadlo, že zase začíná sezóna baseballu…
Dialogue: 0,0:17:44.68,0:17:48.12,main,,0000,0000,0000,,Ty nemáš ráda sporty?
Dialogue: 0,0:17:48.12,0:17:49.92,main,,0000,0000,0000,,Jsi tak sportovní…
Dialogue: 0,0:17:49.92,0:17:52.08,main,,0000,0000,0000,,…takže jsem myslela, že je máš ráda.
Dialogue: 0,0:17:52.08,0:17:54.68,main,,0000,0000,0000,,Když se protáhnou a hrají přesčas…
Dialogue: 0,0:17:54.68,0:17:57.68,main,,0000,0000,0000,,…vysílání nových dílů anime se naprosto pomíchá.
Dialogue: 0,0:17:58.00,0:18:01.56,main,,0000,0000,0000,,Někdy je dokonce show úplně zrušena.
Dialogue: 0,0:18:01.56,0:18:04.76,main,,0000,0000,0000,,Není příjemné, když přijdeš o něco co máš ráda.
Dialogue: 0,0:18:04.76,0:18:07.72,main,,0000,0000,0000,,Když jíš jahodu na jahodovém dortu, Kagami?
Dialogue: 0,0:18:07.72,0:18:08.40,main,,0000,0000,0000,,Proč se ptáš?
Dialogue: 0,0:18:08.72,0:18:12.04,main,,0000,0000,0000,,Jen mě zajímalo, jestli si ten typ co si nechává nejlepší nakonec.
Dialogue: 0,0:18:12.04,0:18:14.68,main,,0000,0000,0000,,Obvykle jím to nejlepší až nakonec…
Dialogue: 0,0:18:14.68,0:18:16.84,main,,0000,0000,0000,,…ale s jahodami to záleží na situaci.
Dialogue: 0,0:18:16.84,0:18:18.92,main,,0000,0000,0000,,A griluješ obě dvě strany hovězího jazyku?
Dialogue: 0,0:18:19.40,0:18:20.72,main,,0000,0000,0000,,Hovězího jazyku?
Dialogue: 0,0:18:21.84,0:18:23.52,main,,0000,0000,0000,,Je hotový skoro hned…
Dialogue: 0,0:18:23.52,0:18:25.28,main,,0000,0000,0000,,…ale je to ještě rychlejší, když griluješ obě strany.
Dialogue: 0,0:18:25.28,0:18:26.36,main,,0000,0000,0000,,To nesmíš.
Dialogue: 0,0:18:26.36,0:18:27.80,main,,0000,0000,0000,,Cibule by spadla.
Dialogue: 0,0:18:27.80,0:18:30.56,main,,0000,0000,0000,,Nepřidáváš cibuli, až nakonec.
Dialogue: 0,0:18:30.56,0:18:32.32,main,,0000,0000,0000,,Tak by se ale neopekla.
Dialogue: 0,0:18:32.80,0:18:35.80,main,,0000,0000,0000,,Ale nejsou tam jen kvůli chuti?
Dialogue: 0,0:18:35.92,0:18:37.32,main,,0000,0000,0000,,Ty to prostě nechápeš…
Dialogue: 0,0:18:37.32,0:18:40.00,main,,0000,0000,0000,,Nešlo by to tak i tak?
Dialogue: 0,0:18:40.64,0:18:44.36,main,,0000,0000,0000,,Jaká strana čokoládového kornoutu je podle tebe hlava? Tenká nebo Tlustá?
Dialogue: 0,0:18:45.96,0:18:46.80,main,,0000,0000,0000,,Tlustá?
Dialogue: 0,0:18:47.28,0:18:48.80,main,,0000,0000,0000,,Taky si to myslím.
Dialogue: 0,0:18:48.80,0:18:49.32,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:18:49.72,0:18:51.08,main,,0000,0000,0000,,A z jakého konce ho jíš?
Dialogue: 0,0:18:52.12,0:18:53.40,main,,0000,0000,0000,,Nemáš odloupnout tenký konec…
Dialogue: 0,0:18:53.40,0:18:55.28,main,,0000,0000,0000,,…a pomocí něho jíst přebývající čokoládu?
Dialogue: 0,0:19:03.40,0:19:07.32,main,,0000,0000,0000,,Máš kromě náš nějaké jiné reálné kamarády?
Dialogue: 0,0:19:08.52,0:19:12.28,main,,0000,0000,0000,,Ještě na základce jsem měla celkem blízké kamarády.
Dialogue: 0,0:19:13.08,0:19:14.16,main,,0000,0000,0000,,Takže máš.
Dialogue: 0,0:19:14.88,0:19:17.28,main,,0000,0000,0000,,Ale dlouho jsem s ní nebyla…
Dialogue: 0,0:19:17.28,0:19:19.00,main,,0000,0000,0000,,Zajímalo by mě co asi dělá…
Dialogue: 0,0:19:21.84,0:19:24.44,main,,0000,0000,0000,,Na základce napsala…
Dialogue: 0,0:19:24.44,0:19:26.28,main,,0000,0000,0000,,…že její sen je stát se čarodějem.
Dialogue: 0,0:19:26.28,0:19:27.40,main,,0000,0000,0000,,Takže je stejná jako ty?!
