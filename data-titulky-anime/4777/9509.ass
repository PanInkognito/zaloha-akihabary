Dialogue: 0,0:00:00.09,0:00:05.92,whitebig,,0000,0000,0000,,{\pos(226,152)}
Dialogue: 0,0:00:01.11,0:00:02.56,main,,0000,0000,0000,,Tak začneme.
Dialogue: 0,0:00:02.57,0:00:03.61,main,,0000,0000,0000,,Jdeme na to.
Dialogue: 0,0:00:04.19,0:00:05.98,main,,0000,0000,0000,,No tak už začněme!
Dialogue: 0,0:00:06.32,0:00:07.49,op1,,0000,0000,0000,,Okolo 3 cm!
Dialogue: 0,0:00:07.49,0:00:08.78,op1,,0000,0000,0000,,Je to snad málo?
Dialogue: 0,0:00:08.78,0:00:09.03,op1,,0000,0000,0000,,Jejda!
Dialogue: 0,0:00:09.03,0:00:10.86,op1,,0000,0000,0000,,Schované pod naší uniformou!
Dialogue: 0,0:00:10.87,0:00:11.91,op1,,0000,0000,0000,,Mám toho nechat?
Dialogue: 0,0:00:11.91,0:00:12.20,op1,,0000,0000,0000,,JEJ!
Dialogue: 0,0:00:12.20,0:00:13.28,op1,,0000,0000,0000,,Jít do toho?
Dialogue: 0,0:00:13.28,0:00:14.08,op1,,0000,0000,0000,,Snažit se?
Dialogue: 0,0:00:14.08,0:00:15.66,op1,,0000,0000,0000,,Vychutnat si každý okamžik. Jo!
Dialogue: 0,0:00:15.66,0:00:16.25,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.26,0:00:16.50,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:16.50,0:00:16.90,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.91,0:00:17.25,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:17.25,0:00:17.87,op1,,0000,0000,0000,,Jen mezi námi dvěma!
Dialogue: 0,0:00:17.87,0:00:20.58,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:00:22.56,0:00:23.36,logo,,0000,0000,0000,,{\move(108,218,202,218,0,0)}LUCKY
Dialogue: 0,0:00:23.36,0:00:25.08,logo,,0000,0000,0000,,{\pos(202,218)}LUCKY
Dialogue: 0,0:00:22.56,0:00:23.36,logo,,0000,0000,0000,,{\move(638,218,459,218,0,0)}STAR
Dialogue: 0,0:00:23.36,0:00:25.08,logo,,0000,0000,0000,,{\pos(459,218)}STAR
Dialogue: 0,0:00:25.51,0:00:26.09,op1,,0000,0000,0000,,Cítím se nějak nesvá.
Dialogue: 0,0:00:26.09,0:00:26.71,op1,,0000,0000,0000,,Co jsem to jedla?
Dialogue: 0,0:00:26.71,0:00:27.30,op1,,0000,0000,0000,,Mám tě hrozně ráda!
Dialogue: 0,0:00:27.30,0:00:27.51,op1,,0000,0000,0000,,Oops!
Dialogue: 0,0:00:27.51,0:00:28.55,op1,,0000,0000,0000,,Jedno z nich je špatně!
Dialogue: 0,0:00:28.55,0:00:29.30,op1,,0000,0000,0000,,Relaxovat!
Dialogue: 0,0:00:29.30,0:00:29.93,op1,,0000,0000,0000,,Nic nedělat!
Dialogue: 0,0:00:29.93,0:00:30.47,op1,,0000,0000,0000,,Flákat se!
Dialogue: 0,0:00:30.47,0:00:31.55,op1,,0000,0000,0000,,Už toho nechej!
Dialogue: 0,0:00:31.55,0:00:33.21,op1,,0000,0000,0000,,Je ta nová holka oblíbená?
Dialogue: 0,0:00:33.21,0:00:34.85,op1,,0000,0000,0000,,Já bych řekla, že je docela normální!
Dialogue: 0,0:00:34.85,0:00:36.35,op1,,0000,0000,0000,,To jsem jediná překvapená?
Dialogue: 0,0:00:36.35,0:00:36.66,op1,,0000,0000,0000,,Vepřové kousky!
Dialogue: 0,0:00:36.66,0:00:37.06,op1,,0000,0000,0000,,Skleněné nudle!
Dialogue: 0,0:00:37.06,0:00:37.39,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:00:37.39,0:00:38.18,op2,,0000,0000,0000,,DA-DA-DA-DA-DA!
Dialogue: 0,0:00:38.73,0:00:39.52,op1,,0000,0000,0000,,Ooh?
Dialogue: 0,0:00:39.52,0:00:40.31,op1,,0000,0000,0000,,Víš co tím myslím?
Dialogue: 0,0:00:39.89,0:00:40.73,op1up,,0000,0000,0000,,Moje brýle…
Dialogue: 0,0:00:40.73,0:00:41.81,op1up,,0000,0000,0000,,Kde mám brýle?
Dialogue: 0,0:00:41.90,0:00:43.86,op1,,0000,0000,0000,,Nedávno jsme hrála tu samou bojovku.
Dialogue: 0,0:00:42.48,0:00:43.86,op1up,,0000,0000,0000,,Bez nich nic nevidím…
Dialogue: 0,0:00:44.90,0:00:45.48,op2,,0000,0000,0000,,POM! POM!
Dialogue: 0,0:00:45.48,0:00:46.53,op1,,0000,0000,0000,,Roztleskávačky!
Dialogue: 0,0:00:46.53,0:00:48.07,op2,,0000,0000,0000,,DÁME SI SLADKOST!
Dialogue: 0,0:00:48.07,0:00:48.65,op2,,0000,0000,0000,,LA! LA!
Dialogue: 0,0:00:48.65,0:00:49.70,op1,,0000,0000,0000,,Párty začíná!
Dialogue: 0,0:00:49.70,0:00:51.28,op2,,0000,0000,0000,,MÁŠ TO V KRVI!
Dialogue: 0,0:00:51.28,0:00:51.86,op2,,0000,0000,0000,,ANO!
Dialogue: 0,0:00:51.87,0:00:52.87,op1,,0000,0000,0000,,Zanechává to dojem.
Dialogue: 0,0:00:52.87,0:00:54.32,op1,,0000,0000,0000,,Malé planetky.
Dialogue: 0,0:00:54.33,0:00:54.87,op1,,0000,0000,0000,,Narazili!
Dialogue: 0,0:00:54.87,0:00:55.49,op1,,0000,0000,0000,,A shořeli!
Dialogue: 0,0:00:55.49,0:00:55.87,op1,,0000,0000,0000,,Bylo to ohromné!
Dialogue: 0,0:00:55.87,0:00:56.62,op1,,0000,0000,0000,,Z plných plic křičíme!
Dialogue: 0,0:00:56.62,0:00:57.18,op1,,0000,0000,0000,,ZPÍVEJ A TANCUJ!
Dialogue: 0,0:00:57.54,0:00:58.83,op1,,0000,0000,0000,,Přiznej si to!
Dialogue: 0,0:00:58.83,0:01:03.29,op1,,0000,0000,0000,,Nakonec se stejně budeme smát!
Dialogue: 0,0:01:03.29,0:01:07.05,op1,,0000,0000,0000,,Protože mám na sobě námořnickou uniformu.
Dialogue: 0,0:01:07.05,0:01:09.92,op2,,0000,0000,0000,,To je ono!
Dialogue: 0,0:01:10.51,0:01:14.05,op1,,0000,0000,0000,,Je teprve pondělí a já už mám špatnou náladu.
Dialogue: 0,0:01:14.05,0:01:15.85,op1,,0000,0000,0000,,Co mám dělat?
Dialogue: 0,0:01:16.06,0:01:19.89,op1,,0000,0000,0000,,Raději si vezmu moji letní uniformu -
Dialogue: 0,0:01:19.89,0:01:22.77,op1,,0000,0000,0000,,- je rozkošná!
Dialogue: 0,0:01:23.02,0:01:24.19,op1,,0000,0000,0000,,Trojdílná uniforma!
Dialogue: 0,0:01:24.19,0:01:25.61,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:01:25.61,0:01:26.06,op1,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.07,0:01:26.48,op1,,0000,0000,0000,,Dej se do toho!
Dialogue: 0,0:01:26.48,0:01:26.86,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.86,0:01:27.48,op1,,0000,0000,0000,,SNAŽ SE!
Dialogue: 0,0:01:27.48,0:01:27.65,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:27.65,0:01:29.54,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:01:31.34,0:01:32.00,main,,0000,0000,0000,,Že?
Dialogue: 0,0:01:32.00,0:01:33.34,main,,0000,0000,0000,,Smrdí to?
Dialogue: 0,0:01:33.34,0:01:34.38,main,,0000,0000,0000,,To teda jo!
Dialogue: 0,0:01:35.59,0:01:38.18,main,,0000,0000,0000,,Zlatý týden je skoro u konce…
Dialogue: 0,0:01:38.18,0:01:40.64,main,,0000,0000,0000,,Už jste se věnovali úkolům?
Dialogue: 0,0:01:42.18,0:01:43.97,main,,0000,0000,0000,,No, vlastně ještě nee…
Dialogue: 0,0:01:44.85,0:01:48.31,main,,0000,0000,0000,,Šak víš, jak je o prázdninách těžké začít něco dělat?
Dialogue: 0,0:01:48.31,0:01:49.48,main,,0000,0000,0000,,Teď máme co dohánět!
Dialogue: 0,0:01:50.11,0:01:50.90,main,,0000,0000,0000,,Že, ségra?
Dialogue: 0,0:01:51.56,0:01:52.31,main,,0000,0000,0000,,Promiň.
Dialogue: 0,0:01:52.32,0:01:54.44,main,,0000,0000,0000,,Já už mám skoro hotovo.
Dialogue: 0,0:01:55.11,0:01:57.03,main,,0000,0000,0000,,Ale přece sis se mnou celou dobu hrála…
Dialogue: 0,0:01:57.56,0:02:00.60,white,,0000,0000,0000,,{\frz-90\pos(390,133)}Snaha
Dialogue: 0,0:01:57.56,0:02:00.60,white,,0000,0000,0000,,{\frz-90\pos(305,215)}a výsledky
Dialogue: 0,0:02:16.00,0:02:17.20,main,,0000,0000,0000,,Tak jsem dojedla.
Dialogue: 0,0:02:21.91,0:02:23.40,main,,0000,0000,0000,,Mám otázku, ohledně této části…
Dialogue: 0,0:02:23.40,0:02:26.19,main,,0000,0000,0000,,Jak bych mohla znát odpověď, když ji nevíš ani ty?
Dialogue: 0,0:02:26.20,0:02:27.30,main,,0000,0000,0000,,Nejsi na výšce?!
Dialogue: 0,0:02:29.95,0:02:31.91,main,,0000,0000,0000,,Jdi vzbudit Tsukasu.
Dialogue: 0,0:02:31.91,0:02:32.70,main,,0000,0000,0000,,To ne.
Dialogue: 0,0:02:32.71,0:02:33.95,main,,0000,0000,0000,,Ona nevstane.
Dialogue: 0,0:02:35.00,0:02:36.80,main,,0000,0000,0000,,Tak už vstávej!
Dialogue: 0,0:02:37.97,0:02:39.67,main,,0000,0000,0000,,Taťka a ségra mají zpoždění.
Dialogue: 0,0:02:39.97,0:02:41.98,main,,0000,0000,0000,,Nemají tohle ráno, něco důležitého?
Dialogue: 0,0:02:42.11,0:02:44.67,main,,0000,0000,0000,,Cože? To je už ráno?
Dialogue: 0,0:02:43.91,0:02:44.57,main2,,0000,0000,0000,,Jsem doma.
Dialogue: 0,0:02:44.68,0:02:45.97,main,,0000,0000,0000,,Už je ráno.
Dialogue: 0,0:02:46.00,0:02:46.70,main,,0000,0000,0000,,Vítej.
Dialogue: 0,0:02:47.92,0:02:49.00,main,,0000,0000,0000,,Dobré ráno…
Dialogue: 0,0:02:49.00,0:02:50.50,main,,0000,0000,0000,,Už je odpoledne.
Dialogue: 0,0:02:50.50,0:02:52.60,main,,0000,0000,0000,,Nemůžu uvěřit, jak dlouho vydržíš spát.
Dialogue: 0,0:02:54.09,0:02:57.00,main,,0000,0000,0000,,Viděli jste včera speciální epizodu Apron of Love?
Dialogue: 0,0:02:57.01,0:02:57.88,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:02:57.92,0:02:59.51,main,,0000,0000,0000,,V půlce jsem usnula.
Dialogue: 0,0:02:59.92,0:03:00.34,main,,0000,0000,0000,,Usnula jsi?
Dialogue: 0,0:03:01.22,0:03:02.93,main,,0000,0000,0000,,Bylo to šílené!
Dialogue: 0,0:03:02.93,0:03:05.47,main,,0000,0000,0000,,Na konci chlápek balancoval na hlavě s červenou mísou!
Dialogue: 0,0:03:15.73,0:03:16.57,main,,0000,0000,0000,,Kagami!
Dialogue: 0,0:03:16.98,0:03:19.19,main,,0000,0000,0000,,Volá ti Miyuki-chan!
Dialogue: 0,0:03:19.19,0:03:19.99,main,,0000,0000,0000,,Okaj.
Dialogue: 0,0:03:20.45,0:03:24.24,main,,0000,0000,0000,,Nechtěla by ses sejít a zkontrolovat si domácí úkoly?
Dialogue: 0,0:03:25.24,0:03:25.91,main,,0000,0000,0000,,To zní dobře!
Dialogue: 0,0:03:25.91,0:03:26.58,main,,0000,0000,0000,,Tak fajn!
Dialogue: 0,0:03:26.99,0:03:27.95,main,,0000,0000,0000,,Kagami!
Dialogue: 0,0:03:27.95,0:03:29.91,main,,0000,0000,0000,,Volá ti Konata-chan!
Dialogue: 0,0:03:30.79,0:03:34.00,main,,0000,0000,0000,,Co kdyby jsme se sešli a dodělali si úkoly?
Dialogue: 0,0:03:34.00,0:03:34.88,main,,0000,0000,0000,,Počkej moment!
Dialogue: 0,0:03:34.88,0:03:37.46,main,,0000,0000,0000,,Ty si to chceš jen opsat, že jo?!
Dialogue: 0,0:03:37.75,0:03:40.17,main,,0000,0000,0000,,Nech mě, opsat si úkol.
Dialogue: 0,0:03:40.17,0:03:41.47,main,,0000,0000,0000,,Rovnou na věc, co?
Dialogue: 0,0:03:41.97,0:03:44.01,main,,0000,0000,0000,,Můžeš si je opsat…
Dialogue: 0,0:03:44.01,0:03:45.55,main,,0000,0000,0000,,ale pokud si nebudeš dělat úkoly sama…
Dialogue: 0,0:03:45.55,0:03:48.35,main,,0000,0000,0000,,…totálně vyhoříš u testu po prázdninách.
Dialogue: 0,0:03:48.85,0:03:49.97,main,,0000,0000,0000,,Máš pravdu…
Dialogue: 0,0:03:50.56,0:03:52.43,main,,0000,0000,0000,,Vlastně si tím jen ubližujeme…
Dialogue: 0,0:03:52.85,0:03:55.60,main,,0000,0000,0000,,Jsem dobrá ve šprtání se den předem, takže je to ok.
Dialogue: 0,0:03:55.61,0:03:56.36,main,,0000,0000,0000,,Tak mě to nech opsat.
Dialogue: 0,0:03:56.36,0:03:57.86,main,,0000,0000,0000,,Alespoň předstírej, že to není správné!
Dialogue: 0,0:04:00.24,0:04:01.28,main,,0000,0000,0000,,Promiňte, že vyrušuji.
Dialogue: 0,0:04:01.70,0:04:03.24,main,,0000,0000,0000,,Vítej!
Dialogue: 0,0:04:01.70,0:04:03.24,main2,,0000,0000,0000,,Oh, vítej!
Dialogue: 0,0:04:03.24,0:04:03.74,main,,0000,0000,0000,,Oh.
Dialogue: 0,0:04:03.91,0:04:05.86,main,,0000,0000,0000,,Ty jsi taky přišla studovat, Izumi-san?
Dialogue: 0,0:04:05.87,0:04:07.70,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:04:08.66,0:04:09.58,main,,0000,0000,0000,,Nicméně…
Dialogue: 0,0:04:09.58,0:04:11.41,main,,0000,0000,0000,,Sotva jste se do toho dali.
Dialogue: 0,0:04:12.12,0:04:14.25,main,,0000,0000,0000,,Co jste dělali během volna?
Dialogue: 0,0:04:15.04,0:04:15.83,main,,0000,0000,0000,,Ehm…
Dialogue: 0,0:04:15.83,0:04:17.71,pinkblack,,0000,0000,0000,,{\frz-90\pos(320,115)}První den
Dialogue: 0,0:04:17.51,0:04:23.51,main,,0000,0000,0000,,Dvacet tři stránek matiky, dvacet stránek historie a třicet stránek angličtiny…
Dialogue: 0,0:04:20.13,0:04:23.71,main2,,0000,0000,0000,,Konečně jsem u Brány vítězství…
Dialogue: 0,0:04:24.33,0:04:26.71,main,,0000,0000,0000,,Máme pět dní volna…
Dialogue: 0,0:04:26.71,0:04:29.61,main,,0000,0000,0000,,Takže po pěti, ne po čtyřech hodinách denně…
Dialogue: 0,0:04:30.70,0:04:32.90,main,,0000,0000,0000,,To znamená 1,15 stránek matiky za hodinu.
Dialogue: 0,0:04:33.82,0:04:35.77,pinkblack,,0000,0000,0000,,{\frz-90\pos(320,115)}Druhý den
Dialogue: 0,0:04:34.89,0:04:37.17,main,,0000,0000,0000,,Dostávám mnohem víc expů, než jsem si myslela.
Dialogue: 0,0:04:35.94,0:04:38.25,main2,,0000,0000,0000,,Jejda. Dneska jsem se ještě neučila!
Dialogue: 0,0:04:38.46,0:04:41.85,main,,0000,0000,0000,,Zajímalo by mě kolik mi chybí do dalšího levelu?
Dialogue: 0,0:04:40.76,0:04:43.89,main2,,0000,0000,0000,,Mám ještě tři dny. Takže se musím učit 6 hodin denně?
Dialogue: 0,0:04:43.06,0:04:44.45,main,,0000,0000,0000,,Tohle je docela vzácné!
Dialogue: 0,0:04:44.49,0:04:46.32,pinkblack,,0000,0000,0000,,{\frz-90\pos(320,115)}Třetí den
Dialogue: 0,0:04:45.05,0:04:47.49,main,,0000,0000,0000,,Nikdo si nepamatuje jeho jméno…
Dialogue: 0,0:04:47.09,0:04:50.07,main2,,0000,0000,0000,,Už to začíná jít vážně pomalu.
Dialogue: 0,0:04:47.49,0:04:49.58,main,,0000,0000,0000,,a to deset let pomáhal na stavbě.
Dialogue: 0,0:04:49.58,0:04:50.10,main,,0000,0000,0000,,Je to Yutaka.
Dialogue: 0,0:04:50.07,0:04:53.64,main2,,0000,0000,0000,,Začíná jít vážně do tuhého! Takže tohle je vše co mám…
Dialogue: 0,0:04:51.00,0:04:53.54,main,,0000,0000,0000,,Už jen dva dny!
Dialogue: 0,0:04:53.64,0:04:55.00,main,,0000,0000,0000,,Musím si vážně pohnout…
Dialogue: 0,0:04:53.96,0:04:55.50,pinkblack,,0000,0000,0000,,{\frz-90\pos(320,115)}Čtvrtý den
Dialogue: 0,0:04:55.00,0:04:57.39,main,,0000,0000,0000,,Zlatý týden je už skoro u konce…
Dialogue: 0,0:04:57.39,0:04:59.86,main,,0000,0000,0000,,Už jste se podívali na vaše úkoly?
Dialogue: 0,0:05:00.94,0:05:03.80,main,,0000,0000,0000,,Co kdyby jsme se sešli a dodělali si úkoly?
Dialogue: 0,0:05:04.26,0:05:07.63,main,,0000,0000,0000,,Chtěla jsem se držet plánu…
Dialogue: 0,0:05:07.64,0:05:09.09,main,,0000,0000,0000,,Mise neúspěšná.
Dialogue: 0,0:05:09.10,0:05:10.68,main,,0000,0000,0000,,Lepší je si to ani neplánovat.
Dialogue: 0,0:05:10.68,0:05:11.76,main,,0000,0000,0000,,Ty jsi ještě horší!
Dialogue: 0,0:05:20.94,0:05:23.65,main,,0000,0000,0000,,Takže to nakonec udělala sama?
Dialogue: 0,0:05:24.44,0:05:27.91,main,,0000,0000,0000,,Myslím, že pořád hluboko uvnitř je dříč.
Dialogue: 0,0:05:31.16,0:05:33.95,main,,0000,0000,0000,,Ale její přesnost dost mizivá.
Dialogue: 0,0:05:33.95,0:05:34.66,main,,0000,0000,0000,,Patetické…
Dialogue: 0,0:05:34.66,0:05:35.79,main,,0000,0000,0000,,Prostě patetické!
Dialogue: 0,0:05:36.04,0:05:37.29,main,,0000,0000,0000,,Ahoj!
Dialogue: 0,0:05:37.29,0:05:39.25,main,,0000,0000,0000,,Tak co? Bavili jste se?
Dialogue: 0,0:05:39.25,0:05:40.33,main,,0000,0000,0000,,Tak znova…
Dialogue: 0,0:05:40.34,0:05:41.92,main,,0000,0000,0000,,Měli jste kupu úkolů…
Dialogue: 0,0:05:41.92,0:05:44.05,main,,0000,0000,0000,,…a test hned po prázdninách…
Dialogue: 0,0:05:44.05,0:05:46.88,main,,0000,0000,0000,,…takže jste asi moc času na zábavu neměli, co?
Dialogue: 0,0:05:47.26,0:05:49.93,main,,0000,0000,0000,,Sotva se to podobalo prázdninám.
Dialogue: 0,0:05:49.93,0:05:50.80,main,,0000,0000,0000,,To je smutné.
Dialogue: 0,0:05:50.80,0:05:52.76,main,,0000,0000,0000,,To já si krásně odpočinula!
Dialogue: 0,0:05:52.93,0:05:53.72,main,,0000,0000,0000,,Paní učitelko.
Dialogue: 0,0:05:54.27,0:05:56.43,main,,0000,0000,0000,,Já si taky odpočinula!
Dialogue: 0,0:05:56.43,0:05:56.68,main,,0000,0000,0000,,Hej.
Dialogue: 0,0:05:58.69,0:06:02.06,main,,0000,0000,0000,,Nejsou tyhle věci navržené, abys nevyhrála?
Dialogue: 0,0:06:02.82,0:06:04.11,main,,0000,0000,0000,,To záleží co chceš vytáhnout.
Dialogue: 0,0:06:04.11,0:06:06.40,main,,0000,0000,0000,,Některé věci je skoro nemožné vytáhnout.
Dialogue: 0,0:06:07.07,0:06:07.95,main,,0000,0000,0000,,Že?
Dialogue: 0,0:06:07.95,0:06:09.66,main,,0000,0000,0000,,Já bych se toho nikdy ani nedotkla.
Dialogue: 0,0:06:14.49,0:06:15.45,main,,0000,0000,0000,,Neuvěřitelné!
Dialogue: 0,0:06:15.45,0:06:16.16,main,,0000,0000,0000,,Nah.
Dialogue: 0,0:06:16.16,0:06:17.87,main,,0000,0000,0000,,Už jsem věděla, že se dá vytáhnout.
Dialogue: 0,0:06:19.04,0:06:19.87,main,,0000,0000,0000,,Takže možná…
Dialogue: 0,0:06:19.87,0:06:22.04,main,,0000,0000,0000,,…kdybych šla po ní…
Dialogue: 0,0:06:22.04,0:06:23.21,main,,0000,0000,0000,,Mohla bych ho vytáhnout.
Dialogue: 0,0:06:23.63,0:06:25.00,main,,0000,0000,0000,,Tak o tom ty přemýšlíš?
Dialogue: 0,0:06:25.63,0:06:27.38,main,,0000,0000,0000,,NE! To ne!
Dialogue: 0,0:06:28.00,0:06:31.18,main,,0000,0000,0000,,A-ale…
Dialogue: 0,0:06:31.18,0:06:32.39,main,,0000,0000,0000,,Jdi na to, ségra!
Dialogue: 0,0:06:37.52,0:06:39.10,main,,0000,0000,0000,,Myslela jsem si, že ho mám…
Dialogue: 0,0:06:39.10,0:06:40.02,main,,0000,0000,0000,,To je zlé…
Dialogue: 0,0:06:42.65,0:06:43.11,main,,0000,0000,0000,,Heh?
Dialogue: 0,0:06:48.36,0:06:48.82,main,,0000,0000,0000,,To ne!
Dialogue: 0,0:06:49.16,0:06:49.82,main,,0000,0000,0000,,Proč?!
Dialogue: 0,0:06:50.32,0:06:52.07,main,,0000,0000,0000,,Významný člověk kdysi řekl…
Dialogue: 0,0:06:52.07,0:06:54.53,main,,0000,0000,0000,,…že ovládat „chytače“ je jako si zakládat a spořit na účet.
Dialogue: 0,0:06:54.53,0:06:56.24,main,,0000,0000,0000,,Chytrý chlápek…
Dialogue: 0,0:06:59.53,0:07:00.90,main,,0000,0000,0000,,To ne! Zkazila jsem to!
Dialogue: 0,0:07:04.17,0:07:06.27,main,,0000,0000,0000,,Vůbec mi to nešlo…
Dialogue: 0,0:07:04.27,0:07:05.71,mainup,,0000,0000,0000,,A vaše score jsou!
Dialogue: 0,0:07:05.71,0:07:07.17,mainup,,0000,0000,0000,,Dosáhla jste úrovně Taiko 1!
Dialogue: 0,0:07:07.17,0:07:09.26,main,,0000,0000,0000,,Máš smysl pro rytmus Kona-chan.
Dialogue: 0,0:07:07.71,0:07:10.01,mainup,,0000,0000,0000,,Dosáhla jste úrovně Taiko 5!
Dialogue: 0,0:07:09.26,0:07:09.63,main,,0000,0000,0000,,Neh.
Dialogue: 0,0:07:09.63,0:07:11.38,main,,0000,0000,0000,,To nemá co dělat s rytmem.
Dialogue: 0,0:07:11.89,0:07:16.89,main,,0000,0000,0000,,Schopnosti co si odneseš s hraním bývají v normálním světě pěkně neužitečné.
Dialogue: 0,0:07:16.89,0:07:19.10,main,,0000,0000,0000,,Ty jsi toho živoucím důkazem.
Dialogue: 0,0:07:38.75,0:07:40.37,main,,0000,0000,0000,,Jsi silná.
Dialogue: 0,0:07:40.37,0:07:41.58,main,,0000,0000,0000,,To jo.
Dialogue: 0,0:07:41.58,0:07:44.21,main,,0000,0000,0000,,Jak často u toho vysedává?
Dialogue: 0,0:07:44.63,0:07:45.63,main,,0000,0000,0000,,Izumi?
Dialogue: 0,0:07:47.00,0:07:48.46,main,,0000,0000,0000,,Izumi dneska chybí?
Dialogue: 0,0:07:49.05,0:07:50.13,main,,0000,0000,0000,,Paní Kuroi.
Dialogue: 0,0:07:50.13,0:07:51.67,main,,0000,0000,0000,,Volá vám Izumi-san.
Dialogue: 0,0:07:52.30,0:07:52.80,main,,0000,0000,0000,,Ano.
Dialogue: 0,0:07:52.80,0:07:54.13,main,,0000,0000,0000,,To jsem já.
Dialogue: 0,0:07:54.64,0:07:56.10,main,,0000,0000,0000,,Paní učitelko, moc se omlouvám…
Dialogue: 0,0:07:56.10,0:07:58.18,main,,0000,0000,0000,,…ale dnes je mi zle…
Dialogue: 0,0:07:58.64,0:08:00.18,main,,0000,0000,0000,,To je neobvyklé, Izumi.
Dialogue: 0,0:08:00.18,0:08:00.72,main,,0000,0000,0000,,Nachladila jsi se?
Dialogue: 0,0:08:00.98,0:08:01.60,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:08:01.60,0:08:03.06,main,,0000,0000,0000,,Je to moc špatná květnová nemoc.
Dialogue: 0,0:08:03.52,0:08:05.48,main,,0000,0000,0000,,Sotva se můžu pohnout…
Dialogue: 0,0:08:05.48,0:08:06.73,main,,0000,0000,0000,,Prostě se sem dovleč!
Dialogue: 0,0:08:09.53,0:08:10.44,main,,0000,0000,0000,,Hej!
Dialogue: 0,0:08:10.44,0:08:11.28,main,,0000,0000,0000,,Odcházíme.
Dialogue: 0,0:08:13.99,0:08:16.62,main,,0000,0000,0000,,Jak to, že máš tak plnou kabelu?
Dialogue: 0,0:08:17.08,0:08:18.78,main,,0000,0000,0000,,Zítra jsou testy…
Dialogue: 0,0:08:18.79,0:08:21.91,main,,0000,0000,0000,,…takže si musím vzít vše domů na celo-noční učící maraton.
Dialogue: 0,0:08:21.91,0:08:22.91,main,,0000,0000,0000,,Neříkej mi…
Dialogue: 0,0:08:22.91,0:08:25.17,main,,0000,0000,0000,,…že se to chceš za noc všechno naučit?
Dialogue: 0,0:08:25.92,0:08:26.88,main,,0000,0000,0000,,Opravdu…
Dialogue: 0,0:08:26.88,0:08:29.92,main,,0000,0000,0000,,Čekat den před testem na odnesení knih domů…
Dialogue: 0,0:08:29.92,0:08:31.55,main,,0000,0000,0000,,je vážně pozdě!
Dialogue: 0,0:08:32.05,0:08:34.34,main,,0000,0000,0000,,Ale pokaždé to takhle zmáknu.
Dialogue: 0,0:08:34.68,0:08:36.47,main,,0000,0000,0000,,Zníš přesvědčivě.
Dialogue: 0,0:08:36.47,0:08:36.89,main,,0000,0000,0000,,No.
Dialogue: 0,0:08:36.89,0:08:39.14,main,,0000,0000,0000,,Šak se předvedeš.
Dialogue: 0,0:08:39.31,0:08:40.14,main,,0000,0000,0000,,První semestr.
Dialogue: 0,0:08:49.19,0:08:50.02,main,,0000,0000,0000,,Okaj!
Dialogue: 0,0:08:50.03,0:08:52.23,main,,0000,0000,0000,,Čas na vrácení testů!
Dialogue: 0,0:08:53.61,0:08:54.28,main,,0000,0000,0000,,Aah!
Dialogue: 0,0:08:54.70,0:08:55.65,main,,0000,0000,0000,,Slušné.
Dialogue: 0,0:08:56.11,0:08:56.99,main,,0000,0000,0000,,Jak sis vedla?
Dialogue: 0,0:08:57.45,0:08:59.16,main,,0000,0000,0000,,Raději se neptej…
Dialogue: 0,0:08:59.16,0:09:00.45,main,,0000,0000,0000,,Ale snažila jsem se…
Dialogue: 0,0:09:00.87,0:09:01.58,main,,0000,0000,0000,,No, teď…
Dialogue: 0,0:09:01.58,0:09:02.54,main,,0000,0000,0000,,Co slečna „jednunocpředtestem“?
Dialogue: 0,0:09:02.91,0:09:04.12,main,,0000,0000,0000,,V pohodě, díky za optání.
Dialogue: 0,0:09:04.12,0:09:05.12,main,,0000,0000,0000,,To je nefér!
Dialogue: 0,0:09:10.88,0:09:11.75,main,,0000,0000,0000,,Jsem hrozně unavená…
Dialogue: 0,0:09:11.76,0:09:13.76,main,,0000,0000,0000,,Nic se mi nechce dělat…
Dialogue: 0,0:09:13.76,0:09:14.38,main,,0000,0000,0000,,No tak.
Dialogue: 0,0:09:14.38,0:09:15.22,main,,0000,0000,0000,,Jdeme.
Dialogue: 0,0:09:17.26,0:09:20.22,main,,0000,0000,0000,,Ale stejně se tu s námi poflakuješ, co?
Dialogue: 0,0:09:21.06,0:09:22.14,main,,0000,0000,0000,,Počítačový festival!
Dialogue: 0,0:09:22.14,0:09:23.77,main,,0000,0000,0000,,Sleva na všechny QUO karty!
Dialogue: 0,0:09:23.77,0:09:24.90,main,,0000,0000,0000,,Musím je mít všechny!
Dialogue: 0,0:09:25.27,0:09:25.89,main,,0000,0000,0000,,Okaj.
Dialogue: 0,0:09:25.89,0:09:26.94,main,,0000,0000,0000,,Čas si najít práci.
Dialogue: 0,0:09:27.31,0:09:28.94,main,,0000,0000,0000,,Jsi tak průhledná.
Dialogue: 0,0:09:31.36,0:09:32.44,main,,0000,0000,0000,,Vážně…
Dialogue: 0,0:09:32.44,0:09:36.32,main,,0000,0000,0000,,Ještě jsem nikdy neslyšela o nikom kdo by zůstal doma kvůli květnové únavě.
Dialogue: 0,0:09:36.32,0:09:38.32,main,,0000,0000,0000,,Nicméně dokážu si představit, jak ji je.
Dialogue: 0,0:09:38.32,0:09:40.16,main,,0000,0000,0000,,Nemá to v názvu nemoc?
Dialogue: 0,0:09:40.16,0:09:42.03,main,,0000,0000,0000,,Nemělo by se to počítat jako vážná choroba?
Dialogue: 0,0:09:42.04,0:09:43.45,main,,0000,0000,0000,,To myslíš vážně?
Dialogue: 0,0:09:45.25,0:09:47.83,main,,0000,0000,0000,,Co je vlastně květnová nemoc?
Dialogue: 0,0:09:48.33,0:09:50.38,main,,0000,0000,0000,,Miyuki.
Dialogue: 0,0:09:50.71,0:09:51.88,main,,0000,0000,0000,,No takže…
Dialogue: 0,0:09:51.88,0:09:53.75,main,,0000,0000,0000,,Většinou se dostavuje po tom co dosáhneš nějakého cíle…
Dialogue: 0,0:09:53.76,0:09:57.05,main,,0000,0000,0000,,jako je například získání nové práce nebo zvládnutí přijímaček na vysokou.
Dialogue: 0,0:09:57.05,0:10:01.51,main,,0000,0000,0000,,Nové vztahy v dubnu taky nejdou zrovna hladce…
Dialogue: 0,0:10:01.51,0:10:04.10,main,,0000,0000,0000,,…nedokážeš příjmout rozdíl mezi…
Dialogue: 0,0:10:04.10,0:10:06.43,main,,0000,0000,0000,,…skutečným světem a tvou vysněnou realitou.
Dialogue: 0,0:10:06.43,0:10:09.27,main,,0000,0000,0000,,Takže se nakonec cítíš bezmocná a unavená.
Dialogue: 0,0:10:09.85,0:10:14.27,main,,0000,0000,0000,,Takže čistá otupělost by se neměla považovat za květnovou nemoc…
Dialogue: 0,0:10:14.28,0:10:16.82,main,,0000,0000,0000,,Krom toho to nemá co dělat s…
Dialogue: 0,0:10:16.82,0:10:18.70,main,,0000,0000,0000,,…druhým ročníkem na střední.
Dialogue: 0,0:10:18.70,0:10:22.70,main,,0000,0000,0000,,Já jsem na jaře vždycky taková pomalejší.
Dialogue: 0,0:10:22.70,0:10:23.78,main,,0000,0000,0000,,Oh, vážně.
Dialogue: 0,0:10:24.24,0:10:26.25,main,,0000,0000,0000,,Ale pořád na tom nejsem tak špatně jako Konata…
Dialogue: 0,0:10:26.25,0:10:28.37,main,,0000,0000,0000,,…ale cítím se neobvykle unavená.
Dialogue: 0,0:10:28.92,0:10:30.46,main,,0000,0000,0000,,Jakmile jsem doma…
Dialogue: 0,0:10:30.46,0:10:32.58,main,,0000,0000,0000,,…nemůžu se donutit dělat úkoly.
Dialogue: 0,0:10:32.59,0:10:33.88,main,,0000,0000,0000,,Já taky.
Dialogue: 0,0:10:37.09,0:10:39.09,main,,0000,0000,0000,,Nikdo to nebude komentovat?
Dialogue: 0,0:10:42.39,0:10:45.31,main,,0000,0000,0000,,Proč vzdycháš, Miyuki-chan?
Dialogue: 0,0:10:46.02,0:10:48.23,main,,0000,0000,0000,,Je mi trapně to v takovém věku říkat…
Dialogue: 0,0:10:48.23,0:10:50.69,main,,0000,0000,0000,,Stejně jako jsem ohledně kontaktní čoček…
Dialogue: 0,0:10:50.69,0:10:53.11,main,,0000,0000,0000,,…ale vyhýbám se zubaři, protože mě děsí…
Dialogue: 0,0:10:53.11,0:10:55.19,main,,0000,0000,0000,,…ale mám kaz.
Dialogue: 0,0:10:55.78,0:10:58.82,main,,0000,0000,0000,,Bolí to i když piji vodu.
Dialogue: 0,0:11:01.53,0:11:02.99,main,,0000,0000,0000,,Není to…
Dialogue: 0,0:11:02.99,0:11:04.45,main,,0000,0000,0000,,…normální?
Dialogue: 0,0:11:04.45,0:11:05.74,main,,0000,0000,0000,,Sama se dost bojím zubaře.
Dialogue: 0,0:11:06.24,0:11:08.20,main,,0000,0000,0000,,Ale proč se mu vyhýbáš…
Dialogue: 0,0:11:08.20,0:11:09.83,main,,0000,0000,0000,,…dokud tvůj zub začne bolet?
Dialogue: 0,0:11:15.55,0:11:16.88,main,,0000,0000,0000,,Je to zvláštní…
Dialogue: 0,0:11:16.88,0:11:19.88,main,,0000,0000,0000,,I když vím, že můj zub bude bolet…
Dialogue: 0,0:11:19.88,0:11:22.84,main,,0000,0000,0000,,pořád se mi nechce k zubaři.
Dialogue: 0,0:11:23.64,0:11:25.26,main,,0000,0000,0000,,Když to poprvé začalo bolet…
Dialogue: 0,0:11:25.26,0:11:27.31,main,,0000,0000,0000,,…řekla jsem si, že nepůjdu k zubaři…
Dialogue: 0,0:11:27.31,0:11:30.06,main,,0000,0000,0000,,…že budu raději žít s bolestí.
Dialogue: 0,0:11:30.48,0:11:32.48,main,,0000,0000,0000,,To je zubař vážně tak děsivý?
Dialogue: 0,0:11:32.48,0:11:34.27,main,,0000,0000,0000,,Nikdy jsem tam nemusela, takže nevím.
Dialogue: 0,0:11:34.77,0:11:36.27,main,,0000,0000,0000,,Tys nikdy nešla k zubaři?
Dialogue: 0,0:11:36.27,0:11:37.98,main,,0000,0000,0000,,To máš štěstí…
Dialogue: 0,0:11:37.98,0:11:40.65,main,,0000,0000,0000,,Je tam ten zápach nemocnice…
Dialogue: 0,0:11:40.65,0:11:42.45,main,,0000,0000,0000,,Nebo nervy drásající čekání.
Dialogue: 0,0:11:43.07,0:11:43.86,main,,0000,0000,0000,,Ooh! Ooh!
Dialogue: 0,0:11:43.87,0:11:46.20,main,,0000,0000,0000,,A zvuk vrtačky mě pokaždé vyděsí!
Dialogue: 0,0:11:46.20,0:11:46.91,main,,0000,0000,0000,,Vrták!
Dialogue: 0,0:11:47.24,0:11:48.29,main,,0000,0000,0000,,Je to děsivé!
Dialogue: 0,0:11:48.95,0:11:51.66,main,,0000,0000,0000,,Ale kluci tam nejspíš chodí rádi, co?
Dialogue: 0,0:11:52.25,0:11:52.67,main,,0000,0000,0000,,Proč?
Dialogue: 0,0:11:52.67,0:11:54.83,main,,0000,0000,0000,,Kluci přece mají rádi vrtačky ne?
Dialogue: 0,0:11:55.29,0:11:56.29,main,,0000,0000,0000,,Dinosauří království!
Dialogue: 0,0:11:56.67,0:12:00.96,main,,0000,0000,0000,,Slyšela jsem, že mají rádi když roboti bojují s vrtáky!
Dialogue: 0,0:12:03.59,0:12:06.85,main,,0000,0000,0000,,Ale nejspíš se jim nelíbí, když jim někdo vrtá zuby…
Dialogue: 0,0:12:07.35,0:12:09.06,main,,0000,0000,0000,,Zajímalo by mě…
Dialogue: 0,0:12:09.06,0:12:11.73,main,,0000,0000,0000,,Nejsou recepční a asistentky…
Dialogue: 0,0:12:11.73,0:12:12.85,main,,0000,0000,0000,,…obvykle pěkné slečny?
Dialogue: 0,0:12:13.27,0:12:15.27,main,,0000,0000,0000,,Když si to teď zmínila, tak mi to tak přijde…
Dialogue: 0,0:12:15.84,0:12:16.86,main,,0000,0000,0000,,Zajímalo by mě proč…
Dialogue: 0,0:12:17.11,0:12:19.86,main,,0000,0000,0000,,Možná je to jedno z jejich kritérií při pohovoru…
Dialogue: 0,0:12:20.32,0:12:22.65,main,,0000,0000,0000,,Exkluzivně krásné asistentky a vrtačky, heh?
Dialogue: 0,0:12:30.25,0:12:33.83,main,,0000,0000,0000,,To vážně zní jako něco co mají chlapi rádi.
Dialogue: 0,0:12:33.83,0:12:34.33,main,,0000,0000,0000,,Tak potom…
Dialogue: 0,0:12:34.33,0:12:36.92,main,,0000,0000,0000,,Jak by měla vypadat ošetřovna pro ženy?
Dialogue: 0,0:12:37.54,0:12:40.30,main,,0000,0000,0000,,Zaprvé se musí zbavit toho zápachu.
Dialogue: 0,0:12:40.71,0:12:45.43,main,,0000,0000,0000,,A místo židlí mít měkké masážní křesla.
Dialogue: 0,0:12:45.89,0:12:48.51,main,,0000,0000,0000,,A taky by měli začlenit aromaterapii, že?
Dialogue: 0,0:12:48.51,0:12:51.35,main,,0000,0000,0000,,A přidat masáž nohou, zatím co vás ošetřují.
Dialogue: 0,0:12:53.31,0:12:54.44,main,,0000,0000,0000,,To zní dobře.
Dialogue: 0,0:12:54.44,0:12:58.65,main,,0000,0000,0000,,A k utišení bolesti by tě měli nechat hrát hry.
Dialogue: 0,0:12:58.65,0:13:00.73,main,,0000,0000,0000,,Mohlo by být trochu obtížné zároveň hrát hry…
Dialogue: 0,0:13:00.73,0:13:03.82,main,,0000,0000,0000,,Ale je to dobrý způsob jak tě odpoutat od bolesti.
Dialogue: 0,0:13:03.82,0:13:06.95,main,,0000,0000,0000,,Mohli by ti dát sluchátka a pustit ti, oblíbenou písničku.
Dialogue: 0,0:13:07.53,0:13:08.24,main,,0000,0000,0000,,To zní dobře.
Dialogue: 0,0:13:08.24,0:13:09.58,main,,0000,0000,0000,,To by bylo taky fajn.
Dialogue: 0,0:13:14.41,0:13:17.46,main,,0000,0000,0000,,Ale pořád se k zubaři necítím.
Dialogue: 0,0:13:17.46,0:13:18.50,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:13:18.96,0:13:19.54,main,,0000,0000,0000,,Co je tohle?!
Dialogue: 0,0:13:20.05,0:13:21.05,main,,0000,0000,0000,,Co se děje?
Dialogue: 0,0:13:21.05,0:13:21.59,main,,0000,0000,0000,,No…
Dialogue: 0,0:13:21.59,0:13:24.30,main,,0000,0000,0000,,Animují další sérii co mám ráda…
Dialogue: 0,0:13:24.30,0:13:27.84,main,,0000,0000,0000,,…ale používají naprosto jiné obsazení z drama CD.
Dialogue: 0,0:13:28.68,0:13:29.89,main,,0000,0000,0000,,Nechápu.
Dialogue: 0,0:13:30.97,0:13:33.64,main,,0000,0000,0000,,Bylo to dílo vyšší síly?
Dialogue: 0,0:13:33.68,0:13:34.31,main,,0000,0000,0000,,Osobní problémy?
Dialogue: 0,0:13:34.31,0:13:34.73,main,,0000,0000,0000,,Peníze?
Dialogue: 0,0:13:35.19,0:13:38.02,main,,0000,0000,0000,,Nikdy si nekupuji drama CD…
Dialogue: 0,0:13:38.02,0:13:39.65,main,,0000,0000,0000,,…takže dokud hlasy sedí k původním postavám…
Dialogue: 0,0:13:39.65,0:13:41.82,main,,0000,0000,0000,,…nezajímá mě koho použijí.
Dialogue: 0,0:13:41.82,0:13:44.11,main,,0000,0000,0000,,Tak to nefunguje, Kagami.
Dialogue: 0,0:13:44.11,0:13:46.74,main,,0000,0000,0000,,Lidé co si na drama CD zvykli…
Dialogue: 0,0:13:46.74,0:13:49.78,main,,0000,0000,0000,,…cítí, že něco není v pořádku, když se začnou dívat na anime.
Dialogue: 0,0:13:49.78,0:13:52.70,main,,0000,0000,0000,,A chce to nějakou dobu, než si zvykneš.
Dialogue: 0,0:13:54.25,0:13:55.08,main,,0000,0000,0000,,No…
Dialogue: 0,0:13:55.08,0:13:57.12,main,,0000,0000,0000,,…jedna dobrá věc z toho ale plyne.
Dialogue: 0,0:13:57.42,0:13:58.42,main,,0000,0000,0000,,Co je to?
Dialogue: 0,0:13:58.42,0:14:00.00,main,,0000,0000,0000,,Drama CD a anime.
Dialogue: 0,0:14:00.00,0:14:02.80,main,,0000,0000,0000,,Můžeš si skrz ně vychutnat dva odlišné světy.
Dialogue: 0,0:14:02.80,0:14:04.21,main,,0000,0000,0000,,Dva za cenu jedné.
Dialogue: 0,0:14:04.76,0:14:06.63,main,,0000,0000,0000,,Ty jsi hardcore fanda, že?
Dialogue: 0,0:14:07.43,0:14:08.59,main,,0000,0000,0000,,Mockrát děkuji!
Dialogue: 0,0:14:08.93,0:14:09.43,main,,0000,0000,0000,,No…
Dialogue: 0,0:14:09.43,0:14:14.39,main,,0000,0000,0000,,Jeden z nás si musí dát Miyukin nuget…
Dialogue: 0,0:14:14.93,0:14:17.56,main,,0000,0000,0000,,Řekněme, že půjde tomu kdo vyhraje „čumící“ soutež.
Dialogue: 0,0:14:17.56,0:14:18.14,main,,0000,0000,0000,,C-
Dialogue: 0,0:14:18.14,0:14:19.40,main,,0000,0000,0000,,Jsi jako malá.
Dialogue: 0,0:14:19.40,0:14:20.65,main,,0000,0000,0000,,Čumící soutěž?
Dialogue: 0,0:14:20.65,0:14:21.81,main,,0000,0000,0000,,To zní zábavně!
Dialogue: 0,0:14:21.82,0:14:23.77,main,,0000,0000,0000,,Kagami nejspíš prohraje.
Dialogue: 0,0:14:23.78,0:14:24.61,main,,0000,0000,0000,,Řekla jsi něco?
Dialogue: 0,0:14:24.74,0:14:26.88,main,,0000,0000,0000,,Tak jo… začínáme!
Dialogue: 0,0:14:34.00,0:14:36.36,main,,0000,0000,0000,,To bylo… wow.
Dialogue: 0,0:14:37.95,0:14:40.87,main,,0000,0000,0000,,Ani nevypadáš jako člověk!
Dialogue: 0,0:14:40.28,0:14:43.04,main2,,0000,0000,0000,,Hej, to trochu přeháníte.
Dialogue: 0,0:14:43.04,0:14:44.67,main,,0000,0000,0000,,Promiňte, že jste museli čekat.
Dialogue: 0,0:14:44.67,0:14:46.21,main,,0000,0000,0000,,Na toaletách byla dlouhá fronta.
Dialogue: 0,0:14:46.88,0:14:48.05,main,,0000,0000,0000,,Stalo se něco?
Dialogue: 0,0:14:48.12,0:14:51.35,main,,0000,0000,0000,,Hej. Ukaž ten obličej Miyuki!
Dialogue: 0,0:14:51.35,0:14:52.62,main,,0000,0000,0000,,To ne.
Dialogue: 0,0:14:52.62,0:14:56.82,main,,0000,0000,0000,,To bylo neuvěřitelné. Nikdy jsem něco takového neviděla.
Dialogue: 0,0:14:57.31,0:14:57.89,main,,0000,0000,0000,,Tak fajn.
Dialogue: 0,0:15:03.06,0:15:04.69,main,,0000,0000,0000,,Je to skvělé pokaždé to vidím.
Dialogue: 0,0:15:05.28,0:15:09.15,main,,0000,0000,0000,,Tohle se ti může hodit u talentových zkoušek na příjmačkách.
Dialogue: 0,0:15:09.61,0:15:10.74,main,,0000,0000,0000,,To je ono!
Dialogue: 0,0:15:10.99,0:15:12.41,main,,0000,0000,0000,,Dostane tě to na vysokou!
Dialogue: 0,0:15:12.41,0:15:13.62,main,,0000,0000,0000,,Zaručeno!
Dialogue: 0,0:15:14.37,0:15:16.20,main,,0000,0000,0000,,Moc mě to nevzrušuje.
Dialogue: 0,0:15:16.75,0:15:20.91,main,,0000,0000,0000,,V televizi dávali ten pořad o tom kolik se toho za rok průměrně přečte…
Dialogue: 0,0:15:20.92,0:15:25.29,main,,0000,0000,0000,,…no a oni vynechali mangu a počítali jen normální knihy.
Dialogue: 0,0:15:25.30,0:15:26.63,main,,0000,0000,0000,,No, manga…
Dialogue: 0,0:15:26.63,0:15:28.59,main,,0000,0000,0000,,Počítají se light novely?
Dialogue: 0,0:15:29.55,0:15:30.76,main,,0000,0000,0000,,Nejsem si jistá.
Dialogue: 0,0:15:30.76,0:15:32.72,main,,0000,0000,0000,,Myslím, že by měli být v pořádku…
Dialogue: 0,0:15:33.30,0:15:34.64,main,,0000,0000,0000,,A co obrázkové knihy?
Dialogue: 0,0:15:36.85,0:15:39.14,main,,0000,0000,0000,,No říká se jim knihy, takže asi jo…
Dialogue: 0,0:15:39.64,0:15:45.40,main,,0000,0000,0000,,Ostatní fantazi jako Harry Potter je považovaný za normální…
Dialogue: 0,0:15:46.07,0:15:48.69,main,,0000,0000,0000,,Takže co vlastně nazveš normální knihou?
Dialogue: 0,0:15:51.20,0:15:53.53,main,,0000,0000,0000,,A ta show skončila tím, že…
Dialogue: 0,0:15:53.53,0:15:57.62,main,,0000,0000,0000,,…prý mladí lidé stále méně čtou…
Dialogue: 0,0:15:58.04,0:16:01.41,main,,0000,0000,0000,,…ale neprospívá ti vlastně…
Dialogue: 0,0:16:01.41,0:16:05.58,main,,0000,0000,0000,,…procházení webu a blogování?
Dialogue: 0,0:16:06.92,0:16:09.13,main,,0000,0000,0000,,Chápu to… a taky ne…
Dialogue: 0,0:16:09.63,0:16:10.13,main,,0000,0000,0000,,Hoj.
Dialogue: 0,0:16:10.13,0:16:11.42,main,,0000,0000,0000,,Tak vyrazíme.
Dialogue: 0,0:16:11.42,0:16:12.34,main,,0000,0000,0000,,Počkej minutku.
Dialogue: 0,0:16:13.22,0:16:13.84,main,,0000,0000,0000,,Promiň.
Dialogue: 0,0:16:13.84,0:16:15.55,main,,0000,0000,0000,,Nemůžu s vámi.
Dialogue: 0,0:16:16.30,0:16:17.85,main,,0000,0000,0000,,Máš něco na práci?
Dialogue: 0,0:16:17.85,0:16:18.39,main,,0000,0000,0000,,To je neobvyklé.
Dialogue: 0,0:16:19.47,0:16:21.31,main,,0000,0000,0000,,Začala jsem pracovat.
Dialogue: 0,0:16:24.23,0:16:25.10,main,,0000,0000,0000,,Počkat!
Dialogue: 0,0:16:25.11,0:16:26.65,main,,0000,0000,0000,,Ona má skutečně práci!
Dialogue: 0,0:16:26.65,0:16:28.52,main,,0000,0000,0000,,To je počítačový festival vážně tak důležitý?!
Dialogue: 0,0:16:30.82,0:16:31.86,main,,0000,0000,0000,,Nicméně…
Dialogue: 0,0:16:31.86,0:16:33.86,main,,0000,0000,0000,,Konata mě překvapila.
Dialogue: 0,0:16:33.86,0:16:35.16,main,,0000,0000,0000,,To teda jo.
Dialogue: 0,0:16:35.16,0:16:37.45,main,,0000,0000,0000,,Skutečně má práci.
Dialogue: 0,0:16:37.45,0:16:38.95,main,,0000,0000,0000,,Myslela jsem si, že si dělá jen srandu.
Dialogue: 0,0:16:39.95,0:16:41.45,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:16:42.16,0:16:45.33,main,,0000,0000,0000,,Ale zajímalo by mě, jak ji někdo vzal.
Dialogue: 0,0:16:45.42,0:16:47.29,main,,0000,0000,0000,,T-to je docela hrubé…
Dialogue: 0,0:16:47.38,0:16:48.09,main,,0000,0000,0000,,Krom toho…
Dialogue: 0,0:16:48.09,0:16:51.55,main,,0000,0000,0000,,Neumím si ji představit dělat manuální práci.
Dialogue: 0,0:16:51.55,0:16:52.80,main,,0000,0000,0000,,T-to je také pravda…
Dialogue: 0,0:16:53.30,0:16:55.13,main,,0000,0000,0000,,Zajímalo by mne, kde pracuje.
Dialogue: 0,0:16:55.64,0:16:57.19,main,,0000,0000,0000,,Možná, že v knihkupectví?
Dialogue: 0,0:16:57.19,0:16:58.35,main,,0000,0000,0000,,Má přece ráda mangu.
Dialogue: 0,0:16:59.72,0:17:00.47,main,,0000,0000,0000,,Promiňte.
Dialogue: 0,0:17:00.47,0:17:00.93,main,,0000,0000,0000,,Vzal bych si tohle.
Dialogue: 0,0:17:00.93,0:17:01.97,main,,0000,0000,0000,,Zadržte.
Dialogue: 0,0:17:01.98,0:17:03.77,main,,0000,0000,0000,,Mám teď moc práce.
Dialogue: 0,0:17:06.38,0:17:07.37,main,,0000,0000,0000,,Ka-ching!
Dialogue: 0,0:17:07.69,0:17:09.52,main,,0000,0000,0000,,Doufám, že ji nevyhodí…
Dialogue: 0,0:17:10.03,0:17:11.19,main,,0000,0000,0000,,Tak co třeba v samoobsluze?
Dialogue: 0,0:17:12.24,0:17:12.90,main,,0000,0000,0000,,Promiňte.
Dialogue: 0,0:17:12.90,0:17:13.44,main,,0000,0000,0000,,Vzal bych si tohle.
Dialogue: 0,0:17:14.57,0:17:16.53,main,,0000,0000,0000,,Čtyřistadevadesátosumyenů.
Dialogue: 0,0:17:18.03,0:17:19.53,main,,0000,0000,0000,,Amátetopřesně.
Dialogue: 0,0:17:21.29,0:17:22.24,main,,0000,0000,0000,,Děkujemepříjďtezase.
Dialogue: 0,0:17:23.46,0:17:24.62,main,,0000,0000,0000,,Pochybuji.
Dialogue: 0,0:17:24.62,0:17:25.87,main,,0000,0000,0000,,Tak co třeba fast food?
Dialogue: 0,0:17:26.29,0:17:27.96,main,,0000,0000,0000,,Směji se. Je s tím problém?
Dialogue: 0,0:17:27.54,0:17:28.92,main2,,0000,0000,0000,,To asi taky nee…
Dialogue: 0,0:17:28.92,0:17:30.84,main,,0000,0000,0000,,Takže rodinné restaurace asi taky padají.
Dialogue: 0,0:17:31.00,0:17:31.84,main,,0000,0000,0000,,Vítejte.
Dialogue: 0,0:17:32.59,0:17:34.67,main,,0000,0000,0000,,Zajímalo by mne, kde pracuje?
Dialogue: 0,0:17:35.88,0:17:37.43,main,,0000,0000,0000,,Dopravní průzkum?
Dialogue: 0,0:17:37.43,0:17:38.34,main,,0000,0000,0000,,To ne.
Dialogue: 0,0:17:38.35,0:17:39.60,main,,0000,0000,0000,,Videopůjčovna?
Dialogue: 0,0:17:39.60,0:17:40.76,main,,0000,0000,0000,,Tím si nejsem jistá.
Dialogue: 0,0:17:40.76,0:17:41.56,main,,0000,0000,0000,,Bar.
Dialogue: 0,0:17:41.56,0:17:42.64,main,,0000,0000,0000,,Neh.
Dialogue: 0,0:17:42.64,0:17:43.52,main,,0000,0000,0000,,Donáška pizzy?
Dialogue: 0,0:17:43.52,0:17:44.64,main,,0000,0000,0000,,Nemá řidičák.
Dialogue: 0,0:17:44.64,0:17:45.89,main,,0000,0000,0000,,Pokladní v supermarketu.
Dialogue: 0,0:17:45.89,0:17:46.39,main,,0000,0000,0000,,Nemožné.
Dialogue: 0,0:17:46.39,0:17:47.31,main,,0000,0000,0000,,Doučování?
Dialogue: 0,0:17:47.31,0:17:48.15,main,,0000,0000,0000,,Nemyslitelné.
Dialogue: 0,0:17:48.15,0:17:49.02,main,,0000,0000,0000,,Pekárna!
Dialogue: 0,0:17:49.61,0:17:50.44,main,,0000,0000,0000,,Takže zůstává…
Dialogue: 0,0:17:52.28,0:17:53.61,main,,0000,0000,0000,,Zeptáme se jí zítra.
Dialogue: 0,0:17:54.19,0:17:55.19,main,,0000,0000,0000,,Hej, Konato.
Dialogue: 0,0:17:55.20,0:17:56.99,main,,0000,0000,0000,,Kde pracuješ?
Dialogue: 0,0:17:57.45,0:17:58.32,main,,0000,0000,0000,,Řekni nám!
Dialogue: 0,0:18:00.53,0:18:02.37,main,,0000,0000,0000,,Normální costplay kavárna.
Dialogue: 0,0:18:03.16,0:18:04.95,main,,0000,0000,0000,,To k ní sedí perfektně!
Dialogue: 0,0:18:04.95,0:18:06.08,main,,0000,0000,0000,,Ale co tam dělá?
Dialogue: 0,0:18:06.08,0:18:09.12,copyrights,,0000,0000,0000,,{\pos(129,76)}Překlad od paralaXe
Dialogue: 0,0:18:06.08,0:18:09.12,copyrights,,0000,0000,0000,,{\pos(85,50)}Domovská stránka: www.otakogu.cz
Dialogue: 0,0:18:12.00,0:18:13.84,main,,0000,0000,0000,,Hodně štěstí!
Dialogue: 0,0:18:13.84,0:18:15.17,main,,0000,0000,0000,,Já jsem Akira Kogami!
Dialogue: 0,0:18:15.17,0:18:17.59,main,,0000,0000,0000,,A tohle je druhá epizoda Lucky Channelu!
Dialogue: 0,0:18:17.59,0:18:20.05,main,,0000,0000,0000,,Akira jde na to!
Dialogue: 0,0:18:20.51,0:18:21.43,main,,0000,0000,0000,,O-kaj!
Dialogue: 0,0:18:21.43,0:18:22.55,main,,0000,0000,0000,,Vedle mě…
Dialogue: 0,0:18:22.56,0:18:25.31,main,,0000,0000,0000,,…máme mého báječného asistenta!
Dialogue: 0,0:18:25.31,0:18:27.14,main,,0000,0000,0000,,Minoru Shiraishi-san!
Dialogue: 0,0:18:27.14,0:18:28.60,main,,0000,0000,0000,,Zamávejte mu!
Dialogue: 0,0:18:29.06,0:18:30.15,main,,0000,0000,0000,,Eh-eh-eh-eh-eh-
Dialogue: 0,0:18:30.48,0:18:33.27,main,,0000,0000,0000,,Já jsem Akira-samin asistent, Minoru Shiraishi.
Dialogue: 0,0:18:33.27,0:18:34.90,main,,0000,0000,0000,,Jsem teprve začátečník…
Dialogue: 0,0:18:34.90,0:18:37.28,main,,0000,0000,0000,,…takže doufám, že si zapamatujete moje jméno.
Dialogue: 0,0:18:37.28,0:18:37.74,main,,0000,0000,0000,,Ano.
Dialogue: 0,0:18:37.95,0:18:39.15,main,,0000,0000,0000,,Tlesk. Tlesk. Tlesk.
Dialogue: 0,0:18:40.37,0:18:42.74,main,,0000,0000,0000,,Jsme teprve na začátku…
Dialogue: 0,0:18:42.74,0:18:46.91,main,,0000,0000,0000,,ale už teď máme záplavu dopisů!
Dialogue: 0,0:18:46.91,0:18:48.83,main,,0000,0000,0000,,Akira je moc šťastná!
Dialogue: 0,0:18:48.83,0:18:49.50,main,,0000,0000,0000,,Díky!
Dialogue: 0,0:18:49.79,0:18:50.25,main,,0000,0000,0000,,Páni,
Dialogue: 0,0:18:50.25,0:18:52.17,main,,0000,0000,0000,,to je vážně něco.
Dialogue: 0,0:18:52.17,0:18:52.96,main,,0000,0000,0000,,Myslím, že bys tomu mohla říkat…
Dialogue: 0,0:18:52.96,0:18:54.84,main,,0000,0000,0000,,…ukazatel Akiriné popularity.
Dialogue: 0,0:18:56.34,0:18:57.26,main,,0000,0000,0000,,Co-
Dialogue: 0,0:18:57.26,0:18:58.22,main,,0000,0000,0000,,Cože?
Dialogue: 0,0:18:58.22,0:18:59.55,main,,0000,0000,0000,,Viděla jsem dnešní epizodu.
Dialogue: 0,0:18:59.55,0:19:00.18,main,,0000,0000,0000,,Ano?
Dialogue: 0,0:19:00.18,0:19:01.01,main,,0000,0000,0000,,Ukázal ses tam?
Dialogue: 0,0:19:01.51,0:19:03.05,main,,0000,0000,0000,,Ve skutečné show.
Dialogue: 0,0:19:03.05,0:19:04.35,main,,0000,0000,0000,,Byls tam, že jo?
Dialogue: 0,0:19:05.47,0:19:06.68,main,,0000,0000,0000,,Byli tak velkorysí a dali mě tam.
Dialogue: 0,0:19:08.64,0:19:10.44,main,,0000,0000,0000,,Je mi to fuk.
Dialogue: 0,0:19:10.44,0:19:11.85,main,,0000,0000,0000,,Ah, jo…
Dialogue: 0,0:19:11.86,0:19:13.90,main,,0000,0000,0000,,Kde, žes to byl?
Dialogue: 0,0:19:14.44,0:19:16.11,main,,0000,0000,0000,,No, myslím, že…
Dialogue: 0,0:19:16.11,0:19:17.78,main,,0000,0000,0000,,…scéna 45, část A.
Dialogue: 0,0:19:17.78,0:19:19.28,main,,0000,0000,0000,,Scéna ve třídě, hned po přestávce.
Dialogue: 0,0:19:21.16,0:19:22.36,main,,0000,0000,0000,,Jo už vím.
Dialogue: 0,0:19:22.37,0:19:24.87,main,,0000,0000,0000,,Moc si nebyl vidět, takže jsem zapomněla.
Dialogue: 0,0:19:25.62,0:19:28.00,main,,0000,0000,0000,,No, tentokrát jsem měl jen jeden řádek.
Dialogue: 0,0:19:28.29,0:19:29.33,main,,0000,0000,0000,,Cožeto?!
Dialogue: 0,0:19:29.33,0:19:30.54,main,,0000,0000,0000,,To ti nestačí?!
Dialogue: 0,0:19:30.54,0:19:31.75,main,,0000,0000,0000,,Ne! Tak jsem to nemyslel!
Dialogue: 0,0:19:32.63,0:19:34.96,main,,0000,0000,0000,,Nejsem si jistá, jestli by se měl nováček jako ty chovat takhle.
Dialogue: 0,0:19:35.59,0:19:38.34,main,,0000,0000,0000,,Když jsem já začínala…
Dialogue: 0,0:19:39.59,0:19:40.92,main,,0000,0000,0000,,Začínala?
Dialogue: 0,0:19:40.93,0:19:42.22,main,,0000,0000,0000,,Kolik ti bylo?
Dialogue: 0,0:19:42.89,0:19:43.89,main,,0000,0000,0000,,Tři.
Dialogue: 0,0:19:43.89,0:19:47.18,main,,0000,0000,0000,,Byla jsem tehdy ráda, že mám vůbec něco.
Dialogue: 0,0:19:47.18,0:19:49.18,main,,0000,0000,0000,,Kvantita není problém.
Dialogue: 0,0:19:49.18,0:19:50.27,main,,0000,0000,0000,,To máš naprostou pravdu.
Dialogue: 0,0:19:50.27,0:19:53.56,main,,0000,0000,0000,,Moje nervy byly nadračky, po tom co kolem mě bylo tolik veteránů.
Dialogue: 0,0:19:53.56,0:19:54.98,main,,0000,0000,0000,,Bylo to moc poučné!
Dialogue: 0,0:19:54.98,0:19:56.44,main,,0000,0000,0000,,Byl jsi pěkně ztuhlý.
Dialogue: 0,0:19:57.28,0:19:58.53,main,,0000,0000,0000,,Vážně moc.
Dialogue: 0,0:19:58.53,0:19:59.90,main,,0000,0000,0000,,Ooh, podívej! Já jsem amatér.
Dialogue: 0,0:20:00.40,0:20:01.49,main,,0000,0000,0000,,Šak víš.
Dialogue: 0,0:20:01.49,0:20:03.66,main,,0000,0000,0000,,To bylo zajímavé…
Dialogue: 0,0:20:03.66,0:20:05.41,main,,0000,0000,0000,,udělej to znovu.
Dialogue: 0,0:20:06.62,0:20:07.62,main,,0000,0000,0000,,Tady?
Dialogue: 0,0:20:07.62,0:20:10.20,main,,0000,0000,0000,,Je tu šance, že si zahraješ zase, co?
Dialogue: 0,0:20:10.87,0:20:11.46,main,,0000,0000,0000,,No, myslím…
Dialogue: 0,0:20:11.46,0:20:13.75,main,,0000,0000,0000,,Tak bys měl raději začít cvičit.
Dialogue: 0,0:20:13.75,0:20:15.29,main,,0000,0000,0000,,Nabízím ti tady pomoc!
Dialogue: 0,0:20:15.96,0:20:16.79,main,,0000,0000,0000,,Ano, madam!
Dialogue: 0,0:20:16.80,0:20:18.42,main,,0000,0000,0000,,Prosím učte mě!
Dialogue: 0,0:20:18.42,0:20:19.30,main,,0000,0000,0000,,Dej mi ty skripta.
Dialogue: 0,0:20:20.63,0:20:22.22,main,,0000,0000,0000,,Jaká, že to byla scéna?
Dialogue: 0,0:20:22.22,0:20:23.43,main,,0000,0000,0000,,Scéna 45.
Dialogue: 0,0:20:24.89,0:20:25.39,main,,0000,0000,0000,,Tahle?
Dialogue: 0,0:20:25.68,0:20:27.93,main,,0000,0000,0000,,Tak já budu slečna Kuroi.
Dialogue: 0,0:20:27.93,0:20:28.89,main,,0000,0000,0000,,Ano, prosím.
Dialogue: 0,0:20:28.89,0:20:30.06,main,,0000,0000,0000,,Připravit a…
Dialogue: 0,0:20:30.52,0:20:31.10,main,,0000,0000,0000,,Akce!
Dialogue: 0,0:20:31.89,0:20:32.73,main,,0000,0000,0000,,Ahoj.
Dialogue: 0,0:20:32.73,0:20:35.15,main,,0000,0000,0000,,Užili jste si prázdnin?
Dialogue: 0,0:20:35.15,0:20:36.19,main,,0000,0000,0000,,Tak jo…
Dialogue: 0,0:20:36.19,0:20:38.69,main,,0000,0000,0000,,Měli jste s-s-
Dialogue: 0,0:20:37.93,0:20:38.69,main2,,0000,0000,0000,,Spoustu.
Dialogue: 0,0:20:38.69,0:20:39.82,main,,0000,0000,0000,,Spoustu domácích úkolů…
Dialogue: 0,0:20:39.82,0:20:42.03,main,,0000,0000,0000,,…a test hned po prázdninách…
Dialogue: 0,0:20:42.03,0:20:45.36,main,,0000,0000,0000,,…takže jste si nejspíš moc srandy neužili.
Dialogue: 0,0:20:45.37,0:20:47.78,main,,0000,0000,0000,,Sotva se to podobalo prázdninám.
Dialogue: 0,0:20:51.00,0:20:51.54,main,,0000,0000,0000,,Co to bylo?
Dialogue: 0,0:20:52.71,0:20:53.71,main,,0000,0000,0000,,Špatné.
Dialogue: 0,0:20:53.71,0:20:56.13,main,,0000,0000,0000,,Vůbec to nebylo procítěné.
Dialogue: 0,0:20:56.13,0:20:56.63,main,,0000,0000,0000,,Cítit-
Dialogue: 0,0:20:56.63,0:20:57.21,main,,0000,0000,0000,,Tady!
Dialogue: 0,0:20:57.21,0:20:57.67,main,,0000,0000,0000,,Právě tady!
Dialogue: 0,0:20:57.67,0:20:59.55,main,,0000,0000,0000,,Hned teď to procítíš!
Dialogue: 0,0:20:59.55,0:21:00.30,main,,0000,0000,0000,,Omlouvám se.
Dialogue: 0,0:21:00.30,0:21:02.97,main,,0000,0000,0000,,Prostě jen číst řádky nestačí… i opice to dokáže.
Dialogue: 0,0:21:03.43,0:21:04.93,main,,0000,0000,0000,,Opice neumí…
Dialogue: 0,0:21:08.60,0:21:10.68,main,,0000,0000,0000,,Je čas se rozloučit!
Dialogue: 0,0:21:10.68,0:21:12.14,main,,0000,0000,0000,,Ten čas ale letí…
Dialogue: 0,0:21:12.14,0:21:14.48,main,,0000,0000,0000,,…když se dobře bavíte, že?
Dialogue: 0,0:21:14.48,0:21:15.48,main,,0000,0000,0000,,Takže…
Dialogue: 0,0:21:15.48,0:21:20.69,main,,0000,0000,0000,,Posílejte nám vaše komentáře a návrhy!
Dialogue: 0,0:21:21.03,0:21:22.90,main,,0000,0000,0000,,Uvidíme se příště.
Dialogue: 0,0:21:22.94,0:21:24.90,main,,0000,0000,0000,,Ahoj-ahoj!
Dialogue: 0,0:21:25.91,0:21:26.70,main,,0000,0000,0000,,Tak jo.
Dialogue: 0,0:21:26.70,0:21:28.28,main,,0000,0000,0000,,Jaké to bylo?
Dialogue: 0,0:21:28.28,0:21:29.78,main,,0000,0000,0000,,Co myslíš?
Dialogue: 0,0:21:29.78,0:21:31.29,main,,0000,0000,0000,,Šak víš…
Dialogue: 0,0:21:31.29,0:21:32.87,main,,0000,0000,0000,,Vypadalo to, že to někoho omráčilo?
Dialogue: 0,0:21:32.87,0:21:34.87,main,,0000,0000,0000,,Neperou se spolu navzájem?
Dialogue: 0,0:21:34.87,0:21:35.66,main,,0000,0000,0000,,Ne…
Dialogue: 0,0:21:35.67,0:21:37.33,main,,0000,0000,0000,,Tak mi to nepřišlo…
Dialogue: 0,0:21:37.33,0:21:38.96,main,,0000,0000,0000,,Tak bys měl…
Dialogue: 0,0:21:41.42,0:21:43.21,main,,0000,0000,0000,,Jsi úžasná, Kono-chan!
Dialogue: 0,0:21:43.21,0:21:44.67,main,,0000,0000,0000,,Vážně si se do toho vložila.
Dialogue: 0,0:21:44.67,0:21:45.76,main,,0000,0000,0000,,Hej, Konato!
Dialogue: 0,0:21:45.76,0:21:47.72,main,,0000,0000,0000,,Jsou ostatní dvě písně takovéto?
Dialogue: 0,0:21:48.55,0:21:50.72,main,,0000,0000,0000,,Je tu něco z anime a z SFX.
Dialogue: 0,0:21:50.72,0:21:52.56,main,,0000,0000,0000,,To není co jsem myslela!
Dialogue: 0,0:21:52.56,0:21:54.81,main,,0000,0000,0000,,Zazpívej něco co zná i zbytek!
Dialogue: 0,0:21:55.52,0:21:57.52,main,,0000,0000,0000,,Můžu zpívat co chci.
Dialogue: 0,0:21:57.52,0:21:58.19,main,,0000,0000,0000,,Tak se podíváme…
Dialogue: 0,0:21:58.19,0:21:59.52,main,,0000,0000,0000,,Tady je ovladač.
Dialogue: 0,0:21:59.52,0:22:00.61,main,,0000,0000,0000,,No tak, Tsukaso.
Dialogue: 0,0:22:00.61,0:22:01.57,main,,0000,0000,0000,,Pokud si neobjednáš píseň…
Dialogue: 0,0:22:01.57,0:22:05.07,main,,0000,0000,0000,,…budeme pořád poslouchat to její skřehotání a písničky z anime.
Dialogue: 0,0:22:05.15,0:22:07.07,main,,0000,0000,0000,,Mě to nevadí.
Dialogue: 0,0:22:07.07,0:22:08.99,main,,0000,0000,0000,,Je to vlastně docela sranda.
Dialogue: 0,0:22:08.99,0:22:10.74,main,,0000,0000,0000,,I ty, Miyuki?!
Dialogue: 0,0:22:11.16,0:22:12.16,main,,0000,0000,0000,,Promiňte jestli přes mě někdo nevidí.
Dialogue: 0,0:22:12.16,0:22:13.37,main,,0000,0000,0000,,Tentokrát budu stát.
Dialogue: 0,0:22:13.37,0:22:15.04,main,,0000,0000,0000,,Co teď?
Dialogue: 0,0:22:15.04,0:22:16.21,main,,0000,0000,0000,,Pokud se při zpěvu nepostavíš…
Dialogue: 0,0:22:16.21,0:22:18.46,main,,0000,0000,0000,,…tvůj hlas nemůže plně zazářit.
Dialogue: 0,0:22:18.54,0:22:20.42,main,,0000,0000,0000,,To budeš zpívat ještě hlasitěji?!
Dialogue: 0,0:22:22.80,0:22:23.80,main,,0000,0000,0000,,Skutečná píseň ze show!
Dialogue: 0,0:22:24.42,0:22:25.63,main,,0000,0000,0000,,To je docela dobrý!
Dialogue: 0,0:22:25.63,0:22:28.38,main,,0000,0000,0000,,Vypadá to, že to je ještě z doby než jsme se narodili.
Dialogue: 0,0:22:29.05,0:22:29.93,mainup2,,0000,0000,0000,,
Dialogue: 0,0:22:30.68,0:22:31.60,mainup2,,0000,0000,0000,,
Dialogue: 0,0:22:32.31,0:22:33.56,mainup2,,0000,0000,0000,,
Dialogue: 0,0:22:37.98,0:22:39.98,mainup2,,0000,0000,0000,,
Dialogue: 0,0:22:39.98,0:22:41.15,mainup2,,0000,0000,0000,,
Dialogue: 0,0:22:41.15,0:22:43.36,mainup2,,0000,0000,0000,,
Dialogue: 0,0:22:43.53,0:22:44.44,mainup2,,0000,0000,0000,,
Dialogue: 0,0:22:44.44,0:22:50.78,mainup2,,0000,0000,0000,,
Dialogue: 0,0:22:47.45,0:22:49.03,main,,0000,0000,0000,,Tady je vaše pití!
Dialogue: 0,0:22:50.16,0:22:51.41,main,,0000,0000,0000,,Díky.
Dialogue: 0,0:22:50.78,0:22:51.41,mainup2,,0000,0000,0000,,
Dialogue: 0,0:22:51.62,0:22:52.37,mainup2,,0000,0000,0000,,
Dialogue: 0,0:22:52.37,0:22:53.16,mainup2,,0000,0000,0000,,
Dialogue: 0,0:22:53.16,0:22:54.37,mainup2,,0000,0000,0000,,
Dialogue: 0,0:22:53.95,0:22:55.12,main,,0000,0000,0000,,Kdo si objednal ten čaj?
Dialogue: 0,0:22:54.37,0:22:55.54,mainup2,,0000,0000,0000,,
Dialogue: 0,0:22:55.58,0:22:57.54,mainup2,,0000,0000,0000,,
Dialogue: 0,0:22:55.58,0:22:56.79,main,,0000,0000,0000,,To je pro mě.
Dialogue: 0,0:22:56.79,0:22:58.41,main,,0000,0000,0000,,A ledový čaj?
Dialogue: 0,0:22:57.54,0:23:00.42,mainup2,,0000,0000,0000,,
Dialogue: 0,0:22:58.42,0:22:59.54,main,,0000,0000,0000,,To je pro mě.
Dialogue: 0,0:22:59.54,0:23:00.87,main,,0000,0000,0000,,A pomerančový džus?
Dialogue: 0,0:23:00.42,0:23:03.34,mainup2,,0000,0000,0000,,
Dialogue: 0,0:23:01.21,0:23:02.08,main,,0000,0000,0000,,Tady.
Dialogue: 0,0:23:02.09,0:23:03.34,main,,0000,0000,0000,,Zázvorové pivo?
Dialogue: 0,0:23:03.50,0:23:06.13,mainup2,,0000,0000,0000,,
Dialogue: 0,0:23:04.59,0:23:05.42,main,,0000,0000,0000,,Promiňte.
Dialogue: 0,0:23:05.42,0:23:07.26,main,,0000,0000,0000,,Můžete to postavit tady?
Dialogue: 0,0:23:06.13,0:23:06.88,mainup2,,0000,0000,0000,,
