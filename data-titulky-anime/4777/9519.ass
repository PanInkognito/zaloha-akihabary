Dialogue: 0,0:00:00.00,0:00:05.44,whitebig,,0000,0000,0000,,{\pos(226,152)}
Dialogue: 0,0:00:00.60,0:00:02.08,main,,0000,0000,0000,,Tak začneme.
Dialogue: 0,0:00:02.08,0:00:03.12,main,,0000,0000,0000,,Jdeme na to.
Dialogue: 0,0:00:03.68,0:00:05.48,main,,0000,0000,0000,,No tak už začněme!
Dialogue: 0,0:00:05.84,0:00:07.00,op1,,0000,0000,0000,,Okolo 3 cm!
Dialogue: 0,0:00:07.00,0:00:08.28,op1,,0000,0000,0000,,Je to snad málo?
Dialogue: 0,0:00:08.28,0:00:08.52,op1,,0000,0000,0000,,Jejda!
Dialogue: 0,0:00:08.52,0:00:10.36,op1,,0000,0000,0000,,Schované pod naší uniformou!
Dialogue: 0,0:00:10.36,0:00:11.40,op1,,0000,0000,0000,,Mám toho nechat?
Dialogue: 0,0:00:11.40,0:00:11.72,op1,,0000,0000,0000,,JEJ!
Dialogue: 0,0:00:11.72,0:00:12.80,op1,,0000,0000,0000,,Jít do toho?
Dialogue: 0,0:00:12.80,0:00:13.60,op1,,0000,0000,0000,,Snažit se?
Dialogue: 0,0:00:13.60,0:00:15.16,op1,,0000,0000,0000,,Vychutnat si každý okamžik. Jo!
Dialogue: 0,0:00:15.16,0:00:15.76,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:15.76,0:00:16.00,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:16.00,0:00:16.40,op1,,0000,0000,0000,,ŘÍKÁM!
Dialogue: 0,0:00:16.40,0:00:16.76,op1,,0000,0000,0000,,WHOO!
Dialogue: 0,0:00:16.76,0:00:17.36,op1,,0000,0000,0000,,Jen mezi námi dvěma!
Dialogue: 0,0:00:17.36,0:00:20.08,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:00:22.08,0:00:22.88,logo,,0000,0000,0000,,{\move(108,218,202,218,0,0)}LUCKY
Dialogue: 0,0:00:22.88,0:00:24.60,logo,,0000,0000,0000,,{\pos(202,218)}LUCKY
Dialogue: 0,0:00:22.08,0:00:22.88,logo,,0000,0000,0000,,{\move(638,218,459,218,0,0)}STAR
Dialogue: 0,0:00:22.88,0:00:24.60,logo,,0000,0000,0000,,{\pos(459,218)}STAR
Dialogue: 0,0:00:25.00,0:00:25.60,op1,,0000,0000,0000,,Cítím se nějak nesvá.
Dialogue: 0,0:00:25.60,0:00:26.20,op1,,0000,0000,0000,,Co jsem to jedla?
Dialogue: 0,0:00:26.20,0:00:26.80,op1,,0000,0000,0000,,Mám tě hrozně ráda!
Dialogue: 0,0:00:26.80,0:00:27.00,op1,,0000,0000,0000,,Oops!
Dialogue: 0,0:00:27.00,0:00:28.04,op1,,0000,0000,0000,,Jedno z nich je špatně!
Dialogue: 0,0:00:28.04,0:00:28.80,op1,,0000,0000,0000,,Relaxovat!
Dialogue: 0,0:00:28.80,0:00:29.44,op1,,0000,0000,0000,,Nic nedělat!
Dialogue: 0,0:00:29.44,0:00:29.96,op1,,0000,0000,0000,,Flákat se!
Dialogue: 0,0:00:29.96,0:00:31.04,op1,,0000,0000,0000,,Už toho nechej!
Dialogue: 0,0:00:31.04,0:00:32.72,op1,,0000,0000,0000,,Je ta nová holka oblíbená?
Dialogue: 0,0:00:32.72,0:00:34.36,op1,,0000,0000,0000,,Já bych řekla, že je docela normální!
Dialogue: 0,0:00:34.36,0:00:35.84,op1,,0000,0000,0000,,To jsem jediná překvapená?
Dialogue: 0,0:00:35.84,0:00:36.16,op1,,0000,0000,0000,,Vepřové kousky!
Dialogue: 0,0:00:36.16,0:00:36.56,op1,,0000,0000,0000,,Skleněné nudle!
Dialogue: 0,0:00:36.56,0:00:36.88,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:00:36.88,0:00:37.68,op2,,0000,0000,0000,,DA-DA-DA-DA-DA!
Dialogue: 0,0:00:38.24,0:00:39.04,op1,,0000,0000,0000,,Ooh?
Dialogue: 0,0:00:39.04,0:00:39.80,op1,,0000,0000,0000,,Víš co tím myslím?
Dialogue: 0,0:00:39.40,0:00:40.24,op1up,,0000,0000,0000,,Moje brýle…
Dialogue: 0,0:00:40.24,0:00:41.32,op1up,,0000,0000,0000,,Kde mám brýle?
Dialogue: 0,0:00:41.40,0:00:43.36,op1,,0000,0000,0000,,Nedávno jsme hrála tu samou bojovku.
Dialogue: 0,0:00:42.00,0:00:43.36,op1up,,0000,0000,0000,,Bez nich nic nevidím…
Dialogue: 0,0:00:44.40,0:00:45.00,op2,,0000,0000,0000,,POM! POM!
Dialogue: 0,0:00:45.00,0:00:46.04,op1,,0000,0000,0000,,Roztleskávačky!
Dialogue: 0,0:00:46.04,0:00:47.56,op2,,0000,0000,0000,,DÁME SI SLADKOST!
Dialogue: 0,0:00:47.56,0:00:48.16,op2,,0000,0000,0000,,LA! LA!
Dialogue: 0,0:00:48.16,0:00:49.20,op1,,0000,0000,0000,,Párty začíná!
Dialogue: 0,0:00:49.20,0:00:50.80,op2,,0000,0000,0000,,MÁŠ TO V KRVI!
Dialogue: 0,0:00:50.80,0:00:51.36,op2,,0000,0000,0000,,ANO!
Dialogue: 0,0:00:51.36,0:00:52.36,op1,,0000,0000,0000,,Zanechává to dojem.
Dialogue: 0,0:00:52.36,0:00:53.84,op1,,0000,0000,0000,,Malé planetky.
Dialogue: 0,0:00:53.84,0:00:54.36,op1,,0000,0000,0000,,Narazili!
Dialogue: 0,0:00:54.36,0:00:55.00,op1,,0000,0000,0000,,A shořeli!
Dialogue: 0,0:00:55.00,0:00:55.36,op1,,0000,0000,0000,,Bylo to ohromné!
Dialogue: 0,0:00:55.36,0:00:56.12,op1,,0000,0000,0000,,Z plných plic křičíme!
Dialogue: 0,0:00:56.12,0:00:56.68,op1,,0000,0000,0000,,ZPÍVEJ A TANCUJ!
Dialogue: 0,0:00:57.04,0:00:58.32,op1,,0000,0000,0000,,Přiznej si to!
Dialogue: 0,0:00:58.32,0:01:02.80,op1,,0000,0000,0000,,Nakonec se stejně budeme smát!
Dialogue: 0,0:01:02.80,0:01:06.56,op1,,0000,0000,0000,,Protože mám na sobě námořnickou uniformu.
Dialogue: 0,0:01:06.56,0:01:09.44,op2,,0000,0000,0000,,To je ono!
Dialogue: 0,0:01:10.00,0:01:13.56,op1,,0000,0000,0000,,Je teprve pondělí a já už mám špatnou náladu.
Dialogue: 0,0:01:13.56,0:01:15.36,op1,,0000,0000,0000,,Co mám dělat?
Dialogue: 0,0:01:15.56,0:01:19.40,op1,,0000,0000,0000,,Raději si vezmu moji letní uniformu -
Dialogue: 0,0:01:19.40,0:01:22.28,op1,,0000,0000,0000,,- je rozkošná!
Dialogue: 0,0:01:22.52,0:01:23.68,op1,,0000,0000,0000,,Trojdílná uniforma!
Dialogue: 0,0:01:23.68,0:01:25.12,op1,,0000,0000,0000,,Počkej chvilku!
Dialogue: 0,0:01:25.12,0:01:25.56,op1,,0000,0000,0000,,JO!
Dialogue: 0,0:01:25.56,0:01:26.00,op1,,0000,0000,0000,,Dej se do toho!
Dialogue: 0,0:01:26.00,0:01:26.36,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:26.36,0:01:27.00,op1,,0000,0000,0000,,SNAŽ SE!
Dialogue: 0,0:01:27.00,0:01:27.16,op2,,0000,0000,0000,,JO!
Dialogue: 0,0:01:27.16,0:01:29.04,op2,,0000,0000,0000,,DRAHOUŠKU, DRAHOUŠKU, JEDEME!
Dialogue: 0,0:01:30.60,0:01:31.64,main,,0000,0000,0000,,To je fakt.
Dialogue: 0,0:01:32.00,0:01:34.04,main,,0000,0000,0000,,Ode dneška nosíme letní uniformy.
Dialogue: 0,0:01:34.04,0:01:36.72,main,,0000,0000,0000,,Někdo jako Konata se nejspíš objeví ještě v zimní.
Dialogue: 0,0:01:37.44,0:01:40.20,main,,0000,0000,0000,,Alespoň jeden ze třídy na to zapomene.
Dialogue: 0,0:01:40.92,0:01:42.36,main,,0000,0000,0000,,Dobré ráno, ségra.
Dialogue: 0,0:01:48.48,0:01:50.16,main,,0000,0000,0000,,Jeden je v tomto domě.
Dialogue: 0,0:01:50.36,0:01:53.16,white,,0000,0000,0000,,{\frz-90\pos(389,161)}lidí
Dialogue: 0,0:01:50.36,0:01:53.16,white,,0000,0000,0000,,{\frz-90\pos(305,211)}Různé druhy
Dialogue: 0,0:01:53.44,0:01:54.16,main,,0000,0000,0000,,Krysy!
Dialogue: 0,0:01:54.84,0:01:58.32,main,,0000,0000,0000,,Zapoměla jsem se přihlásit do té televizní soutěže.
Dialogue: 0,0:01:59.04,0:02:00.56,main,,0000,0000,0000,,Docela jsem chtěla tu cenu.
Dialogue: 0,0:02:00.56,0:02:01.72,main,,0000,0000,0000,,Zástěra z Apron of Love.
Dialogue: 0,0:02:02.00,0:02:03.52,main,,0000,0000,0000,,Nedostatek lásky.
Dialogue: 0,0:02:04.64,0:02:07.48,main,,0000,0000,0000,,Zapoměla jsi protože nemáš dost lásky.
Dialogue: 0,0:02:07.48,0:02:08.24,main,,0000,0000,0000,,To ne.
Dialogue: 0,0:02:08.24,0:02:12.20,main,,0000,0000,0000,,Spíš jsem byla zaneprázdněna se studenskou radou a přípravou na testy.
Dialogue: 0,0:02:12.20,0:02:12.92,main,,0000,0000,0000,,Ne, ne.
Dialogue: 0,0:02:12.92,0:02:14.60,main,,0000,0000,0000,,To jsou jen výmluvy.
Dialogue: 0,0:02:15.08,0:02:16.88,main,,0000,0000,0000,,Nikdy jsem nezapomněla.
Dialogue: 0,0:02:16.88,0:02:19.88,main,,0000,0000,0000,,Nesrovnávej mě s někým kdo celé dny nedělá nic jiného, než se jen baví!
Dialogue: 0,0:02:19.88,0:02:21.84,main,,0000,0000,0000,,A to jak jsi mi to řekla mě vážně naštvalo!
Dialogue: 0,0:02:22.60,0:02:25.28,main,,0000,0000,0000,,Jak vyhraješ tu soutěž?
Dialogue: 0,0:02:25.28,0:02:26.16,main,,0000,0000,0000,,Jednoduše…
Dialogue: 0,0:02:26.44,0:02:27.80,main,,0000,0000,0000,,Láska!
Dialogue: 0,0:02:28.16,0:02:28.96,main,,0000,0000,0000,,Jo, jo.
Dialogue: 0,0:02:28.96,0:02:31.28,main,,0000,0000,0000,,Jsem prostě nemilující ženská.
Dialogue: 0,0:02:31.28,0:02:31.84,main,,0000,0000,0000,,No…
Dialogue: 0,0:02:31.88,0:02:33.08,main,,0000,0000,0000,,Pokud to uděláš jako každý jiný…
Dialogue: 0,0:02:33.08,0:02:35.04,main,,0000,0000,0000,,… tak nevyhraješ.
Dialogue: 0,0:02:35.60,0:02:37.48,main,,0000,0000,0000,,Co dáváš na svoje kresby?
Dialogue: 0,0:02:37.48,0:02:39.08,main,,0000,0000,0000,,Věci co mi pomůžou vyhrát.
Dialogue: 0,0:02:39.36,0:02:40.56,main,,0000,0000,0000,,Jaké?
Dialogue: 0,0:02:40.56,0:02:43.12,main,,0000,0000,0000,,Základ je dobrý pozdrav.
Dialogue: 0,0:02:43.12,0:02:47.44,main,,0000,0000,0000,,Pohlednici musíš upravit, tak aby prosila o poslání.
Dialogue: 0,0:02:47.44,0:02:48.44,main,,0000,0000,0000,,Jako například?
Dialogue: 0,0:02:48.72,0:02:50.08,main,,0000,0000,0000,,Jako…
Dialogue: 0,0:02:50.08,0:02:53.16,main,,0000,0000,0000,,Použít zvýrazňovač na okraje pohlednice.
Dialogue: 0,0:02:54.12,0:02:55.44,main,,0000,0000,0000,,To zazáří.
Dialogue: 0,0:02:56.04,0:02:59.92,main,,0000,0000,0000,,Pak přidáš obrázek související s cenou.
Dialogue: 0,0:02:59.92,0:03:01.52,main,,0000,0000,0000,,Obrázek?
Dialogue: 0,0:03:02.84,0:03:04.60,main,,0000,0000,0000,,Moc dobře ale nemaluji.
Dialogue: 0,0:03:04.60,0:03:05.92,main,,0000,0000,0000,,V tom případě…
Dialogue: 0,0:03:06.48,0:03:09.20,main,,0000,0000,0000,,…jich prostě odešli co nejvíc.
Dialogue: 0,0:03:10.52,0:03:11.76,main,,0000,0000,0000,,To bych mohla zvládnout.
Dialogue: 0,0:03:11.76,0:03:12.72,main,,0000,0000,0000,,Ale…
Dialogue: 0,0:03:12.72,0:03:15.04,main,,0000,0000,0000,,Ať už jich pošleš kolik chceš…
Dialogue: 0,0:03:15.04,0:03:19.28,main,,0000,0000,0000,,…musíš každou napsat ručně.
Dialogue: 0,0:03:19.80,0:03:21.52,main,,0000,0000,0000,,A poslat jich pokaždé sto.
Dialogue: 0,0:03:21.52,0:03:22.56,main,,0000,0000,0000,,Sto?!
Dialogue: 0,0:03:22.56,0:03:24.64,main,,0000,0000,0000,,Je snad něco špatně?
Dialogue: 0,0:03:24.64,0:03:26.36,main,,0000,0000,0000,,Nebude to stát spoustu peněz?
Dialogue: 0,0:03:26.36,0:03:28.80,main,,0000,0000,0000,,Bojuješ za něco co chceš.
Dialogue: 0,0:03:28.80,0:03:31.20,main,,0000,0000,0000,,To vyžaduje vášeň a nasazení!
Dialogue: 0,0:03:31.20,0:03:34.12,main,,0000,0000,0000,,Ale nebylo by levnější to prostě koupit?
Dialogue: 0,0:03:34.12,0:03:36.84,main,,0000,0000,0000,,Některé věci si koupit nemůžeš.
Dialogue: 0,0:03:38.08,0:03:40.20,main,,0000,0000,0000,,Vleze se sto pohledů do schránky?
Dialogue: 0,0:03:40.24,0:03:42.52,main,,0000,0000,0000,,Neposíláš je zároveň…
Dialogue: 0,0:03:42.52,0:03:45.52,main,,0000,0000,0000,,…ale pár teď, pár potom a tak.
Dialogue: 0,0:03:45.52,0:03:47.28,main,,0000,0000,0000,,A to během několika dnů.
Dialogue: 0,0:03:48.40,0:03:51.12,main,,0000,0000,0000,,To záleží na konkrétní soutěži.
Dialogue: 0,0:03:51.12,0:03:53.64,main,,0000,0000,0000,,Někdy je to i risk.
Dialogue: 0,0:03:53.64,0:03:57.00,main,,0000,0000,0000,,Proto je někdy lepší to zkusit bez toho.
Dialogue: 0,0:03:57.00,0:03:58.72,main,,0000,0000,0000,,A někdy je to přesně naopak.
Dialogue: 0,0:03:58.72,0:04:00.20,main,,0000,0000,0000,,Takže co teď?
Dialogue: 0,0:04:00.20,0:04:01.08,main,,0000,0000,0000,,Jak jsem řekla…
Dialogue: 0,0:04:01.08,0:04:01.60,main,,0000,0000,0000,,Láska!
Dialogue: 0,0:04:01.60,0:04:02.28,main,,0000,0000,0000,,Jo, jo.
Dialogue: 0,0:04:02.64,0:04:04.60,main,,0000,0000,0000,,Nicméně, pokud chceš vyhrát…
Dialogue: 0,0:04:04.64,0:04:07.80,main,,0000,0000,0000,,podívej se do pár novin a magazínů
Dialogue: 0,0:04:07.80,0:04:11.84,main,,0000,0000,0000,,a hledej kresby, které pravděpodobně nebudou přitahovat tolik lidí.
Dialogue: 0,0:04:11.84,0:04:13.92,main,,0000,0000,0000,,Budeš mít velkou šanci na úspěch.
Dialogue: 0,0:04:14.64,0:04:15.60,main,,0000,0000,0000,,Zkusím to!
Dialogue: 0,0:04:15.60,0:04:17.00,main,,0000,0000,0000,,Kde je láska teď?
Dialogue: 0,0:04:20.72,0:04:22.48,main,,0000,0000,0000,,No, na to že jste dvojčata…
Dialogue: 0,0:04:22.48,0:04:24.48,main,,0000,0000,0000,,…nevypadáte jedna jako druhá.
Dialogue: 0,0:04:25.68,0:04:30.68,main,,0000,0000,0000,,Jsme totiž dvouvaječná dvojčata.
Dialogue: 0,0:04:31.44,0:04:33.72,main,,0000,0000,0000,,Jednovaječná vypadají stejně…
Dialogue: 0,0:04:33.72,0:04:35.00,main,,0000,0000,0000,,…ale dvou můžou být…
Dialogue: 0,0:04:35.00,0:04:37.20,main,,0000,0000,0000,,…hodně rozdílné jak ve vzhledu, tak v osobnosti.
Dialogue: 0,0:04:37.20,0:04:38.52,main,,0000,0000,0000,,Chápu.
Dialogue: 0,0:04:38.52,0:04:40.64,main,,0000,0000,0000,,Tsakasa je pohodář…
Dialogue: 0,0:04:40.64,0:04:41.64,main,,0000,0000,0000,,…ale Kagami je pěknej blázen.
Dialogue: 0,0:04:41.64,0:04:42.28,main,,0000,0000,0000,,Co že to?!
Dialogue: 0,0:04:43.32,0:04:45.40,main,,0000,0000,0000,,Pokud by jste měli stejné účesy s copánky…
Dialogue: 0,0:04:45.40,0:04:47.08,main,,0000,0000,0000,,…mohli by jste vypadat docela stejně.
Dialogue: 0,0:04:47.76,0:04:48.84,main,,0000,0000,0000,,Kana-chan!
Dialogue: 0,0:04:49.68,0:04:50.16,main,,0000,0000,0000,,To ne.
Dialogue: 0,0:04:50.16,0:04:51.60,main,,0000,0000,0000,,Kagami by nikdy nemohla být roztomilý typ.
Dialogue: 0,0:04:51.60,0:04:52.44,main,,0000,0000,0000,,Sklapni!
Dialogue: 0,0:04:52.44,0:04:54.76,main,,0000,0000,0000,,Nezahrnuj mě tvýma zvrácenýma představama!
Dialogue: 0,0:04:55.32,0:04:56.88,main,,0000,0000,0000,,Můžete používat telepatii?
Dialogue: 0,0:04:57.80,0:04:58.40,main,,0000,0000,0000,,Šak víš.
Dialogue: 0,0:04:58.40,0:04:59.48,main,,0000,0000,0000,,Dvojčata…
Dialogue: 0,0:04:59.48,0:05:01.36,main,,0000,0000,0000,,I když jsou vychována odděleně…
Dialogue: 0,0:05:01.36,0:05:06.16,main,,0000,0000,0000,,…mají podobný životní styl a další věci.
Dialogue: 0,0:05:06.16,0:05:08.08,main,,0000,0000,0000,,A občas si vezmou jiný pár dvojčat.
Dialogue: 0,0:05:08.60,0:05:10.72,main,,0000,0000,0000,,A když je jeden z nich v nebezpečí…
Dialogue: 0,0:05:10.72,0:05:14.28,main,,0000,0000,0000,,…ten druhý to dokáže vycítit na velkou vzdálenost.
Dialogue: 0,0:05:14.76,0:05:17.12,main,,0000,0000,0000,,Jsou tu různé záhadné úkazy.
Dialogue: 0,0:05:17.12,0:05:19.20,main,,0000,0000,0000,,Takže, můžete používat telepatii?
Dialogue: 0,0:05:19.20,0:05:20.48,main,,0000,0000,0000,,No tak do toho.
Dialogue: 0,0:05:20.48,0:05:21.04,main,,0000,0000,0000,,Abrakadabra!
Dialogue: 0,0:05:21.04,0:05:22.88,main,,0000,0000,0000,,Čteš až moc mangy.
Dialogue: 0,0:05:23.20,0:05:25.64,comments,,0000,0000,0000,,
Dialogue: 0,0:05:23.20,0:05:24.36,main,,0000,0000,0000,,To samé s ManaKanou…
Dialogue: 0,0:05:24.76,0:05:25.64,main,,0000,0000,0000,,ManaKana?
Dialogue: 0,0:05:26.04,0:05:29.16,main,,0000,0000,0000,,Slyšela jsem že mají pár rozdílu.
Dialogue: 0,0:05:29.16,0:05:31.68,main,,0000,0000,0000,,Mimochodem, Mana je ta pozorná…
Dialogue: 0,0:05:31.68,0:05:33.36,main,,0000,0000,0000,,… a Kana je ta optimistická.
Dialogue: 0,0:05:33.36,0:05:36.92,main,,0000,0000,0000,,A dokonce zní i trochu jinak když zpívají.
Dialogue: 0,0:05:36.92,0:05:37.92,main,,0000,0000,0000,,To mi připomíná…
Dialogue: 0,0:05:37.92,0:05:42.12,main,,0000,0000,0000,,Přesto, že ManaKana byli dlouho dvouvaječná dvojčata…
Dialogue: 0,0:05:42.12,0:05:45.20,main,,0000,0000,0000,,…nedávno zjistili, že jsou vlastně jednovaječná.
Dialogue: 0,0:05:45.88,0:05:47.04,main,,0000,0000,0000,,Vážně?!
Dialogue: 0,0:05:47.04,0:05:48.84,main,,0000,0000,0000,,Je to vážně tak překvapující?
Dialogue: 0,0:05:48.84,0:05:51.04,main,,0000,0000,0000,,Definice pro jedno a dvouvaječná dvojčata…
Dialogue: 0,0:05:51.04,0:05:52.60,main,,0000,0000,0000,,…se nedávno změnily.
Dialogue: 0,0:05:53.04,0:05:56.36,main,,0000,0000,0000,,Což znamená, že by jste mohli být jednovaječná.
Dialogue: 0,0:05:56.88,0:05:58.76,main,,0000,0000,0000,,Vaše identické rysy by byly…
Dialogue: 0,0:06:00.24,0:06:02.04,main,,0000,0000,0000,,Obě máte rády sladké, že?
Dialogue: 0,0:06:02.92,0:06:03.40,main,,0000,0000,0000,,Miluji ho.
Dialogue: 0,0:06:03.80,0:06:04.16,main,,0000,0000,0000,,Asi.
Dialogue: 0,0:06:04.68,0:06:06.32,main,,0000,0000,0000,,Obě píšete levou rukou.
Dialogue: 0,0:06:07.04,0:06:07.72,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:06:08.12,0:06:08.92,main,,0000,0000,0000,,Jaká je vaše oblíbená barva?
Dialogue: 0,0:06:09.40,0:06:09.96,main,,0000,0000,0000,,Asi bílá.
Dialogue: 0,0:06:10.32,0:06:10.76,main,,0000,0000,0000,,Černá.
Dialogue: 0,0:06:11.04,0:06:12.88,main,,0000,0000,0000,,Stejná barva jako vaše duše.
Dialogue: 0,0:06:12.88,0:06:13.96,main,,0000,0000,0000,,Cos to řekla?!
Dialogue: 0,0:06:17.92,0:06:21.80,main,,0000,0000,0000,,Z toho povídání o vejcích a černém a bílém cukroví mám hlad.
Dialogue: 0,0:06:21.80,0:06:23.60,main,,0000,0000,0000,,Nikdo se nebavil o jídle.
Dialogue: 0,0:06:30.04,0:06:31.68,main,,0000,0000,0000,,Je tu hrozné dusno.
Dialogue: 0,0:06:32.28,0:06:34.68,main,,0000,0000,0000,,Sotva jsme nastoupili a já se už potím.
Dialogue: 0,0:06:34.68,0:06:36.76,main,,0000,0000,0000,,To je tou roční dobou.
Dialogue: 0,0:06:37.00,0:06:38.64,main,,0000,0000,0000,,Horko…
Dialogue: 0,0:06:38.64,0:06:40.68,main,,0000,0000,0000,,Ještě nezapnuli klimatizaci.
Dialogue: 0,0:06:45.72,0:06:46.56,main,,0000,0000,0000,,H-Hej!
Dialogue: 0,0:06:46.92,0:06:49.28,main,,0000,0000,0000,,Horko…
Dialogue: 0,0:06:49.72,0:06:52.92,main,,0000,0000,0000,,Ladybug Samba, punková verze.
Dialogue: 0,0:06:49.72,0:06:52.92,newspaperup,,0000,0000,0000,,
Dialogue: 0,0:06:55.68,0:06:59.36,main,,0000,0000,0000,,Jedna z mých sestřenic se tento měsíc vdává.
Dialogue: 0,0:07:00.76,0:07:02.56,main,,0000,0000,0000,,Takže bude červnová nevěsta.
Dialogue: 0,0:07:02.56,0:07:03.60,main,,0000,0000,0000,,To zní báječně.
Dialogue: 0,0:07:03.60,0:07:04.72,main,,0000,0000,0000,,Jak tahle…
Dialogue: 0,0:07:05.20,0:07:09.92,main,,0000,0000,0000,,…upocená a úmorná roční doba představuje nějaké zvláštní štěstí?
Dialogue: 0,0:07:12.60,0:07:16.16,main,,0000,0000,0000,,Co je radostného na svatbě v červnu?
Dialogue: 0,0:07:17.04,0:07:19.36,main,,0000,0000,0000,,Jméno červen /anglicky June/…
Dialogue: 0,0:07:19.36,0:07:21.08,main,,0000,0000,0000,,…je obecně považováno…
Dialogue: 0,0:07:21.08,0:07:23.68,main,,0000,0000,0000,,…za odvozené ze jména řecké bohyně, Juno.
Dialogue: 0,0:07:23.68,0:07:24.56,main,,0000,0000,0000,,V řecké mytologii…
Dialogue: 0,0:07:24.56,0:07:27.00,main,,0000,0000,0000,,…je Juno známá jako Hera…
Dialogue: 0,0:07:27.00,0:07:29.16,main,,0000,0000,0000,,…bohyně manželství a plodnosti.
Dialogue: 0,0:07:29.64,0:07:33.48,main,,0000,0000,0000,,To proto se říká, že ti kdo se provdají v červnu, měsíci June…
Dialogue: 0,0:07:33.48,0:07:36.28,main,,0000,0000,0000,,…dostanou její ochranu a požehnání.
Dialogue: 0,0:07:37.12,0:07:39.84,main,,0000,0000,0000,,Ale nebyla Hera manželkou Zeuse?
Dialogue: 0,0:07:39.84,0:07:41.04,main,,0000,0000,0000,,Ano, to je pravda.
Dialogue: 0,0:07:41.52,0:07:44.60,main,,0000,0000,0000,,Představuji si Heru extrémně žárlivou…
Dialogue: 0,0:07:44.60,0:07:47.08,main,,0000,0000,0000,,…takže necítím to štěstí a radost.
Dialogue: 0,0:07:47.84,0:07:48.16,main,,0000,0000,0000,,Cože?
Dialogue: 0,0:07:48.72,0:07:51.68,main,,0000,0000,0000,,Chcete mít na své svatbě raději šaty nebo kimono?
Dialogue: 0,0:07:52.12,0:07:54.28,main,,0000,0000,0000,,Raději šaty.
Dialogue: 0,0:07:54.28,0:07:57.12,main,,0000,0000,0000,,Je tu můj sen, už od mala.
Dialogue: 0,0:07:57.92,0:08:00.08,main,,0000,0000,0000,,Také bych si vzala šaty.
Dialogue: 0,0:08:00.08,0:08:02.28,main,,0000,0000,0000,,Ale ani kimono by nebylo špatné.
Dialogue: 0,0:08:03.96,0:08:05.36,main,,0000,0000,0000,,Chtěla bych oboje!
Dialogue: 0,0:08:06.24,0:08:09.72,main,,0000,0000,0000,,Dneska mají minisukně a pestrobarevné šaty.
Dialogue: 0,0:08:10.64,0:08:12.40,main,,0000,0000,0000,,Minisukně na svatbu?
Dialogue: 0,0:08:12.40,0:08:15.72,main,,0000,0000,0000,,Se spoustou ozdob po lemu by to vypadalo roztomile.
Dialogue: 0,0:08:16.08,0:08:17.28,main,,0000,0000,0000,,Spousta snů!
Dialogue: 0,0:08:17.28,0:08:18.36,main,,0000,0000,0000,,Spousta ozdob!
Dialogue: 0,0:08:18.76,0:08:19.80,main,,0000,0000,0000,,Že?
Dialogue: 0,0:08:19.80,0:08:21.80,main,,0000,0000,0000,,Jsi Angel Salvia.
Dialogue: 0,0:08:21.80,0:08:22.36,main,,0000,0000,0000,,Počkat.
Dialogue: 0,0:08:22.36,0:08:24.72,main,,0000,0000,0000,,S tvými vlasy vypadáš spíš jako Daisy.
Dialogue: 0,0:08:25.24,0:08:27.96,main,,0000,0000,0000,,Takže žlutý vršek a minisukně.
Dialogue: 0,0:08:29.88,0:08:30.76,main,,0000,0000,0000,,Tsukasa-san.
Dialogue: 0,0:08:30.76,0:08:32.36,main,,0000,0000,0000,,Minisukně by ti nejspíš slušela.
Dialogue: 0,0:08:32.40,0:08:33.40,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:08:33.40,0:08:34.56,main,,0000,0000,0000,,Oblečení je důležité…
Dialogue: 0,0:08:34.56,0:08:36.88,main,,0000,0000,0000,,…ale osoba, kterou si bereš je důležitá stejně.
Dialogue: 0,0:08:37.36,0:08:38.96,main,,0000,0000,0000,,Přítel z dětství?
Dialogue: 0,0:08:38.96,0:08:40.64,main,,0000,0000,0000,,Nebo někdo bohatý ?
Dialogue: 0,0:08:40.64,0:08:42.40,main,,0000,0000,0000,,Těžké rozhodnutí.
Dialogue: 0,0:08:42.40,0:08:44.72,main,,0000,0000,0000,,Jeden má spoustu peněz…
Dialogue: 0,0:08:44.72,0:08:47.92,main,,0000,0000,0000,,…a schopnost vykouzlit Beho*mi a l***zun.
Dialogue: 0,0:08:48.56,0:08:49.60,main,,0000,0000,0000,,Ale pokud se zvažují city…
Dialogue: 0,0:08:49.88,0:08:51.28,main,,0000,0000,0000,,Ruce dolů Bianco!
Dialogue: 0,0:08:51.60,0:08:52.84,main,,0000,0000,0000,,Oh, Kagami.
Dialogue: 0,0:08:52.84,0:08:54.32,main,,0000,0000,0000,,Ty si ten typ co je na skutečnou lásku, heh?
Dialogue: 0,0:08:54.32,0:08:56.24,main,,0000,0000,0000,,Myslela jsem, že jsi na peníze.
Dialogue: 0,0:08:56.84,0:08:57.96,main,,0000,0000,0000,,Žádný comeback, heh?
Dialogue: 0,0:09:00.92,0:09:02.12,main,,0000,0000,0000,,Hoj, Konato.
Dialogue: 0,0:09:03.32,0:09:06.36,main,,0000,0000,0000,,Mám otázku ohledně hry co jsem si včera půjčila.
Dialogue: 0,0:09:06.40,0:09:07.00,main,,0000,0000,0000,,Oh.
Dialogue: 0,0:09:07.00,0:09:08.64,main,,0000,0000,0000,,Rovnou zadrž, Kagaminyo.
Dialogue: 0,0:09:08.96,0:09:12.08,main,,0000,0000,0000,,Není to větší zábava, když na to přijdeš sama?
Dialogue: 0,0:09:13.12,0:09:14.32,main,,0000,0000,0000,,To máš pravdu.
Dialogue: 0,0:09:14.32,0:09:14.88,main,,0000,0000,0000,,Jo, jo.
Dialogue: 0,0:09:15.28,0:09:18.44,main,,0000,0000,0000,,Když myslíš, že je lepší pokud to vyřeším sama…
Dialogue: 0,0:09:18.44,0:09:20.64,main,,0000,0000,0000,,…budeš si i ty od teď dělat domácí úkoly sama ?
Dialogue: 0,0:09:22.80,0:09:25.20,main,,0000,0000,0000,,Nesetkávají se detektivové v manze a anime…
Dialogue: 0,0:09:25.20,0:09:27.96,main,,0000,0000,0000,,…se strašnou spoustou vražd?
Dialogue: 0,0:09:28.36,0:09:30.28,main,,0000,0000,0000,,Jako v Conanovi nebo Kindaichi.
Dialogue: 0,0:09:30.28,0:09:33.24,main,,0000,0000,0000,,Jinak by neměli příběh.
Dialogue: 0,0:09:33.24,0:09:34.04,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:09:34.04,0:09:36.16,main,,0000,0000,0000,,Nemůžou prostě řešit normální věci?
Dialogue: 0,0:09:36.16,0:09:36.88,main,,0000,0000,0000,,Normální?
Dialogue: 0,0:09:37.16,0:09:40.76,main,,0000,0000,0000,,Jako cestovat po Japonsku a hledat zmizelé osoby.
Dialogue: 0,0:09:41.28,0:09:44.12,main,,0000,0000,0000,,Sledování lidí by šlo dobře přenést do hry.
Dialogue: 0,0:09:44.12,0:09:46.28,main,,0000,0000,0000,,S pořádnou dávkou napětí.
Dialogue: 0,0:09:46.28,0:09:47.44,main,,0000,0000,0000,,Myslím.
Dialogue: 0,0:09:47.44,0:09:50.88,main,,0000,0000,0000,,Také bys mohla sledovat nevěrné manželky.
Dialogue: 0,0:09:50.88,0:09:52.72,main,,0000,0000,0000,,Nebo hledat domácí mazlíčky co se zatoulali?
Dialogue: 0,0:09:52.72,0:09:53.84,main,,0000,0000,0000,,Jo, jo!
Dialogue: 0,0:09:54.16,0:09:56.72,main,,0000,0000,0000,,Ale byla by zábava se na to dívat?
Dialogue: 0,0:09:57.36,0:09:59.40,main,,0000,0000,0000,,Nejspíš vůbec ne.
Dialogue: 0,0:09:59.40,0:09:59.88,main,,0000,0000,0000,,Že?
Dialogue: 0,0:10:00.16,0:10:01.24,main,,0000,0000,0000,,Taky…
Dialogue: 0,0:10:01.24,0:10:02.68,main,,0000,0000,0000,,Případy ve skutečném životě…
Dialogue: 0,0:10:02.68,0:10:05.60,main,,0000,0000,0000,,…neobsahují žádné brilantní triky…
Dialogue: 0,0:10:05.60,0:10:07.12,main,,0000,0000,0000,,…jako ty co najdeš v mange a anime.
Dialogue: 0,0:10:08.48,0:10:09.84,main,,0000,0000,0000,,Myslím že ne.
Dialogue: 0,0:10:09.88,0:10:13.72,main,,0000,0000,0000,,Uměle vyrobené, nevystopovatelné otisky prstů, podle kterých nejde vystopovat pachatel.
Dialogue: 0,0:10:14.28,0:10:16.80,main,,0000,0000,0000,,Věci které jsou o krok napřed před forensními laboratořemi.
Dialogue: 0,0:10:16.80,0:10:17.48,main,,0000,0000,0000,,No…
Dialogue: 0,0:10:17.48,0:10:20.08,main,,0000,0000,0000,,Vše moc dobré by mohlo směřovat k napodobeninám.
Dialogue: 0,0:10:20.08,0:10:22.08,main,,0000,0000,0000,,Nebo být moc citlivé pro odvysílání.
Dialogue: 0,0:10:22.08,0:10:23.24,main,,0000,0000,0000,,Ah, nebo…
Dialogue: 0,0:10:23.44,0:10:27.72,main,,0000,0000,0000,,Opravdu těžké případy skončí neobjasněny nebo…
Dialogue: 0,0:10:27.72,0:10:28.36,main,,0000,0000,0000,,…nevyřešeny.
Dialogue: 0,0:10:28.36,0:10:30.72,main,,0000,0000,0000,,To by bylo špatné ze spousty úhlů!
Dialogue: 0,0:10:31.96,0:10:34.68,main,,0000,0000,0000,,Neštve tě jak několik posledních dnů stále prší?
Dialogue: 0,0:10:34.68,0:10:36.40,main,,0000,0000,0000,,To máš pravdu…
Dialogue: 0,0:10:38.28,0:10:41.56,main,,0000,0000,0000,,Ale já mám ráda déšť…
Dialogue: 0,0:10:42.28,0:10:43.76,main,,0000,0000,0000,,Ale když zmokneš…
Dialogue: 0,0:10:43.76,0:10:45.60,main,,0000,0000,0000,,…tak si ani nemůžeš dát věci ven na usušení…
Dialogue: 0,0:10:45.60,0:10:47.16,main,,0000,0000,0000,,a proto jsem já déšť nikdy ráda neměla.
Dialogue: 0,0:10:47.88,0:10:50.08,main,,0000,0000,0000,,Proč ho máš ráda ty?
Dialogue: 0,0:10:50.08,0:10:54.88,main,,0000,0000,0000,,Protože zruší baseball a můžu se tak dívat na anime.
Dialogue: 0,0:10:54.88,0:10:56.08,main,,0000,0000,0000,,Chápu.
Dialogue: 0,0:10:56.08,0:10:57.80,main,,0000,0000,0000,,Zní to jako něco co bys řekla.
Dialogue: 0,0:10:57.80,0:11:00.52,main,,0000,0000,0000,,Teď mají mnohem víc uzavřených stadiónů.
Dialogue: 0,0:11:05.44,0:11:06.36,main,,0000,0000,0000,,Děje se něco?
Dialogue: 0,0:11:06.88,0:11:12.64,main,,0000,0000,0000,,V anime často vidíš idioty a génie chodit do stejné školy…
Dialogue: 0,0:11:13.16,0:11:14.28,main,,0000,0000,0000,,Hned toho nech.
Dialogue: 0,0:11:14.60,0:11:15.52,main,,0000,0000,0000,,Odpal!
Dialogue: 0,0:11:15.52,0:11:17.32,main,,0000,0000,0000,,Vypadni, pako!
Dialogue: 0,0:11:17.72,0:11:20.16,main,,0000,0000,0000,,Není to ve skutečnosti docela zvláštní?
Dialogue: 0,0:11:20.20,0:11:21.48,main,,0000,0000,0000,,Když si to tak vezmu…
Dialogue: 0,0:11:21.48,0:11:22.72,main,,0000,0000,0000,,…v některých anime…
Dialogue: 0,0:11:22.72,0:11:26.76,main,,0000,0000,0000,,…vypadá že je učiteli stejně jako žákům.
Dialogue: 0,0:11:26.76,0:11:28.04,main,,0000,0000,0000,,Nebo ještě míň.
Dialogue: 0,0:11:28.48,0:11:29.00,main,,0000,0000,0000,,Učitelko.
Dialogue: 0,0:11:29.52,0:11:30.72,main,,0000,0000,0000,,Ano, Ta*****na-san?
Dialogue: 0,0:11:31.16,0:11:34.80,main,,0000,0000,0000,,Svět se vždy zajímal o sledování…
Dialogue: 0,0:11:34.84,0:11:38.24,main,,0000,0000,0000,,…spojitosti mezi fyzickým a mentálním věkem.
Dialogue: 0,0:11:38.24,0:11:38.72,main,,0000,0000,0000,,Nechápu.
Dialogue: 0,0:11:39.20,0:11:41.88,main,,0000,0000,0000,,Není se na co ptát.
Dialogue: 0,0:11:41.88,0:11:44.84,main,,0000,0000,0000,,Ve skutečnosti, nebyl by student který je odlišný od ostatních…
Dialogue: 0,0:11:44.84,0:11:46.52,main,,0000,0000,0000,,…opravdu záhadný?
Dialogue: 0,0:11:48.60,0:11:49.40,main,,0000,0000,0000,,Jo.
Dialogue: 0,0:11:49.40,0:11:51.56,main,,0000,0000,0000,,To stoprocentně souhlasím.
Dialogue: 0,0:11:52.12,0:11:53.80,main,,0000,0000,0000,,Neměla bych to říkat…
Dialogue: 0,0:11:53.80,0:11:56.08,main,,0000,0000,0000,,…ale nevím jak ses na tuhle školu dostala ty, Konato.
Dialogue: 0,0:11:56.64,0:11:58.36,main,,0000,0000,0000,,Seš dobrá v dohánění věcí den předem…
Dialogue: 0,0:11:58.36,0:12:00.40,main,,0000,0000,0000,,…takže ses noc před tím neučila?
Dialogue: 0,0:12:05.52,0:12:06.96,main,,0000,0000,0000,,Cože?
Dialogue: 0,0:12:07.64,0:12:07.96,main,,0000,0000,0000,,Nah.
Dialogue: 0,0:12:07.96,0:12:10.28,main,,0000,0000,0000,,Za jednu noc bych to nestihla.
Dialogue: 0,0:12:10.76,0:12:12.16,main,,0000,0000,0000,,To mě vyděsilo.
Dialogue: 0,0:12:12.16,0:12:14.92,main,,0000,0000,0000,,Jsem ale překvapena, že stále studuješ.
Dialogue: 0,0:12:14.92,0:12:17.76,main,,0000,0000,0000,,To vyšel na DSko nějaký kvíz?
Dialogue: 0,0:12:17.76,0:12:20.08,main,,0000,0000,0000,,Otec stanovil určité podmínky.
Dialogue: 0,0:12:20.48,0:12:21.84,main,,0000,0000,0000,,D by znamenalo Pippin.
Dialogue: 0,0:12:21.84,0:12:23.44,main,,0000,0000,0000,,C by bylo 3DO.
Dialogue: 0,0:12:23.44,0:12:25.48,main,,0000,0000,0000,,B pak PS2.
Dialogue: 0,0:12:25.48,0:12:26.48,main,,0000,0000,0000,,Izuminin otče,
Dialogue: 0,0:12:26.48,0:12:28.80,main,,0000,0000,0000,,vy vážně víte jak jednat s vaší dcerou, co?
Dialogue: 0,0:12:29.24,0:12:30.32,main,,0000,0000,0000,,Zajímalo by mne…
Dialogue: 0,0:12:35.84,0:12:38.96,main,,0000,0000,0000,,Není vážná záhada jak to, že je Tsukasa ve stejné škole jako my?
Dialogue: 0,0:12:38.96,0:12:41.12,main,,0000,0000,0000,,Řekla to naprosto bez mrknutí!
Dialogue: 0,0:12:43.56,0:12:44.52,main,,0000,0000,0000,,Woo-hoo!
Dialogue: 0,0:12:43.96,0:12:45.64,mainup,,0000,0000,0000,,Vrať se za deset let!
Dialogue: 0,0:12:44.52,0:12:46.52,main,,0000,0000,0000,,Zase jsem vyhrála!
Dialogue: 0,0:12:45.68,0:12:47.52,main2,,0000,0000,0000,,Sakra! V bojovkách ji neporazím.
Dialogue: 0,0:12:47.52,0:12:48.72,main,,0000,0000,0000,,Strávila u toho mnohem víc času než já.
Dialogue: 0,0:12:48.80,0:12:51.60,main,,0000,0000,0000,,Klidně mě vyzvi v jiné hře, jestli chceš.
Dialogue: 0,0:12:52.88,0:12:54.20,main,,0000,0000,0000,,Jseš si jistá?
Dialogue: 0,0:12:54.20,0:12:55.64,main,,0000,0000,0000,,Tak potom…
Dialogue: 0,0:12:56.08,0:12:57.60,main,,0000,0000,0000,,Kvíz!
Dialogue: 0,0:12:58.20,0:12:59.28,main,,0000,0000,0000,,T-to ne…
Dialogue: 0,0:12:59.28,0:13:02.08,main,,0000,0000,0000,,Prohrála jsem s Konatou ve vědomostní soutěži?
Dialogue: 0,0:13:02.08,0:13:02.68,main,,0000,0000,0000,,Neuvěřitelné…
Dialogue: 0,0:13:03.92,0:13:05.04,main,,0000,0000,0000,,Neříkej mi…
Dialogue: 0,0:13:05.04,0:13:07.36,main,,0000,0000,0000,,…že si zapamatovala pořadí otázek?
Dialogue: 0,0:13:07.36,0:13:10.08,main,,0000,0000,0000,,Když jde o hry tak má neuvěřitelnou paměť.
Dialogue: 0,0:13:10.80,0:13:12.08,main,,0000,0000,0000,,Co si zahrajeme dál?
Dialogue: 0,0:13:12.08,0:13:13.24,main,,0000,0000,0000,,Nespěchej tak.
Dialogue: 0,0:13:14.08,0:13:15.92,main,,0000,0000,0000,,Heh? To je divné.
Dialogue: 0,0:13:15.92,0:13:19.16,main,,0000,0000,0000,,Neměla jsi Party hru?
Dialogue: 0,0:13:19.16,0:13:22.44,main,,0000,0000,0000,,Jak to, že si pamatuješ všechny hry co mám?
Dialogue: 0,0:13:22.44,0:13:25.12,main,,0000,0000,0000,,V té by jsi mě možná mohla porazit.
Dialogue: 0,0:13:26.24,0:13:27.56,main,,0000,0000,0000,,Hrozně to páchlo!
Dialogue: 0,0:13:28.36,0:13:31.36,main,,0000,0000,0000,,Kagami, sotva si snědla nějaké sušenky.
Dialogue: 0,0:13:31.36,0:13:33.36,main,,0000,0000,0000,,Obvykle je schramstneš.
Dialogue: 0,0:13:33.36,0:13:34.92,main,,0000,0000,0000,,Není tu důvod.
Dialogue: 0,0:13:35.40,0:13:36.24,main,,0000,0000,0000,,Já vím.
Dialogue: 0,0:13:36.24,0:13:38.48,main,,0000,0000,0000,,Hubneš na léto?
Dialogue: 0,0:13:38.48,0:13:40.16,main,,0000,0000,0000,,Tak to není.
Dialogue: 0,0:13:40.52,0:13:42.64,main,,0000,0000,0000,,Né že bys měla někoho komu se můžeš bez plavek ukazovat.
Dialogue: 0,0:13:42.64,0:13:43.40,main,,0000,0000,0000,,Sklapni!
Dialogue: 0,0:13:43.40,0:13:44.68,main,,0000,0000,0000,,To není tvůj problém!
Dialogue: 0,0:13:44.72,0:13:45.36,main,,0000,0000,0000,,Ah, jo.
Dialogue: 0,0:13:45.36,0:13:47.48,main,,0000,0000,0000,,Nenarodili jste se na Tanabatu?
Dialogue: 0,0:13:47.48,0:13:49.36,main,,0000,0000,0000,,Nemáte zanedlouho narozeniny?
Dialogue: 0,0:13:49.36,0:13:50.40,main,,0000,0000,0000,,Jak to že to víš?
Dialogue: 0,0:13:50.40,0:13:52.76,main,,0000,0000,0000,,Pamatuje si naše narozeniny.
Dialogue: 0,0:13:53.44,0:13:54.96,main,,0000,0000,0000,,Sedmnáct, co?
Dialogue: 0,0:13:54.96,0:13:57.12,main,,0000,0000,0000,,Rok před velkýma.
Dialogue: 0,0:13:57.12,0:13:58.88,main,,0000,0000,0000,,Co to znamená?
Dialogue: 0,0:13:58.88,0:14:00.44,main,,0000,0000,0000,,Rok před těma velkýma.
Dialogue: 0,0:14:00.44,0:14:03.32,main,,0000,0000,0000,,…se stávají nešťastné věci.
Dialogue: 0,0:14:04.32,0:14:06.48,main,,0000,0000,0000,,Jak se blíží naše narozeniny…
Dialogue: 0,0:14:06.48,0:14:07.96,main,,0000,0000,0000,,…začínám se bát…
Dialogue: 0,0:14:08.56,0:14:10.68,main,,0000,0000,0000,,…takže jsem koupila několik věcí pro štěstí…
Dialogue: 0,0:14:10.68,0:14:12.64,main,,0000,0000,0000,,…a nějaké magické kameny.
Dialogue: 0,0:14:13.08,0:14:14.84,main,,0000,0000,0000,,To zní jako zábava.
Dialogue: 0,0:14:14.84,0:14:16.08,main,,0000,0000,0000,,To je!
Dialogue: 0,0:14:16.08,0:14:17.80,main,,0000,0000,0000,,Nemohla jsem je přestat nakupovat.
Dialogue: 0,0:14:17.80,0:14:18.64,main,,0000,0000,0000,,A…
Dialogue: 0,0:14:18.64,0:14:20.32,main,,0000,0000,0000,,A já mám amulety proti hněvu.
Dialogue: 0,0:14:20.32,0:14:21.08,main,,0000,0000,0000,,Er, ne.
Dialogue: 0,0:14:21.08,0:14:22.16,main,,0000,0000,0000,,Amulety proti zlu.
Dialogue: 0,0:14:22.16,0:14:23.88,main,,0000,0000,0000,,Amulety proti zuření, aha.
Dialogue: 0,0:14:24.68,0:14:25.44,main,,0000,0000,0000,,Jak…
Dialogue: 0,0:14:25.44,0:14:29.32,main,,0000,0000,0000,,Jak by šlo tě ochránit před otěhotněním a dopravní nehodou zároveň.
Dialogue: 0,0:14:29.32,0:14:30.16,main,,0000,0000,0000,,Jo, jo.
Dialogue: 0,0:14:30.64,0:14:31.80,main,,0000,0000,0000,,Oh, jo.
Dialogue: 0,0:14:31.80,0:14:35.20,main,,0000,0000,0000,,Víte že na vaše narozeniny je den culíku?
Dialogue: 0,0:14:35.88,0:14:37.08,main,,0000,0000,0000,,To nevím.
Dialogue: 0,0:14:37.08,0:14:39.52,main,,0000,0000,0000,,Ale proč 7. červen?
Dialogue: 0,0:14:39.52,0:14:43.36,main,,0000,0000,0000,,Protože Vega z legendy o Tanabatě ho měla.
Dialogue: 0,0:14:43.36,0:14:45.36,main,,0000,0000,0000,,To zní zvláštně.
Dialogue: 0,0:14:45.36,0:14:48.80,main,,0000,0000,0000,,V Japonsku je Společenství Culíku…
Dialogue: 0,0:14:48.80,0:14:53.92,main,,0000,0000,0000,,…a každý rok udělují cenu celebritě o které si myslí, že by jí slušel culík.
Dialogue: 0,0:14:55.08,0:14:56.36,main,,0000,0000,0000,,Vážně…
Dialogue: 0,0:14:56.36,0:14:59.56,main,,0000,0000,0000,,Ale já si myslím, že s culíkem by vypadal dobře každý.
Dialogue: 0,0:15:00.04,0:15:01.16,main,,0000,0000,0000,,Jsi si jistá?
Dialogue: 0,0:15:07.48,0:15:10.56,main,,0000,0000,0000,,Moje vlasy jsou nejkratší…
Dialogue: 0,0:15:10.56,0:15:13.52,main,,0000,0000,0000,,Ale malej culík je dost roztomilý.
Dialogue: 0,0:15:13.52,0:15:15.64,main,,0000,0000,0000,,I tobě to sluší Kona-chan.
Dialogue: 0,0:15:15.64,0:15:17.80,main,,0000,0000,0000,,Mohla bys ho klidně nosit denně.
Dialogue: 0,0:15:18.32,0:15:19.36,main,,0000,0000,0000,,Myslíš?
Dialogue: 0,0:15:20.44,0:15:21.20,main,,0000,0000,0000,,Jasně.
Dialogue: 0,0:15:27.00,0:15:28.40,main,,0000,0000,0000,,S-ségra také…
Dialogue: 0,0:15:28.40,0:15:29.44,main,,0000,0000,0000,,Kagami je…
Dialogue: 0,0:15:29.44,0:15:30.60,main,,0000,0000,0000,,Jasně!
Dialogue: 0,0:15:30.60,0:15:31.36,main,,0000,0000,0000,,Rozumím.
Dialogue: 0,0:15:31.36,0:15:32.84,main,,0000,0000,0000,,Vrátím to jak to bylo!
Dialogue: 0,0:15:33.24,0:15:34.64,main,,0000,0000,0000,,Mužná jako válečník.
Dialogue: 0,0:15:34.64,0:15:35.72,main,,0000,0000,0000,,Cože to!
Dialogue: 0,0:15:37.48,0:15:41.24,main,,0000,0000,0000,,Nenosí opilci kolem hlav často zavázané kravaty?
Dialogue: 0,0:15:41.24,0:15:43.16,main,,0000,0000,0000,,Má to nějaký význam?
Dialogue: 0,0:15:43.16,0:15:44.40,main,,0000,0000,0000,,Oh, tohle?
Dialogue: 0,0:15:44.40,0:15:46.00,main,,0000,0000,0000,,Není to proto, že když si povolí kravatu…
Dialogue: 0,0:15:46.00,0:15:48.00,main,,0000,0000,0000,,…a snaží se jí přetáhnout přes hlavu…
Dialogue: 0,0:15:48.00,0:15:49.96,main,,0000,0000,0000,,…tak se zasekne?
Dialogue: 0,0:15:51.60,0:15:52.88,main,,0000,0000,0000,,V tom případě…
Dialogue: 0,0:15:52.88,0:15:56.40,main,,0000,0000,0000,,…je o dost divnější proč si sebou vždy nosí krabičku.
Dialogue: 0,0:15:56.40,0:15:57.72,main,,0000,0000,0000,,Krabičku?
Dialogue: 0,0:15:57.72,0:16:00.12,main,,0000,0000,0000,,Tu věc co sebou nosí Kato-chan z Drifters.
Dialogue: 0,0:16:00.76,0:16:02.32,main,,0000,0000,0000,,Nechápu…
Dialogue: 0,0:16:02.32,0:16:04.28,main,,0000,0000,0000,,Dnešní opilci…
Dialogue: 0,0:16:04.28,0:16:09.12,main,,0000,0000,0000,,…mají skoro vždy kolem hlav kravatu a nosí si krabičku.
Dialogue: 0,0:16:09.12,0:16:09.88,main,,0000,0000,0000,,Že?
Dialogue: 0,0:16:10.24,0:16:13.84,main,,0000,0000,0000,,Doufám, že Vega a Altair se letos potkají.
Dialogue: 0,0:16:14.32,0:16:16.12,main,,0000,0000,0000,,Sviť. Sviť. Sviť.
Dialogue: 0,0:16:16.12,0:16:18.88,main,,0000,0000,0000,,Doufám, že ty tvoje amulety dokážou zázrak.
Dialogue: 0,0:16:18.88,0:16:20.24,main,,0000,0000,0000,,Starej se o sebe.
Dialogue: 0,0:16:21.32,0:16:24.32,main,,0000,0000,0000,,Na počítačovém festivalu budou všechny varianty QUO kart.
Dialogue: 0,0:16:24.76,0:16:25.60,main,,0000,0000,0000,,Oh, jo…
Dialogue: 0,0:16:25.60,0:16:27.72,main,,0000,0000,0000,,Tys je vlastně vždycky sbírala…
Dialogue: 0,0:16:27.72,0:16:29.20,main,,0000,0000,0000,,…ale použila si je někdy?
Dialogue: 0,0:16:29.60,0:16:30.60,main,,0000,0000,0000,,To ne.
Dialogue: 0,0:16:31.44,0:16:32.80,main,,0000,0000,0000,,Tak proč se snažit?
Dialogue: 0,0:16:33.24,0:16:35.40,main,,0000,0000,0000,,Není to přirozené, chtít každou věc…
Dialogue: 0,0:16:35.40,0:16:37.40,main,,0000,0000,0000,,… související s anime které máš ráda?
Dialogue: 0,0:16:37.40,0:16:39.32,main,,0000,0000,0000,,Jako fanoušek. Jako sběratel.
Dialogue: 0,0:16:39.32,0:16:40.36,main,,0000,0000,0000,,Ne tak docela…
Dialogue: 0,0:16:40.36,0:16:41.68,main,,0000,0000,0000,,…pokud je nikdy nepoužiješ…
Dialogue: 0,0:16:41.68,0:16:43.32,main,,0000,0000,0000,,Nemůžeš je použít!
Dialogue: 0,0:16:43.32,0:16:45.04,main,,0000,0000,0000,,Musíš se o ně dobře starat!
Dialogue: 0,0:16:45.04,0:16:49.16,main,,0000,0000,0000,,Vážně mě štve když vidím jak je někdo používá.
Dialogue: 0,0:16:49.16,0:16:51.64,main,,0000,0000,0000,,Tyhle věci se nesmí používat!
Dialogue: 0,0:16:51.64,0:16:55.44,main,,0000,0000,0000,,Tím spíš je prodat nebo vydražit.
Dialogue: 0,0:16:55.44,0:16:57.04,main,,0000,0000,0000,,Pokud je použiješ…
Dialogue: 0,0:16:57.04,0:16:58.88,main,,0000,0000,0000,,…tak prohraješ jako fanoušek i jako sběratel!
Dialogue: 0,0:16:59.88,0:17:01.48,main,,0000,0000,0000,,Nemám co říct, když se tak rozjedeš.
Dialogue: 0,0:17:01.92,0:17:05.40,main,,0000,0000,0000,,V mém případě potřebuji alespoň tři.
Dialogue: 0,0:17:05.40,0:17:07.40,main,,0000,0000,0000,,Jednu k zachování.
Dialogue: 0,0:17:07.40,0:17:09.76,main,,0000,0000,0000,,Mám na ni speciální krabičku.
Dialogue: 0,0:17:09.80,0:17:12.96,main,,0000,0000,0000,,Pak jednu na kterou se občas podívám.
Dialogue: 0,0:17:12.96,0:17:13.88,main,,0000,0000,0000,,A na co tu poslední?
Dialogue: 0,0:17:13.88,0:17:14.60,main,,0000,0000,0000,,Extra.
Dialogue: 0,0:17:14.60,0:17:15.52,main,,0000,0000,0000,,Na co?
Dialogue: 0,0:17:15.52,0:17:21.04,main,,0000,0000,0000,,Nechceš vždy dva nebo tři kusy od něčeho co máš ráda?
Dialogue: 0,0:17:23.56,0:17:24.20,main,,0000,0000,0000,,Jo, jo.
Dialogue: 0,0:17:24.20,0:17:25.04,main,,0000,0000,0000,,Ty vlastně ne.
Dialogue: 0,0:17:25.04,0:17:26.64,main,,0000,0000,0000,,To jen já jsem geek.
Dialogue: 0,0:17:26.64,0:17:27.44,main,,0000,0000,0000,,Podivín.
Dialogue: 0,0:17:27.44,0:17:28.12,main,,0000,0000,0000,,Ne.
Dialogue: 0,0:17:28.12,0:17:31.16,main,,0000,0000,0000,,Celkem rozumím co tím myslíš.
Dialogue: 0,0:17:32.16,0:17:34.48,main,,0000,0000,0000,,Čekala bych, že Tsukasa je populární u kluků…
Dialogue: 0,0:17:34.48,0:17:36.44,main,,0000,0000,0000,,…ale nikdy jsem nic takového neslyšela.
Dialogue: 0,0:17:37.36,0:17:40.16,main,,0000,0000,0000,,Tsukasa je docela normální a moc nevyčnívá.
Dialogue: 0,0:17:41.12,0:17:43.64,main,,0000,0000,0000,,Není to tím, že má šílenou sestru, která ji střeží jako orel?
Dialogue: 0,0:17:43.64,0:17:44.84,main,,0000,0000,0000,,Řeklas něco?
Dialogue: 0,0:17:45.36,0:17:47.32,main,,0000,0000,0000,,Nejsem si jistá, jestli jsem úplně normální…
Dialogue: 0,0:17:47.32,0:17:49.72,main,,0000,0000,0000,,…je to spíš tím že Kona-chan a Yuki-chan jsou vážně unikátní.
Dialogue: 0,0:17:50.16,0:17:53.24,main,,0000,0000,0000,,Miyuki zná vše možné.
Dialogue: 0,0:17:53.24,0:17:54.16,main,,0000,0000,0000,,A je pěkná.
Dialogue: 0,0:17:54.24,0:17:55.16,main,,0000,0000,0000,,Má dobrou postavu.
Dialogue: 0,0:17:55.16,0:17:56.36,main,,0000,0000,0000,,Nosí brýle.
Dialogue: 0,0:17:56.36,0:17:57.40,main,,0000,0000,0000,,Bojí se zubaře.
Dialogue: 0,0:17:57.40,0:17:57.92,main,,0000,0000,0000,,Je to nešika.
Dialogue: 0,0:17:57.92,0:17:59.08,main,,0000,0000,0000,,Neví si rady ve spoustě věcí.
Dialogue: 0,0:17:58.52,0:18:00.56,main2,,0000,0000,0000,,Hej. Tos trochu vykolejila.
Dialogue: 0,0:18:01.20,0:18:04.24,main,,0000,0000,0000,,Ale je pravda, že jsem o Miyuki nic neslyšela.
Dialogue: 0,0:18:04.24,0:18:05.72,main,,0000,0000,0000,,Možná je to proto, že je tak pěkná…
Dialogue: 0,0:18:05.72,0:18:07.16,main,,0000,0000,0000,,…že se ji každý bojí oslovit?
Dialogue: 0,0:18:07.16,0:18:08.68,main,,0000,0000,0000,,To pochybuji.
Dialogue: 0,0:18:08.68,0:18:09.52,main,,0000,0000,0000,,Je milá…
Dialogue: 0,0:18:09.52,0:18:10.88,main,,0000,0000,0000,,…a je ostatním hodně nápomocná.
Dialogue: 0,0:18:10.88,0:18:12.12,main,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:18:12.12,0:18:14.44,main,,0000,0000,0000,,Možná ji zve spousta kluků.
Dialogue: 0,0:18:14.44,0:18:16.16,main,,0000,0000,0000,,Ale ona je odmítá?
Dialogue: 0,0:18:16.20,0:18:17.52,main,,0000,0000,0000,,Odmítá, heh…
Dialogue: 0,0:18:17.52,0:18:18.08,main,,0000,0000,0000,,Nah.
Dialogue: 0,0:18:18.08,0:18:20.68,main,,0000,0000,0000,,Možná si neuvědomuje, že ji zvou ven na rande.
Dialogue: 0,0:18:20.68,0:18:22.32,main,,0000,0000,0000,,To je možné.
Dialogue: 0,0:18:22.32,0:18:24.32,main,,0000,0000,0000,,Yuki-chan může být v tomto trochu nemotorná…
Dialogue: 0,0:18:24.32,0:18:26.68,main,,0000,0000,0000,,…takže nepřímý přístup nemusí fungovat.
Dialogue: 0,0:18:26.68,0:18:30.20,main,,0000,0000,0000,,Páni, kluci z naší třídy jsou kničemu.
Dialogue: 0,0:18:30.20,0:18:32.60,main,,0000,0000,0000,,To ani jeden nemá odvahu…
Dialogue: 0,0:18:32.60,0:18:35.44,main,,0000,0000,0000,,…aby řekl co cítí k Miyuki?
Dialogue: 0,0:18:35.44,0:18:36.76,main,,0000,0000,0000,,Asi ne…
Dialogue: 0,0:18:36.76,0:18:39.80,main,,0000,0000,0000,,Ve třídě nemáme moc maniaků.
Dialogue: 0,0:18:40.28,0:18:43.44,main,,0000,0000,0000,,Miyukin typ je největší obětí maniaků a úchlů.
Dialogue: 0,0:18:45.16,0:18:46.60,main,,0000,0000,0000,,Maniaků, heh?
Dialogue: 0,0:18:47.64,0:18:48.56,main,,0000,0000,0000,,Že?
Dialogue: 0,0:18:48.56,0:18:49.16,main,,0000,0000,0000,,Oh.
Dialogue: 0,0:18:49.16,0:18:51.40,main,,0000,0000,0000,,Už je tak pozdě.
Dialogue: 0,0:18:51.40,0:18:53.28,main,,0000,0000,0000,,Budu muset jít.
Dialogue: 0,0:18:53.60,0:18:54.36,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:18:54.80,0:18:58.44,main,,0000,0000,0000,,Užijte si to tu.
Dialogue: 0,0:18:58.96,0:19:00.76,main,,0000,0000,0000,,Tak já půjdu.
Dialogue: 0,0:19:01.28,0:19:02.48,main,,0000,0000,0000,,Zatím!
Dialogue: 0,0:19:02.48,0:19:03.72,main,,0000,0000,0000,,Pa-pa.
Dialogue: 0,0:19:05.76,0:19:06.72,main,,0000,0000,0000,,Nicméně…
Dialogue: 0,0:19:06.72,0:19:10.16,main,,0000,0000,0000,,Jak jsem vám říkala ten příběh…
Dialogue: 0,0:19:11.28,0:19:12.04,main,,0000,0000,0000,,O-Oh?
Dialogue: 0,0:19:12.80,0:19:13.96,main,,0000,0000,0000,,M-Moc se omlouvám!
Dialogue: 0,0:19:14.20,0:19:15.32,main,,0000,0000,0000,,Jaj!
Dialogue: 0,0:19:15.80,0:19:17.16,main,,0000,0000,0000,,Vážně se omlouvám.
Dialogue: 0,0:19:17.16,0:19:18.68,main,,0000,0000,0000,,To nic…
Dialogue: 0,0:19:18.72,0:19:20.40,main,,0000,0000,0000,,To se stane vždycky, co?
Dialogue: 0,0:19:20.40,0:19:22.68,main,,0000,0000,0000,,Já to udělala včera.
Dialogue: 0,0:19:22.68,0:19:23.68,main,,0000,0000,0000,,Já také.
Dialogue: 0,0:19:24.08,0:19:26.60,main,,0000,0000,0000,,Každý to udělal alespoň jednou.
Dialogue: 0,0:19:26.60,0:19:27.80,main,,0000,0000,0000,,Že, Konato?
Dialogue: 0,0:19:27.80,0:19:28.52,main,,0000,0000,0000,,Já ne.
Dialogue: 0,0:19:29.20,0:19:31.92,main,,0000,0000,0000,,To je to co bys nazvala schopnost rozjet lidi.
Dialogue: 0,0:19:32.64,0:19:33.40,main,,0000,0000,0000,,Miyuki-san.
Dialogue: 0,0:19:33.92,0:19:34.60,main,,0000,0000,0000,,Dobrá práce!
Dialogue: 0,0:19:34.64,0:19:37.64,copyrights,,0000,0000,0000,,{\pos(185,50)}Překlad od paralaXe.
Dialogue: 0,0:19:34.64,0:19:37.64,copyrights,,0000,0000,0000,,{\pos(199,76)}Domovská stránka - www.otakogu.cz
Dialogue: 0,0:19:40.48,0:19:41.60,main,,0000,0000,0000,,Hodně štěstí!
Dialogue: 0,0:19:41.96,0:19:42.68,main,,0000,0000,0000,,Ahoj!
Dialogue: 0,0:19:42.68,0:19:45.68,main,,0000,0000,0000,,Dnes je tu další žůžovní epizoda našeho Lucky Channelu!
Dialogue: 0,0:19:46.36,0:19:48.44,main,,0000,0000,0000,,Já jsme váš průvodce, Akira Kogami.
Dialogue: 0,0:19:48.44,0:19:48.88,main,,0000,0000,0000,,A…
Dialogue: 0,0:19:48.88,0:19:49.76,main,,0000,0000,0000,,Uh.
Dialogue: 0,0:19:49.76,0:19:52.52,main,,0000,0000,0000,,Já jsem Akira-samin asistent, Minoru Shiraishi.
Dialogue: 0,0:19:52.52,0:19:53.12,main,,0000,0000,0000,,Je mi ctí.
Dialogue: 0,0:19:53.48,0:19:54.44,main,,0000,0000,0000,,Okaj, okaj.
Dialogue: 0,0:19:54.44,0:19:56.52,main,,0000,0000,0000,,Tak tohle máme za sebou…
Dialogue: 0,0:19:56.52,0:19:57.84,main,,0000,0000,0000,,Ta-da!
Dialogue: 0,0:19:57.84,0:19:59.44,main,,0000,0000,0000,,Dnes již po třetí!
Dialogue: 0,0:19:59.44,0:20:01.04,main,,0000,0000,0000,,Co si o tom myslíš, Shiraishi-sane?
Dialogue: 0,0:20:01.04,0:20:02.20,main,,0000,0000,0000,,Už si na to zvykáš?
Dialogue: 0,0:20:02.20,0:20:02.88,main,,0000,0000,0000,,No…
Dialogue: 0,0:20:02.88,0:20:04.12,main,,0000,0000,0000,,Ještě ne…
Dialogue: 0,0:20:04.12,0:20:04.64,main,,0000,0000,0000,,Omlouvám se.
Dialogue: 0,0:20:05.88,0:20:10.36,main,,0000,0000,0000,,Cítím, že se mi Shiraishi-san ještě neotevřel.
Dialogue: 0,0:20:11.00,0:20:13.20,main,,0000,0000,0000,,A vypadá to, že sedíme dál od sebe než normálně.
Dialogue: 0,0:20:13.72,0:20:16.08,main,,0000,0000,0000,,Tomuhle říkají emoční odstup?
Dialogue: 0,0:20:16.08,0:20:16.56,main,,0000,0000,0000,,Fňuk.
Dialogue: 0,0:20:17.20,0:20:19.16,main,,0000,0000,0000,,T-T-To není pravda!
Dialogue: 0,0:20:19.76,0:20:20.56,main,,0000,0000,0000,,Okaj!
Dialogue: 0,0:20:20.56,0:20:23.20,main,,0000,0000,0000,,Jdeme na to!
Dialogue: 0,0:20:23.20,0:20:23.88,main,,0000,0000,0000,,Um…
Dialogue: 0,0:20:23.88,0:20:26.84,main,,0000,0000,0000,,Dnes máme něco co budou fanoušci milovat!
Dialogue: 0,0:20:26.84,0:20:27.84,main,,0000,0000,0000,,Hádejte co?
Dialogue: 0,0:20:27.84,0:20:31.00,main,,0000,0000,0000,,Budeme dělat naše první představení členů normálního natáčení!
Dialogue: 0,0:20:31.00,0:20:31.84,main,,0000,0000,0000,,Da! Da-da-dum!
Dialogue: 0,0:20:32.24,0:20:33.12,main,,0000,0000,0000,,Okaj, uh…
Dialogue: 0,0:20:33.12,0:20:35.36,main,,0000,0000,0000,,Dnes vám představíme Miyuki Takara-san.
Dialogue: 0,0:20:35.76,0:20:36.92,main,,0000,0000,0000,,Tak se podíváme…
Dialogue: 0,0:20:36.92,0:20:41.24,main,,0000,0000,0000,,Reprezentuje třídu, dobrá výchova, zna-
Dialogue: 0,0:20:41.24,0:20:42.28,main,,0000,0000,0000,,Znalosti.
Dialogue: 0,0:20:42.28,0:20:43.36,main,,0000,0000,0000,,Znalosti…
Dialogue: 0,0:20:43.36,0:20:45.92,main,,0000,0000,0000,,…ve všech možných směrech. Perfektní člověk.
Dialogue: 0,0:20:45.92,0:20:46.80,main,,0000,0000,0000,,Shiraishi-sane.
Dialogue: 0,0:20:46.80,0:20:49.00,main,,0000,0000,0000,,Ty znáš Miyuki-san, že?
Dialogue: 0,0:20:49.00,0:20:50.76,main,,0000,0000,0000,,Ale samozřejmě!
Dialogue: 0,0:20:50.76,0:20:52.40,main,,0000,0000,0000,,Je hrozně skvělá, že?
Dialogue: 0,0:20:52.40,0:20:54.00,main,,0000,0000,0000,,Ty její brýle!
Dialogue: 0,0:20:54.00,0:20:55.72,main,,0000,0000,0000,,A taky je hrozně krásná.
Dialogue: 0,0:20:55.72,0:20:57.16,main,,0000,0000,0000,,Ten její pohled…
Dialogue: 0,0:20:57.16,0:20:59.96,main,,0000,0000,0000,,…čekaly byste, že je nepřístupná…
Dialogue: 0,0:20:59.96,0:21:02.80,main,,0000,0000,0000,,…ale ve skutečnosti je velice přátelská…
Dialogue: 0,0:21:02.80,0:21:05.72,main,,0000,0000,0000,,Přála bych si abys tak moc nemluvil v radio show…
Dialogue: 0,0:21:07.04,0:21:09.20,main,,0000,0000,0000,,Když jde o Miyuki tak nemůžeš přestat mluvit.
Dialogue: 0,0:21:09.96,0:21:10.68,main,,0000,0000,0000,,Ne!
Dialogue: 0,0:21:10.68,0:21:11.96,main,,0000,0000,0000,,Tak to není…
Dialogue: 0,0:21:12.00,0:21:13.04,main,,0000,0000,0000,,No, jo.
Dialogue: 0,0:21:13.04,0:21:14.88,main,,0000,0000,0000,,Takže Miyuki Takara-san?
Dialogue: 0,0:21:14.92,0:21:16.28,main,,0000,0000,0000,,Pěkná tvářička.
Dialogue: 0,0:21:16.28,0:21:17.44,main,,0000,0000,0000,,Velké prsa.
Dialogue: 0,0:21:17.44,0:21:18.92,main,,0000,0000,0000,,A další hezké části.
Dialogue: 0,0:21:19.44,0:21:21.72,main,,0000,0000,0000,,To má ale štěstí, že je tak pěkná, co?
Dialogue: 0,0:21:20.16,0:21:21.72,main2,,0000,0000,0000,,Akiro-sama, to je trochu…
Dialogue: 0,0:21:21.76,0:21:23.60,main,,0000,0000,0000,,Maniaci ji milují.
Dialogue: 0,0:21:23.60,0:21:24.32,main,,0000,0000,0000,,Ale šak víš…
Dialogue: 0,0:21:23.60,0:21:24.32,main2,,0000,0000,0000,,Miniaci, to je trochu…
Dialogue: 0,0:21:24.32,0:21:27.40,main,,0000,0000,0000,,Pokud de o maniaky, tak já mám obrovský úspěch u pedofilů…
Dialogue: 0,0:21:27.40,0:21:29.08,main,,0000,0000,0000,,To je špatné!
Dialogue: 0,0:21:29.08,0:21:30.96,main,,0000,0000,0000,,Je čas se rozloučit!
Dialogue: 0,0:21:31.08,0:21:31.96,main,,0000,0000,0000,,A tak…
Dialogue: 0,0:21:31.96,0:21:38.84,main,,0000,0000,0000,,Posílejte nám vaše komentáře a příspěvky k show prosím!
Dialogue: 0,0:21:39.28,0:21:40.96,main,,0000,0000,0000,,Těšte se na příště!
Dialogue: 0,0:21:40.96,0:21:42.44,main,,0000,0000,0000,,Ahoj!
Dialogue: 0,0:21:43.64,0:21:45.32,main,,0000,0000,0000,,Tak jaká je ve skutečnosti?
Dialogue: 0,0:21:45.72,0:21:46.80,main,,0000,0000,0000,,Naprosto ta samá.
Dialogue: 0,0:21:46.80,0:21:47.84,main,,0000,0000,0000,,To ne!
Dialogue: 0,0:21:47.84,0:21:51.00,main,,0000,0000,0000,,Nevykouří o přestávce v zákulisí krabičku cigaret?
Dialogue: 0,0:21:51.00,0:21:52.96,main,,0000,0000,0000,,To se neboj. Není jako ty.
Dialogue: 0,0:21:57.00,0:21:58.48,main,,0000,0000,0000,,To byla zábava!
Dialogue: 0,0:21:58.48,0:22:00.12,main,,0000,0000,0000,,Zvláštní písnička.
Dialogue: 0,0:22:00.12,0:22:03.28,main,,0000,0000,0000,,Ráda konečně slyším písničku kterou si všichni můžou vychutnat.
Dialogue: 0,0:22:03.84,0:22:05.80,main,,0000,0000,0000,,Znáš další, Kagami.
Dialogue: 0,0:22:06.36,0:22:08.12,main,,0000,0000,0000,,Uhádneš to podle intra.
Dialogue: 0,0:22:08.12,0:22:10.32,main,,0000,0000,0000,,Je to další bláznivost?
Dialogue: 0,0:22:14.24,0:22:15.36,main,,0000,0000,0000,,Cože to je za písničku?
Dialogue: 0,0:22:15.36,0:22:16.64,main,,0000,0000,0000,,Nevím…
Dialogue: 0,0:22:16.64,0:22:17.28,main,,0000,0000,0000,,Kagami.
Dialogue: 0,0:22:17.28,0:22:18.28,main,,0000,0000,0000,,Odpověď je…
Dialogue: 0,0:22:19.20,0:22:20.88,main,,0000,0000,0000,,Je to z FMP. FMP. (Full Metal Panic)
Dialogue: 0,0:22:21.80,0:22:23.92,main,,0000,0000,0000,,A je to Kagamina oblíbená…
Dialogue: 0,0:22:23.92,0:22:25.00,main,,0000,0000,0000,,Fumoffu!
Dialogue: 0,0:22:25.00,0:22:26.12,main,,0000,0000,0000,,Vážně…
Dialogue: 0,0:22:26.12,0:22:28.00,main,,0000,0000,0000,,Já jen četla mangu…
Dialogue: 0,0:22:28.00,0:22:29.40,main,,0000,0000,0000,,…a viděla jen kousek.
Dialogue: 0,0:22:29.40,0:22:39.36,mainup,,0000,0000,0000,,Řekni dne kdy slzy začnou téct…
Dialogue: 0,0:22:33.20,0:22:34.72,main,,0000,0000,0000,,Jsi vážně dobrá Kona-chan!
Dialogue: 0,0:22:34.72,0:22:37.00,main,,0000,0000,0000,,Zníš jako dospělá.
Dialogue: 0,0:22:37.00,0:22:39.36,main,,0000,0000,0000,,To to jen imituje?
Dialogue: 0,0:22:39.52,0:22:46.16,mainup,,0000,0000,0000,,…chci tě mít u sebe.
Dialogue: 0,0:22:42.84,0:22:43.44,main,,0000,0000,0000,,Tak?
Dialogue: 0,0:22:43.44,0:22:45.44,main,,0000,0000,0000,,Co budete zpívat?
Dialogue: 0,0:22:46.12,0:22:47.96,main,,0000,0000,0000,,Zajímalo by mě, jestli je tady něco co dokážu zazpívat…
Dialogue: 0,0:22:47.96,0:22:50.08,main,,0000,0000,0000,,Neznám moc písniček…
Dialogue: 0,0:22:49.12,0:22:58.76,mainup,,0000,0000,0000,,Těžké časy přijdou jistě…
Dialogue: 0,0:22:50.08,0:22:52.76,main,,0000,0000,0000,,To máš fuk.
Dialogue: 0,0:22:52.76,0:22:55.44,main,,0000,0000,0000,,Tak zpívej co chceš!
Dialogue: 0,0:22:55.44,0:22:56.24,main,,0000,0000,0000,,Um…
Dialogue: 0,0:22:56.24,0:22:57.80,main,,0000,0000,0000,,Tak se podívám.
Dialogue: 0,0:22:57.80,0:23:00.04,main,,0000,0000,0000,,Nejsem si moc jistá mým zpíváním…
Dialogue: 0,0:22:58.76,0:23:07.52,mainup,,0000,0000,0000,,Čím víc se snažíme, tím víc ztrácíme potuchy o odpovědi.
Dialogue: 0,0:23:00.04,0:23:01.48,main,,0000,0000,0000,,To je v pohodě.
Dialogue: 0,0:23:01.48,0:23:03.08,main,,0000,0000,0000,,Slyšíme tě jen my.
Dialogue: 0,0:23:03.08,0:23:04.60,main,,0000,0000,0000,,Jen v klidu.
Dialogue: 0,0:23:04.60,0:23:06.12,main,,0000,0000,0000,,Vážně?
Dialogue: 0,0:23:06.12,0:23:07.08,main,,0000,0000,0000,,V tom případě…
Dialogue: 0,0:23:08.36,0:23:16.36,mainup,,0000,0000,0000,,Pokud vidíš duhy po dešti nad naším městem…
Dialogue: 0,0:23:16.72,0:23:20.16,mainup,,0000,0000,0000,,Tak se odhodlej.
Dialogue: 0,0:23:20.28,0:23:24.40,mainup,,0000,0000,0000,,Něco má právě začít…
Dialogue: 0,0:23:24.40,0:23:26.40,mainup,,0000,0000,0000,,Další epizoda
Dialogue: 0,0:23:26.40,0:23:28.40,mainup,,0000,0000,0000,,


