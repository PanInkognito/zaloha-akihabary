Dialogue: 0,0:00:05.60,0:00:06.94,Default,Both,0000,0000,0000,,Dobrou chuť.
Dialogue: 0,0:00:08.84,0:00:11.09,Default,Takeru,0000,0000,0000,,Mňauko, podala bys mi sojovku?
Dialogue: 0,0:00:12.29,0:00:13.15,Default,Nya,0000,0000,0000,,Na.
Dialogue: 0,0:00:13.40,0:00:15.93,Default,Takeru,0000,0000,0000,,To je sezamovej olej...
Dialogue: 0,0:00:16.59,0:00:19.02,Default,Nya,0000,0000,0000,,Jejda, chybka. Promiň!
Dialogue: 0,0:00:19.02,0:00:20.95,Default,Takeru,0000,0000,0000,,V pohodě. Skočím pro ni.
Dialogue: 0,0:00:27.46,0:00:30.96,soy sauce,Text,0000,0000,0000,,Sojová omáčka
Dialogue: 0,0:00:27.46,0:00:30.96,sesame oil,Text,0000,0000,0000,,Sezamový olej
Dialogue: 0,0:00:31.90,0:00:32.94,Default,Takeru,0000,0000,0000,,Uri...
Dialogue: 0,0:00:52.01,0:00:54.44,Default,Nya,0000,0000,0000,,Takeru je smutnej.
Dialogue: 0,0:00:56.17,0:00:57.74,Default,Both,0000,0000,0000,,Děkuji za jídlo.
Dialogue: 0,0:01:00.36,0:01:01.39,Default,Takeru,0000,0000,0000,,Odcházím.
Dialogue: 0,0:01:01.39,0:01:02.70,Default,Nya,0000,0000,0000,,Měj se hezky!
Dialogue: 0,0:01:11.87,0:01:12.91,Default,Nya,0000,0000,0000,,Takeru!
Dialogue: 0,0:01:14.06,0:01:15.26,Default,Takeru,0000,0000,0000,,Mňauko?
Dialogue: 0,0:01:15.56,0:01:17.99,Default,Nya,0000,0000,0000,,Byla jsem poblíž,\Ntak jsem za tebou přišla!
Dialogue: 0,0:01:17.99,0:01:19.32,Default,Nya,0000,0000,0000,,Jo a...
Dialogue: 0,0:01:20.27,0:01:21.94,Default,Nya,0000,0000,0000,,Zapomněl sis tohle.
Dialogue: 0,0:01:21.94,0:01:23.51,Default,Takeru,0000,0000,0000,,Děkuju...
Dialogue: 0,0:01:24.94,0:01:26.07,Default,Takeru,0000,0000,0000,,Co to je?
Dialogue: 0,0:01:27.41,0:01:28.62,Default,Nya,0000,0000,0000,,Otevřu to!
Dialogue: 0,0:01:27.70,0:01:28.39,Default,Takeru,0000,0000,0000,,Poč—
Dialogue: 0,0:01:29.32,0:01:30.21,Default,Takeru,0000,0000,0000,,Hej!
Dialogue: 0,0:01:33.08,0:01:35.90,Default,Nya,0000,0000,0000,,Vyděsilo mě to! Všude se to rozstříklo!
Dialogue: 0,0:01:35.90,0:01:37.78,Default,Nya,0000,0000,0000,,Co to je?!
Dialogue: 0,0:01:36.15,0:01:37.78,Default,Takeru,0000,0000,0000,,Bože...
Dialogue: 0,0:01:37.78,0:01:40.51,Default,Nya,0000,0000,0000,,O tomhle mi nikdo neřekl!
Dialogue: 0,0:01:44.54,0:01:47.04,Default,Nya,0000,0000,0000,,Takeru, konečně ses usmál.
Dialogue: 0,0:02:09.14,0:02:13.69,Etotama OP Splash,Title,0000,0000,0000,,{\fad(232,1)}E{\c&H3EE294&}TO{\c&HC2D94D&}TA{\c&HD86181&}MA
Dialogue: 0,0:03:34.02,0:03:37.27,Etotama Ep Title,Title,0000,0000,0000,,{\fad(469,1)}Králičí oči, králičí uši
Dialogue: 0,0:03:37.27,0:03:38.97,Default,Someone,0000,0000,0000,,Omlouvám se za čekání.
Dialogue: 0,0:03:37.67,0:03:39.59,Default,Someone,0000,0000,0000,,Děkujeme.
Dialogue: 0,0:03:40.30,0:03:41.30,Default,Takeru,0000,0000,0000,,Díky.
Dialogue: 0,0:03:41.30,0:03:43.63,Default,Key,0000,0000,0000,,Hned ti donesu dortík.
Dialogue: 0,0:03:47.33,0:03:48.41,Default,Uri,0000,0000,0000,,Omlouvám se za zdržení.
Dialogue: 0,0:03:48.66,0:03:50.62,Default,Uri,0000,0000,0000,,Tady je váš dortík, jo, jo.
Dialogue: 0,0:03:50.86,0:03:52.00,Default,Takeru,0000,0000,0000,,"Jo, jo"?
Dialogue: 0,0:04:00.73,0:04:02.20,Default,Nya,0000,0000,0000,,Horký!
Dialogue: 0,0:04:00.73,0:04:02.64,Default,Takeru,0000,0000,0000,,Počkat! Uri, ty žiješ!
Dialogue: 0,0:04:02.20,0:04:04.33,Default,Nya,0000,0000,0000,,Počkej, není! Je to vlažný!
Dialogue: 0,0:04:02.64,0:04:05.43,Default,Takeru,0000,0000,0000,,Opravdu?! Nevěřím tomu!
Dialogue: 0,0:04:04.33,0:04:05.43,Default,Nya,0000,0000,0000,,Tys to udělal schválně!
Dialogue: 0,0:04:05.43,0:04:07.93,opening at,Text,0000,0000,0000,,Dnes otevíráme\Nv 11:00 !
Dialogue: 0,0:04:05.43,0:04:07.93,closed pls com again,Text,0000,0000,0000,,Pro dnešek zavřeno\NProsím, přijďte zas.
Dialogue: 0,0:04:10.52,0:04:11.27,Default,Takeru,0000,0000,0000,,Takže...
Dialogue: 0,0:04:11.62,0:04:13.64,Default,Takeru,0000,0000,0000,,Co to má všechno znamenat?
Dialogue: 0,0:04:14.14,0:04:15.52,Default,Uri,0000,0000,0000,,Co myslíš?
Dialogue: 0,0:04:15.52,0:04:18.77,Default,Mo,0000,0000,0000,,Ano! To vůbec není špatné!
Dialogue: 0,0:04:19.07,0:04:21.40,Default,Takeru,0000,0000,0000,,Vždyť ses rozpustila do světla a umřela!
Dialogue: 0,0:04:21.40,0:04:23.15,Default,Takeru,0000,0000,0000,,To jste si tu porážku s Chu vymyslely?!
Dialogue: 0,0:04:23.38,0:04:24.69,Default,Nya,0000,0000,0000,,Nevymyslely.
Dialogue: 0,0:04:24.69,0:04:26.54,Default,Uri,0000,0000,0000,,Já vážně zemřela.
Dialogue: 0,0:04:26.54,0:04:28.54,Default,Nya,0000,0000,0000,,Ale vrátila se.
Dialogue: 0,0:04:28.77,0:04:29.74,Default,Takeru,0000,0000,0000,,Vrátila?
Dialogue: 0,0:04:29.74,0:04:32.49,Default,Uri,0000,0000,0000,,Ano. I když zemřeme,
Dialogue: 0,0:04:32.49,0:04:35.67,Default,Uri,0000,0000,0000,,tak se po určité době zase vrátíme.
Dialogue: 0,0:04:35.67,0:04:37.15,Default,Takeru,0000,0000,0000,,Co je to za systém?
Dialogue: 0,0:04:37.15,0:04:39.21,Default,Nya,0000,0000,0000,,Je to evidentní.
Dialogue: 0,0:04:39.47,0:04:42.89,Default,Key,0000,0000,0000,,{\i1}Evidentní? {\i0}Na tohle jsi taky zapomněla, Mňauko?
Dialogue: 0,0:04:44.00,0:04:45.20,Default,Piyo,0000,0000,0000,,Blbá kočka...
Dialogue: 0,0:04:45.20,0:04:47.96,Default,Inu,0000,0000,0000,,Uri, vrátila ses před třemi dny, že?
Dialogue: 0,0:04:47.96,0:04:50.64,Default,Usa,0000,0000,0000,,Proč jsi nešla rovnou k Takerovi?
Dialogue: 0,0:04:51.13,0:04:53.36,Default,Uri,0000,0000,0000,,Nejdřív jsem dostala přednášku
Dialogue: 0,0:04:53.36,0:04:57.36,Default,Uri,0000,0000,0000,,od výkonného výboru ETM12\Nza zasahování do boje.
Dialogue: 0,0:04:57.36,0:05:01.09,Default,Uri,0000,0000,0000,,Omlouvám se, že sis musel dělat starosti, Takeru.
Dialogue: 0,0:05:01.09,0:05:03.45,Default,Takeru,0000,0000,0000,,No, dokud jsi naživu...
Dialogue: 0,0:05:03.45,0:05:04.65,Default,Takeru,0000,0000,0000,,Ale ty!
Dialogue: 0,0:05:04.65,0:05:07.25,Default,Takeru,0000,0000,0000,,Když jsi to věděla, mělas mi to říct, Mňauko!
Dialogue: 0,0:05:07.25,0:05:09.27,Default,Nya,0000,0000,0000,,Sama jsem to zjistila teprve včera.
Dialogue: 0,0:05:09.27,0:05:10.83,Default,Nya,0000,0000,0000,,Prostě jsem ti to zapomněla říct.
Dialogue: 0,0:05:10.83,0:05:14.14,Default,Takeru,0000,0000,0000,,Proto jsi teda byla dneska nezvykle milá
Dialogue: 0,0:05:14.14,0:05:15.75,Default,Takeru,0000,0000,0000,,a snažila ses mě rozveselit?
Dialogue: 0,0:05:15.75,0:05:19.51,Default,Mo,0000,0000,0000,,Mňauko, věnuj mi taky trochu své laskavosti!
Dialogue: 0,0:05:19.51,0:05:22.17,Default,Nya,0000,0000,0000,,Dobře, že jsem tu úchylačku nejdřív svázala.
Dialogue: 0,0:05:22.63,0:05:26.49,Default,Uri,0000,0000,0000,,Takže Mňauka tě uklidňovala, když jsem tu nebyla?
Dialogue: 0,0:05:26.49,0:05:27.34,Default,Nya,0000,0000,0000,,Je to tak?
Dialogue: 0,0:05:27.34,0:05:29.30,Default,Takeru,0000,0000,0000,,Myslím, že jo...
Dialogue: 0,0:05:29.30,0:05:30.07,Default,Nya,0000,0000,0000,,Odmítám!
Dialogue: 0,0:05:30.07,0:05:30.95,Default,Takeru,0000,0000,0000,,Proč?!
Dialogue: 0,0:05:30.55,0:05:31.39,Cluster,Text,0000,0000,0000,,Yandere \NTsundere\NMalá sestřička\NMalej ďáblík\NUklidnující\NBíložravec\NMasožravec\NVášnivá\NFlegmatic\NMoe
Dialogue: 0,0:05:30.95,0:05:34.05,Default,Nya,0000,0000,0000,,Nepotřebuju osobnost, která už tu je!
Dialogue: 0,0:05:36.77,0:05:39.08,Default,Nya,0000,0000,0000,,Chci mít originální osobnost
Dialogue: 0,0:05:39.08,0:05:41.48,Default,Nya,0000,0000,0000,,přesahující lidské chápání\Na být jediná svého druhu!
Dialogue: 0,0:05:43.63,0:05:45.57,Default,Mo,0000,0000,0000,,Oh! Úžasný!
Dialogue: 0,0:05:45.57,0:05:48.19,Default,Piyo,0000,0000,0000,,Nemůžeš se prostě stát něčím originálním.
Dialogue: 0,0:05:48.19,0:05:49.70,Default,Nya,0000,0000,0000,,To není pravda!
Dialogue: 0,0:05:50.13,0:05:51.71,Default,Nya,0000,0000,0000,,Jsem génius,
Dialogue: 0,0:05:51.71,0:05:55.04,Default,Nya,0000,0000,0000,,takže se sama perfektně a dokonale změním!
Dialogue: 0,0:05:56.55,0:05:57.86,Default,Usa,0000,0000,0000,,Cos to řekla?
Dialogue: 0,0:05:57.86,0:05:58.42,Default,Nya,0000,0000,0000,,Mňau?
Dialogue: 0,0:05:58.42,0:06:00.65,Default,Usa,0000,0000,0000,,Amatér by neměl
Dialogue: 0,0:05:58.42,0:06:03.21,menacing,Text,0000,0000,0000,,Hrozivý
Dialogue: 0,0:06:00.65,0:06:03.21,Default,Usa,0000,0000,0000,,mluvit o změně tak lehce.
Dialogue: 0,0:06:03.64,0:06:06.80,Default,Usa,0000,0000,0000,,Ztracené děti si neuvědomují, že jsou ztracené!
Dialogue: 0,0:06:07.03,0:06:07.77,Default,Nya,0000,0000,0000,,Cože?!
Dialogue: 0,0:06:07.77,0:06:11.10,Default,Takeru,0000,0000,0000,,Správně. Mňauka je ztracená\Nna své cestě k sebepoznání.
Dialogue: 0,0:06:11.10,0:06:11.68,Default,Nya,0000,0000,0000,,Deprimuješ mě!
Dialogue: 0,0:06:12.38,0:06:16.06,Default,Usa,0000,0000,0000,,Osobnost, kterou nelze vysvětlit, neexistuje!
Dialogue: 0,0:06:16.06,0:06:16.93,Default,Nya,0000,0000,0000,,Šok!
Dialogue: 0,0:06:17.66,0:06:19.04,Default,Takeru,0000,0000,0000,,S takhle nestálou osobností
Dialogue: 0,0:06:19.04,0:06:21.10,Default,Takeru,0000,0000,0000,,si tě nikdo nezapamatuje,\Nani když se do toho zvěrokruhu dostaneš.
Dialogue: 0,0:06:21.10,0:06:22.44,Default,Takeru,0000,0000,0000,,Hned budeš zapomenuta.
Dialogue: 0,0:06:22.44,0:06:23.32,Default,Nya,0000,0000,0000,,Vážně?!
Dialogue: 0,0:06:24.15,0:06:24.91,Default,Nya,0000,0000,0000,,To ne...
Dialogue: 0,0:06:28.62,0:06:30.61,Default,Nya,0000,0000,0000,,Nebudou si mě pamatovat...
Dialogue: 0,0:06:30.70,0:06:33.20,cat joined the zodiac q,Text,0000,0000,0000,,{\fad(273,1)}Věděli jste, že se kočka přidala k čínskému zvěrokruhu?
Dialogue: 0,0:06:31.46,0:06:33.20,Default Italics,King,0000,0000,0000,,Co? Kočka?
Dialogue: 0,0:06:33.70,0:06:34.45,Default Italics,King,0000,0000,0000,,Nesmysl!
Dialogue: 0,0:06:34.80,0:06:36.32,Default,Nya,0000,0000,0000,,To je hrozné!
Dialogue: 0,0:06:36.32,0:06:39.41,Default,Nya,0000,0000,0000,,Pokud se to stane,\Nnikdy nedostanu žádnou Sol/Lull!
Dialogue: 0,0:06:39.41,0:06:40.49,Default,Key,0000,0000,0000,,Je to hrozné.
Dialogue: 0,0:06:40.49,0:06:41.27,Default,Inu,0000,0000,0000,,Vážně.
Dialogue: 0,0:06:41.27,0:06:44.46,Default,Usa,0000,0000,0000,,Vy dvě se nesnažte mluvit, jako by se vás to netýkalo.
Dialogue: 0,0:06:44.46,0:06:53.51,ep 1 flashabck,Text,0000,0000,0000,,Epizoda 1
Dialogue: 0,0:06:44.92,0:06:48.26,Default,Usa,0000,0000,0000,,Jen si vzpomeňte, co se stalo v prvním díle.
Dialogue: 0,0:06:48.38,0:06:50.11,Default,Usa,0000,0000,0000,,I když jste součástí čínského zvěrokruhu,
Dialogue: 0,0:06:48.47,0:06:53.51,LOSERS,Text,0000,0000,0000,,Poraženi
Dialogue: 0,0:06:50.11,0:06:53.19,Default,Usa,0000,0000,0000,,tak si vás lidé ve skutečnosti nepamatují!
Dialogue: 0,0:06:53.77,0:06:54.85,Default,Inu,0000,0000,0000,,Pravda!
Dialogue: 0,0:06:54.85,0:06:55.95,Default,Key,0000,0000,0000,,To je pravda!
Dialogue: 0,0:06:55.95,0:06:58.27,Default,Piyo,0000,0000,0000,,Nikdo si nás nepamatuje!
Dialogue: 0,0:06:58.69,0:07:02.06,Default,Takeru,0000,0000,0000,,To znamená,\Nže žádná z vašich osobností nevyniká.
Dialogue: 0,0:07:02.06,0:07:02.93,Default,All,0000,0000,0000,,Depresivní!
Dialogue: 0,0:07:02.93,0:07:05.33,Default,Usa,0000,0000,0000,,Celé ty dva tisíce let jsem si vždy myslela,
Dialogue: 0,0:07:05.33,0:07:08.28,Default,Usa,0000,0000,0000,,že se vy všechny neumíte prodat!
Dialogue: 0,0:07:08.28,0:07:09.76,Default,Usa,0000,0000,0000,,Za prvé, vy dvě!
Dialogue: 0,0:07:09.76,0:07:10.70,Default,InuKey,0000,0000,0000,,Ano?!
Dialogue: 0,0:07:10.70,0:07:11.97,Default,Usa,0000,0000,0000,,Vaše osobnosti se překrývají!
Dialogue: 0,0:07:11.97,0:07:12.53,Default,InuKey,0000,0000,0000,,Co?!
Dialogue: 0,0:07:12.66,0:07:14.03,Default,Usa,0000,0000,0000,,A ty...
Dialogue: 0,0:07:14.03,0:07:14.87,Default,Piyo,0000,0000,0000,,Jo?
Dialogue: 0,0:07:14.87,0:07:15.86,Default,Usa,0000,0000,0000,,... selhalas hned na startu!
Dialogue: 0,0:07:16.78,0:07:17.80,Default,Usa,0000,0000,0000,,Ty...
Dialogue: 0,0:07:18.10,0:07:20.50,Default,Usa,0000,0000,0000,,Ty ses příliš usadila v roli vysvětlujícího charakteru!
Dialogue: 0,0:07:21.31,0:07:22.37,Default,Usa,0000,0000,0000,,A nakonec...
Dialogue: 0,0:07:22.67,0:07:23.52,Default,Usa,0000,0000,0000,,Mňauko!
Dialogue: 0,0:07:23.52,0:07:24.13,Default,Nya,0000,0000,0000,,Copak?!
Dialogue: 0,0:07:24.45,0:07:28.15,Default,Usa,0000,0000,0000,,Proměnlivost tvojí osobnosti\Nje nad rámec únosnosti!
Dialogue: 0,0:07:28.15,0:07:28.88,Default,Nya,0000,0000,0000,,Doopravdy?!
Dialogue: 0,0:07:28.88,0:07:32.38,Default,Nya,0000,0000,0000,,To ne! Takeru, dnes v noci budeme mít nouzové osobnostní plánovací jednání!
Dialogue: 0,0:07:32.38,0:07:36.64,panic panic,Text,0000,0000,0000,,Panika, panika
Dialogue: 0,0:07:33.80,0:07:36.85,Default,Takeru,0000,0000,0000,,To není jako říct:\N"Dnes v noci budeme mít sukiyaki."
Dialogue: 0,0:07:36.85,0:07:37.64,Default,Nya,0000,0000,0000,,Sukiyaki?!
Dialogue: 0,0:07:37.64,0:07:40.73,Default,Mo,0000,0000,0000,,To bylo nejlepší kopání a dupání, co jsem kdy měla.
Dialogue: 0,0:07:41.03,0:07:43.40,Default,Usa,0000,0000,0000,,{\i1}Tvoje{\i0} osobnost nikdy nezklame.
Dialogue: 0,0:07:44.14,0:07:45.69,Default,Nya,0000,0000,0000,,Dobrá práce, holky!
Dialogue: 0,0:07:45.69,0:07:48.23,Default,Nya,0000,0000,0000,,Mám hlad! Rychle pospěšme domů, dát si sukiyaki!
Dialogue: 0,0:07:48.79,0:07:52.36,Default,Nya,0000,0000,0000,,{\a6}La, la, la! La, la, la! Sukiyaki!
Dialogue: 0,0:07:49.28,0:07:52.36,Default,Takeru,0000,0000,0000,,Její osobnost je nyní plně ovládnuta sukiyaki.
Dialogue: 0,0:07:53.02,0:07:56.74,Default,Nya,0000,0000,0000,,A až budeme jíst sukiyaki, můžeme uspořádat oslavu na počest Urinina znovuzrození!
Dialogue: 0,0:08:01.97,0:08:03.86,Default,Key,0000,0000,0000,,Naše osobnosti nevyčnívají.
Dialogue: 0,0:08:03.86,0:08:06.46,Default,Inu,0000,0000,0000,,Neumíme se prodat.
Dialogue: 0,0:08:06.69,0:08:08.67,Default,InuKey,0000,0000,0000,,Naše osobnosti se překrývají.
Dialogue: 0,0:08:08.97,0:08:12.47,Default,Key,0000,0000,0000,,Pokud sis to myslela víc jak\Ndva tisíce let, měla jsi nám to říct dřív!
Dialogue: 0,0:08:13.09,0:08:16.22,Default,Inu,0000,0000,0000,,Ale říct to takhle přímo bolí, co?
Dialogue: 0,0:08:18.03,0:08:21.35,Default,Key,0000,0000,0000,,Neříkala Mňauka, že budou mít dneska s Takerem plánovací schůzku?
Dialogue: 0,0:08:21.66,0:08:23.35,Default,Inu,0000,0000,0000,,Jo.
Dialogue: 0,0:08:25.78,0:08:29.07,Flashback,Takeru,0000,0000,0000,,To znamená, že žádná z vašich osobností nevyniká.
Dialogue: 0,0:08:29.33,0:08:30.47,Flashback,Usa,0000,0000,0000,,A ty...
Dialogue: 0,0:08:30.47,0:08:31.36,Flashback,Piyo,0000,0000,0000,,Jo?
Dialogue: 0,0:08:31.36,0:08:32.28,Flashback,Usa,0000,0000,0000,,... selhalas hned na startu!
Dialogue: 0,0:08:33.76,0:08:36.45,Default,Piyo,0000,0000,0000,,Nemůžu ta slova dostat z hlavy.
Dialogue: 0,0:08:43.33,0:08:45.75,Default,Usa,0000,0000,0000,,Teď, když jsem jim to všem řekla,
Dialogue: 0,0:08:45.75,0:08:47.72,Default,Usa,0000,0000,0000,,to nemůžu nechat být.
Dialogue: 0,0:08:48.28,0:08:49.71,Default,Usa,0000,0000,0000,,Inabamane č. 1, změna plánu.
Dialogue: 0,0:08:49.98,0:08:52.67,Default,Usa,0000,0000,0000,,Jedeme do Takerova domu.
Dialogue: 0,0:08:52.67,0:08:53.22,Default,Inabaman,0000,0000,0000,,Ano.
Dialogue: 0,0:08:56.21,0:08:58.07,Default,Usa,0000,0000,0000,,Haló, číslo 55?
Dialogue: 0,0:08:58.44,0:08:59.97,Default,Usa,0000,0000,0000,,Ano, to jsem já.
Dialogue: 0,0:08:59.97,0:09:02.24,Default,Usa,0000,0000,0000,,Proveďte obvyklé přípravy.
Dialogue: 0,0:09:05.76,0:09:06.99,Default,Uri,0000,0000,0000,,Nakupování!
Dialogue: 0,0:09:06.99,0:09:07.53,Default,Nya,0000,0000,0000,,Maso!
Dialogue: 0,0:09:07.53,0:09:08.69,Default,Uri,0000,0000,0000,,Nakupování!
Dialogue: 0,0:09:08.69,0:09:09.57,Default,Nya,0000,0000,0000,,Maso!
Dialogue: 0,0:09:09.19,0:09:10.48,sukiyaki,Text,0000,0000,0000,,Sukiyaki
Dialogue: 0,0:09:10.87,0:09:12.80,Default,Nya,0000,0000,0000,,Vypadá chutně!
Dialogue: 0,0:09:12.80,0:09:14.60,Default,Mo,0000,0000,0000,,Tady máme vzorky zdarma!
Dialogue: 0,0:09:14.60,0:09:15.78,Default,Nya,0000,0000,0000,,Mňau! Cože?!
Dialogue: 0,0:09:16.24,0:09:20.12,NYTAN ONLY TO THE SALE,Text,0000,0000,0000,,Mňauka
Dialogue: 0,0:09:22.16,0:09:24.96,thanks for ur purchase,Text,0000,0000,0000,,Děkujeme\N za nákup!
Dialogue: 0,0:09:22.64,0:09:24.40,Default,Nya,0000,0000,0000,,Čas na sukiyaki!
Dialogue: 0,0:09:25.24,0:09:26.01,Default,Inu,0000,0000,0000,,Čus.
Dialogue: 0,0:09:26.01,0:09:27.33,Default,Key,0000,0000,0000,,Jsme tu.
Dialogue: 0,0:09:27.55,0:09:28.91,Default,Takeru,0000,0000,0000,,Jak jste se sem dostaly?
Dialogue: 0,0:09:28.91,0:09:29.80,Default,InuKey,0000,0000,0000,,Tady.
Dialogue: 0,0:09:29.84,0:09:31.09,HOLE,Text,0000,0000,0000,,Ďoura
Dialogue: 0,0:09:31.09,0:09:33.05,Default,Takeru,0000,0000,0000,,A to jsem ji lehce zakryl...
Dialogue: 0,0:09:33.05,0:09:34.51,Default,Nya,0000,0000,0000,,Prostě nás lehce popírá!
Dialogue: 0,0:09:34.84,0:09:36.71,Default,Uri,0000,0000,0000,,Poté, cos byl celou dobu vystaven našim rozmarům,
Dialogue: 0,0:09:36.71,0:09:39.80,Default,Uri,0000,0000,0000,,jsem si říkala, že bys nás asi moc ochotně nepřijal...
Dialogue: 0,0:09:39.80,0:09:41.56,Default,Uri,0000,0000,0000,,Pořád nám zbývá dost práce.
Dialogue: 0,0:09:41.97,0:09:43.33,Default,Inu,0000,0000,0000,,Ty jsi tady taky, Piyo?
Dialogue: 0,0:09:43.33,0:09:45.67,Default,Piyo,0000,0000,0000,,Hej, lidi, proč jste ta—
Dialogue: 0,0:09:45.67,0:09:46.31,Default,Piyo,0000,0000,0000,,Au!
Dialogue: 0,0:09:47.10,0:09:47.81,one step,Text,0000,0000,0000,,jeden krok
Dialogue: 0,0:09:47.81,0:09:48.56,one step,Text,0000,0000,0000,,druhý krok  jeden krok
Dialogue: 0,0:09:48.56,0:09:49.10,one step,Text,0000,0000,0000,,třetí krok  druhý krok  jeden krok
Dialogue: 0,0:09:51.44,0:09:53.71,Default,Takeru,0000,0000,0000,,Vypadá to, že zase zapomněla.
Dialogue: 0,0:09:56.62,0:09:58.73,Default,Nya,0000,0000,0000,,Ona opravdu selhala hned na startu.
Dialogue: 0,0:09:58.73,0:10:01.37,Default,Uri,0000,0000,0000,,Jenže možná právě proto, že se jí to stává zas a znovu,
Dialogue: 0,0:10:01.37,0:10:03.88,Default,Uri,0000,0000,0000,,její osobnost svým způsobem tak trochu vyčnívá...
Dialogue: 0,0:10:05.55,0:10:06.37,Default,Nya,0000,0000,0000,,Co se to děje?!
Dialogue: 0,0:10:06.81,0:10:08.05,Default,Inu,0000,0000,0000,,To je ona!
Dialogue: 0,0:10:08.37,0:10:10.25,Default,Mo,0000,0000,0000,, UTC,
Dialogue: 0,0:10:10.25,0:10:12.88,Default,Mo,0000,0000,0000,,helikoptéra zaječí společnosti!
Dialogue: 0,0:10:13.19,0:10:15.13,Default,Takeru,0000,0000,0000,,Zaječí společnosti?
Dialogue: 0,0:10:28.35,0:10:32.06,Default,Takeru,0000,0000,0000,,Co to bylo za Hollywoodský nástup?
Dialogue: 0,0:10:33.54,0:10:35.92,Default,Usa,0000,0000,0000,,Možná jsem jen chtěla, abys zažil
Dialogue: 0,0:10:35.92,0:10:38.65,Default,Usa,0000,0000,0000,,rozsah mé vlastní produkce!
Dialogue: 0,0:10:39.03,0:10:44.03,Etotama Eyecatch,Text,0000,0000,0000,,{\c&HB71905&}{\fs21}[Zásady zvěrokruhu ~1. článek: Božský rituál] \NVýběrový festival Eto-shin (ETM12) je posvátným rituálem,\N při kterém Eto-musume uplatňují své právo\Nna výzvu k souboji s Eto-shin, z něhož nemohou uniknout.\NPři souboji dávají v sázku veškerou svou Sol/Lull.
Dialogue: 0,0:10:44.03,0:10:49.08,Etotama Eyecatch 2,Text,0000,0000,0000,,{\fs21}{\c&HB71905&}[Zásady zvěrokruhu ~2. článek: Čas] \NVýběrový festival Eto-shin (ETM12)\Ntrvá šest dní po každých šesti letech\Nv návaznosti na šestiletý cyklus,\Nkterý bohové potřebují k obnovení Sol/Lull\Nv božském světě, světě Eto i lidském.
Dialogue: 0,0:10:53.52,0:10:54.92,Default,Usa,0000,0000,0000,,A tak...
Dialogue: 0,0:10:55.55,0:11:00.18,improvement book,Text,0000,0000,0000,,{\c&H1212C3&}Usa-tan{\c&H0C0C0C&} uvádí\N Projekt{\c&H1212C3&} na zlepšení\N {\c&H101011&}Mňaučina vystupování
Dialogue: 0,0:10:56.30,0:11:00.18,Default,Usa,0000,0000,0000,,Připravila jsem návrh na zlepšení Mňaučiny osobnosti!
Dialogue: 0,0:11:00.18,0:11:02.47,Default,Nya,0000,0000,0000,,Kdo si myslíš, že jsi?
Dialogue: 0,0:11:03.33,0:11:04.85,Default,Usa,0000,0000,0000,,Promiň, že jsem se nepředstavila dřív.
Dialogue: 0,0:11:04.85,0:11:10.56,meow 1,Text,0000,0000,0000,,{\b1}Výkonná ředitelka Usa-tan Company \NProducentka Usa-tan Productions\NReprezentantka fitka Usa-tan\NHlavní režisérka Vizuálních efektů Usa-tan\NŘeditelka nakladatelství Usa-tan\NPrezidentka UTC Electronics \NVýkonná ředitelka potravin UTC
Dialogue: 0,0:11:05.48,0:11:08.49,Default,Usa,0000,0000,0000,,Jsem generální ředitelkou Usa Company,
Dialogue: 0,0:11:08.49,0:11:12.06,Default,Usa,0000,0000,0000,,producentka Usa Productions a je toho víc.
Dialogue: 0,0:11:12.06,0:11:14.42,Default,Nya,0000,0000,0000,,Těší mě...
Dialogue: 0,0:11:14.72,0:11:15.94,Default,Nya,0000,0000,0000,,Počkej, co to znamená?!
Dialogue: 0,0:11:16.20,0:11:19.57,Default,Usa,0000,0000,0000,,Jsem profesionální producentka, nebo zkráceně Usa P. P.!
Dialogue: 0,0:11:20.62,0:11:21.41,Default,Nya,0000,0000,0000,,Vážně?
Dialogue: 0,0:11:21.70,0:11:24.24,Default,Usa,0000,0000,0000,,Každý živý tvor je mýma rukama
Dialogue: 0,0:11:24.33,0:11:25.59,Default,Usa,0000,0000,0000,,osvobozen od nedostatku osobnosti!
Dialogue: 0,0:11:25.59,0:11:28.16,Default,Usa,0000,0000,0000,,Vytvořím ti dokonalou osobnost!
Dialogue: 0,0:11:25.62,0:11:28.16,utc respond,Text,0000,0000,0000,,UTC bude i nadále reagovat na hlasy svých zákazníků.
Dialogue: 0,0:11:25.62,0:11:28.16,customer sat,Text,0000,0000,0000,,Č. 1. {\c&H4082DB&}Ve spokojenosti zákazníků
Dialogue: 0,0:11:28.41,0:11:29.94,Default,InuKey,0000,0000,0000,,Paráda!
Dialogue: 0,0:11:29.94,0:11:32.01,Default,Inu,0000,0000,0000,,Netušila jsem, že je Usa tak úžasná!
Dialogue: 0,0:11:32.01,0:11:35.42,Default,Key,0000,0000,0000,,Tak proto dokázala\Ntak pěkně poukázat na naše slabosti!
Dialogue: 0,0:11:35.42,0:11:38.01,Default,Takeru,0000,0000,0000,,Joj, ty dvě jsou tak nevinné...
Dialogue: 0,0:11:35.42,0:11:38.81,Default,Inu,0000,0000,0000,,Tak to je! Usa P. P. je úžasná!
Dialogue: 0,0:11:38.01,0:11:39.54,Default,Nya,0000,0000,0000,,Všechno to smrdí podezřele.
Dialogue: 0,0:11:38.81,0:11:40.90,Default - top,Key,0000,0000,0000,,Jo! Usa P. P. je úžasná!
Dialogue: 0,0:11:39.54,0:11:42.91,Default,Uri,0000,0000,0000,,Zajímalo by mě,\Njestli si uvědomují, že ta zkratka zní jako
Dialogue: 0,0:11:40.90,0:11:41.80,Default - top,Inu,0000,0000,0000,,Tak úžasná!
Dialogue: 0,0:11:41.80,0:11:42.72,Default - top,Inu,0000,0000,0000,,Příliš úžasná!
Dialogue: 0,0:11:42.72,0:11:43.96,Default - top,Key,0000,0000,0000,,Chci být jako P. P.!
Dialogue: 0,0:11:42.91,0:11:44.35,Default,Uri,0000,0000,0000,,když cenzuruješ něco, co nemůžeš na veřejnosti říct...
Dialogue: 0,0:11:43.96,0:11:44.97,Default - top,Inu,0000,0000,0000,,Já taky!
Dialogue: 0,0:11:44.35,0:11:46.87,Default,Mo,0000,0000,0000,,Ale mé citlivé erotické vnímání detekuje
Dialogue: 0,0:11:46.36,0:11:47.84,Default - top,Key,0000,0000,0000,,Tak skvělá!
Dialogue: 0,0:11:46.87,0:11:48.74,Default,Mo,0000,0000,0000,,časté používání "P."
Dialogue: 0,0:11:47.84,0:11:49.43,Default - top,Inu,0000,0000,0000,,Úžasná!
Dialogue: 0,0:11:48.74,0:11:51.14,Default,Takeru,0000,0000,0000,,Mimochodem, je ten vrtulník v pořádku?
Dialogue: 0,0:11:51.43,0:11:54.77,Default,Usa,0000,0000,0000,,Co, vy jste si toho nevšimli? To byly zvláštní efekty.
Dialogue: 0,0:11:54.77,0:11:55.78,Default,All,0000,0000,0000,,Vážně?!
Dialogue: 0,0:11:55.78,0:11:57.36,Default,Takeru,0000,0000,0000,,Ale vypadalo to tak reálně!
Dialogue: 0,0:11:57.66,0:11:59.79,Default,Usa,0000,0000,0000,,Použili jsme 4D holografický systém
Dialogue: 0,0:11:59.79,0:12:01.75,Default,Usa,0000,0000,0000,,spolu s mistrovskou stereofonní zvukovou aparaturou.
Dialogue: 0,0:12:01.75,0:12:03.85,Default,Usa,0000,0000,0000,,A k dosažení reálnějšího efektu
Dialogue: 0,0:12:03.85,0:12:06.89,Default,Usa,0000,0000,0000,,jsme vytvořili trojrozměrné vibrace\Nza pomoci obřího dmychadla,
Dialogue: 0,0:12:06.89,0:12:08.87,Default,Usa,0000,0000,0000,,které dokonale vytvořilo demoliční scénu.
Dialogue: 0,0:12:08.87,0:12:11.29,Default,Takeru,0000,0000,0000,,Můj dům byl upraven bez mého vědomí?!
Dialogue: 0,0:12:11.55,0:12:14.22,Default,Usa,0000,0000,0000,,Teď ukončete tohle bezvýznamné tlachání.
Dialogue: 0,0:12:16.17,0:12:17.67,poster title green,Text,0000,0000,0000,,Průzkum možností,\Njak by se kočka mohla pomocí ETM12\N přidat k čínskému zvěrokruhu
Dialogue: 0,0:12:17.67,0:12:21.41,Default,Usa,0000,0000,0000,,Nejdříve jsem zkoumala propojení kočky
Dialogue: 0,0:12:21.41,0:12:25.03,Default,Usa,0000,0000,0000,,s čínským zvěrokruhem přes ETM12.
Dialogue: 0,0:12:25.43,0:12:26.43,Default,Nya,0000,0000,0000,,Cože?!
Dialogue: 0,0:12:26.43,0:12:27.68,Default,Takeru,0000,0000,0000,,Zaujalo ji to.
Dialogue: 0,0:12:28.03,0:12:29.86,Default,Nya,0000,0000,0000,,Jaký byl výsledek?
Dialogue: 0,0:12:31.02,0:12:34.10,wants a cat,Text,0000,0000,0000,,Chtějí kočku
Dialogue: 0,0:12:31.02,0:12:34.58,Default,Usa,0000,0000,0000,,Tyhle tři chtějí kočku v čínském zvěrokruhu.
Dialogue: 0,0:12:34.23,0:12:35.44,wants a cat - Copy,Text,0000,0000,0000,,Mistr duchovní síly, Dora
Dialogue: 0,0:12:34.58,0:12:35.90,Default,Usa,0000,0000,0000,,Jsou to tři mistři,
Dialogue: 0,0:12:35.52,0:12:37.06,wants a cat - Copy,Text,0000,0000,0000,,Mistr síly a techniky, Shima
Dialogue: 0,0:12:35.90,0:12:38.40,Default,Usa,0000,0000,0000,,kteří budují Mňaučinu duchovní sílu,\Nučí ji techniky, trénují ji
Dialogue: 0,0:12:37.11,0:12:40.78,wants a cat - Copy,Text,0000,0000,0000,,Mistr moudrosti, Uri
Dialogue: 0,0:12:38.40,0:12:40.78,Default,Usa,0000,0000,0000,,a rozšiřují její znalosti.
Dialogue: 0,0:12:41.13,0:12:43.55,Default,Takeru,0000,0000,0000,,Uri, ty jsi taky Mňaučin mistr?
Dialogue: 0,0:12:43.55,0:12:44.39,Default,Uri,0000,0000,0000,,Jo, jo.
Dialogue: 0,0:12:44.39,0:12:47.27,Default,Usa,0000,0000,0000,,A Chu, která Mňauku nesnáší,
Dialogue: 0,0:12:44.86,0:12:46.78,wants a cat - Copy,Text,0000,0000,0000,,Krysa (Zi Shu), Chu
Dialogue: 0,0:12:46.99,0:12:49.79,DOESNT WANT CAT,Text,0000,0000,0000,,Nechtějí kočku
Dialogue: 0,0:12:47.27,0:12:50.04,Default,Usa,0000,0000,0000,,je jedna ze čtyř, kdo kočku ve zvěrokruhu nechtějí.
Dialogue: 0,0:12:50.04,0:12:53.25,Default,Usa,0000,0000,0000,,A tady jsou ti, kterým je jedno,\Njestli se kočka připojí, nebo ne.
Dialogue: 0,0:12:50.04,0:12:54.12,dont care either way,Text,0000,0000,0000,,Nestarají se
Dialogue: 0,0:12:53.25,0:12:54.54,Default,Usa,0000,0000,0000,,Ti se nepočítají.
Dialogue: 0,0:12:54.54,0:12:55.71,Default,Key,0000,0000,0000,,Pravda...
Dialogue: 0,0:12:56.37,0:12:57.75,Default,Inu,0000,0000,0000,,Když se ptáš takhle, je nám to asi jedno.
Dialogue: 0,0:12:57.92,0:13:01.76,wants a cat - Copy,Text,0000,0000,0000,,Bůvol (Chou Niu), Milka
Dialogue: 0,0:12:58.19,0:13:00.48,Default,Usa,0000,0000,0000,,Takhle je příliš úchylná, takže se nepočítá.
Dialogue: 0,0:13:00.48,0:13:01.76,Default,Mo,0000,0000,0000,,Oh, můj bože!
Dialogue: 0,0:13:02.05,0:13:04.47,Default,Usa,0000,0000,0000,,Takže je to tři proti čtyřem,
Dialogue: 0,0:13:04.47,0:13:07.42,Default,Usa,0000,0000,0000,,Mňauka se ke zvěrokruhu připojit nemůže. Konec.
Dialogue: 0,0:13:08.02,0:13:09.89,Default,Nya,0000,0000,0000,,Co je to za nedokončený průzkum?!
Dialogue: 0,0:13:10.21,0:13:11.49,Default,Usa,0000,0000,0000,,Z toho důvodu
Dialogue: 0,0:13:11.49,0:13:15.02,Default,Usa,0000,0000,0000,,jsem vypracovala plán na rozvoj tvé osobnosti,
Dialogue: 0,0:13:15.02,0:13:18.10,Default,Usa,0000,0000,0000,,tak abys vynikala, i když nejsi součástí zvěrokruhu.
Dialogue: 0,0:13:18.10,0:13:20.28,Default,Nya,0000,0000,0000,,Tos opravdu udělala?
Dialogue: 0,0:13:20.28,0:13:21.40,Default,Takeru,0000,0000,0000,,Ona si s ní hraje.
Dialogue: 0,0:13:23.60,0:13:27.52,Default,Usa,0000,0000,0000,,S tímto plánem se staneš znovuzrozenou Mňaukou.
Dialogue: 0,0:13:27.52,0:13:29.41,Default,Nya,0000,0000,0000,,Znovuzrozená... Mňauka...
Dialogue: 0,0:13:29.69,0:13:30.97,Default,Usa,0000,0000,0000,,Nebuď pyšná.
Dialogue: 0,0:13:30.97,0:13:33.91,Default,Usa,0000,0000,0000,,Nech všechno na Use P. P.
Dialogue: 0,0:13:35.75,0:13:39.22,Default,Usa,0000,0000,0000,,Usa P. P. P. P. P. P. P...
Dialogue: 0,0:13:39.22,0:13:41.75,Default,Takeru,0000,0000,0000,,Ona jí vymývá mozek!
Dialogue: 0,0:13:41.75,0:13:42.76,Default,Mo,0000,0000,0000,,Né!
Dialogue: 0,0:13:42.76,0:13:45.07,Default,Mo,0000,0000,0000,,Probuď se, Mňauko!
Dialogue: 0,0:13:46.81,0:13:48.26,Default,Nya,0000,0000,0000,,Co jsem to...
Dialogue: 0,0:13:49.05,0:13:50.64,Default,Usa,0000,0000,0000,,Skoro jsem ji měla...
Dialogue: 0,0:13:50.24,0:13:52.45,Default,Mo,0000,0000,0000,,Mňauko... Mňauko...
Dialogue: 0,0:13:50.64,0:13:52.45,Default,Key,0000,0000,0000,,Uso P.P., teď mě!
Dialogue: 0,0:13:52.45,0:13:54.40,Default,Inu,0000,0000,0000,,Uprav nás!
Dialogue: 0,0:13:54.40,0:13:54.98,Default,Usa,0000,0000,0000,,Co?
Dialogue: 0,0:13:55.40,0:13:56.97,Default,Inu,0000,0000,0000,,Ukaž nám, co můžem dělat!
Dialogue: 0,0:13:56.97,0:13:58.54,Default,Key,0000,0000,0000,,Budem se snažit!
Dialogue: 0,0:14:02.97,0:14:05.43,Default,Usa,0000,0000,0000,,Má producentská duše se chvěje!
Dialogue: 0,0:14:05.43,0:14:07.95,Default,Usa,0000,0000,0000,,P. P. paprsek!
Dialogue: 0,0:14:17.38,0:14:19.25,Default,Usa,0000,0000,0000,,Přeměna kompletní.
Dialogue: 0,0:14:19.58,0:14:23.45,Default,Usa,0000,0000,0000,,Dala jsem vám osobnost dávno ztracených dvojčat.
Dialogue: 0,0:14:24.62,0:14:28.51,Default,Uri,0000,0000,0000,,Použila překrývavost psa a opice,\Naby z nich udělala dvojčata! Jo, jo!
Dialogue: 0,0:14:29.04,0:14:30.52,Default,Key,0000,0000,0000,,Takže je to v pořádku...
Dialogue: 0,0:14:31.54,0:14:34.64,Default,Inu,0000,0000,0000,,... že jsme se překrývaly!
Dialogue: 0,0:14:35.22,0:14:37.07,Default,Usa,0000,0000,0000,,Jen jsem z vašich nedostatků udělala přednost.
Dialogue: 0,0:14:37.07,0:14:39.52,Default,Usa,0000,0000,0000,,Tohle je skutečné umění Usy P. P.!
Dialogue: 0,0:14:41.15,0:14:42.59,Default,Mo,0000,0000,0000,,Bravo!
Dialogue: 0,0:14:44.07,0:14:45.15,Default,Nya,0000,0000,0000,,Jdu do toho!
Dialogue: 0,0:14:45.54,0:14:48.38,Default,Nya,0000,0000,0000,,Vezmu si tvoje návrhy!
Dialogue: 0,0:14:48.38,0:14:50.82,Default,Usa,0000,0000,0000,,Takže... v tom případě...
Dialogue: 0,0:14:51.10,0:14:53.78,Default,Usa,0000,0000,0000,,Potřebuju, abys nejdřív\Nzaplatila poplatek 100 milionů jenů.
Dialogue: 0,0:14:53.78,0:14:54.93,Default,Nya,0000,0000,0000,,Proč 100 milionů?!
Dialogue: 0,0:14:54.83,0:15:02.33,solo debut,Text,0000,0000,0000,,Poplatek za uvedení do společnosti
Dialogue: 0,0:14:54.93,0:14:58.10,Default,Usa,0000,0000,0000,,Začneme s poplatkem za uvedení\Ndo společnosti po určení tvé nové osobnosti,
Dialogue: 0,0:14:58.08,0:15:04.04,yacht,Text,0000,0000,0000,,Jachtařský poplatek
Dialogue: 0,0:14:58.10,0:15:00.46,Default,Usa,0000,0000,0000,,poté zaplatíme jachtu pro veřejné vystoupení,
Dialogue: 0,0:15:00.46,0:15:04.04,Default,Usa,0000,0000,0000,,dále postavíme nahrávací studio, připravíme koncert,
Dialogue: 0,0:15:00.46,0:15:05.34,studio,Text,0000,0000,0000,,Nahrávací studio
Dialogue: 0,0:15:02.33,0:15:10.34,solo debut,Text,0000,0000,0000,,Poplatek za uvedení do společnosti                         Rezervace koncenrtního sálu
Dialogue: 0,0:15:04.04,0:15:06.80,Default,Usa,0000,0000,0000,,poté vydražíme první tuňáky, zařídíme komplexní lázně atp.
Dialogue: 0,0:15:04.04,0:15:10.34,yacht,Text,0000,0000,0000,,Jachtařský poplatek                             Poplatek za první tuňákovou aukci
Dialogue: 0,0:15:05.34,0:15:10.34,studio,Text,0000,0000,0000,,Nahrávací studio                                  Komplexní péče v lázních
Dialogue: 0,0:15:06.80,0:15:10.34,Default,Usa,0000,0000,0000,,Usa P. P. už zařídí veškerou vhodnou finanční obsluhu.
Dialogue: 0,0:15:10.56,0:15:13.78,Default,Nya,0000,0000,0000,,To je brutální! Po tom, jak jsi mě motivovala!
Dialogue: 0,0:15:13.78,0:15:17.21,Default,Nya,0000,0000,0000,,Změnila jsi Key a Inu a nic jsi jim neúčtovala!
Dialogue: 0,0:15:17.51,0:15:21.07,Default,Usa,0000,0000,0000,,Ony jsou členkami zvěrokruhu.\NTy jsi jen Eto-musume.
Dialogue: 0,0:15:17.68,0:15:21.40,etoshin,Text,0000,0000,0000,,Eto-{\c&H0095DB&}shin
Dialogue: 0,0:15:19.69,0:15:21.40,etomusume,Text,0000,0000,0000,,Eto-musume
Dialogue: 0,0:15:21.81,0:15:24.11,Default,Usa,0000,0000,0000,,Musíš očekávat rozdíly v zacházení.
Dialogue: 0,0:15:26.34,0:15:28.24,Default,Nya,0000,0000,0000,,V tom případě tě vyzývám k bitvě!
Dialogue: 0,0:15:28.83,0:15:31.49,Default,Nya,0000,0000,0000,,Když vyhraju, změníš mě zadarmo!
Dialogue: 0,0:15:31.90,0:15:34.45,Default,Nya,0000,0000,0000,,A taky mi dáš svoji Sol/Lull pečeť!
Dialogue: 0,0:15:34.75,0:15:36.24,Default,Usa,0000,0000,0000,,To zní zajímavě.
Dialogue: 0,0:15:37.04,0:15:38.04,Default,Usa,0000,0000,0000,,Pak...
Dialogue: 0,0:15:38.77,0:15:40.75,Default,Usa,0000,0000,0000,,Začněme bitvu!
Dialogue: 0,0:15:39.50,0:15:40.75,approved,Text,0000,0000,0000,,Schválena
Dialogue: 0,0:15:41.83,0:15:44.29,Default,Usa,0000,0000,0000,,Eto-tama! Sol/Lull změna!
Dialogue: 0,0:15:42.42,0:15:42.92,rabbit seal,Text,0000,0000,0000,,Zajíc
Dialogue: 0,0:15:59.08,0:16:01.95,Default,Usa,0000,0000,0000,,Zaječí Eto-shin, Usa!
Dialogue: 0,0:16:07.57,0:16:08.83,Default,Nya,0000,0000,0000,,Co to?!
Dialogue: 0,0:16:08.83,0:16:10.69,Default,Usa,0000,0000,0000,,Vypadáš směšně.
Dialogue: 0,0:16:19.05,0:16:20.26,Default,Nya,0000,0000,0000,,Zvol si pravidla!
Dialogue: 0,0:16:20.26,0:16:22.44,Default,Usa,0000,0000,0000,,Jistě, jistě. Dáš si čaj?
Dialogue: 0,0:16:22.44,0:16:23.25,Default,Nya,0000,0000,0000,,Děkuju.
Dialogue: 0,0:16:25.61,0:16:27.25,Default,Nya,0000,0000,0000,,Počkej, ne!
Dialogue: 0,0:16:28.31,0:16:29.57,Default,Nya,0000,0000,0000,,Bojuj!
Dialogue: 0,0:16:31.48,0:16:32.97,Default,Usa,0000,0000,0000,,"Podívej se tam"
Dialogue: 0,0:16:33.36,0:16:35.33,Default,Usa,0000,0000,0000,,Budem hrát třikrát.
Dialogue: 0,0:16:35.33,0:16:36.30,Default,Nya,0000,0000,0000,,Dobře.
Dialogue: 0,0:16:36.30,0:16:38.96,Default,Usa,0000,0000,0000,,Pokud mě jednou porazíš,
Dialogue: 0,0:16:38.96,0:16:41.51,Default,Usa,0000,0000,0000,,dám ti svoji Sol/Lull pečeť...
Dialogue: 0,0:16:43.27,0:16:46.01,Default,Usa,0000,0000,0000,,A změním tě...
Dialogue: 0,0:16:46.01,0:16:49.27,Default,Usa,0000,0000,0000,,zdarma!
Dialogue: 0,0:16:53.39,0:16:56.28,Default,Uri,0000,0000,0000,,Tohle pravidlo ji znevýhodňuje.
Dialogue: 0,0:16:56.60,0:16:58.09,Default,Takeru,0000,0000,0000,,Celou dobu
Dialogue: 0,0:16:58.09,0:17:00.74,Default,Takeru,0000,0000,0000,,sleduje Usin prst...
Dialogue: 0,0:17:00.74,0:17:03.02,Default,Usa,0000,0000,0000,,Čas na první kolo!
Dialogue: 0,0:17:04.17,0:17:05.54,Default,Nya,0000,0000,0000,,Do toho!
Dialogue: 0,0:17:05.89,0:17:08.94,Default,Usa,0000,0000,0000,,Kámen, nůžky, papír!
Dialogue: 0,0:17:12.92,0:17:14.84,Default,Usa,0000,0000,0000,,Podívej se...
Dialogue: 0,0:17:15.42,0:17:16.48,Default,Takeru,0000,0000,0000,,Zavřela oči!
Dialogue: 0,0:17:16.48,0:17:18.85,Default,Uri,0000,0000,0000,,To je na Mňauku působivé!
Dialogue: 0,0:17:18.85,0:17:19.62,Default,Usa,0000,0000,0000,,... tam!
Dialogue: 0,0:17:20.25,0:17:21.62,Default,Nya,0000,0000,0000,,Fungovalo to!
Dialogue: 0,0:17:21.62,0:17:23.68,Default,Usa,0000,0000,0000,,Zastavení času!
Dialogue: 0,0:17:28.77,0:17:30.15,Default,Nya,0000,0000,0000,,Cos to udělala?!
Dialogue: 0,0:17:30.15,0:17:33.15,Default,Usa,0000,0000,0000,,Vypadá to, že jsem první kolo vyhrála.
Dialogue: 0,0:17:33.15,0:17:36.34,Default,Takeru,0000,0000,0000,,Neřekla to Usa docela čistě?
Dialogue: 0,0:17:36.34,0:17:37.19,Default,Uri,0000,0000,0000,,Jo, jo.
Dialogue: 0,0:17:37.19,0:17:38.62,Default,Mo,0000,0000,0000,,Oh! Fantastické!
Dialogue: 0,0:17:38.86,0:17:40.49,Default,Usa,0000,0000,0000,,Čas na druhé kolo.
Dialogue: 0,0:17:40.94,0:17:43.20,Default,Usa,0000,0000,0000,,Kámen, nůžky, papír!
Dialogue: 0,0:17:44.29,0:17:46.28,Default,Usa,0000,0000,0000,,Podívej se...
Dialogue: 0,0:17:46.86,0:17:49.02,Default,Nya,0000,0000,0000,,Ta věc je podezřelá!
Dialogue: 0,0:17:50.44,0:17:53.21,Default,Nya,0000,0000,0000,,Zastavení času!
Dialogue: 0,0:17:55.75,0:17:59.68,Default,Usa,0000,0000,0000,,Hloupoučká. Copak si myslíš, že se čas zastaví?
Dialogue: 0,0:18:00.84,0:18:03.76,Default,Usa,0000,0000,0000,,Tohle je tvá poslední šance, Mňauko.
Dialogue: 0,0:18:04.49,0:18:06.67,Default,Uri,0000,0000,0000,,Prohrála i druhé kolo.
Dialogue: 0,0:18:06.67,0:18:08.97,Default,Takeru,0000,0000,0000,,Ty hodinky nezastaví čas?
Dialogue: 0,0:18:08.97,0:18:10.49,Default,Uri,0000,0000,0000,,Je to velmi zvláštní...
Dialogue: 0,0:18:10.49,0:18:12.55,Default,Uri,0000,0000,0000,,Pravda! Pojďme to zjistit!
Dialogue: 0,0:18:12.55,0:18:13.19,Default,Takeru,0000,0000,0000,,Jak?
Dialogue: 0,0:18:13.19,0:18:15.56,Default,Uri,0000,0000,0000,,Vrátíme to a kouknem se snímek po snímku.
Dialogue: 0,0:18:15.56,0:18:16.70,Default,Uri,0000,0000,0000,,To můžeš?
Dialogue: 0,0:18:17.00,0:18:19.52,Default,Uri,0000,0000,0000,,Tohle je první kolo.
Dialogue: 0,0:18:19.99,0:18:21.84,Default Italics,Usa,0000,0000,0000,,Kámen...
Dialogue: 0,0:18:22.38,0:18:24.70,Default Italics,Usa,0000,0000,0000,,Nůžky...
Dialogue: 0,0:18:25.47,0:18:27.60,Default Italics,Usa,0000,0000,0000,,Papír!
Dialogue: 0,0:18:29.64,0:18:31.52,Default,Uri,0000,0000,0000,,Věděla, jak Mňauka pohne prsty!
Dialogue: 0,0:18:31.52,0:18:33.13,Default,Uri,0000,0000,0000,,Dokonalý faul!
Dialogue: 0,0:18:33.13,0:18:34.20,Default,Takeru,0000,0000,0000,,Úžasný...
Dialogue: 0,0:18:34.20,0:18:36.48,Default,Uri,0000,0000,0000,,To je Usina super rychlost.
Dialogue: 0,0:18:36.48,0:18:37.67,Default,Takeru,0000,0000,0000,,Věděla jsi o tom?
Dialogue: 0,0:18:37.67,0:18:40.28,Default,Uri,0000,0000,0000,,Ne, vidím ji poprvé.
Dialogue: 0,0:18:41.74,0:18:45.04,Default,Uri,0000,0000,0000,,A tohle je moment "podívej se tam".
Dialogue: 0,0:18:45.04,0:18:47.23,Default,Usa,0000,0000,0000,,Zastavení...
Dialogue: 0,0:18:47.23,0:18:50.69,Default,Usa,0000,0000,0000,,času!
Dialogue: 0,0:18:51.49,0:18:52.74,Default,Usa,0000,0000,0000,,Neuvěřitelná síla!
Dialogue: 0,0:18:53.07,0:18:55.96,Default,Uri,0000,0000,0000,,Zdá se, že na to měla spoustu času.
Dialogue: 0,0:18:55.96,0:18:58.19,Default,Takeru,0000,0000,0000,,Za tu chvilku toho stihla dost!
Dialogue: 0,0:18:59.49,0:19:01.11,Default,Uri,0000,0000,0000,,Neskutečný.
Dialogue: 0,0:19:01.11,0:19:03.05,Default,Uri,0000,0000,0000,,To je něco,
Dialogue: 0,0:19:03.05,0:19:05.38,Default,Uri,0000,0000,0000,,co se svou super rychlostí dokáže jen Usa.
Dialogue: 0,0:19:05.38,0:19:07.58,Default,Takeru,0000,0000,0000,,Mňauka je teď vážně v nevýhodě.
Dialogue: 0,0:19:07.58,0:19:08.46,Default,Takeru,0000,0000,0000,,Co bude dělat?
Dialogue: 0,0:19:08.76,0:19:10.07,Default,Uri,0000,0000,0000,,Pravda...
Dialogue: 0,0:19:10.62,0:19:12.69,Default,Uri,0000,0000,0000,,Její jedinou šancí je naklonit si štěstěnu.
Dialogue: 0,0:19:12.69,0:19:14.00,Default,Takeru,0000,0000,0000,,Nemožný...
Dialogue: 0,0:19:14.00,0:19:16.71,Default,Usa,0000,0000,0000,,Dobře, do třetice všeho dobrého i zlého,
Dialogue: 0,0:19:16.98,0:19:18.38,Default,Usa,0000,0000,0000,,je čas na třetí kolo!
Dialogue: 0,0:19:20.84,0:19:22.18,Default,Usa,0000,0000,0000,,Kámen...
Dialogue: 0,0:19:23.18,0:19:24.18,Default,Usa,0000,0000,0000,,Nůžky...
Dialogue: 0,0:19:23.87,0:19:26.64,Default,Nya,0000,0000,0000,,{\a6}Budu se držet svého štěstí!
Dialogue: 0,0:19:30.85,0:19:32.69,Default,Nya,0000,0000,0000,,Mňau!
Dialogue: 0,0:19:33.06,0:19:36.58,Default,Nya,0000,0000,0000,,A teď...
Dialogue: 0,0:19:36.86,0:19:38.65,Default,Nya,0000,0000,0000,,Podívej se.
Dialogue: 0,0:19:38.65,0:19:39.86,Default,Nya,0000,0000,0000,,Támhle!
Dialogue: 0,0:19:39.86,0:19:41.11,Default,Nya,0000,0000,0000,,Támhle!
Dialogue: 0,0:19:41.11,0:19:42.32,Default,Nya,0000,0000,0000,,Támhle!
Dialogue: 0,0:19:58.05,0:19:58.59,rabbit seal - centre,Text,0000,0000,0000,,Zajíc
Dialogue: 0,0:19:58.93,0:20:01.17,Default,Nya,0000,0000,0000,,Usina Sol/Lull pečeť...
Dialogue: 0,0:20:02.25,0:20:03.59,Default,Nya,0000,0000,0000,,... je moje!
Dialogue: 0,0:20:05.14,0:20:07.03,Default Italics,Voice,0000,0000,0000,,Bitva ukončena.
Dialogue: 0,0:20:05.14,0:20:07.24,Default,Uri,0000,0000,0000,,To bylo úžasný, Mňauko!
Dialogue: 0,0:20:07.03,0:20:09.83,Default Italics,Voice,0000,0000,0000,,Vypněte to prosím a začněte znovu.
Dialogue: 0,0:20:07.24,0:20:09.10,Default,Mo,0000,0000,0000,,Jak vzrušující!
Dialogue: 0,0:20:10.99,0:20:12.65,Default,Nya,0000,0000,0000,,Dokázala jsem to!
Dialogue: 0,0:20:13.83,0:20:15.65,Default,Nya,0000,0000,0000,,Popadla jsem své štěstí za nohu!
Dialogue: 0,0:20:16.14,0:20:18.36,Default,Takeru,0000,0000,0000,,Ale proč jsi takhle chytla Usinu nohu?
Dialogue: 0,0:20:19.10,0:20:20.36,Default,Nya,0000,0000,0000,,Je to králičí pacička.
Dialogue: 0,0:20:20.60,0:20:22.88,Default,Takeru,0000,0000,0000,,Králičí pacička? Jako tahle?
Dialogue: 0,0:20:22.88,0:20:24.92,Default,Uri,0000,0000,0000,,Je to ozdoba, kterou mají lidé
Dialogue: 0,0:20:24.92,0:20:27.62,Default,Uri,0000,0000,0000,,už od starověku pro štěstí.
Dialogue: 0,0:20:28.84,0:20:30.37,Default,Usa,0000,0000,0000,,Byla jsem neopatrná...
Dialogue: 0,0:20:30.37,0:20:33.66,Default,Mo,0000,0000,0000,,Takže tvoje nohy jsou erotogenní zónou, Uso?
Dialogue: 0,0:20:33.66,0:20:35.38,Default,Usa,0000,0000,0000,,Co to říkáš?!
Dialogue: 0,0:20:35.68,0:20:38.40,Default,Mo,0000,0000,0000,,Stačí se jen podívat.
Dialogue: 0,0:20:38.40,0:20:40.52,Default,Mo,0000,0000,0000,,Podle tvého obličeje a hlasu
Dialogue: 0,0:20:40.52,0:20:42.81,Default,Mo,0000,0000,0000,,sis prošla erotickou extází!
Dialogue: 0,0:20:45.12,0:20:48.52,Default,Uri,0000,0000,0000,,Rozumím. Usa se stydí, že prohrála.
Dialogue: 0,0:20:48.52,0:20:50.93,Default,Takeru,0000,0000,0000,,Nevím, jestli bys měla říkat, že šlo o štěstí...
Dialogue: 0,0:20:50.93,0:20:53.71,Default,Nya,0000,0000,0000,,Mimochodem, jsem o další krok blíže ke zvěrokruhu!
Dialogue: 0,0:20:53.71,0:20:56.72,Default,Nya,0000,0000,0000,,Nyní se dej do práce a změň mě!
Dialogue: 0,0:20:56.72,0:20:57.82,Default,Nya,0000,0000,0000,,Zdarma!
Dialogue: 0,0:20:58.31,0:21:00.15,Default,Usa,0000,0000,0000,,Myslím, že nemám na vybranou.
Dialogue: 0,0:21:00.15,0:21:01.59,Default,Usa,0000,0000,0000,,Tak tady to máš.
Dialogue: 0,0:21:00.32,0:21:04.41,improvement book,Text,0000,0000,0000,,Projekt na zlepšení Mňauky\N{\fs36}základní tréninkový \N{\fs33}program
Dialogue: 0,0:21:01.59,0:21:04.41,Default,Nya,0000,0000,0000,,Mňau? Tohle je jiné než předtím.
Dialogue: 0,0:21:04.41,0:21:07.34,Default,Usa,0000,0000,0000,,Začneme se sty miliony na tvé břišní svaly, záda a prsa.
Dialogue: 0,0:21:05.03,0:21:09.16,abs,Text,0000,0000,0000,,Břicho
Dialogue: 0,0:21:05.41,0:21:09.16,back,Text,0000,0000,0000,,Záda
Dialogue: 0,0:21:05.82,0:21:09.16,pushups,Text,0000,0000,0000,,Prsa
Dialogue: 0,0:21:07.34,0:21:10.63,Default,Usa,0000,0000,0000,,Tohle je jeden set a dokončíš ho za tři měsíce.
Dialogue: 0,0:21:09.16,0:21:09.41,abs,,0000,0000,0000,,Břicho                                     Břicho
Dialogue: 0,0:21:09.16,0:21:09.41,back,,0000,0000,0000,,Záda                                      Záda
Dialogue: 0,0:21:09.16,0:21:09.41,pushups,,0000,0000,0000,,Prsa                              Prsa
Dialogue: 0,0:21:09.41,0:21:12.70,abs,,0000,0000,0000,,Břicho                                   Břicho                                   Břicho
Dialogue: 0,0:21:09.41,0:21:12.70,back,,0000,0000,0000,,Záda                                      Záda                                   Záda
Dialogue: 0,0:21:09.41,0:21:12.70,pushups,,0000,0000,0000,,Prsa                                       Prsa                                     Prsa
Dialogue: 0,0:21:10.63,0:21:11.86,Default,Nya,0000,0000,0000,,O čem to mluvíš?
Dialogue: 0,0:21:12.97,0:21:14.54,Default,Usa,0000,0000,0000,,Vzhledem k tomu, že nejsi Eto-shin,
Dialogue: 0,0:21:14.54,0:21:18.37,Default,Usa,0000,0000,0000,,je to jen součást toho, co musíš udělat, aby sis upevnila svou osobnost.
Dialogue: 0,0:21:18.37,0:21:19.76,Default,Nya,0000,0000,0000,,Co to sakra?!
Dialogue: 0,0:21:19.76,0:21:22.42,Default,Usa,0000,0000,0000,,Teprve až dokončíš\Nzákladní trénink a zlepšíš si životní styl,
Dialogue: 0,0:21:22.42,0:21:25.68,Default,Usa,0000,0000,0000,,budeš připravena na změnu Usou P. P.
Dialogue: 0,0:21:26.49,0:21:27.48,Default,Nya,0000,0000,0000,,Nemožné.
Dialogue: 0,0:21:28.24,0:21:29.68,Default,Mo,0000,0000,0000,,Ano!
Dialogue: 0,0:21:29.94,0:21:31.55,Default,Usa,0000,0000,0000,,Neutečeš.
Dialogue: 0,0:21:31.55,0:21:34.23,Default,Usa,0000,0000,0000,,Ne teď, když jsem přijala tvou žádost na změnu!
Dialogue: 0,0:21:34.49,0:21:35.93,Default,Key,0000,0000,0000,,Bojuj, Mňauko!
Dialogue: 0,0:21:35.93,0:21:37.98,Default,Inu,0000,0000,0000,,Buď úžasná jako my!
Dialogue: 0,0:21:37.98,0:21:40.79,Default,Uri,0000,0000,0000,,Budeme ti fandit, zatímco uděláme sukiyaki.
Dialogue: 0,0:21:40.79,0:21:41.73,Default,Nya,0000,0000,0000,,Nikdy!
Dialogue: 0,0:21:41.73,0:21:43.50,Default,Usa,0000,0000,0000,,Začínáme!
Dialogue: 0,0:21:43.50,0:21:44.58,Default,Nya,0000,0000,0000,,Nechci!
Dialogue: 0,0:21:44.58,0:21:47.05,Default,Mo,0000,0000,0000,,Pomůžu ti, rukou a zadkem!
Dialogue: 0,0:21:47.05,0:21:48.62,Default,Nya,0000,0000,0000,,Nechci to dělat!
Dialogue: 0,0:21:48.20,0:21:52.70,meow 1,Text,0000,0000,0000,,{\fs18}{\b1}Králičí pacička \NKouzlo pro štestí \NČtyřlístek \NJetel \NZelenina \NHorký hrnec\NSukiyaki
Dialogue: 0,0:21:48.98,0:21:51.05,Default,Takeru,0000,0000,0000,,Tohle jsou výsledky hledání..
Dialogue: 0,0:21:51.05,0:21:52.19,Default,Takeru,0000,0000,0000,,To udělala Mňauka?
Dialogue: 0,0:21:52.70,0:21:56.21,meow 1,Text,0000,0000,0000,,{\fs18}{\b1}Zelenina\NKoření\NSukiyaki \NHovězí \NMaso \NRozveselit ho \NSmutek
Dialogue: 0,0:21:56.21,0:21:57.71,meow 1,Text,0000,0000,0000,,{\fs18}{\b1}Hovězí \NMaso \NRozveselit ho \NBez energie \NTakeru je smutný \NVčerejšek
Dialogue: 0,0:21:58.29,0:21:59.59,Default,Takeru,0000,0000,0000,,Co to sakra je?
Dialogue: 0,0:23:35.01,0:23:47.82,NEXT EP,Text,0000,0000,0000,,Příště
Dialogue: 0,0:23:35.69,0:23:38.85,Default,Usa,0000,0000,0000,,Ti, kdo se narodili v roce zajíce, \Nmají za svou šťastnou věc zlatou peněženku.
Dialogue: 0,0:23:38.85,0:23:40.16,Default,Nya,0000,0000,0000,,Ne zaječí pacičku?
Dialogue: 0,0:23:40.16,0:23:41.73,Default,Usa,0000,0000,0000,,Můžete očekávat zlepšení svého štěstí!
Dialogue: 0,0:23:41.73,0:23:45.05,Default,Usa,0000,0000,0000,,Nebo u sebe můžete mít knížku a vypadat tak chytřejší.
Dialogue: 0,0:23:45.05,0:23:47.82,Default,Nya,0000,0000,0000,,Hej! Snažíš se shazovat zaječí pacičky?!
Dialogue: 0,0:23:47.82,0:23:50.03,next ep splash 1,Title,0000,0000,0000,,Výživné ovčí obohacování
Dialogue: 0,0:23:47.82,0:23:50.03,Default,Takeru,0000,0000,0000,,Příště: Výživné ovčí obohacování.
Dialogue: 0,0:23:50.03,0:23:54.02,rabbit endcard,Text,0000,0000,0000,,Zajíc
Dialogue: 0,0:23:50.03,0:23:54.02,my producer's soul,Text,0000,0000,0000,,Moje producentská duše se chvěje!
Dialogue: 0,0:23:50.03,0:23:54.02,endcard,Text,0000,0000,0000,,Příští týden se znovu potkáme! \NIlustrace: Aisaka Yuka
