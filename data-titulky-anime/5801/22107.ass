Dialogue: 0,0:00:32.16,0:00:33.67,Chi1,Chi,0000,0000,0000,,Co to je?
Dialogue: 0,0:00:34.53,0:00:35.37,Chi1,BC,0000,0000,0000,,Měj se.
Dialogue: 0,0:00:35.37,0:00:36.22,Chi1,Chi,0000,0000,0000,,Huh?
Dialogue: 0,0:00:37.80,0:00:39.22,Chi1,Chi,0000,0000,0000,,Kam jdeš?
Dialogue: 0,0:00:39.85,0:00:42.54,Chi1,Chi,0000,0000,0000,,Hej, kam to jdeš?
Dialogue: 0,0:00:42.54,0:00:43.92,Chi1,Chi,0000,0000,0000,,Hej.
Dialogue: 0,0:00:47.91,0:00:48.65,Chi1,BC,0000,0000,0000,,Pojď za mnou.
Dialogue: 0,0:00:52.12,0:00:53.07,Chi1,Chi,0000,0000,0000,,Huh?
Dialogue: 0,0:00:54.91,0:00:56.07,Chi1,Chi,0000,0000,0000,,Schovávaná?
Dialogue: 0,0:00:56.84,0:00:59.07,Chi1,Chi,0000,0000,0000,,Hele, my hrajeme na schovávanou?
Dialogue: 0,0:01:05.48,0:01:08.59,Chi1,Chi,0000,0000,0000,,Co to děláš?!
Dialogue: 0,0:01:23.73,0:01:25.68,Chi1,Chi,0000,0000,0000,,Nenávidím je.
Dialogue: 0,0:01:44.89,0:01:47.43,Chi1,Chi,0000,0000,0000,,To byla strašidelná schovávaná.
Dialogue: 0,0:01:47.43,0:01:50.28,Chi1,Chi,0000,0000,0000,,Musím se ve schovávané zlepšit!
Dialogue: 0,0:01:51.76,0:01:52.42,Chi1,BC,0000,0000,0000,,Neboj.
Dialogue: 0,0:01:56.40,0:02:00.47,Chi1,BC,0000,0000,0000,,Z výšky už tak strašidelní nejsou.
Dialogue: 0,0:02:01.13,0:02:02.45,Chi1,Chi,0000,0000,0000,,Heh?
Dialogue: 0,0:02:08.08,0:02:10.79,Chi1,Chi,0000,0000,0000,,Chi tam nevyleze!
Dialogue: 0,0:02:12.58,0:02:13.65,Chi1,BC,0000,0000,0000,,Nevadí.
Dialogue: 0,0:02:15.47,0:02:18.11,Chi1,BC,0000,0000,0000,,To se časem naučíš.
Dialogue: 0,0:02:18.95,0:02:20.01,Chi1,Chi,0000,0000,0000,,Opravdu?!
Dialogue: 0,0:02:21.27,0:02:22.28,Chi1,BC,0000,0000,0000,,Ano.
Dialogue: 0,0:02:22.71,0:02:24.62,Chi1,BC,0000,0000,0000,,Jsi přece kočka, ne?
Dialogue: 0,0:02:25.92,0:02:27.45,Chi1,Chi,0000,0000,0000,,Kočka?
Dialogue: 0,0:02:29.69,0:02:30.46,Chi1,BC,0000,0000,0000,,Měj se.
Dialogue: 0,0:02:31.31,0:02:32.61,Chi1,Chi,0000,0000,0000,,Ne!
Dialogue: 0,0:02:34.85,0:02:36.40,Chi1,Chi,0000,0000,0000,,Já nejsem kočka!
Dialogue: 0,0:02:37.36,0:02:38.64,Chi1,Chi,0000,0000,0000,,Jsem Chi!
