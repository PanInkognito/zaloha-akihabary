Dialogue: 0,0:36:00.22,0:36:05.49,Default,,0000,0000,0000,,~ {\i1}Když zhasnou světla v mém pokoji,{\i0} ~
Dialogue: 0,0:36:05.49,0:36:10.69,Default,,0000,0000,0000,,~ {\i1}objevíš se mi před očima.{\i0} ~
Dialogue: 0,0:36:10.69,0:36:13.49,Default,,0000,0000,0000,,~ {\i1}I když zkouším sevřít víčka pevněji...{\i0} ~
Dialogue: 0,0:36:13.49,0:36:21.29,Default,,0000,0000,0000,,~ {\i1}stále se mě držíš...{\i0} ~
Dialogue: 0,0:36:21.29,0:36:23.69,Default,,0000,0000,0000,,~ {\i1}jsi v mém srdci....{\i0} ~
Dialogue: 0,0:36:23.69,0:36:27.76,Default,,0000,0000,0000,,~ {\i1}Všechno mi rozjasňuješ,{\i0} ~
Dialogue: 0,0:36:27.76,0:36:30.09,Default,,0000,0000,0000,,~ {\i1}nedokážu ti uniknout.{\i0} ~
Dialogue: 0,0:36:30.09,0:36:35.22,Default,,0000,0000,0000,,~ {\i1}Miláčku...{\i0} ~
Dialogue: 0,0:36:35.22,0:36:40.42,Default,,0000,0000,0000,,~ {\i1}i když vzpomínky vyblednou,{\i0} ~
Dialogue: 0,0:36:40.42,0:36:47.22,Default,,0000,0000,0000,,~ {\i1}my zůstanem pod jedním nebem.{\i0} ~
Dialogue: 0,0:36:47.22,0:36:52.42,Default,,0000,0000,0000,,~ {\i1}Najednou zhasnou světla...{\i0} ~
Dialogue: 0,0:36:52.42,0:36:57.69,Default,,0000,0000,0000,,~ {\i1}a dny se zpomalí.{\i0} ~
Dialogue: 0,0:36:57.69,0:37:00.22,Default,,0000,0000,0000,,~ {\i1}Jakoby se nic nestalo...{\i0} ~
Dialogue: 0,0:37:00.22,0:37:04.22,Default,,0000,0000,0000,,~ {\i1}Vrátíš se ke mně znovu?{\i0} ~
Dialogue: 0,0:37:04.22,0:37:06.62,Default,,0000,0000,0000,,~ {\i1}Co bych měla udělat?{\i0} ~
Dialogue: 0,0:37:06.62,0:37:11.82,Default,,0000,0000,0000,,~ {\i1}Miláčku...{\i0} ~
Dialogue: 0,0:37:11.82,0:37:17.02,Default,,0000,0000,0000,,~ {\i1}i když vzpomínky vyblednou...{\i0} ~
Dialogue: 0,0:37:17.02,0:37:22.16,Default,,0000,0000,0000,,~ {\i1}Miláčku...{\i0} ~
Dialogue: 0,0:37:22.16,0:37:27.42,Default,,0000,0000,0000,,~ {\i1}přijď,najdi mě a zůstaň po mém boku.{\i0} ~
Dialogue: 0,0:37:27.42,0:37:32.62,Default,,0000,0000,0000,,~ {\i1}Právě to teď potřebuju.{\i0} ~
Dialogue: 0,0:37:32.62,0:37:37.89,Default,,0000,0000,0000,,~ {\i1}Bez tebe jsou všechny dny stejné.{\i0} ~
Dialogue: 0,0:37:37.89,0:37:43.09,Default,,0000,0000,0000,,~ {\i1}Kdy budu moct jen o tobě snít?{\i0} ~
Dialogue: 0,0:37:43.09,0:37:49.22,Default,,0000,0000,0000,,~ {\i1}Jsme pod jedním nebem.{\i0} ~
