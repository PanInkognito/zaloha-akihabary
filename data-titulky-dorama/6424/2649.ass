Dialogue: 0,0:00:05.00,0:00:25.00,Default,,0000,0000,0000,,SK Titulky: seesska.blog.cz/\NHyumi\N ENG: DramaFever
Dialogue: 0,0:00:44.83,0:00:46.13,Default,,0000,0000,0000,,Chutia dobre.
Dialogue: 0,0:00:49.43,0:00:51.13,Default,,0000,0000,0000,,Ak ich chceš vyhodiť,
Dialogue: 0,0:00:51.13,0:00:53.04,Default,,0000,0000,0000,,môžem si ich nechať?
Dialogue: 0,0:00:53.04,0:00:56.44,Default,,0000,0000,0000,,Ako vidíš, mám rád jablká.
Dialogue: 0,0:01:07.35,0:01:12.26,Default,,0000,0000,0000,,Ja...utečiem.
Dialogue: 0,0:01:12.26,0:01:14.06,Default,,0000,0000,0000,,Utečiem!
Dialogue: 0,0:01:14.06,0:01:16.46,Default,,0000,0000,0000,,Čo je na tom zlé?
Dialogue: 0,0:01:16.46,0:01:20.46,Default,,0000,0000,0000,,Utečiem!
Dialogue: 0,0:01:30.57,0:01:34.98,Default,,0000,0000,0000,,Väčšinou dievčatá vyzerajú pekne keď plačú.
Dialogue: 0,0:01:35.88,0:01:38.58,Default,,0000,0000,0000,,Ty nie si na plač stvorená.
Dialogue: 0,0:01:38.58,0:01:42.99,Default,,0000,0000,0000,,Prestaň plakať. Prestaň!
Dialogue: 0,0:02:49.35,0:02:50.75,Default,,0000,0000,0000,,Čo to?
Dialogue: 0,0:02:50.75,0:02:54.06,Default,,0000,0000,0000,,Samotárka a JB randia?
Dialogue: 0,0:02:54.06,0:02:54.36,Default,,0000,0000,0000,,Naozaj?
Dialogue: 0,0:02:54.36,0:02:54.96,Default,,0000,0000,0000,,Vážne?
Dialogue: 0,0:02:54.96,0:02:56.56,Default,,0000,0000,0000,,Ukáž.
Dialogue: 0,0:02:56.56,0:02:57.66,Default,,0000,0000,0000,,Vážne, aká si malicherná.
Dialogue: 0,0:02:57.66,0:02:58.76,Default,,0000,0000,0000,,Ty.
Dialogue: 0,0:02:59.16,0:03:00.36,Default,,0000,0000,0000,,Mobily sú tu zakázané.
Dialogue: 0,0:03:00.36,0:03:02.17,Default,,0000,0000,0000,,Ako si ho sem doniesla?
Dialogue: 0,0:03:02.17,0:03:03.47,Default,,0000,0000,0000,,Čože?
Dialogue: 0,0:03:03.47,0:03:05.37,Default,,0000,0000,0000,,Ako tu môže platiť tak bezdôvodný zákon?
Dialogue: 0,0:03:07.27,0:03:09.17,Default,,0000,0000,0000,,To je SiWoo.
Dialogue: 0,0:03:09.17,0:03:11.47,Default,,0000,0000,0000,,Na Kirin strednej je zakázané randiť...
Dialogue: 0,0:03:11.47,0:03:13.98,Default,,0000,0000,0000,,Porušili to?
Dialogue: 0,0:03:13.98,0:03:15.28,Default,,0000,0000,0000,,Také nečakané.
Dialogue: 0,0:03:15.28,0:03:17.68,Default,,0000,0000,0000,,{\i1}SiWoo!{\i0}
Dialogue: 0,0:03:20.38,0:03:23.59,Default,,0000,0000,0000,,Konečne si sa vrátil!
Dialogue: 0,0:03:23.59,0:03:25.79,Default,,0000,0000,0000,,Skvelá práca! Skvelá práca!
Dialogue: 0,0:03:25.79,0:03:26.49,Default,,0000,0000,0000,,Hej...
Dialogue: 0,0:03:26.49,0:03:29.49,Default,,0000,0000,0000,,Povedal som ti, aby si takáto nebola,\Nkeď je tu toľko ľudí naokolo.
Dialogue: 0,0:03:29.49,0:03:31.99,Default,,0000,0000,0000,,Aký chutný. Chýbal si mi.
Dialogue: 0,0:03:31.99,0:03:34.40,Default,,0000,0000,0000,,Prečo si prišiel až teraz?
Dialogue: 0,0:03:34.40,0:03:36.60,Default,,0000,0000,0000,,Si zvedavá, prečo si ma doteraz nevidela?
Dialogue: 0,0:03:36.60,0:03:39.20,Default,,0000,0000,0000,,Preto, lebo som chcel aby si takáto bola, no čo?
Dialogue: 0,0:03:41.90,0:03:43.91,Default,,0000,0000,0000,,Nabudúce, nenecháš nunu v strachu, že?
Dialogue: 0,0:03:43.91,0:03:46.01,Default,,0000,0000,0000,,Dobre, dobre, dobre.
Dialogue: 0,0:03:46.01,0:03:48.81,Default,,0000,0000,0000,,Fajn, fajn.
Dialogue: 0,0:03:48.81,0:03:50.31,Default,,0000,0000,0000,,Aigoo. Aký poslušný.
Dialogue: 0,0:03:58.02,0:04:00.22,Default,,0000,0000,0000,,Prečo sa tak trasieš?
Dialogue: 0,0:04:00.22,0:04:02.33,Default,,0000,0000,0000,,Si chorý?
Dialogue: 0,0:04:29.85,0:04:35.76,Default,,0000,0000,0000,,Nemusíš ma ľutovať.
Dialogue: 0,0:04:35.76,0:04:38.36,Default,,0000,0000,0000,,Kto povedal, že vyzeráš ľútostivo?
Dialogue: 0,0:04:38.36,0:04:41.06,Default,,0000,0000,0000,,Nemám rád, keď ženy plačú.
Dialogue: 0,0:04:59.98,0:05:02.69,Default,,0000,0000,0000,,Čo to za vzhľad?
Dialogue: 0,0:05:02.69,0:05:06.09,Default,,0000,0000,0000,,Nepovedala som?
Dialogue: 0,0:05:06.09,0:05:13.70,Default,,0000,0000,0000,,Neznášam najviac na svete, keď máš na sebe prach.
Dialogue: 0,0:05:13.70,0:05:16.10,Default,,0000,0000,0000,,Keďže sa takto o seba staráš,
Dialogue: 0,0:05:16.10,0:05:18.20,Default,,0000,0000,0000,,Shin HaeSung z nižšej vrstvy,
Dialogue: 0,0:05:18.20,0:05:21.00,Default,,0000,0000,0000,,sa bude k tebe túliť.
Dialogue: 0,0:05:21.90,0:05:23.91,Default,,0000,0000,0000,,Miešanie sa s tými z nižšej úrovne,
Dialogue: 0,0:05:23.91,0:05:28.01,Default,,0000,0000,0000,,si začala ako prvá ty, nie je tak?
Dialogue: 0,0:05:28.01,0:05:29.81,Default,,0000,0000,0000,,Ak Shin HaeSung je z nízkej vrstvy,
Dialogue: 0,0:05:29.81,0:05:32.62,Default,,0000,0000,0000,,nie je ním aj Jin YuJin?
Dialogue: 0,0:05:32.62,0:05:39.02,Default,,0000,0000,0000,,Nebodaj sa cítiš menej cenný pred ním?
Dialogue: 0,0:05:39.02,0:05:42.02,Default,,0000,0000,0000,,Zrejme mám pravdu.
Dialogue: 0,0:05:42.02,0:05:45.03,Default,,0000,0000,0000,,Žiarliš?
Dialogue: 0,0:05:45.03,0:05:51.93,Default,,0000,0000,0000,,Robíš to preto, aby si mi schválne ublížil?
Dialogue: 0,0:05:51.93,0:05:55.64,Default,,0000,0000,0000,,Ak naozaj to robíš...
Dialogue: 0,0:05:55.64,0:05:59.04,Default,,0000,0000,0000,,tak si NG.\N(NG - Nie dobrý)
Dialogue: 0,0:05:59.04,0:06:02.55,Default,,0000,0000,0000,,Nevieš čo je NG?
Dialogue: 0,0:06:02.55,0:06:04.75,Default,,0000,0000,0000,,Už odkedy si mal zakázané aktivity\N a prišiel do tejto školy,
Dialogue: 0,0:06:04.75,0:06:08.45,Default,,0000,0000,0000,,tak je niečo zle s tvojou dušou.
Dialogue: 0,0:06:08.45,0:06:12.86,Default,,0000,0000,0000,,Máš súcit k nemelodickému dievčaťu,
Dialogue: 0,0:06:12.86,0:06:16.16,Default,,0000,0000,0000,,nenechaj svoj štandart upadnúť.
Dialogue: 0,0:06:16.16,0:06:19.86,Default,,0000,0000,0000,,Pretože sa kvôli tomu cítim lacná.
Dialogue: 0,0:06:19.86,0:06:21.26,Default,,0000,0000,0000,,Lacná?
Dialogue: 0,0:06:21.26,0:06:23.47,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:06:24.27,0:06:28.47,Default,,0000,0000,0000,,Celý čas, čo sme predtým spolu strávili,
Dialogue: 0,0:06:28.47,0:06:32.78,Default,,0000,0000,0000,,mi dodáva pocit, akoby som bola lacnou ikonou.
Dialogue: 0,0:06:34.78,0:06:37.58,Default,,0000,0000,0000,,Odvolaj to.
Dialogue: 0,0:06:38.08,0:06:41.38,Default,,0000,0000,0000,,Túžba trvá len chvíľku.
Dialogue: 0,0:06:41.38,0:06:44.89,Default,,0000,0000,0000,,Ľudské pocity sa menia veľmi ľahko.
Dialogue: 0,0:06:46.49,0:06:50.29,Default,,0000,0000,0000,,Pripomeniem ti...
Dialogue: 0,0:06:50.69,0:06:54.30,Default,,0000,0000,0000,,že tvoju slabosť voči ženám čo plačú...
Dialogue: 0,0:06:54.30,0:06:57.70,Default,,0000,0000,0000,,je zlozvyk, ktorý si ešte neopustil.
Dialogue: 0,0:06:58.50,0:07:02.21,Default,,0000,0000,0000,,Povedal som ti, aby si to odvolala.
Dialogue: 0,0:07:04.41,0:07:07.61,Default,,0000,0000,0000,,Pozerajúc sa na teba, čo si spravil,
Dialogue: 0,0:07:08.21,0:07:13.32,Default,,0000,0000,0000,,mi pripomína prečo sme sa rozišli.
Dialogue: 0,0:07:16.42,0:07:26.03,Default,,0000,0000,0000,,To, že máš tak slabé srdce, je skutočne NG.
Dialogue: 0,0:07:26.03,0:07:29.83,Default,,0000,0000,0000,,Myslela som si, že si sa zmenil.
Dialogue: 0,0:07:29.83,0:07:33.04,Default,,0000,0000,0000,,Ale si stále taký istý.
Dialogue: 0,0:07:59.36,0:08:03.17,Default,,0000,0000,0000,,Presne ako príslovie "ponúkni prst a zoberie celú ruku",
Dialogue: 0,0:08:03.17,0:08:06.47,Default,,0000,0000,0000,,si naozaj schopná.
Dialogue: 0,0:08:07.47,0:08:11.57,Default,,0000,0000,0000,,Použiť jablká aby si zo seba spravila chúďatko.
Dialogue: 0,0:08:11.57,0:08:13.38,Default,,0000,0000,0000,,Asi sa to ťažko znáša.
Dialogue: 0,0:08:13.38,0:08:15.58,Default,,0000,0000,0000,,Vďaka práci otca na farme,
Dialogue: 0,0:08:15.58,0:08:18.28,Default,,0000,0000,0000,,mám tieto jablká.
Dialogue: 0,0:08:18.28,0:08:20.88,Default,,0000,0000,0000,,Tak nehovor len tak o nich.
Dialogue: 0,0:08:20.88,0:08:23.59,Default,,0000,0000,0000,,Vie to aj tvoj otec?
Dialogue: 0,0:08:23.59,0:08:28.19,Default,,0000,0000,0000,,Že používaš jablká na zvábenie muža.
Dialogue: 0,0:08:31.49,0:08:34.90,Default,,0000,0000,0000,,Viem, že som spravila zle.
Dialogue: 0,0:08:34.90,0:08:38.50,Default,,0000,0000,0000,,Spravila som chybu, lebo som nič dobré neurobila.
Dialogue: 0,0:08:38.50,0:08:41.30,Default,,0000,0000,0000,,Nemôžeš mi iba odpustiť,
Dialogue: 0,0:08:41.30,0:08:43.81,Default,,0000,0000,0000,,ako kamarátka?
Dialogue: 0,0:08:43.81,0:08:46.41,Default,,0000,0000,0000,,Kamarátka?
Dialogue: 0,0:08:46.41,0:08:52.22,Default,,0000,0000,0000,,Za to že sme v tej istej škole,\N a v tej istej izbe,
Dialogue: 0,0:08:52.22,0:08:56.12,Default,,0000,0000,0000,,si niečo zle vysvetľuješ...
Dialogue: 0,0:08:56.12,0:09:00.12,Default,,0000,0000,0000,,Nie som tvoja kamarátka.
Dialogue: 0,0:09:00.12,0:09:01.82,Default,,0000,0000,0000,,Len sa nepredvádzaj s tým, ako si balíš veci.
Dialogue: 0,0:09:01.82,0:09:04.83,Default,,0000,0000,0000,,Ale ak to chceš spraviť, do toho.
Dialogue: 0,0:09:09.83,0:09:12.64,Default,,0000,0000,0000,,Aj keby to bolo pre to aby som ťa nevidela, aj tak odídem.
Dialogue: 0,0:09:12.64,0:09:14.44,Default,,0000,0000,0000,,Tak sa neboj.
Dialogue: 0,0:09:14.44,0:09:18.14,Default,,0000,0000,0000,,Nie je to skvelá správa pre uši?
Dialogue: 0,0:09:18.14,0:09:26.15,Default,,0000,0000,0000,,Oh áno, a keď odídeš,\N nenos to tričko.
Dialogue: 0,0:09:26.15,0:09:31.85,Default,,0000,0000,0000,,Musíš dať každému najavo, že\N si škaredým káčatkom?
Dialogue: 0,0:09:33.36,0:09:35.66,Default,,0000,0000,0000,,Už keď vidím vaše tváre,
Dialogue: 0,0:09:35.66,0:09:39.16,Default,,0000,0000,0000,,som zmätená.
Dialogue: 0,0:10:17.70,0:10:20.60,Default,,0000,0000,0000,,Deje sa niečo?
Dialogue: 0,0:10:20.60,0:10:22.61,Default,,0000,0000,0000,,Prečo na nej vyhrávaš tak hlasno?
Dialogue: 0,0:10:22.61,0:10:24.31,Default,,0000,0000,0000,,Ale nie.
Dialogue: 0,0:10:24.31,0:10:27.91,Default,,0000,0000,0000,,Iba som sa snažil zložiť HaeSungovú pieseň.
Dialogue: 0,0:10:30.51,0:10:37.42,Default,,0000,0000,0000,,Ak na nej hráš, uvoľňuje to stres?
Dialogue: 0,0:10:38.32,0:10:41.42,Default,,0000,0000,0000,,Chceš to skúsiť?
Dialogue: 0,0:10:59.94,0:11:01.54,Default,,0000,0000,0000,,Drž ju trochu pevnejšie.
Dialogue: 0,0:11:01.54,0:11:05.65,Default,,0000,0000,0000,,Toto je nota A.
Dialogue: 0,0:11:22.57,0:11:27.07,Default,,0000,0000,0000,,Chcem niečo povedať,
Dialogue: 0,0:11:27.07,0:11:31.17,Default,,0000,0000,0000,,tak mohla by si nás na chvíľku nechať samých?
Dialogue: 0,0:11:31.97,0:11:35.58,Default,,0000,0000,0000,,Len to povedz.
Dialogue: 0,0:11:40.48,0:11:44.09,Default,,0000,0000,0000,,Keď sme mali nácvik na tej ulici,
Dialogue: 0,0:11:44.09,0:11:46.49,Default,,0000,0000,0000,,za to, že som odišla bez opýtania...
Dialogue: 0,0:11:46.49,0:11:49.09,Default,,0000,0000,0000,,sa ospravedlňujem.
Dialogue: 0,0:11:50.79,0:11:53.20,Default,,0000,0000,0000,,To je v pohode...
Dialogue: 0,0:11:53.20,0:11:56.60,Default,,0000,0000,0000,,aj tak to tak bolo lepšie.
Dialogue: 0,0:11:56.60,0:12:02.00,Default,,0000,0000,0000,,Keďže Lian spievala, odozva bola dobrá.
Dialogue: 0,0:12:05.61,0:12:12.22,Default,,0000,0000,0000,,Stále sa na mňa hneváš?
Dialogue: 0,0:12:12.72,0:12:16.62,Default,,0000,0000,0000,,To by bola strata času.
Dialogue: 0,0:12:16.62,0:12:19.42,Default,,0000,0000,0000,,Vlastne som ti vďačný...
Dialogue: 0,0:12:19.42,0:12:22.13,Default,,0000,0000,0000,,keby to bolo skutočné vystúpenie\N a ty by si utiekla,
Dialogue: 0,0:12:22.13,0:12:27.33,Default,,0000,0000,0000,,už len pomyslenie na to je hrôzostrašné.
Dialogue: 0,0:12:27.73,0:12:32.74,Default,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:12:34.44,0:12:36.94,Default,,0000,0000,0000,,To je všetko čo vieš povedať?
Dialogue: 0,0:12:36.94,0:12:42.35,Default,,0000,0000,0000,,Aký je tvoj pravý dôvod, aby si sa stala speváčkou?
Dialogue: 0,0:12:42.35,0:12:46.75,Default,,0000,0000,0000,,Ak nemáš talent, maj aspoň túžbu.
Dialogue: 0,0:12:47.75,0:12:51.15,Default,,0000,0000,0000,,Aký je tvoj pravý motív?
Dialogue: 0,0:12:51.15,0:12:53.66,Default,,0000,0000,0000,,Je naozaj taká zábava zahrávať sa s ľuďmi?
Dialogue: 0,0:12:53.66,0:12:55.16,Default,,0000,0000,0000,,Hovoriť mi také veci-
Dialogue: 0,0:12:55.16,0:12:58.86,Default,,0000,0000,0000,,Plačeš si len tak pred hocikým...
Dialogue: 0,0:12:58.86,0:13:02.37,Default,,0000,0000,0000,,dúfajúc, že ťa pochopí.
Dialogue: 0,0:13:02.37,0:13:06.47,Default,,0000,0000,0000,,Nepovedz mi...že si myslíš, že môžeš mužov zviesť
Dialogue: 0,0:13:06.47,0:13:10.47,Default,,0000,0000,0000,,tým, že sa budeš správať ako chudera?
Dialogue: 0,0:13:10.67,0:13:14.08,Default,,0000,0000,0000,,Ak ma nepoznáš,
Dialogue: 0,0:13:14.08,0:13:15.78,Default,,0000,0000,0000,,ako môžeš mi niečo také povedať?
Dialogue: 0,0:13:15.78,0:13:19.88,Default,,0000,0000,0000,,Nie, poznám ťa veľmi dobre.
Dialogue: 0,0:13:19.88,0:13:21.28,Default,,0000,0000,0000,,Aj dôvod, prečo niekoľko rokov sa ti nedarí na konkurzov,
Dialogue: 0,0:13:21.28,0:13:24.99,Default,,0000,0000,0000,,že ťa nezobrali ani raz...
Dialogue: 0,0:13:24.99,0:13:29.39,Default,,0000,0000,0000,,Rozumiem tomu veľmi dobre, ale prečo nie ty?
Dialogue: 0,0:13:29.39,0:13:33.00,Default,,0000,0000,0000,,Správaš sa ako chúďatko, zranená a vystresovaná...
Dialogue: 0,0:13:33.00,0:13:35.10,Default,,0000,0000,0000,,Nemôžem ti odpustiť.
Dialogue: 0,0:13:35.10,0:13:41.60,Default,,0000,0000,0000,,Stačí. Už som s tebou skončila.
Dialogue: 0,0:13:44.81,0:13:49.61,Default,,0000,0000,0000,,Nahrávač. Vráť mi ho predtým než odídeš.
Dialogue: 0,0:14:29.35,0:14:31.75,Default,,0000,0000,0000,,Prečo hovoríš veci, ktoré nemyslíš vážne?
Dialogue: 0,0:14:35.86,0:14:39.76,Default,,0000,0000,0000,,Prepáč. Naučím ťa hrať na gitare inokedy.
Dialogue: 0,0:14:51.57,0:14:57.48,Default,,0000,0000,0000,,=Viem, že si bol ku mne hnusný,=
Dialogue: 0,0:14:57.48,0:15:01.98,Default,,0000,0000,0000,,=aby si ma niečo naučil.=
Dialogue: 0,0:15:03.69,0:15:05.39,Default,,0000,0000,0000,,=Myslel si si, že to naozaj poviem?=
Dialogue: 0,0:15:05.39,0:15:08.19,Default,,0000,0000,0000,,=Ty otravný idiot!=
Dialogue: 0,0:15:08.19,0:15:09.49,Default,,0000,0000,0000,,=Ako bláznivý človek.\NKaždú chvíľu máš rád niečo iné.=
Dialogue: 0,0:15:09.49,0:15:12.09,Default,,0000,0000,0000,,=Raz prichádzaš raz odchádzaš, prichádzaš a odchádzaš.=
Dialogue: 0,0:15:12.09,0:15:14.80,Default,,0000,0000,0000,,{\i1}Kto som, kto som?{\i0}
Dialogue: 0,0:15:14.80,0:15:18.90,Default,,0000,0000,0000,,=Všetci ma ignorujú, myslíš, že je to ľahké?=
Dialogue: 0,0:15:18.90,0:15:21.70,Default,,0000,0000,0000,,=Talentovaný človek ako ty...=
Dialogue: 0,0:15:25.01,0:15:26.71,Default,,0000,0000,0000,,Som náladový?
Dialogue: 0,0:15:26.71,0:15:28.21,Default,,0000,0000,0000,,Prečo je to tak frustrujúce?
Dialogue: 0,0:15:28.21,0:15:31.01,Default,,0000,0000,0000,,Oh! Tiež som taký.
Dialogue: 0,0:15:31.01,0:15:34.72,Default,,0000,0000,0000,,Moja nálada sa mení z dobrej na zlú a opačne\N niekoľkokrát denne.
Dialogue: 0,0:15:35.12,0:15:37.92,Default,,0000,0000,0000,,Kedy sa to stáva?
Dialogue: 0,0:15:37.92,0:15:42.53,Default,,0000,0000,0000,,Keď ju nemôžem vidieť, cítim sa hrozne.
Dialogue: 0,0:15:42.53,0:15:45.23,Default,,0000,0000,0000,,A keď ju vidím, cítim sa dobre.
Dialogue: 0,0:15:46.33,0:15:50.43,Default,,0000,0000,0000,,A keď ju vidím s niekým iným,\N som naštvaný.
Dialogue: 0,0:15:50.43,0:15:54.34,Default,,0000,0000,0000,,Ju? Hovoríš o Hane, však?
Dialogue: 0,0:15:54.34,0:15:56.04,Default,,0000,0000,0000,,Máš ju rád, že?
Dialogue: 0,0:15:56.04,0:15:58.24,Default,,0000,0000,0000,,Nie, nemám ju rád...
Dialogue: 0,0:15:58.24,0:15:59.74,Default,,0000,0000,0000,,Tak viditeľné!
Dialogue: 0,0:15:59.74,0:16:02.25,Default,,0000,0000,0000,,Chytila si tvoje srdce odkedy ste spolu spievali, že?
Dialogue: 0,0:16:03.45,0:16:04.75,Default,,0000,0000,0000,,Dokonca si ju nechal spievať tvoju časť,
Dialogue: 0,0:16:04.75,0:16:07.25,Default,,0000,0000,0000,,a stále ju šťastne nasledoval.
Dialogue: 0,0:16:07.65,0:16:09.15,Default,,0000,0000,0000,,Amatér.
Dialogue: 0,0:16:09.15,0:16:11.05,Default,,0000,0000,0000,,Nie som amatér.
Dialogue: 0,0:16:11.95,0:16:13.06,Default,,0000,0000,0000,,Ani nevieš rozoznať verejný vzhľad od súkromného,
Dialogue: 0,0:16:13.06,0:16:15.66,Default,,0000,0000,0000,,a zamiluješ sa jednostranne ako amatér.
Dialogue: 0,0:16:15.66,0:16:17.16,Default,,0000,0000,0000,,Ak to nie je byť amatérom, tak potom čo?
Dialogue: 0,0:16:17.16,0:16:20.06,Default,,0000,0000,0000,,Nie je to jednostranné!
Dialogue: 0,0:16:21.36,0:16:24.57,Default,,0000,0000,0000,,Tiež ma má rada.
Dialogue: 0,0:16:27.37,0:16:29.77,Default,,0000,0000,0000,,Hej, čo má na tebe rada?
Dialogue: 0,0:16:29.77,0:16:33.78,Default,,0000,0000,0000,,Chutný, chutný, chutný!\N Robí mi takéto veci.
Dialogue: 0,0:16:33.78,0:16:36.48,Default,,0000,0000,0000,,Keď dievča povie chlapcov, že je roztomilý,
Dialogue: 0,0:16:36.48,0:16:39.48,Default,,0000,0000,0000,,tak je to synonymum od toho, že ho má rada.
Dialogue: 0,0:16:39.48,0:16:42.19,Default,,0000,0000,0000,,Hana to robí aj SiWoo-ovi.
Dialogue: 0,0:16:42.19,0:16:43.49,Default,,0000,0000,0000,,Je taká ku všetkým.
Dialogue: 0,0:16:43.49,0:16:44.89,Default,,0000,0000,0000,,Je taká ku všetkým.
Dialogue: 0,0:16:48.49,0:16:52.20,Default,,0000,0000,0000,,Aigoo! Dospelý muž sa ešte stále rád necháva naštvať.
Dialogue: 0,0:16:55.10,0:16:56.10,Default,,0000,0000,0000,,Nemôžem zaspať,
Dialogue: 0,0:16:56.10,0:16:58.60,Default,,0000,0000,0000,,asi si pôjdem zatrénovať.
Dialogue: 0,0:17:36.54,0:17:38.64,Default,,0000,0000,0000,,Zdravím všetkých.
Dialogue: 0,0:17:38.64,0:17:44.15,Default,,0000,0000,0000,,Som študentka z Kirin Strednej z 3.ročníka, Shin HaeSung.
Dialogue: 0,0:17:46.55,0:17:49.25,Default,,0000,0000,0000,,More oceánu, nebeská hviezda.\N(Hae - more, Sung - hviezda)
Dialogue: 0,0:17:50.15,0:17:52.46,Default,,0000,0000,0000,,Hviezda ukrytá v hlubinách oceánu.
Dialogue: 0,0:17:53.86,0:17:56.26,Default,,0000,0000,0000,,Doteraz,
Dialogue: 0,0:17:56.26,0:17:58.66,Default,,0000,0000,0000,,som sa nikdy poriadne nepredviedla...
Dialogue: 0,0:18:01.06,0:18:02.57,Default,,0000,0000,0000,,to je v poriadku.
Dialogue: 0,0:18:03.77,0:18:07.77,Default,,0000,0000,0000,,Vždy bude deň,\N keď zažiarim na kúsku oblohy.
Dialogue: 0,0:18:12.28,0:18:20.98,Default,,0000,0000,0000,,Slečna HaeSung, naozaj plánujete,\N odísť z Kirin Strednej?
Dialogue: 0,0:18:22.69,0:18:29.49,Default,,0000,0000,0000,,Áno. Plánujem vzdať sa snu herečky,
Dialogue: 0,0:18:29.49,0:18:31.89,Default,,0000,0000,0000,,a budem sa snažiť nájsť inú cestu.
Dialogue: 0,0:18:33.50,0:18:38.40,Default,,0000,0000,0000,,Tak potom slečna HaeSung,
Dialogue: 0,0:18:38.40,0:18:44.81,Default,,0000,0000,0000,,nebude speváčkou...\N Ak nebudete speváčkou,
Dialogue: 0,0:18:46.91,0:18:50.11,Default,,0000,0000,0000,,premýšľali ste o tom, čo iné by ste chceli robiť?
Dialogue: 0,0:18:51.01,0:18:59.22,Default,,0000,0000,0000,,Nie...nie.
Dialogue: 0,0:19:01.72,0:19:08.93,Default,,0000,0000,0000,,Okrem speváčky,\N som o inej ceste nepremýšľala.
Dialogue: 0,0:19:17.94,0:19:20.34,Default,,0000,0000,0000,,{\i1}Ďalší, číslo 314, Jang WooJae.{\i0}
Dialogue: 0,0:19:20.34,0:19:21.84,Default,,0000,0000,0000,,{\i1}Hej, WooJae.{\i0}
Dialogue: 0,0:19:22.45,0:19:24.85,Default,,0000,0000,0000,,{\i1}WooJae, mal by si si nájsť inú cestu,{\i0}
Dialogue: 0,0:19:24.85,0:19:26.85,Default,,0000,0000,0000,,{\i1}a už nestrácať svoj čas.{\i0}
Dialogue: 0,0:19:28.05,0:19:35.56,Default,,0000,0000,0000,,{\i1}- Tu niet inej cesty.\N- Ako?{\i0}
Dialogue: 0,0:19:44.07,0:19:45.57,Default,,0000,0000,0000,,{\i1}Prosím vysloboďte ma.{\i0}
Dialogue: 0,0:19:45.97,0:19:48.17,Default,,0000,0000,0000,,{\i1}Ak sa nedostanem sem,{\i0}
Dialogue: 0,0:19:48.17,0:19:50.67,Default,,0000,0000,0000,,{\i1}tak sa zabijem!{\i0}
Dialogue: 0,0:19:51.17,0:19:52.88,Default,,0000,0000,0000,,{\i1}Berte to ako záchranu života človeka a-{\i0}
Dialogue: 0,0:19:52.88,0:19:55.68,Default,,0000,0000,0000,,{\i1}WooJae. WooJae.{\i0}
Dialogue: 0,0:19:55.68,0:19:57.78,Default,,0000,0000,0000,,{\i1}Takto sa kláňať už vyšlo z módy...{\i0}
Dialogue: 0,0:19:57.78,0:19:59.38,Default,,0000,0000,0000,,{\i1}dnes si už piaty.{\i0}
Dialogue: 0,0:19:59.38,0:20:00.18,Default,,0000,0000,0000,,{\i1}Vieš to?{\i0}
Dialogue: 0,0:20:00.18,0:20:02.39,Default,,0000,0000,0000,,{\i1}Každý povedal, že zomrie.{\i0}
Dialogue: 0,0:20:02.39,0:20:05.69,Default,,0000,0000,0000,,{\i1}Ako môžem všetkých zachrániť?{\i0}
Dialogue: 0,0:20:07.39,0:20:09.09,Default,,0000,0000,0000,,{\i1}Prosím odíď.{\i0}
Dialogue: 0,0:20:09.09,0:20:10.29,Default,,0000,0000,0000,,{\i1}Ďalší!{\i0}
Dialogue: 0,0:20:13.10,0:20:14.40,Default,,0000,0000,0000,,{\i1}Takže tvoje meno je Jang WooJae?{\i0}
Dialogue: 0,0:20:20.20,0:20:25.11,Default,,0000,0000,0000,,{\i1}Hudba nie je niečo, čo dokážeš len učením sa.{\i0}
Dialogue: 0,0:20:25.11,0:20:29.51,Default,,0000,0000,0000,,{\i1}Inšpiráciu, talent a vnútorné pocity,{\i0}
Dialogue: 0,0:20:29.51,0:20:31.11,Default,,0000,0000,0000,,{\i1}to všetko musíš mať.{\i0}
Dialogue: 0,0:20:33.52,0:20:37.52,Default,,0000,0000,0000,,{\i1}Nesnaž sa niekoho neúspečne imitovať,\N len preto že vyzerá skvele.{\i0}
Dialogue: 0,0:20:38.02,0:20:40.62,Default,,0000,0000,0000,,{\i1}Ako môže tanečník postrádať sebavedomie?{\i0}
Dialogue: 0,0:20:40.62,0:20:41.82,Default,,0000,0000,0000,,{\i1}Prečo byť tak skleslý?{\i0}
Dialogue: 0,0:20:41.82,0:20:45.53,Default,,0000,0000,0000,,{\i1}No tak. Zdvihni plecia, sadni si rovno.{\i0}
Dialogue: 0,0:20:45.53,0:20:47.63,Default,,0000,0000,0000,,{\i1}Hrudník von, schovaj bradu.{\i0}
Dialogue: 0,0:20:47.63,0:20:48.33,Default,,0000,0000,0000,,{\i1}Hrudník von, schovaj bradu.{\i0}
Dialogue: 0,0:20:48.33,0:20:50.23,Default,,0000,0000,0000,,{\i1}Vzpriam sa, nechaj svoju dušu vzrásť.{\i0}
Dialogue: 0,0:20:50.23,0:20:52.34,Default,,0000,0000,0000,,{\i1}Keď si hral na gitare,\Nbol si tak zhrbený.{\i0}
Dialogue: 0,0:20:52.34,0:20:56.14,Default,,0000,0000,0000,,{\i1}Pozri sa na mňa, aký som sebavedomý. G nota!{\i0}
Dialogue: 0,0:20:56.14,0:20:58.54,Default,,0000,0000,0000,,{\i1}Táto G nota je moja! Plná sebavedomia!{\i0}
Dialogue: 0,0:21:41.88,0:21:48.89,Default,,0000,0000,0000,,-=Hello to myself*, pre HaeSung=-\N (*Ahoj ja)
Dialogue: 0,0:21:54.80,0:21:57.70,Default,,0000,0000,0000,,SamDong. Naozaj chcem mobil,
Dialogue: 0,0:21:57.70,0:21:58.50,Default,,0000,0000,0000,,a naozaj sa chcem zamilovať.
Dialogue: 0,0:21:58.50,0:22:00.40,Default,,0000,0000,0000,,Ako môžu byť zákazy tak strašidelné?
Dialogue: 0,0:22:00.40,0:22:02.10,Default,,0000,0000,0000,,Čo mám spraviť?
Dialogue: 0,0:22:02.51,0:22:05.41,Default,,0000,0000,0000,,Je čas ísť spať, mala by som spať.
Dialogue: 0,0:22:06.61,0:22:10.01,Default,,0000,0000,0000,,Čo to? Kto zhasol svetlá?
Dialogue: 0,0:22:10.01,0:22:14.52,Default,,0000,0000,0000,,Svetlá sp vypnuté. Všetky sa vypínajú o pol noci.
Dialogue: 0,0:22:17.22,0:22:20.92,Default,,0000,0000,0000,,Čo? Ako tu môžu byť také zákony?
Dialogue: 0,0:22:20.92,0:22:23.93,Default,,0000,0000,0000,,Boh vie.
Dialogue: 0,0:22:25.93,0:22:29.43,Default,,0000,0000,0000,,Ale, prečo sa ešte HaeSung nevrátila?
Dialogue: 0,0:22:29.43,0:22:32.03,Default,,0000,0000,0000,,Prečo by neprišla?
Dialogue: 0,0:22:32.03,0:22:34.64,Default,,0000,0000,0000,,Zase si niečo povedala?
Dialogue: 0,0:22:35.04,0:22:37.74,Default,,0000,0000,0000,,Vždy keď je tu problém,\N musím byť ja, čo to urobila?
Dialogue: 0,0:22:39.34,0:22:43.75,Default,,0000,0000,0000,,Na tomto svete, je niekoľko nemeniach sa faktorov.
Dialogue: 0,0:22:43.75,0:22:45.75,Default,,0000,0000,0000,,Ak zraním človeka,
Dialogue: 0,0:22:45.75,0:22:49.45,Default,,0000,0000,0000,,ja budem zranená dvakrát toľko.
Dialogue: 0,0:22:49.45,0:22:53.16,Default,,0000,0000,0000,,Takže, predtým než stretneš nahnevaného Boha,\Njej už viac neubližuj.
Dialogue: 0,0:22:55.66,0:22:57.16,Default,,0000,0000,0000,,Vážne...
Dialogue: 0,0:22:57.16,0:22:59.16,Default,,0000,0000,0000,,Čo že som spravila?
Dialogue: 0,0:22:59.86,0:23:01.96,Default,,0000,0000,0000,,Ah.
Dialogue: 0,0:23:07.57,0:23:13.68,Default,,0000,0000,0000,,Hej ženská, rýchlo nájdu HaeSung a priveď ju späť.
Dialogue: 0,0:23:13.68,0:23:16.48,Default,,0000,0000,0000,,Nechcem ju počuť blabotať celú noc.
Dialogue: 0,0:23:32.90,0:23:33.60,Default,,0000,0000,0000,,Čo je?
Dialogue: 0,0:23:34.10,0:23:36.20,Default,,0000,0000,0000,,Je ti tu niečo nepríjemné?
Dialogue: 0,0:23:36.20,0:23:38.90,Default,,0000,0000,0000,,Neverím, že je tu taká škola.
Dialogue: 0,0:23:38.90,0:23:40.30,Default,,0000,0000,0000,,Hoci som šialená,
Dialogue: 0,0:23:40.30,0:23:42.10,Default,,0000,0000,0000,,všetci sú tu takí.
Dialogue: 0,0:23:42.10,0:23:43.01,Default,,0000,0000,0000,,Zase a zase.
Dialogue: 0,0:23:43.01,0:23:45.01,Default,,0000,0000,0000,,Nepovedal som ti, aby si hovorila slušnejšie?
Dialogue: 0,0:23:46.11,0:23:48.41,Default,,0000,0000,0000,,Používanie mobilov je zakázané.
Dialogue: 0,0:23:48.41,0:23:51.01,Default,,0000,0000,0000,,Čo je to za školský zákon?
Dialogue: 0,0:23:51.01,0:23:54.62,Default,,0000,0000,0000,,Nestaraj sa o školu ako o škôlku, oci.
Dialogue: 0,0:23:54.62,0:23:58.02,Default,,0000,0000,0000,,Nemala by si robiť veci, ktoré ti iní povedia aby si nerobila.
Dialogue: 0,0:24:01.12,0:24:02.83,Default,,0000,0000,0000,,A...
Dialogue: 0,0:24:15.74,0:24:17.44,Default,,0000,0000,0000,,Aké frustrujúce! Skutočne!
Dialogue: 0,0:24:47.27,0:24:48.57,Default,,0000,0000,0000,,Kto je to?
Dialogue: 0,0:24:55.18,0:24:56.58,Default,,0000,0000,0000,,Čo sa deje?
Dialogue: 0,0:24:56.58,0:24:58.78,Default,,0000,0000,0000,,Že si prídeš do mojej izby zavčas rána.
Dialogue: 0,0:25:03.19,0:25:04.79,Default,,0000,0000,0000,,Je tu Shin HaeSung?
Dialogue: 0,0:25:08.29,0:25:10.19,Default,,0000,0000,0000,,Niečo jej chcem dať.
Dialogue: 0,0:25:16.30,0:25:17.30,Default,,0000,0000,0000,,Čo je to?
Dialogue: 0,0:25:21.80,0:25:23.71,Default,,0000,0000,0000,,Hello to myself.
Dialogue: 0,0:25:28.41,0:25:29.51,Default,,0000,0000,0000,,Čo to je?
Dialogue: 0,0:25:30.71,0:25:33.02,Default,,0000,0000,0000,,Chceš jej dať pesničku, čo si zložil?
Dialogue: 0,0:25:33.42,0:25:35.22,Default,,0000,0000,0000,,Shin HaeSung?
Dialogue: 0,0:25:48.13,0:25:49.43,Default,,0000,0000,0000,,Nie je v izbe?
Dialogue: 0,0:25:50.23,0:25:52.03,Default,,0000,0000,0000,,Včera večer odišla.
Dialogue: 0,0:25:52.03,0:25:55.14,Default,,0000,0000,0000,,Pretože osoba pred tebou,\N ju príliš otravovala.
Dialogue: 0,0:26:05.95,0:26:07.35,Default,,0000,0000,0000,,Hej!
Dialogue: 0,0:26:18.36,0:26:19.66,Default,,0000,0000,0000,,Hej! HaeSung.
Dialogue: 0,0:26:23.47,0:26:24.57,Default,,0000,0000,0000,,Shin HaeSung!
Dialogue: 0,0:26:25.37,0:26:26.97,Default,,0000,0000,0000,,Nie je to tvoj kamarát?
Dialogue: 0,0:26:29.57,0:26:31.07,Default,,0000,0000,0000,,Hlupaňa.
Dialogue: 0,0:26:31.07,0:26:34.38,Default,,0000,0000,0000,,Ak chceš odísť, tak si aspoň toto zober so sebou.
Dialogue: 0,0:26:34.38,0:26:36.28,Default,,0000,0000,0000,,Nebol si to ty, čo to chcel odo mňa späť?
Dialogue: 0,0:26:36.28,0:26:37.88,Default,,0000,0000,0000,,Poďme otec.
Dialogue: 0,0:26:37.88,0:26:40.78,Default,,0000,0000,0000,,Čo si zlého spravila, že musíš utiecť\N v takejto panike?
Dialogue: 0,0:26:40.78,0:26:43.09,Default,,0000,0000,0000,,Budeš robiť takého hlúpe veci až do konca?
Dialogue: 0,0:26:43.09,0:26:45.79,Default,,0000,0000,0000,,Oci. Prosím poďme.
Dialogue: 0,0:26:45.79,0:26:46.79,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:26:47.99,0:26:49.59,Default,,0000,0000,0000,,Vypočuj si to.
Dialogue: 0,0:26:51.79,0:26:53.40,Default,,0000,0000,0000,,Musíš si to vypočuť.
Dialogue: 0,0:26:53.40,0:26:55.40,Default,,0000,0000,0000,,Musíš...musíš!
Dialogue: 0,0:26:58.60,0:27:01.30,Default,,0000,0000,0000,,Nemyslel som vážne, čo som povedal.
Dialogue: 0,0:27:01.30,0:27:04.11,Default,,0000,0000,0000,,Nevieš rozoznať klamstvo od pravdy? Hlupáčik.
Dialogue: 0,0:27:08.91,0:27:11.71,Default,,0000,0000,0000,,Ako môže byť taký až do konca?
Dialogue: 0,0:27:11.71,0:27:14.82,Default,,0000,0000,0000,,Kto je tu skutočným debilom?
Dialogue: 0,0:27:14.82,0:27:18.22,Default,,0000,0000,0000,,To dieťa, má ťa rado?
Dialogue: 0,0:27:18.22,0:27:21.12,Default,,0000,0000,0000,,Nie je to tak, otec.
Dialogue: 0,0:27:40.64,0:27:41.54,Default,,0000,0000,0000,,Oh, dobre.
Dialogue: 0,0:27:42.25,0:27:43.95,Default,,0000,0000,0000,,Študent, ktorého som spomínal...
Dialogue: 0,0:27:43.95,0:27:45.35,Default,,0000,0000,0000,,je tento študent.
Dialogue: 0,0:27:46.45,0:27:48.75,Default,,0000,0000,0000,,Jin YuJin?
Dialogue: 0,0:27:48.75,0:27:50.05,Default,,0000,0000,0000,,Rád ťa spoznávam.
Dialogue: 0,0:27:51.75,0:27:53.26,Default,,0000,0000,0000,,Kto ste?
Dialogue: 0,0:27:56.66,0:28:00.56,Default,,0000,0000,0000,,Ako môže študent umelectva nepoznať\Nnášho najlepšieho skladateľa?
Dialogue: 0,0:28:00.56,0:28:02.97,Default,,0000,0000,0000,,Toto je skladateľ Shin JaeIn.
Dialogue: 0,0:28:04.37,0:28:06.97,Default,,0000,0000,0000,,Vieš, tie piesne, čo si predtým zložil...
Dialogue: 0,0:28:06.97,0:28:11.17,Default,,0000,0000,0000,,Po tom čo ich vypočul, chce byť tvojím učiteľom.
Dialogue: 0,0:28:11.17,0:28:13.08,Default,,0000,0000,0000,,Dobre sa uč od svojho učiteľa.
Dialogue: 0,0:28:13.68,0:28:15.18,Default,,0000,0000,0000,,Moje piesne?
Dialogue: 0,0:28:15.78,0:28:18.28,Default,,0000,0000,0000,,Nenechávam len tak niekoho počúvať moje piesne.
Dialogue: 0,0:28:18.28,0:28:19.88,Default,,0000,0000,0000,,Ako ste ich mohli počuť?
Dialogue: 0,0:28:22.69,0:28:26.19,Default,,0000,0000,0000,,Ah, hovoríte o pesničkách, čo som spravil\N pre ostatných na tú úlohu.
Dialogue: 0,0:28:26.19,0:28:27.69,Default,,0000,0000,0000,,To je nič.
Dialogue: 0,0:28:27.69,0:28:30.59,Default,,0000,0000,0000,,Iba som sa tak hral,\N netreba na to mínať svoje úsilie,
Dialogue: 0,0:28:30.59,0:28:32.70,Default,,0000,0000,0000,,niekoho známeho ako ste Vy.
Dialogue: 0,0:28:33.40,0:28:34.80,Default,,0000,0000,0000,,Ja...
Dialogue: 0,0:28:35.90,0:28:39.00,Default,,0000,0000,0000,,chcem žiť a robiť hudbu, ktorú chcem robiť,
Dialogue: 0,0:28:39.00,0:28:43.51,Default,,0000,0000,0000,,takže nemíňajte na mňa toľko energie.
Dialogue: 0,0:28:48.01,0:28:50.01,Default,,0000,0000,0000,,Je ako tiger.
Dialogue: 0,0:28:50.01,0:28:51.41,Default,,0000,0000,0000,,Veľmi zaujímavé.
Dialogue: 0,0:28:52.72,0:28:57.32,Default,,0000,0000,0000,,Tak ti prenechávam tohto neposlušného tigra.
Dialogue: 0,0:28:57.32,0:28:58.82,Default,,0000,0000,0000,,To beriem.
Dialogue: 0,0:28:59.42,0:29:01.42,Default,,0000,0000,0000,,V porovnaní so študentmi, ktorí sa riadia radmi,
Dialogue: 0,0:29:01.42,0:29:03.33,Default,,0000,0000,0000,,a nemajú talent,
Dialogue: 0,0:29:03.33,0:29:05.93,Default,,0000,0000,0000,,študenti, ktorí sú talentovaní a majú svoje názory,
Dialogue: 0,0:29:05.93,0:29:08.33,Default,,0000,0000,0000,,je výhodnejšie učiť.
Dialogue: 0,0:29:20.04,0:29:23.65,Default,,0000,0000,0000,,Tiež som nečakala, že odíde tak rýchlo.
Dialogue: 0,0:29:23.65,0:29:26.65,Default,,0000,0000,0000,,Niečo jej musím poslať.
Dialogue: 0,0:29:27.85,0:29:29.35,Default,,0000,0000,0000,,Je to veľmi dôležité?
Dialogue: 0,0:29:31.05,0:29:32.16,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:29:39.86,0:29:42.27,Default,,0000,0000,0000,,Počula som, že žije v ChungJu.
Dialogue: 0,0:29:42.77,0:29:44.77,Default,,0000,0000,0000,,Pošli to expresnou poštou.
Dialogue: 0,0:29:58.28,0:29:59.08,Default,,0000,0000,0000,,Kam chceš ísť?
Dialogue: 0,0:30:00.68,0:30:02.89,Default,,0000,0000,0000,,No mám rada ZOO.
Dialogue: 0,0:30:02.89,0:30:03.99,Default,,0000,0000,0000,,ZOO.
Dialogue: 0,0:30:07.59,0:30:07.69,Default,,0000,0000,0000,,Čo sa deje?
Dialogue: 0,0:30:08.49,0:30:09.99,Default,,0000,0000,0000,,Prečo si tu?
Dialogue: 0,0:30:12.70,0:30:14.10,Default,,0000,0000,0000,,A čo ty?
Dialogue: 0,0:30:14.10,0:30:16.00,Default,,0000,0000,0000,,Prečo si otvoril niečie dvere?
Dialogue: 0,0:30:16.00,0:30:18.20,Default,,0000,0000,0000,,Rýchlo ma hoď na autobusovú stanicu.
Dialogue: 0,0:30:18.20,0:30:19.50,Default,,0000,0000,0000,,Je to dôležité.
Dialogue: 0,0:30:22.31,0:30:23.61,Default,,0000,0000,0000,,Myslíš si, že som išiel si do Ameriky spraviť vodičák,
Dialogue: 0,0:30:23.61,0:30:26.41,Default,,0000,0000,0000,,aby som mohol byť tvojim šoférom?
Dialogue: 0,0:30:26.41,0:30:28.21,Default,,0000,0000,0000,,Už som ti povedal, že je to dôležité.
Dialogue: 0,0:30:51.83,0:30:53.54,Default,,0000,0000,0000,,Prečo chceš ísť na stanicu?
Dialogue: 0,0:30:54.84,0:30:55.64,Default,,0000,0000,0000,,Čo sa vlastne deje?
Dialogue: 0,0:30:56.74,0:30:59.34,Default,,0000,0000,0000,,Nebudem sa pýtať na váš vzťah,
Dialogue: 0,0:30:59.34,0:31:01.04,Default,,0000,0000,0000,,tak tiež sa ma nič nepýtaj,
Dialogue: 0,0:31:01.04,0:31:02.45,Default,,0000,0000,0000,,a nič zle nepochop.
Dialogue: 0,0:31:04.35,0:31:07.05,Default,,0000,0000,0000,,Tu netreba nič zle chápať medzi nami.
Dialogue: 0,0:31:09.65,0:31:12.46,Default,,0000,0000,0000,,Len tak ideme na jazdu.
Dialogue: 0,0:31:12.86,0:31:17.36,Default,,0000,0000,0000,,Povedala som, že sa cítim pod psa tak ma zobral na jazdu.
Dialogue: 0,0:31:19.76,0:31:23.77,Default,,0000,0000,0000,,SiWoo by nepoužil svoje auto na to, aby len tak\N niekoho zobral,
Dialogue: 0,0:31:23.77,0:31:25.37,Default,,0000,0000,0000,,tak veľa toho viem.
Dialogue: 0,0:31:30.77,0:31:32.48,Default,,0000,0000,0000,,Len taká jazda.
Dialogue: 0,0:31:32.48,0:31:34.58,Default,,0000,0000,0000,,Veď si tiež v aute, nie?
Dialogue: 0,0:31:38.98,0:31:42.49,Default,,0000,0000,0000,,Spojte časti a vytvoríte základ.
Dialogue: 0,0:31:42.49,0:31:46.29,Default,,0000,0000,0000,,Tón akordu a napätia sú spojené so zvukom akordu,
Dialogue: 0,0:31:46.29,0:31:49.09,Default,,0000,0000,0000,,a zahŕňajúc tieto menej špeciálne situácie...
Dialogue: 0,0:31:51.09,0:31:52.80,Default,,0000,0000,0000,,Čo budeš teraz robiť, keď si unudená?
Dialogue: 0,0:31:52.80,0:31:54.70,Default,,0000,0000,0000,,Už nemáš byť ku komu hnusná.
Dialogue: 0,0:31:55.00,0:31:56.30,Default,,0000,0000,0000,,Nemôžeš byť ticho?
Dialogue: 0,0:31:56.30,0:31:58.00,Default,,0000,0000,0000,,Nemám pravdu?
Dialogue: 0,0:31:58.00,0:32:00.30,Default,,0000,0000,0000,,Každý deň niekoho šikanuješ,\N až kým nemôžu zaspať.
Dialogue: 0,0:32:00.30,0:32:02.51,Default,,0000,0000,0000,,Je moja vina, že odišla?
Dialogue: 0,0:32:02.51,0:32:05.61,Default,,0000,0000,0000,,Tak potom koho iného?
Dialogue: 0,0:32:05.61,0:32:07.61,Default,,0000,0000,0000,,Ty si hlavnou vinníčkou.
Dialogue: 0,0:32:07.61,0:32:10.91,Default,,0000,0000,0000,,Myslíš, že som sa jej dokázala stále vyhýbať?
Dialogue: 0,0:32:10.91,0:32:13.02,Default,,0000,0000,0000,,Nevyhýbala si sa jej aj ty?
Dialogue: 0,0:32:13.02,0:32:14.32,Default,,0000,0000,0000,,Omo, omo, omo.
Dialogue: 0,0:32:14.32,0:32:16.22,Default,,0000,0000,0000,,Ako môžeš povedať tak bolestné slová?
Dialogue: 0,0:32:16.22,0:32:20.52,Default,,0000,0000,0000,,Naozaj, ja a ona sme blízke priateľky.
Dialogue: 0,0:32:20.52,0:32:22.23,Default,,0000,0000,0000,,Ah, vážne...
Dialogue: 0,0:32:22.23,0:32:24.73,Default,,0000,0000,0000,,Si tak hlučná, že sa nemôžem sústrediť.
Dialogue: 0,0:32:25.53,0:32:28.83,Default,,0000,0000,0000,,Učiteľka, prestaňte učiť.
Dialogue: 0,0:32:31.93,0:32:33.34,Default,,0000,0000,0000,,Čože?
Dialogue: 0,0:32:33.34,0:32:35.94,Default,,0000,0000,0000,,Prečo? Si nespokojná s hodinou?
Dialogue: 0,0:32:35.94,0:32:38.84,Default,,0000,0000,0000,,Prečo Shin HaeSung išla preč?
Dialogue: 0,0:32:38.84,0:32:41.64,Default,,0000,0000,0000,,Každý o tom teraz hovorí.
Dialogue: 0,0:32:41.64,0:32:44.35,Default,,0000,0000,0000,,Učiteľka, Vy sama budete naďalej učiť, akoby sa nič nestalo?
Dialogue: 0,0:32:44.35,0:32:45.35,Default,,0000,0000,0000,,Hej....
Dialogue: 0,0:32:45.35,0:32:46.85,Default,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:32:46.85,0:32:50.05,Default,,0000,0000,0000,,Ani sme sa s ňou poriadne nerozlúčili.
Dialogue: 0,0:32:50.05,0:32:52.76,Default,,0000,0000,0000,,Nič ste nám o tom nepovedali.
Dialogue: 0,0:32:52.76,0:32:53.76,Default,,0000,0000,0000,,To je moc.
Dialogue: 0,0:32:53.76,0:32:55.36,Default,,0000,0000,0000,,To je pravda.
Dialogue: 0,0:32:55.36,0:32:57.36,Default,,0000,0000,0000,,{\i1}Prečo musela odísť?{\i0}
Dialogue: 0,0:32:57.36,0:32:59.36,Default,,0000,0000,0000,,{\i1}Aký je dôvod?{\i0}
Dialogue: 0,0:32:59.36,0:33:01.66,Default,,0000,0000,0000,,HaeSung, prečo odišla zo školy?
Dialogue: 0,0:33:02.37,0:33:05.97,Default,,0000,0000,0000,,Kamarátka potichu odišla bez rozlúčky.
Dialogue: 0,0:33:05.97,0:33:07.97,Default,,0000,0000,0000,,Keby ste to bola Vy, učiteľka, zostali by ste ticho?
Dialogue: 0,0:33:09.17,0:33:10.67,Default,,0000,0000,0000,,Čo sa deje?
Dialogue: 0,0:33:13.38,0:33:15.18,Default,,0000,0000,0000,,Ohľadom toho...
Dialogue: 0,0:33:26.49,0:33:29.49,Default,,0000,0000,0000,,{\i1}Pre mojich vzácnych priateľov,{\i0}
Dialogue: 0,0:33:29.49,0:33:32.99,Default,,0000,0000,0000,,{\i1}Keby som videla vaše tváre, bála som sa,\N že by som nemala na to odísť.{\i0}
Dialogue: 0,0:33:32.99,0:33:35.40,Default,,0000,0000,0000,,{\i1}Preto som namiesto toho napísala list.{\i0}
Dialogue: 0,0:33:37.10,0:33:39.80,Default,,0000,0000,0000,,{\i1}Čas, ktorý sme spolu strávili,{\i0}
Dialogue: 0,0:33:39.80,0:33:43.11,Default,,0000,0000,0000,,{\i1}nikdy naň pozbytok svojho života nezabudnem.{\i0}
Dialogue: 0,0:33:43.11,0:33:45.21,Default,,0000,0000,0000,,{\i1}Vždy ste pre mňa spievali,\N keď som prežívala ťažké časy,{\i0}
Dialogue: 0,0:33:45.21,0:33:47.91,Default,,0000,0000,0000,,{\i1}ako talentovaný spevák Pak HongJu,{\i0}
Dialogue: 0,0:33:47.91,0:33:51.61,Default,,0000,0000,0000,,{\i1}najlepší tanečník Kirin Strednej UiBong,{\i0}
Dialogue: 0,0:33:52.01,0:33:56.22,Default,,0000,0000,0000,,{\i1}priateľka, ktorá je ako talizman - SunDong,{\i0}
Dialogue: 0,0:33:56.22,0:34:00.52,Default,,0000,0000,0000,,{\i1}a tiež niekedy nevyspitateľný,{\i0}
Dialogue: 0,0:34:00.52,0:34:02.33,Default,,0000,0000,0000,,{\i1}ale vždy hovoriaci o loajalite, YuJin.{\i0}
Dialogue: 0,0:34:04.13,0:34:09.13,Default,,0000,0000,0000,,{\i1}Odteraz už nemôžem byť na vašej strane a počúvať\N ako spievate.{\i0}
Dialogue: 0,0:34:09.13,0:34:10.83,Default,,0000,0000,0000,,{\i1}Je to naozaj škoda, avšak...{\i0}
Dialogue: 0,0:34:12.44,0:34:14.74,Default,,0000,0000,0000,,{\i1}Nech je to unavujúce akokoľvek,{\i0}
Dialogue: 0,0:34:14.74,0:34:17.24,Default,,0000,0000,0000,,{\i1}keď vás počujem spievať,{\i0}
Dialogue: 0,0:34:17.24,0:34:20.34,Default,,0000,0000,0000,,{\i1}moja sila začne rásť.{\i0}
Dialogue: 0,0:34:21.54,0:34:26.75,Default,,0000,0000,0000,,{\i1}Všetci z vás budete rýchlo populárni a objavíte\N sa v televízií, všakže?{\i0}
Dialogue: 0,0:34:26.75,0:34:29.75,Default,,0000,0000,0000,,{\i1}Pretože všetci ste skvelí.{\i0}
Dialogue: 0,0:34:30.55,0:34:33.26,Default,,0000,0000,0000,,{\i1}Moje sny sa tu práve končia,{\i0}
Dialogue: 0,0:34:33.26,0:34:36.16,Default,,0000,0000,0000,,{\i1}ale vy si ich musíte splniť.{\i0}
Dialogue: 0,0:34:41.66,0:34:42.67,Default,,0000,0000,0000,,Plačeš?
Dialogue: 0,0:34:43.37,0:34:44.87,Default,,0000,0000,0000,,UiBong, ešte aj ty?
Dialogue: 0,0:34:45.87,0:34:48.27,Default,,0000,0000,0000,,Čo je to s dneškom?
Dialogue: 0,0:34:48.27,0:34:50.17,Default,,0000,0000,0000,,YuJin.
Dialogue: 0,0:34:50.17,0:34:52.98,Default,,0000,0000,0000,,HaeSung zanechala list predtým než odišla.
Dialogue: 0,0:34:52.98,0:34:55.88,Default,,0000,0000,0000,,Zrejme sa neplánuje vrátiť.
Dialogue: 0,0:34:59.18,0:35:01.78,Default,,0000,0000,0000,,Čo to robíš?
Dialogue: 0,0:35:04.49,0:35:06.99,Default,,0000,0000,0000,,Shin HaeSung neodíde.
Dialogue: 0,0:35:06.99,0:35:08.59,Default,,0000,0000,0000,,Prestaňte plakať.
Dialogue: 0,0:35:10.79,0:35:11.79,Default,,0000,0000,0000,,Na dnešné vystúpenie,
Dialogue: 0,0:35:13.60,0:35:15.90,Default,,0000,0000,0000,,spravme projekt pre návrat Shin HaeSung.
Dialogue: 0,0:35:16.90,0:35:20.20,Default,,0000,0000,0000,,Už som vymyslel plán.
Dialogue: 0,0:35:20.20,0:35:23.81,Default,,0000,0000,0000,,Vymyslel som, ako ju prínutiť vrátiť sa, keď príde.
Dialogue: 0,0:35:24.41,0:35:25.61,Default,,0000,0000,0000,,Shin HaeSung.
Dialogue: 0,0:35:26.91,0:35:29.21,Default,,0000,0000,0000,,Garantuješ, že sa vráti?
Dialogue: 0,0:35:29.21,0:35:30.81,Default,,0000,0000,0000,,Neveríš mi?
Dialogue: 0,0:35:31.21,0:35:33.22,Default,,0000,0000,0000,,Potom,
Dialogue: 0,0:35:33.22,0:35:36.32,Default,,0000,0000,0000,,všetko čo musme spraviť, je dnes dobre vystúpiť, však?
Dialogue: 0,0:35:49.73,0:35:51.33,Default,,0000,0000,0000,,Čo sa deje?
Dialogue: 0,0:35:51.33,0:35:53.84,Default,,0000,0000,0000,,Tento deň konečne nadišiel.
Dialogue: 0,0:35:53.84,0:35:55.04,Default,,0000,0000,0000,,Aký deň?
Dialogue: 0,0:35:55.04,0:35:57.24,Default,,0000,0000,0000,,Vytvoril som skupinu.
Dialogue: 0,0:35:57.24,0:36:00.74,Default,,0000,0000,0000,,Volá sa Kirin Umeleckej Strednej Yang JiMan Roadband.
Dialogue: 0,0:36:00.74,0:36:02.75,Default,,0000,0000,0000,,JMY!
Dialogue: 0,0:36:05.15,0:36:07.25,Default,,0000,0000,0000,,Pouličná skupina huh?
Dialogue: 0,0:36:07.25,0:36:09.75,Default,,0000,0000,0000,,Nuž, nemusí to tak byť.
Dialogue: 0,0:36:09.75,0:36:12.96,Default,,0000,0000,0000,,Začiatok je možno malý,\N ale koniec je určite veľký.
Dialogue: 0,0:36:12.96,0:36:16.46,Default,,0000,0000,0000,,Teraz začíname len s malými pouličnými vystúpeniami.
Dialogue: 0,0:36:16.86,0:36:18.86,Default,,0000,0000,0000,,Prečo by som mal ísť a pozrieť sa na nich?
Dialogue: 0,0:36:18.86,0:36:22.37,Default,,0000,0000,0000,,Dobre som to tie decká naučil.
Dialogue: 0,0:36:22.37,0:36:23.57,Default,,0000,0000,0000,,Hoci sme nemali adekvátne skúšanie,
Dialogue: 0,0:36:23.57,0:36:24.87,Default,,0000,0000,0000,,majú dobré základy.
Dialogue: 0,0:36:24.87,0:36:27.47,Default,,0000,0000,0000,,Okoloidúci boli okúzlení.
Dialogue: 0,0:36:27.47,0:36:29.47,Default,,0000,0000,0000,,Kto to všetko spravil?
Dialogue: 0,0:36:29.47,0:36:31.27,Default,,0000,0000,0000,,Ja.
Dialogue: 0,0:36:33.98,0:36:36.28,Default,,0000,0000,0000,,Učiteľ Yang JiMan,
Dialogue: 0,0:36:36.28,0:36:39.78,Default,,0000,0000,0000,,už nie ste dieťaťom, máte rád, keď\N Vás niekto chváli?
Dialogue: 0,0:36:41.18,0:36:42.79,Default,,0000,0000,0000,,To nie.
Dialogue: 0,0:36:42.79,0:36:47.99,Default,,0000,0000,0000,,Len ak pôjdete sa pozrieť, budete ohúrený.
Dialogue: 0,0:36:47.99,0:36:49.89,Default,,0000,0000,0000,,Tak sa pôjdeme pozrieť.
Dialogue: 0,0:36:50.49,0:36:52.09,Default,,0000,0000,0000,,Prídete? Naozaj?
Dialogue: 0,0:37:00.50,0:37:05.21,Default,,0000,0000,0000,,V skupine, ktorú vytvoril,\N je pravdepodobne líderom Jin YuJin.
Dialogue: 0,0:37:05.91,0:37:10.91,Default,,0000,0000,0000,,Ak je to tak, veci sa pohnú ďalej rýchlejšie,\N než som si myslel.
Dialogue: 0,0:37:10.91,0:37:12.01,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:37:27.63,0:37:29.13,Default,,0000,0000,0000,,To je Lian.
Dialogue: 0,0:37:31.43,0:37:33.44,Default,,0000,0000,0000,,Je škola zaujímavá?
Dialogue: 0,0:37:34.64,0:37:36.84,Default,,0000,0000,0000,,Tak-tak.
Dialogue: 0,0:37:36.84,0:37:37.84,Default,,0000,0000,0000,,Ale,
Dialogue: 0,0:37:39.24,0:37:41.14,Default,,0000,0000,0000,,čo tu robíte?
Dialogue: 0,0:37:42.85,0:37:48.15,Default,,0000,0000,0000,,Vytváram najlepšiu skupinu krajiny.
Dialogue: 0,0:37:48.75,0:37:51.35,Default,,0000,0000,0000,,Už som o tom rozprával s vedúcim Lee-m,
Dialogue: 0,0:37:51.35,0:37:54.76,Default,,0000,0000,0000,,aby sme vytvorili súťaž, kde vyberieme najlepšieho super idola.
Dialogue: 0,0:37:55.86,0:37:58.56,Default,,0000,0000,0000,,Na našej škole?
Dialogue: 0,0:37:59.66,0:38:04.27,Default,,0000,0000,0000,,Preto ste sa rozprávali s Jin YuJinom?
Dialogue: 0,0:38:04.27,0:38:07.97,Default,,0000,0000,0000,,Áno, to dieťa. Stojí za povšimnutie si.
Dialogue: 0,0:38:08.17,0:38:10.07,Default,,0000,0000,0000,,Počul som, že dnes večer bude nejaké vystúpenie.
Dialogue: 0,0:38:10.07,0:38:11.87,Default,,0000,0000,0000,,Rozmýšľal som, že sa tam pôjdem pozrieť.
Dialogue: 0,0:38:13.38,0:38:15.38,Default,,0000,0000,0000,,A čo HershE a I:dn?
Dialogue: 0,0:38:16.18,0:38:18.28,Default,,0000,0000,0000,,Čo máme robiť?
Dialogue: 0,0:38:19.78,0:38:22.49,Default,,0000,0000,0000,,Nuž, v tom si nie som istý.
Dialogue: 0,0:38:22.49,0:38:24.49,Default,,0000,0000,0000,,Z mojho pohľadu,
Dialogue: 0,0:38:24.49,0:38:27.19,Default,,0000,0000,0000,,neviem či budete kandidátmi super idola?
Dialogue: 0,0:38:33.80,0:38:37.20,Default,,0000,0000,0000,,Tento večer, vystupujem aj ja.
Dialogue: 0,0:38:38.30,0:38:41.20,Default,,0000,0000,0000,,Mali by ste aj mne venovať pozornosť.
Dialogue: 0,0:38:42.41,0:38:45.71,Default,,0000,0000,0000,,Naozaj? Tak o tebe ešte porozmýšľam.
Dialogue: 0,0:39:03.43,0:39:05.93,Default,,0000,0000,0000,,Po rezignovaní v škole Vaše srdce a myseľ je len v tomto obchode.
Dialogue: 0,0:39:06.33,0:39:08.13,Default,,0000,0000,0000,,Ako ti ide? Zarábate?
Dialogue: 0,0:39:08.13,0:39:11.73,Default,,0000,0000,0000,,Hej. Choď, odíď. Nejdeš do školy?
Dialogue: 0,0:39:12.64,0:39:16.04,Default,,0000,0000,0000,,Prosím nebráňte mi v mojom biznise. Rýchlo odíďte zákazník.
Dialogue: 0,0:39:18.44,0:39:20.44,Default,,0000,0000,0000,,Prečo ste si ma vybrali?
Dialogue: 0,0:39:21.94,0:39:23.15,Default,,0000,0000,0000,,Čože?
Dialogue: 0,0:39:24.15,0:39:26.15,Default,,0000,0000,0000,,Jasné, že ste si ma vybrali, tak potom prečo ste ma opustili?
Dialogue: 0,0:39:26.15,0:39:28.55,Default,,0000,0000,0000,,Prečo ste si ma museli vybrať?
Dialogue: 0,0:39:29.25,0:39:31.95,Default,,0000,0000,0000,,Riaditeľ by mal byť zodpovedný, keď si vyberie študenta.
Dialogue: 0,0:39:32.96,0:39:35.86,Default,,0000,0000,0000,,O akých nezmysloch to tu táraš? Čo?
Dialogue: 0,0:39:35.86,0:39:39.16,Default,,0000,0000,0000,,Učiteľ sa vzdá žiaka, žiak sa vzdá školy.
Dialogue: 0,0:39:39.66,0:39:42.47,Default,,0000,0000,0000,,Vážne, ja zošaliem.
Dialogue: 0,0:39:44.27,0:39:45.77,Default,,0000,0000,0000,,Čo to hovoríš?
Dialogue: 0,0:39:50.27,0:39:51.37,Default,,0000,0000,0000,,Čo je to?
Dialogue: 0,0:39:58.78,0:40:02.79,Default,,0000,0000,0000,,Ak dnes neprídete, nepôjdem do školy.
Dialogue: 0,0:40:05.69,0:40:07.69,Default,,0000,0000,0000,,Ako keby si do školy chodil kvôli mne.
Dialogue: 0,0:40:08.79,0:40:10.69,Default,,0000,0000,0000,,Hej. Vyjasnime si veci.
Dialogue: 0,0:40:10.69,0:40:14.90,Default,,0000,0000,0000,,Nehovoril si, že ja som sa vzdal školy?
Dialogue: 0,0:40:15.20,0:40:16.90,Default,,0000,0000,0000,,Skutočnosťou je, že ma vyhodili.
Dialogue: 0,0:40:19.00,0:40:22.31,Default,,0000,0000,0000,,Varoval som Vás, začína to o šiestej.
Dialogue: 0,0:40:25.71,0:40:27.31,Default,,0000,0000,0000,,A nie som riaditeľom.
Dialogue: 0,0:40:32.82,0:40:33.92,Default,,0000,0000,0000,,Vážne...
Dialogue: 0,0:40:50.23,0:40:51.83,Default,,0000,0000,0000,,Toto stvorenie siaha za hranicu uverenia.
Dialogue: 0,0:40:55.14,0:40:56.64,Default,,0000,0000,0000,,Čo zase?
Dialogue: 0,0:40:59.44,0:41:01.14,Default,,0000,0000,0000,,Zdravím.
Dialogue: 0,0:41:02.75,0:41:04.55,Default,,0000,0000,0000,,Učiteľka Ahn, ako ste sem prišla?
Dialogue: 0,0:41:07.65,0:41:12.16,Default,,0000,0000,0000,,Ja...asi som spravila niečo zle, pane.
Dialogue: 0,0:41:13.86,0:41:19.86,Default,,0000,0000,0000,,HaeSung odchádza zo školy kvôli mne.
Dialogue: 0,0:41:31.37,0:41:35.98,Default,,0000,0000,0000,,Lebo som nechcela, aby moji študenti boli pod\N manažmentom Hyeon učiteľky.
Dialogue: 0,0:41:35.98,0:41:40.78,Default,,0000,0000,0000,,Ak ste pokarhaný Vy, tak aj ja, viete to?
Dialogue: 0,0:41:41.58,0:41:43.59,Default,,0000,0000,0000,,Viem. Tak pokračujte.
Dialogue: 0,0:41:43.59,0:41:48.49,Default,,0000,0000,0000,,Imitovala som učiteľku Hyeon.
Dialogue: 0,0:41:48.49,0:41:53.30,Default,,0000,0000,0000,,Prísne. A ešte k tomu, pred otcom HaeSung.
Dialogue: 0,0:41:54.00,0:41:57.60,Default,,0000,0000,0000,,Ale včera zrazu chcela odísť zo školy.
Dialogue: 0,0:41:57.60,0:42:00.60,Default,,0000,0000,0000,,Nečakala som ani ja, že to takto skončí.
Dialogue: 0,0:42:01.10,0:42:04.21,Default,,0000,0000,0000,,Tiež som chcela, aby sa mi darilo.
Dialogue: 0,0:42:04.61,0:42:08.21,Default,,0000,0000,0000,,Ale neviem, komu sa vyrozprávať.
Dialogue: 0,0:42:10.21,0:42:14.52,Default,,0000,0000,0000,,Zničila som život študentky?
Dialogue: 0,0:42:16.22,0:42:20.52,Default,,0000,0000,0000,,Tak prečo ste to spravili? Nepodobá sa to na Vás.
Dialogue: 0,0:42:27.83,0:42:28.83,Default,,0000,0000,0000,,Už ani nie som riaditeľom.\N Kde je teraz HaeSung?
Dialogue: 0,0:42:34.24,0:42:36.34,Default,,0000,0000,0000,,Ale má dom v ChungJu, že?
Dialogue: 0,0:42:51.25,0:42:53.96,Default,,0000,0000,0000,,Je tu toľko ľudí. Ako môžeme nájsť Shin HaeSung?
Dialogue: 0,0:42:53.96,0:42:57.76,Default,,0000,0000,0000,,Má veľkú hlavu.\N Hľadajte ženy s veľkou hlavou.
Dialogue: 0,0:42:57.76,0:43:00.86,Default,,0000,0000,0000,,Kde že býva? ChungJu? CheongJu?
Dialogue: 0,0:43:00.86,0:43:02.27,Default,,0000,0000,0000,,ChungJu.
Dialogue: 0,0:43:25.99,0:43:30.09,Default,,0000,0000,0000,,Vypočuť si čo? Je to len môj hlas.
Dialogue: 0,0:43:36.00,0:43:38.40,Default,,0000,0000,0000,,Je tu autobus. Poďme.
Dialogue: 0,0:43:59.52,0:44:00.52,Default,,0000,0000,0000,,Porozprávajme sa.
Dialogue: 0,0:44:02.13,0:44:03.43,Default,,0000,0000,0000,,Kto si?
Dialogue: 0,0:44:06.03,0:44:10.33,Default,,0000,0000,0000,,Prepáčte, že som sa nepredstavil.\N Som kamarát HaeSung, JB.
Dialogue: 0,0:44:11.53,0:44:17.64,Default,,0000,0000,0000,,Ten JB. Frajer HaeSung?
Dialogue: 0,0:44:20.64,0:44:24.85,Default,,0000,0000,0000,,Otec. Sme iba kamaráti.
Dialogue: 0,0:44:24.85,0:44:26.65,Default,,0000,0000,0000,,Je to kamarát.
Dialogue: 0,0:44:30.45,0:44:32.86,Default,,0000,0000,0000,,Prišiel som, aby som ti dal toto.
Dialogue: 0,0:44:42.17,0:44:47.47,Default,,0000,0000,0000,,Ak je to preto, aby si ju vzal späť, tak je neskoro.
Dialogue: 0,0:44:47.47,0:44:50.77,Default,,0000,0000,0000,,Nebolo ľahké pre ňu rozhodnúť sa.\N Prosím, nenechaj ju znovu si to premyslieť.
Dialogue: 0,0:44:52.98,0:44:56.58,Default,,0000,0000,0000,,Dúfam, že môžem študovať spolu s ňou.
Dialogue: 0,0:44:56.58,0:44:59.38,Default,,0000,0000,0000,,HaeSung-iná budúcnosť bude veľmi ľahká.
Dialogue: 0,0:44:59.38,0:45:03.59,Default,,0000,0000,0000,,Tak jej dovoľte, ďalej študovať hudbu.
Dialogue: 0,0:45:03.59,0:45:08.19,Default,,0000,0000,0000,,Si jej frajerom, pre teba sa to ľahko hovorí.
Dialogue: 0,0:45:08.19,0:45:15.00,Default,,0000,0000,0000,,Ale ak máš tú schopnosť, tak prečo je tá ťažká práca tak marná?
Dialogue: 0,0:45:15.40,0:45:17.60,Default,,0000,0000,0000,,Povedz, prečo ma zavolala učiteľka?
Dialogue: 0,0:45:17.60,0:45:21.80,Default,,0000,0000,0000,,Povedala, že HaeSung nemá talent, dovolila nech ju zoberiem späť domov.
Dialogue: 0,0:45:23.41,0:45:26.61,Default,,0000,0000,0000,,Ja som tá, čo to povedala a je mi to hrozne ľúto.
Dialogue: 0,0:45:30.11,0:45:33.22,Default,,0000,0000,0000,,Prišla som sa ospravedlniť.
Dialogue: 0,0:45:33.22,0:45:36.02,Default,,0000,0000,0000,,Prehnala som to.
Dialogue: 0,0:45:38.82,0:45:42.23,Default,,0000,0000,0000,,Otče, porozprávajme sa.
Dialogue: 0,0:45:51.33,0:45:53.84,Default,,0000,0000,0000,,Zdravím, pane, ukázali by ste mi Váš občiansky preukaz?
Dialogue: 0,0:46:02.45,0:46:05.55,Default,,0000,0000,0000,,Vrchný riaditeľ, spraví niečo polícia?
Dialogue: 0,0:46:08.15,0:46:10.35,Default,,0000,0000,0000,,Učiteľ Yang by mal vedieť, čo spraviť, že?
Dialogue: 0,0:46:10.35,0:46:12.46,Default,,0000,0000,0000,,Áno. Hneď idem.
Dialogue: 0,0:46:22.67,0:46:24.57,Default,,0000,0000,0000,,Či je to hlúposť alebo naivita,
Dialogue: 0,0:46:25.87,0:46:30.27,Default,,0000,0000,0000,,je to iba sprostá novinka, prečo by polícia niečo robila?
Dialogue: 0,0:46:30.67,0:46:37.48,Default,,0000,0000,0000,,Pretože je naivný a poslušný.\N Je svojsky roztomilý.
Dialogue: 0,0:46:48.89,0:46:51.59,Default,,0000,0000,0000,,Ak ju chcete takto presvedčiť, aby zostala,
Dialogue: 0,0:46:51.59,0:46:54.60,Default,,0000,0000,0000,,nikdy by ste nepovedali niečo, čo by ju zranilo.
Dialogue: 0,0:46:54.60,0:46:57.20,Default,,0000,0000,0000,,Áno, to je pravda. Otče.
Dialogue: 0,0:46:57.20,0:47:00.70,Default,,0000,0000,0000,,Je nám to veľmi ľúto. HaeSung má 19 rokov.
Dialogue: 0,0:47:00.70,0:47:05.31,Default,,0000,0000,0000,,Do 19-tich rokoch bolo veľa času na zistenie,\N aké má talenty.
Dialogue: 0,0:47:05.31,0:47:06.51,Default,,0000,0000,0000,,Talenty?
Dialogue: 0,0:47:06.51,0:47:07.91,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:47:08.71,0:47:14.02,Default,,0000,0000,0000,,Pre mňa nie je dôležité, či má alebo nemá talenty.
Dialogue: 0,0:47:14.02,0:47:19.22,Default,,0000,0000,0000,,Už od začiatku som ju sem neposlal preto,\N aby sa stala speváčkou.
Dialogue: 0,0:47:20.52,0:47:26.83,Default,,0000,0000,0000,,Len preto, lebo povedala že to chce\N a že zomrie, ak tak neučiní.
Dialogue: 0,0:47:26.83,0:47:30.13,Default,,0000,0000,0000,,Len preto, aby to vyskúšala, zlyhala a vzdala sa.
Dialogue: 0,0:47:32.24,0:47:33.54,Default,,0000,0000,0000,,Otec.
Dialogue: 0,0:47:35.14,0:47:38.64,Default,,0000,0000,0000,,Za tie tri roky v škole,
Dialogue: 0,0:47:38.64,0:47:44.45,Default,,0000,0000,0000,,sa ani raz nevrátila domov šťastná.
Dialogue: 0,0:47:46.25,0:47:50.45,Default,,0000,0000,0000,,Každý deň ako vädnúca tráva toho istého\N a bezfarbeného vzhľadu.
Dialogue: 0,0:47:51.75,0:47:56.66,Default,,0000,0000,0000,,Aj keď bola taká, tak preto, aby sa mohla stať speváčkou.
Dialogue: 0,0:47:56.66,0:47:59.26,Default,,0000,0000,0000,,Každý večer trénovala tak, aby som o tom nevedel.
Dialogue: 0,0:48:02.57,0:48:05.37,Default,,0000,0000,0000,,Ak rozumiete jej utláčanému srdcu,
Dialogue: 0,0:48:05.37,0:48:10.07,Default,,0000,0000,0000,,prosím Vás učiteľov, prestaňte hovoriť veci, ktoré ju môžu zraniť.
Dialogue: 0,0:48:11.57,0:48:14.78,Default,,0000,0000,0000,,Ešte k tomu pred rodičmi.
Dialogue: 0,0:48:14.78,0:48:18.68,Default,,0000,0000,0000,,Ako to muselo byť ťažké pre študenta za tie tri roky?
Dialogue: 0,0:48:19.38,0:48:23.89,Default,,0000,0000,0000,,Nie, to nie je tak. Otče, ako môžete tak premýšľať?
Dialogue: 0,0:48:23.89,0:48:27.79,Default,,0000,0000,0000,,Počas toho času, nikdy sme neobjavili jej talenty.
Dialogue: 0,0:48:27.79,0:48:29.09,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:48:30.69,0:48:32.60,Default,,0000,0000,0000,,Hovoríte, že ste ich nikdy neobjavili?
Dialogue: 0,0:48:32.60,0:48:33.60,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:48:41.60,0:48:42.51,Default,,0000,0000,0000,,O, otče-
Dialogue: 0,0:48:42.91,0:48:43.61,Default,,0000,0000,0000,,Otče.
Dialogue: 0,0:48:51.71,0:48:55.02,Default,,0000,0000,0000,,Tu stoj a spievaj.
Dialogue: 0,0:48:55.02,0:48:56.72,Default,,0000,0000,0000,,Prosím?
Dialogue: 0,0:48:56.72,0:48:58.62,Default,,0000,0000,0000,,Otče.
Dialogue: 0,0:48:58.62,0:49:06.33,Default,,0000,0000,0000,,Ak medzi týmito ľuďmi sa nájde jeden,\N ktorý uzná tvoje schopnosti,
Dialogue: 0,0:49:06.33,0:49:08.33,Default,,0000,0000,0000,,tak zruším odchod zo školy.
Dialogue: 0,0:49:09.33,0:49:12.94,Default,,0000,0000,0000,,Otče. To nie. Nebuďte taký.
Dialogue: 0,0:49:12.94,0:49:14.44,Default,,0000,0000,0000,,Rýchlo mi zaspievaj!
Dialogue: 0,0:49:48.47,0:49:54.58,Default,,0000,0000,0000,,~ {\i1}Ahoj. Toto je 11.mesiac roku 2011.{\i0} ~
Dialogue: 0,0:49:54.58,0:49:58.45,Default,,0000,0000,0000,,~ {\i1}Je veľmi chladná zima.{\i0} ~
Dialogue: 0,0:50:00.78,0:50:06.89,Default,,0000,0000,0000,,~ {\i1}Kde si? Ako ďaleko si zašiel?{\i0} ~
Dialogue: 0,0:50:06.89,0:50:11.29,Default,,0000,0000,0000,,~ {\i1}Ten sen, ktorý je tak žiadaný.{\i0} ~
Dialogue: 0,0:50:11.29,0:50:19.10,Default,,0000,0000,0000,,~ {\i1}Moje slzy padajú, už pohmoždené.{\i0} ~
Dialogue: 0,0:50:19.10,0:50:23.74,Default,,0000,0000,0000,,~ {\i1}Nemám silu na povstanie.{\i0} ~
Dialogue: 0,0:50:23.74,0:50:30.15,Default,,0000,0000,0000,,~ {\i1}Asi sa na mne budeš smiať.{\i0} ~
Dialogue: 0,0:50:30.15,0:50:36.05,Default,,0000,0000,0000,,~ {\i1}Usmej sa pre mňa, tak znovu povstanem.{\i0} ~
Dialogue: 0,0:50:36.05,0:50:42.43,Default,,0000,0000,0000,,~ {\i1}Môžeš ma upokojiť, aby som už neplakala?{\i0} ~
Dialogue: 0,0:50:42.43,0:50:48.33,Default,,0000,0000,0000,,~ {\i1}Usmej sa pre mňa, aby som znovu povstala.{\i0} ~
Dialogue: 0,0:50:50.83,0:50:52.84,Default,,0000,0000,0000,,Čo dobrého si spravila, aby si plakala?
Dialogue: 0,0:50:54.14,0:50:56.04,Default,,0000,0000,0000,,Necítiš sa ani trochu zle?
Dialogue: 0,0:50:59.54,0:51:01.74,Default,,0000,0000,0000,,Celé tieto roky, je to jediná vec, ktorú si sa naučila.
Dialogue: 0,0:51:01.74,0:51:03.55,Default,,0000,0000,0000,,Ani raz nevyužitá a odhodená preč.
Dialogue: 0,0:51:03.55,0:51:04.95,Default,,0000,0000,0000,,Necítiš sa zle?
Dialogue: 0,0:51:08.35,0:51:13.46,Default,,0000,0000,0000,,Myslela si si, že otec nebude dúfať v tvoje šťastie?
Dialogue: 0,0:51:15.66,0:51:17.76,Default,,0000,0000,0000,,Viac než ktokoľvek iný, kto by dúfal pre tvoje šťastie,
Dialogue: 0,0:51:17.76,0:51:19.26,Default,,0000,0000,0000,,povedz mi kto, ty hlupaňa.
Dialogue: 0,0:51:24.87,0:51:29.97,Default,,0000,0000,0000,,Vraciame sa. Už tu nie je nič k videniu.
Dialogue: 0,0:51:42.29,0:51:43.49,Default,,0000,0000,0000,,To je vážne skvelé.
Dialogue: 0,0:51:46.89,0:51:50.39,Default,,0000,0000,0000,,Učiteľ, toto miesto nie je zlé.
Dialogue: 0,0:51:50.39,0:51:52.60,Default,,0000,0000,0000,,Nuž, ako inak.
Dialogue: 0,0:51:52.90,0:51:54.60,Default,,0000,0000,0000,,Nie je to na vysielanie?
Dialogue: 0,0:51:55.90,0:51:57.10,Default,,0000,0000,0000,,Neurobím to.
Dialogue: 0,0:51:57.10,0:51:58.40,Default,,0000,0000,0000,,Čo je teraz?
Dialogue: 0,0:51:58.40,0:52:00.50,Default,,0000,0000,0000,,Vážne, prečo nemôžeš?
Dialogue: 0,0:52:00.50,0:52:02.31,Default,,0000,0000,0000,,To je skvelé.
Dialogue: 0,0:52:02.31,0:52:06.21,Default,,0000,0000,0000,,Presne. Nedúfali sme všetci, že raz nás budú vysielať?
Dialogue: 0,0:52:06.21,0:52:09.21,Default,,0000,0000,0000,,To bol váš sen. Nie môj.
Dialogue: 0,0:52:09.21,0:52:11.91,Default,,0000,0000,0000,,Máme vystupovať pre Shin HaeSung.
Dialogue: 0,0:52:11.91,0:52:15.32,Default,,0000,0000,0000,,Ak budeme vystupovať tu, nepríde.
Dialogue: 0,0:52:15.72,0:52:17.62,Default,,0000,0000,0000,,Nebude dobré, keď nás bude počuť cez vysielanie?
Dialogue: 0,0:52:17.62,0:52:22.53,Default,,0000,0000,0000,,Hej, na rozdiel od toho, že by nás videla, vysielanie je dojemnejšie.
Dialogue: 0,0:52:23.33,0:52:25.43,Default,,0000,0000,0000,,Taktiež, nie je to len pre HaeSung.
Dialogue: 0,0:52:25.43,0:52:27.83,Default,,0000,0000,0000,,Je to aj pre riaditeľa.
Dialogue: 0,0:52:27.83,0:52:28.63,Default,,0000,0000,0000,,A teba tiež.
Dialogue: 0,0:52:28.63,0:52:31.03,Default,,0000,0000,0000,,Nedal si riaditeľovi pozvánku?
Dialogue: 0,0:52:31.03,0:52:32.03,Default,,0000,0000,0000,,Presne.
Dialogue: 0,0:52:32.03,0:52:34.74,Default,,0000,0000,0000,,Ukážme mu našu skvelú stránku.
Dialogue: 0,0:52:34.74,0:52:36.54,Default,,0000,0000,0000,,Len aby sa vrátil.
Dialogue: 0,0:52:36.54,0:52:38.44,Default,,0000,0000,0000,,Hej, pracoval som tak ťažko, len aby som sa kvôli tebe\N pripravil na vystúpenie.
Dialogue: 0,0:52:38.44,0:52:39.94,Default,,0000,0000,0000,,Musíš byť takýto?
Dialogue: 0,0:52:39.94,0:52:42.04,Default,,0000,0000,0000,,A takisto, ostatní chcú tiež byť vo vysielaní aspoň raz.
Dialogue: 0,0:52:42.04,0:52:44.25,Default,,0000,0000,0000,,Ako môžeš myslieť len na seba?
Dialogue: 0,0:52:47.85,0:52:50.25,Default,,0000,0000,0000,,Rozumiem. Spravím to. Proste to spravím.
Dialogue: 0,0:52:52.36,0:52:53.66,Default,,0000,0000,0000,,Ah, to je úžasné!
Dialogue: 0,0:52:53.66,0:52:55.56,Default,,0000,0000,0000,,Ako ale príde riaditeľ?
Dialogue: 0,0:52:55.56,0:52:57.46,Default,,0000,0000,0000,,Povedz mu, že sa zmenila ulica.
Dialogue: 0,0:52:57.46,0:52:58.66,Default,,0000,0000,0000,,Dobre.
Dialogue: 0,0:52:58.66,0:52:59.66,Default,,0000,0000,0000,,Napíšem mu.
Dialogue: 0,0:52:59.66,0:53:01.16,Default,,0000,0000,0000,,Poďme aspoň raz si to precvičiť.
Dialogue: 0,0:53:01.56,0:53:02.67,Default,,0000,0000,0000,,Poďme.
Dialogue: 0,0:53:07.17,0:53:11.17,Default,,0000,0000,0000,,Učiteľ. Miesto sa zmenilo na KBC vysielaciu halu.
Dialogue: 0,0:53:11.17,0:53:12.48,Default,,0000,0000,0000,,Prosím prídite.
Dialogue: 0,0:53:12.48,0:53:15.28,Default,,0000,0000,0000,,Ak nie, naozaj končím v škole.
Dialogue: 0,0:53:27.99,0:53:30.29,Default,,0000,0000,0000,,Bude sa to spievať pri konci.
Dialogue: 0,0:53:30.79,0:53:32.50,Default,,0000,0000,0000,,Trochu sa zatrénujeme.
Dialogue: 0,0:53:34.60,0:53:37.20,Default,,0000,0000,0000,,Čo je to? Prvýkrát to vidím.
Dialogue: 0,0:53:47.61,0:53:49.71,Default,,0000,0000,0000,,Bála som sa o vás, preto som prišla.
Dialogue: 0,0:53:50.41,0:53:52.82,Default,,0000,0000,0000,,Bezo mňa, kto tu bude spievať?
Dialogue: 0,0:53:53.22,0:53:55.42,Default,,0000,0000,0000,,Som členom tejto skupiny.
Dialogue: 0,0:53:56.82,0:54:00.92,Default,,0000,0000,0000,,YuJin mi poslal správu a povedal mi o zmene miesta.
Dialogue: 0,0:54:04.63,0:54:07.83,Default,,0000,0000,0000,,Odkedy sme si tak blízki?
Dialogue: 0,0:54:18.04,0:54:20.54,Default,,0000,0000,0000,,Je to pesnička napísaná HaeSung.
Dialogue: 0,0:54:20.54,0:54:24.75,Default,,0000,0000,0000,,Pred chvíľkou som ju dorobil.
Dialogue: 0,0:54:26.55,0:54:28.95,Default,,0000,0000,0000,,"Hello to myself"
Dialogue: 0,0:54:30.75,0:54:32.26,Default,,0000,0000,0000,,Čo to je?
Dialogue: 0,0:54:33.06,0:54:35.36,Default,,0000,0000,0000,,Dávaš jej svoju kompozíciu?
Dialogue: 0,0:54:35.36,0:54:36.96,Default,,0000,0000,0000,,Shin HaeSung?
Dialogue: 0,0:55:16.40,0:55:17.40,Default,,0000,0000,0000,,Je to hotové?
Dialogue: 0,0:55:17.40,0:55:20.20,Default,,0000,0000,0000,,Dnes nebudete ľutovať.
Dialogue: 0,0:55:20.20,0:55:22.00,Default,,0000,0000,0000,,Nacvičovali sme veľmi veľa.
Dialogue: 0,0:55:22.00,0:55:23.81,Default,,0000,0000,0000,,Áno, presne.
Dialogue: 0,0:55:48.33,0:55:50.23,Default,,0000,0000,0000,,Hlupaňa.
Dialogue: 0,0:55:50.23,0:55:52.03,Default,,0000,0000,0000,,Piano nie je zlé.
Dialogue: 0,0:55:53.94,0:55:55.84,Default,,0000,0000,0000,,Skús si to znovu vypočuť.
Dialogue: 0,0:55:55.84,0:55:57.54,Default,,0000,0000,0000,,Tvoja pesnička nie je zlá.
Dialogue: 0,0:55:57.54,0:55:59.94,Default,,0000,0000,0000,,Naozaj dobrá.
Dialogue: 0,0:55:59.94,0:56:03.95,Default,,0000,0000,0000,,Dnes o šiestej budeme vystupovať s tvojou pesničkou.
Dialogue: 0,0:56:03.95,0:56:05.15,Default,,0000,0000,0000,,Uisti sa, že prídeš.
Dialogue: 0,0:56:05.15,0:56:07.45,Default,,0000,0000,0000,,Príď a zaspievaj nám,
Dialogue: 0,0:56:07.45,0:56:09.35,Default,,0000,0000,0000,,tak či tak, je to tvoja pieseň.
Dialogue: 0,0:56:09.35,0:56:11.05,Default,,0000,0000,0000,,Budeme na teba čakať.
Dialogue: 0,0:56:16.56,0:56:18.36,Default,,0000,0000,0000,,{\i1}Táto skupina je veľmi unikátna.{\i0}
Dialogue: 0,0:56:18.36,0:56:20.56,Default,,0000,0000,0000,,Je to špecialitka Kirin Strednej.
Dialogue: 0,0:56:20.96,0:56:23.47,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:56:23.47,0:56:28.57,Default,,0000,0000,0000,,Tak najprv, ten, kto ma inšpiroval s mojou rockovou dušou, je môj idol.
Dialogue: 0,0:56:28.87,0:56:30.77,Default,,0000,0000,0000,,Rock&roll je odporom.
Dialogue: 0,0:56:30.87,0:56:34.38,Default,,0000,0000,0000,,Je to ako povedať čo chceme bez limitu.
Dialogue: 0,0:56:38.28,0:56:43.29,Default,,0000,0000,0000,,Kirin Stredná je samotná dušou rock&roll-u.
Dialogue: 0,0:56:46.99,0:56:49.19,Default,,0000,0000,0000,,Mojím idolom je...
Dialogue: 0,0:56:50.29,0:56:53.00,Default,,0000,0000,0000,,Učiteľ Ju JungWan.
Dialogue: 0,0:56:54.30,0:56:57.90,Default,,0000,0000,0000,,Líder skupiny Milky Way, Ju JungWan?
Dialogue: 0,0:56:57.90,0:56:59.30,Default,,0000,0000,0000,,Presne tak.
Dialogue: 0,0:57:00.10,0:57:03.51,Default,,0000,0000,0000,,Odkedy ma rešpektuje? To je nonsens.
Dialogue: 0,0:57:03.51,0:57:06.01,Default,,0000,0000,0000,,Ale zradil ma.
Dialogue: 0,0:57:06.01,0:57:11.51,Default,,0000,0000,0000,,Nie, on zradil všetkých a odišiel.
Dialogue: 0,0:57:12.92,0:57:16.92,Default,,0000,0000,0000,,Pretože veľká spoločnost zhrabla našu školu.
Dialogue: 0,0:57:19.22,0:57:22.73,Default,,0000,0000,0000,,Naša škola, ktorá bola plná voľných snov,
Dialogue: 0,0:57:22.73,0:57:25.33,Default,,0000,0000,0000,,sa teraz stala cvičiskom spoločnosti,
Dialogue: 0,0:57:25.43,0:57:30.13,Default,,0000,0000,0000,,všetkými aspektami potláčajúc sny študentov.
Dialogue: 0,0:57:30.13,0:57:34.44,Default,,0000,0000,0000,,Zdôrazňujúc študentom, ako nasledovať štandarty idolov.
Dialogue: 0,0:57:34.44,0:57:37.24,Default,,0000,0000,0000,,Ako môže niekto iba stáť a prizerať sa?
Dialogue: 0,0:57:38.34,0:57:41.24,Default,,0000,0000,0000,,Kamarát to hovorí preto,
Dialogue: 0,0:57:41.24,0:57:46.45,Default,,0000,0000,0000,,aby ukázal veľkú náklonnosť riaditeľovi.
Dialogue: 0,0:57:50.35,0:57:52.86,Default,,0000,0000,0000,,Lian, už je to dlho.
Dialogue: 0,0:57:52.86,0:57:54.36,Default,,0000,0000,0000,,Čím si bola tak zaneprázdnená?
Dialogue: 0,0:57:54.36,0:57:57.36,Default,,0000,0000,0000,,To je Lian, Lian.
Dialogue: 0,0:57:57.36,0:57:59.26,Default,,0000,0000,0000,,Ako vidíte,
Dialogue: 0,0:57:59.26,0:58:01.26,Default,,0000,0000,0000,,zúčastňovala som sa školy.
Dialogue: 0,0:58:01.26,0:58:05.37,Default,,0000,0000,0000,,Naučila som sa skladať piesne, čo som predtým nevedela.
Dialogue: 0,0:58:06.07,0:58:08.17,Default,,0000,0000,0000,,Kedy sa naučila skladať?
Dialogue: 0,0:58:08.17,0:58:10.47,Default,,0000,0000,0000,,Skladať? Musí to byť skvelé.
Dialogue: 0,0:58:10.47,0:58:13.18,Default,,0000,0000,0000,,Nemôžeme si niečo málo vypočuť?
Dialogue: 0,0:58:16.98,0:58:23.49,Default,,0000,0000,0000,,Lian! Lian! Lian!
Dialogue: 0,0:58:53.62,0:58:56.12,Default,,0000,0000,0000,,Aigoo. Nie je to hlas Lian?
Dialogue: 0,0:58:56.12,0:59:00.22,Default,,0000,0000,0000,,Áno. Zrejme sa pesnička verejne vysiela.
Dialogue: 0,0:59:00.22,0:59:03.33,Default,,0000,0000,0000,,Lian vie ako skladať piesne, aké skvelé.
Dialogue: 0,0:59:03.83,0:59:05.83,Default,,0000,0000,0000,,Počkať!
Dialogue: 0,0:59:05.83,0:59:07.73,Default,,0000,0000,0000,,To je pieseň HaeSung.
Dialogue: 0,1:00:21.20,1:00:24.81,Default,,0000,0000,0000,,Toto, je moja skladba.
Dialogue: 0,1:00:26.01,1:00:28.11,Default,,0000,0000,0000,,Napísala som ju.
Dialogue: 0,1:01:44.69,1:01:48.29,Default,,0000,0000,0000,,To je JB!
Dialogue: 0,1:02:13.72,1:02:17.12,Default,,0000,0000,0000,,Si NG.
Dialogue: 0,1:02:29.13,1:02:32.54,Default,,0000,0000,0000,,Čo sa deje?
Dialogue: 0,1:02:48.65,1:02:51.05,Default,,0000,0000,0000,,Pusti ma. Pusti ma a porozprávajme sa.
Dialogue: 0,1:02:51.45,1:02:54.36,Default,,0000,0000,0000,,Predtým si takáto nebola.
Dialogue: 0,1:02:54.36,1:02:56.06,Default,,0000,0000,0000,,Povedala som ti, aby si ma pustil!
Dialogue: 0,1:02:56.06,1:02:59.16,Default,,0000,0000,0000,,Odpovedz mi. Prečo si to spravila?
Dialogue: 0,1:03:00.06,1:03:01.26,Default,,0000,0000,0000,,Čo som spravila?
Dialogue: 0,1:03:01.26,1:03:03.57,Default,,0000,0000,0000,,Dobre si vedela, že je to pieseň HaeSung.
Dialogue: 0,1:03:03.57,1:03:06.27,Default,,0000,0000,0000,,Dobre si vedela, že mám o tú pieseň záujem.
Dialogue: 0,1:03:06.27,1:03:08.67,Default,,0000,0000,0000,,Preto si to schválne spravila?
Dialogue: 0,1:03:09.17,1:03:11.97,Default,,0000,0000,0000,,Musíš ukradnúť aj poslednú vec, ktorú má?
Dialogue: 0,1:03:12.68,1:03:16.48,Default,,0000,0000,0000,,Myslíš si, že žiarlim na tvoj vzťah s ňou,
Dialogue: 0,1:03:16.98,1:03:19.18,Default,,0000,0000,0000,,a preto som jej ukradla pieseň?
Dialogue: 0,1:03:21.08,1:03:22.89,Default,,0000,0000,0000,,Myslíš iba na také veci?
Dialogue: 0,1:03:22.89,1:03:24.89,Default,,0000,0000,0000,,Nie to som myslel.
Dialogue: 0,1:03:25.99,1:03:29.49,Default,,0000,0000,0000,,Všetko čo má je túžba k hudbe.
Dialogue: 0,1:03:29.49,1:03:32.90,Default,,0000,0000,0000,,Zobrala si jej aj poslednú nádej.
Dialogue: 0,1:03:33.70,1:03:36.00,Default,,0000,0000,0000,,Vieš, akú krutú vec si spravila?
Dialogue: 0,1:03:38.30,1:03:40.60,Default,,0000,0000,0000,,Som krutá?
Dialogue: 0,1:03:41.10,1:03:42.51,Default,,0000,0000,0000,,Nádej.
Dialogue: 0,1:03:43.21,1:03:46.21,Default,,0000,0000,0000,,Nemám voľný čas na to, aby som sa trápila\N nádejmi iných ľudí.
Dialogue: 0,1:03:46.21,1:03:48.71,Default,,0000,0000,0000,,Mysli na to z inej perspektívy.
Dialogue: 0,1:03:49.41,1:03:52.22,Default,,0000,0000,0000,,Máš niečo, čo ti patrí a niekto ti to ukradne,
Dialogue: 0,1:03:52.22,1:03:53.22,Default,,0000,0000,0000,,vieš aký je to pocit?
Dialogue: 0,1:03:53.22,1:03:55.32,Default,,0000,0000,0000,,Viem, viem veľmi dobre.
Dialogue: 0,1:03:58.12,1:04:00.82,Default,,0000,0000,0000,,Niečo, čo by nemalo byť ukradnute,\N bolo ukradnuté.
Dialogue: 0,1:04:02.73,1:04:04.63,Default,,0000,0000,0000,,Nič zlé som nespravila.
Dialogue: 0,1:04:04.63,1:04:09.33,Default,,0000,0000,0000,,Už tam kde si teraz, si veľmi oslnivá.
Dialogue: 0,1:04:09.33,1:04:10.83,Default,,0000,0000,0000,,Nie.
Dialogue: 0,1:04:11.53,1:04:14.74,Default,,0000,0000,0000,,Je jedno ako oslnivá je hviezda,
Dialogue: 0,1:04:14.74,1:04:17.84,Default,,0000,0000,0000,,ak nie je videná, tak sa rýchlo na ňu zabudne.
Dialogue: 0,1:04:19.94,1:04:22.45,Default,,0000,0000,0000,,Vedel si to?
Dialogue: 0,1:04:22.45,1:04:25.35,Default,,0000,0000,0000,,Vrchný riaditeľ tentoraz,
Dialogue: 0,1:04:25.35,1:04:28.25,Default,,0000,0000,0000,,chce vytvoriť novú skupinu.
Dialogue: 0,1:04:28.25,1:04:30.75,Default,,0000,0000,0000,,Možno v krátkom čase,
Dialogue: 0,1:04:30.75,1:04:33.16,Default,,0000,0000,0000,,ťa Jin YuJin nahradí.
Dialogue: 0,1:04:35.36,1:04:37.76,Default,,0000,0000,0000,,Nebudem ako ty.
Dialogue: 0,1:04:38.36,1:04:40.96,Default,,0000,0000,0000,,Prizerať sa vlastnými očami,\N ako ti je ukradnutá tvoja pozícia.
Dialogue: 0,1:05:42.02,1:05:44.53,Default,,0000,0000,0000,,-=Náhľad k ďalšej epizóde=-
Dialogue: 0,1:05:45.13,1:05:46.33,Default,,0000,0000,0000,,Si v poriadku?
Dialogue: 0,1:05:46.33,1:05:48.53,Default,,0000,0000,0000,,Autobus odišiel.
Dialogue: 0,1:05:48.53,1:05:49.93,Default,,0000,0000,0000,,Je to ako pocit maturity.
Dialogue: 0,1:05:49.93,1:05:52.03,Default,,0000,0000,0000,,Vážne, ako maturita.
Dialogue: 0,1:05:52.03,1:05:53.04,Default,,0000,0000,0000,,Čo to má znamenať?
Dialogue: 0,1:05:53.04,1:05:54.54,Default,,0000,0000,0000,,Ani ste to so mnou osobne neprebrali.
Dialogue: 0,1:05:54.54,1:05:57.04,Default,,0000,0000,0000,,Super idol Kirin Strednej, Jin YuJin.
Dialogue: 0,1:05:57.04,1:05:58.64,Default,,0000,0000,0000,,Spravím z teba najlepšiu hviezdu národa.
Dialogue: 0,1:05:58.64,1:06:01.64,Default,,0000,0000,0000,,Včera nebol Jin YuJin jediný, ktorý oslňoval,\N ale ja.
Dialogue: 0,1:06:01.64,1:06:04.85,Default,,0000,0000,0000,,Nikdy si nemala dôkaz svojich schopností.
Dialogue: 0,1:06:04.85,1:06:06.55,Default,,0000,0000,0000,,Vždy si bol takýto?
Dialogue: 0,1:06:06.55,1:06:07.75,Default,,0000,0000,0000,,Nie si taký, lebo mi závidíš?
Dialogue: 0,1:06:07.75,1:06:09.85,Default,,0000,0000,0000,,Necháš aby sa ľudia trápili až kým nevykrvácajú.
Dialogue: 0,1:06:09.85,1:06:11.25,Default,,0000,0000,0000,,Máš ju rád?
Dialogue: 0,1:06:11.25,1:06:12.86,Default,,0000,0000,0000,,M-Mám ťa rád.
Dialogue: 0,1:06:13.16,1:06:14.26,Default,,0000,0000,0000,,Čože?
