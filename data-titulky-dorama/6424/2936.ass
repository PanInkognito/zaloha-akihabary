Dialogue: 0,0:00:00.00,0:00:15.00,Default,,0000,0000,0000,,SK Titulky: Hyumi - seesska.blog.cz/\N ENG Subs: DramaFever
Dialogue: 0,0:01:30.00,0:01:31.57,Default,,0000,0000,0000,,Bola skvelá!
Dialogue: 0,0:01:31.70,0:01:33.54,Default,,0000,0000,0000,,Nebolo to zlé.
Dialogue: 0,0:01:35.20,0:01:37.44,Default,,0000,0000,0000,,Wow, to nie je sranda.
Dialogue: 0,0:01:37.57,0:01:39.94,Default,,0000,0000,0000,,Prečo som tak nervózny?
Dialogue: 0,0:01:40.04,0:01:41.68,Default,,0000,0000,0000,,Chceš toto?
Dialogue: 0,0:01:41.81,0:01:43.45,Default,,0000,0000,0000,,Nepotrebujem lieky.
Dialogue: 0,0:01:43.61,0:01:45.25,Default,,0000,0000,0000,,Hej, si v poriadku?
Dialogue: 0,0:01:45.48,0:01:49.55,Default,,0000,0000,0000,,Zavri oči, vyčisti myseľ.
Dialogue: 0,0:01:49.75,0:01:52.42,Default,,0000,0000,0000,,Nie som HongJu.
Dialogue: 0,0:01:52.69,0:01:55.36,Default,,0000,0000,0000,,Ja nie som HongJu.
Dialogue: 0,0:01:55.92,0:01:58.43,Default,,0000,0000,0000,,Čo to robíš?
Dialogue: 0,0:01:58.56,0:02:00.13,Default,,0000,0000,0000,,Kontrola mysle.
Dialogue: 0,0:02:00.23,0:02:01.06,Default,,0000,0000,0000,,Neboj sa.
Dialogue: 0,0:02:01.20,0:02:03.23,Default,,0000,0000,0000,,Aj tak nevyhráš.
Dialogue: 0,0:02:03.50,0:02:05.23,Default,,0000,0000,0000,,Ťažko povedať.
Dialogue: 0,0:02:05.43,0:02:06.37,Default,,0000,0000,0000,,Pravda,
Dialogue: 0,0:02:06.50,0:02:08.64,Default,,0000,0000,0000,,všetci tu máme šancu.
Dialogue: 0,0:02:08.74,0:02:10.77,Default,,0000,0000,0000,,Nana, prečo si vždy na jeho strane?
Dialogue: 0,0:02:10.91,0:02:11.94,Default,,0000,0000,0000,,Lian!
Dialogue: 0,0:02:13.94,0:02:15.44,Default,,0000,0000,0000,,Aké to bolo? Bola som dobrá?
Dialogue: 0,0:02:15.64,0:02:17.45,Default,,0000,0000,0000,,Bola si skvelá.
Dialogue: 0,0:02:17.58,0:02:19.38,Default,,0000,0000,0000,,Lepšia než ja.
Dialogue: 0,0:02:19.58,0:02:21.52,Default,,0000,0000,0000,,Aké to bolo? Myslíte, že budem prvá?
Dialogue: 0,0:02:21.65,0:02:24.75,Default,,0000,0000,0000,,Možno, ak prehrám.
Dialogue: 0,0:02:26.19,0:02:29.66,Default,,0000,0000,0000,,Ale, JB tu stále nie je?
Dialogue: 0,0:02:29.76,0:02:33.26,Default,,0000,0000,0000,,Máš pravdu, je na rade.
Dialogue: 0,0:02:40.70,0:02:43.20,Default,,0000,0000,0000,,Hej, ako si mohol niekoho nechať strachovať sa o teba?
Dialogue: 0,0:02:43.64,0:02:45.94,Default,,0000,0000,0000,,Mysleli sme si, že neprídeš. Vystrašil si nás.
Dialogue: 0,0:02:46.14,0:02:47.21,Default,,0000,0000,0000,,Čo som spravil?
Dialogue: 0,0:02:47.38,0:02:49.38,Default,,0000,0000,0000,,Za to ako dodržujem sľub, ako si si mohol myslieť, že sa vzdám svojho vystúpenie?
Dialogue: 0,0:02:49.58,0:02:52.25,Default,,0000,0000,0000,,Mal by si mi to v budúcnosti dobre oplatiť.
Dialogue: 0,0:02:52.38,0:02:53.82,Default,,0000,0000,0000,,Pred pár rokmi som si vybudoval veľkú životnosť,
Dialogue: 0,0:02:53.95,0:02:55.22,Default,,0000,0000,0000,,len aby som vyplnil diery, čo si napáchal.
Dialogue: 0,0:02:55.38,0:02:56.32,Default,,0000,0000,0000,,Prečo si taký?
Dialogue: 0,0:02:56.52,0:02:57.52,Default,,0000,0000,0000,,Ak vyhrám,
Dialogue: 0,0:02:57.65,0:03:00.32,Default,,0000,0000,0000,,budeš to ľutovať.
Dialogue: 0,0:03:00.46,0:03:02.66,Default,,0000,0000,0000,,Prosím, určite vyhraj.
Dialogue: 0,0:03:02.82,0:03:05.53,Default,,0000,0000,0000,,Ešte si sa nepoklonil predo mnou.
Dialogue: 0,0:03:05.63,0:03:07.40,Default,,0000,0000,0000,,Dnes je ten deň, keď sa to stane.
Dialogue: 0,0:03:07.53,0:03:08.90,Default,,0000,0000,0000,,Nie.
Dialogue: 0,0:03:09.10,0:03:11.53,Default,,0000,0000,0000,,Toto je deň, keď ty si kľakneš predo mnou.
Dialogue: 0,0:03:11.63,0:03:14.84,Default,,0000,0000,0000,,Je deň, keď si kľakneš predo mnou.
Dialogue: 0,0:03:15.04,0:03:17.34,Default,,0000,0000,0000,,Idem na javisko.
Dialogue: 0,0:04:20.37,0:04:22.17,Default,,0000,0000,0000,,Čo je s ním?
Dialogue: 0,0:04:22.30,0:04:23.97,Default,,0000,0000,0000,,Vyliečil sa úplne?
Dialogue: 0,0:05:19.03,0:05:21.40,Default,,0000,0000,0000,,Išiel na javisko takto?
Dialogue: 0,0:05:21.60,0:05:24.90,Default,,0000,0000,0000,,Na čo by tam išiel, ak sa poriadne nevyliečil?
Dialogue: 0,0:06:07.74,0:06:11.58,Default,,0000,0000,0000,,Prečo si to doteraz skrýval?
Dialogue: 0,0:06:12.71,0:06:15.72,Default,,0000,0000,0000,,Nepovedal si, že je to fajn?
Dialogue: 0,0:06:17.95,0:06:20.82,Default,,0000,0000,0000,,To je koniec môjho dôvtipu.
Dialogue: 0,0:06:21.12,0:06:23.63,Default,,0000,0000,0000,,Ako sa môžem len tak vzdať?
Dialogue: 0,0:06:27.00,0:06:31.33,Default,,0000,0000,0000,,Doteraz, bol tanec pre mňa všetkým.
Dialogue: 0,0:06:33.27,0:06:36.20,Default,,0000,0000,0000,,Nemôžem len tak prestať, nie?
Dialogue: 0,0:06:40.51,0:06:44.25,Default,,0000,0000,0000,,Či už to pokazím alebo budem úspešný,
Dialogue: 0,0:06:45.08,0:06:47.75,Default,,0000,0000,0000,,je jedno ako, musím ísť ďalej.
Dialogue: 0,0:06:47.95,0:06:53.19,Default,,0000,0000,0000,,Len tak nebudem mať čo ľutovať.
Dialogue: 0,0:06:53.89,0:06:56.89,Default,,0000,0000,0000,,Kvôli mne,
Dialogue: 0,0:06:57.06,0:06:58.83,Default,,0000,0000,0000,,si takto skončil.
Dialogue: 0,0:06:58.96,0:07:02.06,Default,,0000,0000,0000,,Lebo som ťa neznášala.
Dialogue: 0,0:07:06.64,0:07:11.21,Default,,0000,0000,0000,,Nie, to vďaka tebe som to sem dotiahol.
Dialogue: 0,0:07:11.31,0:07:13.78,Default,,0000,0000,0000,,Ďakujem ti.
Dialogue: 0,0:07:17.15,0:07:23.89,Default,,0000,0000,0000,,Pred rokom si videla moje debut vystúpenie, že?
Dialogue: 0,0:07:26.89,0:07:31.26,Default,,0000,0000,0000,,Dnešné finálne vystúpenie.
Dialogue: 0,0:07:31.39,0:07:34.60,Default,,0000,0000,0000,,Som rád, že si ho videla.
Dialogue: 0,0:07:36.20,0:07:41.70,Default,,0000,0000,0000,,Či je už začiatok alebo koniec,
Dialogue: 0,0:07:41.90,0:07:44.27,Default,,0000,0000,0000,,vždy si bola pri mne,
Dialogue: 0,0:07:44.44,0:07:48.78,Default,,0000,0000,0000,,dala si mi silu.
Dialogue: 0,0:07:59.32,0:08:03.16,Default,,0000,0000,0000,,Môj desiatý pár tenisiek.
Dialogue: 0,0:08:09.63,0:08:11.63,Default,,0000,0000,0000,,Darček.
Dialogue: 0,0:08:17.01,0:08:19.07,Default,,0000,0000,0000,,V budúcnosti ich bude o 9 viac.
Dialogue: 0,0:08:19.27,0:08:21.64,Default,,0000,0000,0000,,Keď vynosím 9 párov tenisiek,
Dialogue: 0,0:08:21.84,0:08:23.51,Default,,0000,0000,0000,,určite zdebutujem.
Dialogue: 0,0:08:26.45,0:08:29.15,Default,,0000,0000,0000,,Toto je prvý pár.
Dialogue: 0,0:08:29.32,0:08:34.72,Default,,0000,0000,0000,,Potom ti dám ďalšie postupne.
Dialogue: 0,0:08:34.89,0:08:37.26,Default,,0000,0000,0000,,Ako prstene pre páriky.
Dialogue: 0,0:08:38.76,0:08:41.40,Default,,0000,0000,0000,,Nepracovala si tiež na sebe so mnou?
Dialogue: 0,0:08:41.53,0:08:45.13,Default,,0000,0000,0000,,Plná snov?
Dialogue: 0,0:08:45.33,0:08:49.77,Default,,0000,0000,0000,,Pracovala si viac ako ja.
Dialogue: 0,0:08:49.94,0:08:52.77,Default,,0000,0000,0000,,Tvoje sny a túžby,
Dialogue: 0,0:08:52.94,0:08:55.71,Default,,0000,0000,0000,,ako veľmi si sa snažila a koľkej bolesti si prešla,
Dialogue: 0,0:08:55.88,0:08:59.35,Default,,0000,0000,0000,,všetko som videl.
Dialogue: 0,0:09:00.82,0:09:04.29,Default,,0000,0000,0000,,Teraz ti budem fandiť.
Dialogue: 0,0:09:04.45,0:09:08.76,Default,,0000,0000,0000,,Zaspievaj pieseň pre seba.
Dialogue: 0,0:09:35.22,0:09:38.32,Default,,0000,0000,0000,,Predtým než budem spievať,
Dialogue: 0,0:09:38.45,0:09:43.22,Default,,0000,0000,0000,,chcem sa každému predstaviť.
Dialogue: 0,0:09:47.53,0:09:52.83,Default,,0000,0000,0000,,Viem veľmi dobre ako ma vidíte.
Dialogue: 0,0:09:53.03,0:09:55.34,Default,,0000,0000,0000,,Tvár dievčenskej skupiny.
Dialogue: 0,0:09:55.47,0:09:57.21,Default,,0000,0000,0000,,Drzá.
Dialogue: 0,0:09:57.34,0:10:00.38,Default,,0000,0000,0000,,Nevie hrať.
Dialogue: 0,0:10:00.51,0:10:05.21,Default,,0000,0000,0000,,Som s tým teraz veľmi dobre oboznámená.
Dialogue: 0,0:10:05.41,0:10:09.52,Default,,0000,0000,0000,,Prečo som vôbec chcela spievať?
Dialogue: 0,0:10:09.65,0:10:12.79,Default,,0000,0000,0000,,Prečo som chcela ísť na javisko?
Dialogue: 0,0:10:12.95,0:10:16.46,Default,,0000,0000,0000,,Na to som zabudla.
Dialogue: 0,0:10:16.66,0:10:23.10,Default,,0000,0000,0000,,Ale, stretla som divnú priateľku.
Dialogue: 0,0:10:23.27,0:10:29.04,Default,,0000,0000,0000,,Takú, čo nemala žiadny talent, a chcela byť speváčkou.
Dialogue: 0,0:10:29.14,0:10:33.84,Default,,0000,0000,0000,,Aby som bola úprimná, závidím jej.
Dialogue: 0,0:10:34.04,0:10:37.35,Default,,0000,0000,0000,,Lebo aspoň jasne vedela,
Dialogue: 0,0:10:37.48,0:10:41.62,Default,,0000,0000,0000,,aké sú jej sny.
Dialogue: 0,0:10:41.85,0:10:47.76,Default,,0000,0000,0000,,A, tiež som stretla priateľa, ktorý ma chránil,
Dialogue: 0,0:10:47.92,0:10:50.06,Default,,0000,0000,0000,,a poradil sa ako prvý namiesto mňa.
Dialogue: 0,0:10:50.23,0:10:52.56,Default,,0000,0000,0000,,Je pravda, že odstúpite zo show biznisu?
Dialogue: 0,0:10:52.73,0:10:54.50,Default,,0000,0000,0000,,Prosím povedzte nám k tomu niečo.
Dialogue: 0,0:10:54.73,0:10:57.43,Default,,0000,0000,0000,,Stačí. Už sa nič nepýtajte.
Dialogue: 0,0:10:57.57,0:11:00.00,Default,,0000,0000,0000,,Toto je škola. Škola.
Dialogue: 0,0:11:02.67,0:11:09.24,Default,,0000,0000,0000,,{\i1}Ten priateľ mi pomohol uvedomiť si,\N že si musím vážiť samú seba.{\i0}
Dialogue: 0,0:11:10.31,0:11:12.88,Default,,0000,0000,0000,,Taktiež,
Dialogue: 0,0:11:13.62,0:11:16.18,Default,,0000,0000,0000,,keď sme začali ako stážisti,
Dialogue: 0,0:11:16.38,0:11:21.49,Default,,0000,0000,0000,,bol tu priateľ, čo stál pri mne.
Dialogue: 0,0:11:21.69,0:11:25.13,Default,,0000,0000,0000,,Ten priateľ...
Dialogue: 0,0:11:25.26,0:11:29.60,Default,,0000,0000,0000,,chce aby som zaspievala pre seba.
Dialogue: 0,0:11:34.47,0:11:37.97,Default,,0000,0000,0000,,Môj desiatý pár tenisiek.
Dialogue: 0,0:11:38.41,0:11:41.61,Default,,0000,0000,0000,,Teraz ti budem fandiť.
Dialogue: 0,0:11:41.78,0:11:44.55,Default,,0000,0000,0000,,Zaspievaj sama sebe.
Dialogue: 0,0:11:54.86,0:11:59.93,Default,,0000,0000,0000,,Mojím snom je spievať.
Dialogue: 0,0:12:04.67,0:12:07.60,Default,,0000,0000,0000,,Je to moja prvá skladba,
Dialogue: 0,0:12:07.74,0:12:11.67,Default,,0000,0000,0000,,a text napísal Jin YuJin.
Dialogue: 0,0:12:11.84,0:12:15.11,Default,,0000,0000,0000,,Pesnička sa volá "Deň za dňom".
Dialogue: 0,0:12:18.75,0:12:22.42,Default,,0000,0000,0000,,Po kom je? Taká vychovaná.
Dialogue: 0,0:12:24.42,0:12:27.42,Default,,0000,0000,0000,,Aj keď sme boli s ňou, ani sme netušili
Dialogue: 0,0:12:27.56,0:12:30.43,Default,,0000,0000,0000,,že mala Lian také trable.
Dialogue: 0,0:12:30.53,0:12:33.43,Default,,0000,0000,0000,,Je jedna z tých, čo nepreukazuje svoje pocity.
Dialogue: 0,0:12:48.10,0:12:54.14,Default,,0000,0000,0000,,~ {\i1}Už dávno som mala sen.{\i0} ~
Dialogue: 0,0:12:54.14,0:13:01.51,Default,,0000,0000,0000,,~ {\i1}Stretnem ho niekedy?{\i0} ~
Dialogue: 0,0:13:01.51,0:13:09.45,Default,,0000,0000,0000,,~ {\i1}Stále sa vraciam na to isté miesto.{\i0} ~
Dialogue: 0,0:13:09.45,0:13:16.66,Default,,0000,0000,0000,,~ {\i1}Bez sebavedomia\Niba slzy, čo stekajú mi po lícach.{\i0} ~
Dialogue: 0,0:13:16.66,0:13:20.30,Default,,0000,0000,0000,,~ {\i1}Deň za dňom,{\i0} ~
Dialogue: 0,0:13:20.30,0:13:23.97,Default,,0000,0000,0000,,~ {\i1}s vetrom v ceste,{\i0} ~
Dialogue: 0,0:13:23.97,0:13:31.04,Default,,0000,0000,0000,,~ {\i1}bojím sa, že sny odletia a stratia sa.{\i0} ~
Dialogue: 0,0:13:31.04,0:13:40.95,Default,,0000,0000,0000,,~ {\i1}Už dávno sa to písalo v článku,\N ako sa sľúbilo.{\i0} ~
Dialogue: 0,0:13:40.95,0:13:43.12,Default,,0000,0000,0000,,~ {\i1}Ako dni ubiehajú,{\i0} ~
Dialogue: 0,0:13:43.12,0:13:47.72,Default,,0000,0000,0000,,~ {\i1}nestratím nádej.{\i0} ~
Dialogue: 0,0:13:49.93,0:13:54.40,Default,,0000,0000,0000,,~ {\i1}Možno veľa zapochybujem,{\i0} ~
Dialogue: 0,0:13:54.40,0:14:02.87,Default,,0000,0000,0000,,~ {\i1}alebo sa uvidím plakať.\N Avšak...{\i0} ~
Dialogue: 0,0:14:03.24,0:14:06.91,Default,,0000,0000,0000,,~ {\i1}ako dni ubiehajú,{\i0} ~
Dialogue: 0,0:14:06.91,0:14:10.48,Default,,0000,0000,0000,,~ {\i1}nesúc sa vetrom,{\i0} ~
Dialogue: 0,0:14:10.48,0:14:17.72,Default,,0000,0000,0000,,~ {\i1}moje sny ma naplnia.{\i0} ~
Dialogue: 0,0:14:17.72,0:14:27.46,Default,,0000,0000,0000,,~ {\i1}Už dávno sa to písalo v článku,\N ako sa sľúbilo. {\i0} ~
Dialogue: 0,0:14:27.46,0:14:28.36,Default,,0000,0000,0000,,~ {\i1}Ako dni ubiehajú,{\i0} ~
Dialogue: 0,0:14:28.36,0:14:34.60,Default,,0000,0000,0000,,~ {\i1}nestratím nádej.{\i0} ~
Dialogue: 0,0:14:34.60,0:14:35.64,Default,,0000,0000,0000,,~ {\i1}Ako dni ubiehajú,{\i0} ~
Dialogue: 0,0:14:35.64,0:14:44.38,Default,,0000,0000,0000,,~ {\i1}nestratím nádej.{\i0} ~
Dialogue: 0,0:15:28.77,0:15:31.61,Default,,0000,0000,0000,,To dievča spieva dobre.
Dialogue: 0,0:15:31.91,0:15:33.71,Default,,0000,0000,0000,,Budeme jej fanúšičkami?
Dialogue: 0,0:15:33.91,0:15:35.21,Default,,0000,0000,0000,,Prečo nie?
Dialogue: 0,0:15:36.85,0:15:38.21,Default,,0000,0000,0000,,Lian bola dnes najlepšia,
Dialogue: 0,0:15:38.35,0:15:40.02,Default,,0000,0000,0000,,kedy sa tak zlepšila?
Dialogue: 0,0:15:40.22,0:15:43.35,Default,,0000,0000,0000,,Nie je to vďaka dobrej práci učiteľa?
Dialogue: 0,0:15:43.52,0:15:46.29,Default,,0000,0000,0000,,Učiteľka Ahn, odkedy si takto dôveruješ?
Dialogue: 0,0:15:46.66,0:15:50.53,Default,,0000,0000,0000,,To nie ja, ale tá Cukrová Kocka.\N (mieri tým na učiteľku Hyeon)
Dialogue: 0,0:16:06.51,0:16:09.01,Default,,0000,0000,0000,,Už ťa nemôžem nasledovať.
Dialogue: 0,0:16:09.14,0:16:12.75,Default,,0000,0000,0000,,Prečo? Ideš sa mi plne pokloniť?
Dialogue: 0,0:16:13.52,0:16:17.09,Default,,0000,0000,0000,,Viem, že naše cesty sú rozdielne.
Dialogue: 0,0:16:17.19,0:16:20.69,Default,,0000,0000,0000,,Nie si unavený z rock duše?
Dialogue: 0,0:16:20.82,0:16:22.09,Default,,0000,0000,0000,,Nie.
Dialogue: 0,0:16:22.09,0:16:24.53,Default,,0000,0000,0000,,Nechcem byť ignorantský ako ty.
Dialogue: 0,0:16:24.69,0:16:27.70,Default,,0000,0000,0000,,Si beznádejný.
Dialogue: 0,0:16:29.00,0:16:31.57,Default,,0000,0000,0000,,Spievať balady je fajn.
Dialogue: 0,0:16:31.77,0:16:35.24,Default,,0000,0000,0000,,Ale, prečo ignorantsky brať veci hlava nehlava?
Dialogue: 0,0:16:35.67,0:16:38.24,Default,,0000,0000,0000,,Nakoniec si spadol a ťahal si si nohu.
Dialogue: 0,0:16:38.34,0:16:40.71,Default,,0000,0000,0000,,Si krvácal tak moc.
Dialogue: 0,0:16:40.91,0:16:42.98,Default,,0000,0000,0000,,Snažíš sa mi ukázať súcit?
Dialogue: 0,0:16:43.24,0:16:45.75,Default,,0000,0000,0000,,Vyvolávaš tu teraz moje ochraňujúce inštinkty?
Dialogue: 0,0:16:46.35,0:16:49.55,Default,,0000,0000,0000,,Tváriš sa za neviniatko, metieš ľudí.
Dialogue: 0,0:16:49.72,0:16:51.49,Default,,0000,0000,0000,,V tomto ohľade si presne ako HaeSung.
Dialogue: 0,0:16:52.99,0:16:55.06,Default,,0000,0000,0000,,Mám aj takú stránku?
Dialogue: 0,0:16:55.22,0:16:57.73,Default,,0000,0000,0000,,Neviem čo o tebe povedať.
Dialogue: 0,0:16:58.29,0:17:01.16,Default,,0000,0000,0000,,Urob dobre, aj za mňa.
Dialogue: 0,0:17:01.36,0:17:02.60,Default,,0000,0000,0000,,Stačí.
Dialogue: 0,0:17:02.80,0:17:04.90,Default,,0000,0000,0000,,Aj keď sa o mňa nebudeš strachovať,
Dialogue: 0,0:17:05.03,0:17:07.04,Default,,0000,0000,0000,,budem dobrý tak či tak.
Dialogue: 0,0:17:07.17,0:17:09.17,Default,,0000,0000,0000,,Len pozeraj.
Dialogue: 0,0:17:28.16,0:17:29.86,Default,,0000,0000,0000,,Je tam.
Dialogue: 0,0:17:30.03,0:17:31.59,Default,,0000,0000,0000,,Človek najmenej hodiaci sa na toto javisko,
Dialogue: 0,0:17:31.79,0:17:33.23,Default,,0000,0000,0000,,je on.
Dialogue: 0,0:17:33.36,0:17:34.50,Default,,0000,0000,0000,,Slobodná duša.
Dialogue: 0,0:17:34.60,0:17:37.80,Default,,0000,0000,0000,,Ako "Rock" a "konkurz" do seba zapadá?
Dialogue: 0,0:17:37.93,0:17:38.87,Default,,0000,0000,0000,,Čo je na tom zlé?
Dialogue: 0,0:17:38.97,0:17:39.94,Default,,0000,0000,0000,,Aj ja som bol rock spevákom.
Dialogue: 0,0:17:40.10,0:17:42.17,Default,,0000,0000,0000,,A stal som sa vrchným riaditeľom.
Dialogue: 0,0:17:42.40,0:17:45.67,Default,,0000,0000,0000,,A preto ťa všetci kritizujú?
Dialogue: 0,0:17:46.11,0:17:47.88,Default,,0000,0000,0000,,A čo keď som bol rock spevákom,
Dialogue: 0,0:17:48.04,0:17:50.11,Default,,0000,0000,0000,,a teraz som hyeong-om poradcu?
Dialogue: 0,0:18:04.73,0:18:09.03,Default,,0000,0000,0000,,Ako dlho ho chceš nasledovať?
Dialogue: 0,0:18:11.03,0:18:14.34,Default,,0000,0000,0000,,Budeš náhradníkom celý život?
Dialogue: 0,0:18:14.54,0:18:17.34,Default,,0000,0000,0000,,Ak budeš naďalej takýto,
Dialogue: 0,0:18:17.54,0:18:21.41,Default,,0000,0000,0000,,nikdy neuvidíš čo je pred ním.
Dialogue: 0,0:18:47.49,0:18:49.89,Default,,0000,0000,0000,,-=O 8 mesiacov neskôr=-
Dialogue: 0,0:19:19.23,0:19:20.77,Default,,0000,0000,0000,,Vrátili ste sa sem po svetovom turné,
Dialogue: 0,0:19:20.97,0:19:21.84,Default,,0000,0000,0000,,povedzte nám vaše pocity.
Dialogue: 0,0:19:22.04,0:19:24.91,Default,,0000,0000,0000,,Počuli sme, že americký producent osobne\N vezme do rúk ďalší album.
Dialogue: 0,0:19:25.11,0:19:27.21,Default,,0000,0000,0000,,Lian, hovorí sa, že budete Hollywoodskou hviezdou.
Dialogue: 0,0:19:27.41,0:19:29.11,Default,,0000,0000,0000,,Môžete nám to dosvedčiť?
Dialogue: 0,0:19:29.41,0:19:31.28,Default,,0000,0000,0000,,Dnes je oslava našich promócií.
Dialogue: 0,0:19:31.41,0:19:32.15,Default,,0000,0000,0000,,Už sa ponáhľame,
Dialogue: 0,0:19:32.35,0:19:34.85,Default,,0000,0000,0000,,takže čo tak urobiť rozhovor po ceremóniách?
Dialogue: 0,0:19:35.22,0:19:36.35,Default,,0000,0000,0000,,Iba jedna otázka.
Dialogue: 0,0:19:36.48,0:19:39.59,Default,,0000,0000,0000,,Lian. Nevyhrali ste posledný konkurz?
Dialogue: 0,0:19:39.79,0:19:42.66,Default,,0000,0000,0000,,Prečo ste sa vzdali aktivít?
Dialogue: 0,0:19:52.47,0:19:55.97,Default,,0000,0000,0000,,{\i1}Jin YuJin... Jin YuJin!{\i0}
Dialogue: 0,0:19:56.17,0:19:58.67,Default,,0000,0000,0000,,Hej! Jin YuJin!
Dialogue: 0,0:19:58.81,0:20:02.88,Default,,0000,0000,0000,,Rýchlo a spievaj!\N Prečo tam len tak stojíš?
Dialogue: 0,0:20:10.29,0:20:13.49,Default,,0000,0000,0000,,Je nervózny? To sa na neho nepodobá.
Dialogue: 0,0:20:13.66,0:20:15.12,Default,,0000,0000,0000,,Máš pravdu.
Dialogue: 0,0:20:33.48,0:20:36.68,Default,,0000,0000,0000,,G nota...
Dialogue: 0,0:20:37.35,0:20:40.18,Default,,0000,0000,0000,,Čo to robí?
Dialogue: 0,0:20:43.55,0:20:46.66,Default,,0000,0000,0000,,Teraz to je on.
Dialogue: 0,0:20:51.16,0:20:52.93,Default,,0000,0000,0000,,Ešte raz.
Dialogue: 0,0:21:05.74,0:21:08.68,Default,,0000,0000,0000,,G nota...
Dialogue: 0,0:21:16.42,0:21:18.12,Default,,0000,0000,0000,,Skončil som.
Dialogue: 0,0:21:18.49,0:21:20.79,Default,,0000,0000,0000,,Koniec.
Dialogue: 0,0:21:25.73,0:21:28.43,Default,,0000,0000,0000,,Čo to s ním je?
Dialogue: 0,0:21:28.80,0:21:30.57,Default,,0000,0000,0000,,To je ono?
Dialogue: 0,0:21:30.73,0:21:32.73,Default,,0000,0000,0000,,Viete,
Dialogue: 0,0:21:32.93,0:21:37.04,Default,,0000,0000,0000,,merali sme sem celú cestu až z Ameriky\N a toto je všetko čo nám ukážete? No tak!
Dialogue: 0,0:21:39.81,0:21:42.44,Default,,0000,0000,0000,,To je všetko čo som si pripravil.
Dialogue: 0,0:21:43.01,0:21:45.25,Default,,0000,0000,0000,,Ako to, že mi nikto netlieska?
Dialogue: 0,0:21:52.32,0:21:54.89,Default,,0000,0000,0000,,Ste sklamaní?
Dialogue: 0,0:21:55.06,0:21:56.20,Default,,0000,0000,0000,,Chcete si to vypočuť ešte raz?
Dialogue: 0,0:22:10.71,0:22:13.44,Default,,0000,0000,0000,,Som takýto, lebo som len takto dobrý.
Dialogue: 0,0:22:14.24,0:22:18.68,Default,,0000,0000,0000,,V budúcnosti vyriešim, čo chcem robiť,
Dialogue: 0,0:22:18.85,0:22:22.92,Default,,0000,0000,0000,,a keď sa znovu stretneme, nechám vás vypočuť si ma.
Dialogue: 0,0:22:23.08,0:22:26.66,Default,,0000,0000,0000,,Skutočnú hudbu Jin YuJina.
Dialogue: 0,0:22:43.84,0:22:44.81,Default,,0000,0000,0000,,Prečo to spravil?
Dialogue: 0,0:22:44.97,0:22:46.11,Default,,0000,0000,0000,,Mohol vyhrať.
Dialogue: 0,0:22:46.27,0:22:48.08,Default,,0000,0000,0000,,Aký je tvoj sen?
Dialogue: 0,0:22:48.21,0:22:51.31,Default,,0000,0000,0000,,Sny nemusia byť veľké.
Dialogue: 0,0:22:52.08,0:22:53.11,Default,,0000,0000,0000,,Ako?
Dialogue: 0,0:22:53.11,0:22:56.02,Default,,0000,0000,0000,,Aby sme šťastne žili každý deň,
Dialogue: 0,0:22:56.15,0:22:57.89,Default,,0000,0000,0000,,nie je to dobrý sen?
Dialogue: 0,0:22:58.09,0:23:00.12,Default,,0000,0000,0000,,Je dobrý, veľmi dobrý sen.
Dialogue: 0,0:23:09.06,0:23:11.00,Default,,0000,0000,0000,,Naozaj dokážeš ľudí prekvapiť.
Dialogue: 0,0:23:11.40,0:23:13.44,Default,,0000,0000,0000,,Si úžasný YuJin. Tak úžasný!
Dialogue: 0,0:23:13.60,0:23:14.57,Default,,0000,0000,0000,,Vedela si?
Dialogue: 0,0:23:14.74,0:23:16.24,Default,,0000,0000,0000,,Čo sa týka úžasnosti, som najlepší,
Dialogue: 0,0:23:16.40,0:23:18.97,Default,,0000,0000,0000,,a čo sa týka prekvapovaní ľudí, to až potom.
Dialogue: 0,0:23:19.71,0:23:22.71,Default,,0000,0000,0000,,Jin YuJin!
Dialogue: 0,0:23:23.68,0:23:25.55,Default,,0000,0000,0000,,Skoro som zabudol,
Dialogue: 0,0:23:25.71,0:23:30.35,Default,,0000,0000,0000,,že byť niekým trénovaný, mi nesedí.
Dialogue: 0,0:23:30.52,0:23:33.86,Default,,0000,0000,0000,,Chcem iba robiť hudbu čo mám rád.
Dialogue: 0,0:23:34.02,0:23:36.22,Default,,0000,0000,0000,,Možno nedostaneš ďalšiu šancu.
Dialogue: 0,0:23:36.42,0:23:38.33,Default,,0000,0000,0000,,Pre koho sú možnosti?
Dialogue: 0,0:23:38.53,0:23:40.26,Default,,0000,0000,0000,,Pre Shin PD-ho?
Dialogue: 0,0:23:40.43,0:23:44.43,Default,,0000,0000,0000,,Alebo pre Super Idola?
Dialogue: 0,0:23:46.67,0:23:49.47,Default,,0000,0000,0000,,Kvôli tým choro-sediacim šatám,
Dialogue: 0,0:23:49.60,0:23:53.14,Default,,0000,0000,0000,,som utekal tých 10 rokov.
Dialogue: 0,0:23:53.88,0:23:57.31,Default,,0000,0000,0000,,Strácať čas šatmi pre Super Idola,
Dialogue: 0,0:23:57.45,0:23:59.88,Default,,0000,0000,0000,,je veľká škoda.
Dialogue: 0,0:24:01.42,0:24:04.82,Default,,0000,0000,0000,,Hovoríš, že sa všetkého vzdávaš?
Dialogue: 0,0:24:04.95,0:24:09.16,Default,,0000,0000,0000,,Nevzdávam sa, ale rozhodujem sa.
Dialogue: 0,0:24:15.46,0:24:17.00,Default,,0000,0000,0000,,Jin YuJin!
Dialogue: 0,0:24:30.65,0:24:32.55,Default,,0000,0000,0000,,Žiadny darček pre mňa?
Dialogue: 0,0:24:33.75,0:24:35.35,Default,,0000,0000,0000,,Aké staromódne.
Dialogue: 0,0:24:36.82,0:24:38.85,Default,,0000,0000,0000,,Ani darček.
Dialogue: 0,0:24:39.02,0:24:40.06,Default,,0000,0000,0000,,Ako si sa stala svetovou stár,
Dialogue: 0,0:24:40.22,0:24:42.62,Default,,0000,0000,0000,,stratila si emócie k ostatným.
Dialogue: 0,0:24:42.82,0:24:45.49,Default,,0000,0000,0000,,Má sa mama dobre?
Dialogue: 0,0:24:45.66,0:24:46.83,Default,,0000,0000,0000,,Keďže si nikdy neprišla,
Dialogue: 0,0:24:47.00,0:24:48.70,Default,,0000,0000,0000,,zvýšili sme ceny.
Dialogue: 0,0:24:48.90,0:24:50.37,Default,,0000,0000,0000,,Čoskoro prídem,
Dialogue: 0,0:24:50.53,0:24:53.77,Default,,0000,0000,0000,,a zjem všetko, čo som si predtým neobjednala.
Dialogue: 0,0:24:55.47,0:24:58.01,Default,,0000,0000,0000,,Chcem sa ťa niečo spýtať.
Dialogue: 0,0:24:58.11,0:24:59.04,Default,,0000,0000,0000,,A čo?
Dialogue: 0,0:24:59.21,0:25:01.18,Default,,0000,0000,0000,,Nie si výherkyňou?
Dialogue: 0,0:25:01.48,0:25:04.55,Default,,0000,0000,0000,,Ale prečo si neodštartovala sólo?
Dialogue: 0,0:25:04.71,0:25:09.42,Default,,0000,0000,0000,,Namiesto toho sia le zostala robiť Super Idol aktivity s ostatnými?
Dialogue: 0,0:25:10.62,0:25:15.32,Default,,0000,0000,0000,,Pretože sme priatelia.
Dialogue: 0,0:25:15.76,0:25:18.56,Default,,0000,0000,0000,,Na leveli čo som dnes,
Dialogue: 0,0:25:18.73,0:25:21.26,Default,,0000,0000,0000,,som vďaka priateľom.
Dialogue: 0,0:25:21.56,0:25:24.53,Default,,0000,0000,0000,,Bude to ťažko keď budem sama.
Dialogue: 0,0:25:24.70,0:25:26.90,Default,,0000,0000,0000,,Preto si sa vzdala?
Dialogue: 0,0:25:27.07,0:25:31.24,Default,,0000,0000,0000,,Nevzdala som sa, rozhodla som sa, rozhodla.
Dialogue: 0,0:25:32.74,0:25:35.51,Default,,0000,0000,0000,,Čo? Si dojatý?
Dialogue: 0,0:25:35.68,0:25:37.71,Default,,0000,0000,0000,,Teraz to hráš, však?
Dialogue: 0,0:25:39.08,0:25:40.92,Default,,0000,0000,0000,,Snažíš sa byť milá.
Dialogue: 0,0:25:41.12,0:25:42.95,Default,,0000,0000,0000,,Nerob to.
Dialogue: 0,0:25:43.12,0:25:45.92,Default,,0000,0000,0000,,Naozaj, naozaj mám rád milé ženy.
Dialogue: 0,0:25:46.09,0:25:47.79,Default,,0000,0000,0000,,Nesedí ti to.
Dialogue: 0,0:25:47.96,0:25:49.06,Default,,0000,0000,0000,,Nerob to.
Dialogue: 0,0:25:51.33,0:25:53.53,Default,,0000,0000,0000,,Naozaj spolu chodíte?
Dialogue: 0,0:25:54.70,0:25:57.33,Default,,0000,0000,0000,,Hej, JB. Prišiel si.
Dialogue: 0,0:25:57.53,0:25:59.03,Default,,0000,0000,0000,,Príde Shin HaeSung alebo nie?
Dialogue: 0,0:25:59.20,0:26:00.54,Default,,0000,0000,0000,,Prečo ťa to zaujíma?
Dialogue: 0,0:26:00.70,0:26:02.07,Default,,0000,0000,0000,,Vlastníš ju?
Dialogue: 0,0:26:02.24,0:26:04.01,Default,,0000,0000,0000,,Nemôžem na ňu ani myslieť?
Dialogue: 0,0:26:04.17,0:26:05.61,Default,,0000,0000,0000,,Prečo sa tak hneváš?
Dialogue: 0,0:26:05.74,0:26:07.08,Default,,0000,0000,0000,,Tento nespratník, to nebude fungovať.
Dialogue: 0,0:26:07.24,0:26:08.44,Default,,0000,0000,0000,,Zahrajme si znovu v basketbale.
Dialogue: 0,0:26:08.64,0:26:10.65,Default,,0000,0000,0000,,Jasnačka, na čo sa vsadíme?
Dialogue: 0,0:26:10.81,0:26:11.75,Default,,0000,0000,0000,,Z toho ako chodíš,
Dialogue: 0,0:26:11.88,0:26:13.42,Default,,0000,0000,0000,,usudzujem že noha je fajn.
Dialogue: 0,0:26:13.58,0:26:15.68,Default,,0000,0000,0000,,Ak prehráš dnešný zápas,
Dialogue: 0,0:26:15.85,0:26:17.59,Default,,0000,0000,0000,,tak sa mi poriadne ukloníš.
Dialogue: 0,0:26:17.72,0:26:18.99,Default,,0000,0000,0000,,Samozrejme.
Dialogue: 0,0:26:19.25,0:26:22.82,Default,,0000,0000,0000,,A čo tak prejsť 50 krokov pri klánaní?
Dialogue: 0,0:26:22.99,0:26:25.29,Default,,0000,0000,0000,,50 krokov je málo. 100 krokov!
Dialogue: 0,0:26:25.46,0:26:28.76,Default,,0000,0000,0000,,Dohodli sme sa na vsádzke. Hotovo!
Dialogue: 0,0:26:28.90,0:26:30.80,Default,,0000,0000,0000,,To bolí!
Dialogue: 0,0:26:31.23,0:26:32.87,Default,,0000,0000,0000,,Tak príde HaeSung alebo nie?
Dialogue: 0,0:26:33.03,0:26:35.27,Default,,0000,0000,0000,,Prečo sa to pýtaš JB-a?
Dialogue: 0,0:26:35.44,0:26:39.14,Default,,0000,0000,0000,,Samozrejme preto, lebo ma má rada.
Dialogue: 0,0:26:39.71,0:26:42.54,Default,,0000,0000,0000,,HaeSung má rada mňa.
Dialogue: 0,0:26:46.45,0:26:49.58,Default,,0000,0000,0000,,Čo to je? Nepovedz mi...
Dialogue: 0,0:26:49.78,0:26:54.76,Default,,0000,0000,0000,,To je vec, ktorú nosíš aby si sa stala super stár? Niečo také?
Dialogue: 0,0:26:54.96,0:26:58.03,Default,,0000,0000,0000,,K, Song SamDong sunbae mi to dal.
Dialogue: 0,0:26:58.19,0:27:00.86,Default,,0000,0000,0000,,Čo? Nie je to povera?
Dialogue: 0,0:27:01.03,0:27:02.90,Default,,0000,0000,0000,,Nie je to povera.
Dialogue: 0,0:27:03.06,0:27:05.40,Default,,0000,0000,0000,,Nestala som sa prvou v Super Idolovi?
Dialogue: 0,0:27:06.03,0:27:08.44,Default,,0000,0000,0000,,Je to povera, povera.
Dialogue: 0,0:27:08.60,0:27:10.91,Default,,0000,0000,0000,,Lian, ak sa vrátiš ako úspešná z Ameriky,
Dialogue: 0,0:27:11.07,0:27:14.68,Default,,0000,0000,0000,,len vtedy tomu uverím.
Dialogue: 0,0:27:15.28,0:27:22.58,Default,,0000,0000,0000,,Len sa dívaj, či vyhrám ja alebo HaeSung.
Dialogue: 0,0:27:26.92,0:27:29.26,Default,,0000,0000,0000,,Učiteľka!
Dialogue: 0,0:27:29.59,0:27:30.63,Default,,0000,0000,0000,,Ďakujeme.
Dialogue: 0,0:27:30.83,0:27:32.73,Default,,0000,0000,0000,,Ďakujem.
Dialogue: 0,0:27:33.56,0:27:36.30,Default,,0000,0000,0000,,Ďakujeme za Vaše doterajšie úsilie.
Dialogue: 0,0:27:36.50,0:27:39.87,Default,,0000,0000,0000,,Áno, išlo vám to skvele.
Dialogue: 0,0:27:43.81,0:27:46.07,Default,,0000,0000,0000,,Deti už prómujú.
Dialogue: 0,0:27:46.21,0:27:47.64,Default,,0000,0000,0000,,Je to divný pocit.
Dialogue: 0,0:27:47.78,0:27:49.31,Default,,0000,0000,0000,,Len keby tu bolo viac času,
Dialogue: 0,0:27:49.48,0:27:51.55,Default,,0000,0000,0000,,našim deťom by to išlo lepšie.
Dialogue: 0,0:27:51.71,0:27:53.78,Default,,0000,0000,0000,,Aigoo učiteľka Ahn, si príliš chamtivá.
Dialogue: 0,0:27:53.95,0:27:55.02,Default,,0000,0000,0000,,Vieš, ako sa decká zlepšili?
Dialogue: 0,0:27:55.18,0:28:00.52,Default,,0000,0000,0000,,Áno? Preto keby tu bolo viac času...
Dialogue: 0,0:28:00.72,0:28:03.02,Default,,0000,0000,0000,,Urobia ako uznajú za vhodné.
Dialogue: 0,0:28:03.16,0:28:04.46,Default,,0000,0000,0000,,Nepovedala si predtým,
Dialogue: 0,0:28:04.63,0:28:06.80,Default,,0000,0000,0000,,že študenti nie sú odpoveďou, ale otázkou?
Dialogue: 0,0:28:07.00,0:28:10.90,Default,,0000,0000,0000,,Promócie nie je koniec, ale začiatok niečoho nového.
Dialogue: 0,0:28:11.53,0:28:14.17,Default,,0000,0000,0000,,Učiteľka Hyeon, znieš ako skutočná učiteľka.
Dialogue: 0,0:28:14.67,0:28:15.97,Default,,0000,0000,0000,,Vážne?
Dialogue: 0,0:28:17.14,0:28:20.88,Default,,0000,0000,0000,,Je dosť možné, že ešte nezačali.
Dialogue: 0,0:28:25.75,0:28:27.78,Default,,0000,0000,0000,,Skvelá práca. Pozri sa na tie tuky na tvári.
Dialogue: 0,0:28:27.95,0:28:30.39,Default,,0000,0000,0000,,Čo s nimi?
Dialogue: 0,0:28:49.87,0:28:51.64,Default,,0000,0000,0000,,Čo? To čo?
Dialogue: 0,0:28:51.81,0:28:54.44,Default,,0000,0000,0000,,Je ťažké si na to zvyknúť.
Dialogue: 0,0:28:56.14,0:28:59.51,Default,,0000,0000,0000,,Zvykni si, som hviezda.
Dialogue: 0,0:28:59.68,0:29:01.72,Default,,0000,0000,0000,,Svetová stár.
Dialogue: 0,0:29:01.88,0:29:05.92,Default,,0000,0000,0000,,- Svetová stár.\N- Aká svetová stár?
Dialogue: 0,0:29:06.05,0:29:07.46,Default,,0000,0000,0000,,Ak si svetová stár,
Dialogue: 0,0:29:07.62,0:29:09.52,Default,,0000,0000,0000,,tak ja som celosvetová stár.
Dialogue: 0,0:29:10.59,0:29:13.96,Default,,0000,0000,0000,,Ľudia hovoria, že čím lepším sa staneš, tým viac pokorným by si mal byť-
Dialogue: 0,0:29:14.13,0:29:15.03,Default,,0000,0000,0000,,Vyzerá byť fajn,
Dialogue: 0,0:29:15.20,0:29:17.57,Default,,0000,0000,0000,,prečo si taký, Žiarliš?
Dialogue: 0,0:29:20.37,0:29:22.44,Default,,0000,0000,0000,,Hej, prečo si vždy na strane HongJua?
Dialogue: 0,0:29:22.80,0:29:25.74,Default,,0000,0000,0000,,Aké šarmy skrýva?
Dialogue: 0,0:29:27.93,0:29:31.03,Default,,0000,0000,0000,,Oslava promócií Kirin Umeleckej Strednej.
Dialogue: 0,0:29:40.46,0:29:44.16,Default,,0000,0000,0000,,Teraz môže každý stáť na veľkom javisku,
Dialogue: 0,0:29:44.33,0:29:48.20,Default,,0000,0000,0000,,a sebavedome viesť svoje vystúpenie.
Dialogue: 0,0:29:48.36,0:29:50.70,Default,,0000,0000,0000,,To, ako si zariadite vaše javisko,
Dialogue: 0,0:29:50.83,0:29:54.50,Default,,0000,0000,0000,,záleží od vašeho úsilia.
Dialogue: 0,0:30:02.38,0:30:05.61,Default,,0000,0000,0000,,Hoci som podnikateľom,
Dialogue: 0,0:30:05.75,0:30:08.75,Default,,0000,0000,0000,,aj ja poznám tento dopyt,
Dialogue: 0,0:30:08.92,0:30:11.69,Default,,0000,0000,0000,,nemôžete žiť v dlhoch voči ostatným.
Dialogue: 0,0:30:11.85,0:30:17.19,Default,,0000,0000,0000,,Čo dlžíte vašim priateľom a učiteľom,
Dialogue: 0,0:30:17.33,0:30:20.56,Default,,0000,0000,0000,,dlhy lekcií.
Dialogue: 0,0:30:20.70,0:30:26.67,Default,,0000,0000,0000,,Povedzte im, že určite keď ich znovu uvidíte, tak im to oplatíte.
Dialogue: 0,0:30:37.35,0:30:43.08,Default,,0000,0000,0000,,Teraz, zastupujúca absolventov promócií, Shin HaeSung, nám povie príhovor.
Dialogue: 0,0:31:07.18,0:31:10.05,Default,,0000,0000,0000,,HaeSung mala pôvodne povedať príhovor,
Dialogue: 0,0:31:10.21,0:31:12.81,Default,,0000,0000,0000,,ale kvôli časovým obmedzeniam,
Dialogue: 0,0:31:12.98,0:31:16.52,Default,,0000,0000,0000,,poslala list z Ameriky.
Dialogue: 0,0:31:16.65,0:31:21.19,Default,,0000,0000,0000,,To bude ako jej príhovor.
Dialogue: 0,0:31:22.79,0:31:24.73,Default,,0000,0000,0000,,Ani mi o tom nepovedala.
Dialogue: 0,0:31:24.93,0:31:28.13,Default,,0000,0000,0000,,Vidíš, poslala ťa k vode.
Dialogue: 0,0:31:40.91,0:31:44.71,Default,,0000,0000,0000,,Drahí priatelia z Kirin Strednej, ktorí mi chýbajú,
Dialogue: 0,0:31:44.91,0:31:49.08,Default,,0000,0000,0000,,už odkedy sme boli malí, učili nás snívať.
Dialogue: 0,0:31:49.22,0:31:56.39,Default,,0000,0000,0000,,{\i1}Dospelí hovoria, čím väčší je ten sen, tým viac sa ti bude dariť.{\i0}
Dialogue: 0,0:31:54.48,0:31:57.08,Default,,0000,0000,0000,,Diplom. Shin HaeSung.
Dialogue: 0,0:31:56.56,0:32:04.03,Default,,0000,0000,0000,,{\i1}Nezabúdajte, že čím viac sa usilujete, tým viac uspejete.{\i0}
Dialogue: 0,0:32:04.17,0:32:10.87,Default,,0000,0000,0000,,{\i1}Avšak, cesta sna nebude hladká.{\i0}
Dialogue: 0,0:32:16.01,0:32:18.45,Default,,0000,0000,0000,,{\i1}Kvôli vám, iní trpia.{\i0}
Dialogue: 0,0:32:18.65,0:32:20.18,Default,,0000,0000,0000,,{\i1}Aspoň sa snažte aby tí ľudia{\i0}
Dialogue: 0,0:32:20.32,0:32:21.92,Default,,0000,0000,0000,,{\i1}sa necítili nepokojne keď vás počujú.{\i0}
Dialogue: 0,0:32:22.12,0:32:26.35,Default,,0000,0000,0000,,{\i1}Shin HaeSung je naozaj gramblavá.{\i0}
Dialogue: 0,0:32:26.49,0:32:30.43,Default,,0000,0000,0000,,{\i1}Necíťte sa beznádejnými kvôli nevydareniam.{\i0}
Dialogue: 0,0:32:30.59,0:32:35.63,Default,,0000,0000,0000,,{\i1}Stačí! NG! Naozaj! Koľkokrát sme to už spravili?{\i0}
Dialogue: 0,0:32:35.80,0:32:37.57,Default,,0000,0000,0000,,{\i1}Je ťažko vyroniť nejaké slzy?{\i0}
Dialogue: 0,0:32:37.73,0:32:40.54,Default,,0000,0000,0000,,{\i1}Musíš dať do toho emócie. Emócie.{\i0}
Dialogue: 0,0:32:40.67,0:32:43.10,Default,,0000,0000,0000,,{\i1}Prepáčte.{\i0}
Dialogue: 0,0:32:46.44,0:32:50.65,Default,,0000,0000,0000,,{\i1}Cítiť sa komplikovane a zničení realitou.{\i0}
Dialogue: 0,0:32:54.35,0:32:58.32,Default,,0000,0000,0000,,{\i1}Kto si myslíš, že si? Náhradník?{\i0}
Dialogue: 0,0:32:58.62,0:33:01.86,Default,,0000,0000,0000,,{\i1}Prečo som náhradník?{\i0}
Dialogue: 0,0:33:06.70,0:33:09.46,Default,,0000,0000,0000,,{\i1}A tiež môžu nastať nehody.{\i0}
Dialogue: 0,0:33:09.60,0:33:14.24,Default,,0000,0000,0000,,{\i1}Vzdáte sa svojich snov, potom bude ťažšie postaviť sa.{\i0}
Dialogue: 0,0:33:23.88,0:33:27.62,Default,,0000,0000,0000,,{\i1}Ale, kašlite na to, povzneste sa nad tým.{\i0}
Dialogue: 0,0:33:27.78,0:33:30.62,Default,,0000,0000,0000,,{\i1}Meniace sa zmeny momentov,{\i0}
Dialogue: 0,0:33:30.79,0:33:33.36,Default,,0000,0000,0000,,{\i1}nikdy ich nenechajte byť dôvodom vzdania sa svojich snov.{\i0}
Dialogue: 0,0:33:33.56,0:33:39.19,Default,,0000,0000,0000,,{\i1}Verte, že niekde tu budú priatelia,\N ktorí vo vás veria a fandia vám.{\i0}
Dialogue: 0,0:34:02.61,0:34:05.11,Default,,0000,0000,0000,,-=Milky Way=-
Dialogue: 0,0:34:09.76,0:34:14.46,Default,,0000,0000,0000,,{\i1}Hoci zmenou vašich snov sú promócie školy,{\i0}
Dialogue: 0,0:34:14.63,0:34:17.37,Default,,0000,0000,0000,,{\i1}a hoci zmeny sa objavia po vstupe do spoločnosti,{\i0}
Dialogue: 0,0:34:17.50,0:34:19.23,Default,,0000,0000,0000,,{\i1}nebudeme sa cítiť na tej ceste osamotene.{\i0}
Dialogue: 0,0:34:19.40,0:34:21.10,Default,,0000,0000,0000,,{\i1}Všetci z nás,{\i0}
Dialogue: 0,0:34:21.24,0:34:25.07,Default,,0000,0000,0000,,{\i1}navždy zostaneme ako priatelia,\N čo podporujú sny toho druhého, však?{\i0}
Dialogue: 0,0:34:25.24,0:34:28.98,Default,,0000,0000,0000,,{\i1}Sme priateľky, však?{\i0}
Dialogue: 0,0:34:29.14,0:34:32.25,Default,,0000,0000,0000,,{\i1}Si moja priateľka.{\i0}
Dialogue: 0,0:34:33.42,0:34:36.52,Default,,0000,0000,0000,,{\i1}Dnes je naozaj šťastný deň.{\i0}
Dialogue: 0,0:34:36.69,0:34:40.99,Default,,0000,0000,0000,,{\i1}Ako žiariaca hviezda zďaleka.{\i0}
Dialogue: 0,0:34:41.16,0:34:46.73,Default,,0000,0000,0000,,{\i1}Delíme od seba naše cesty, aby sme dokázali,\N že sme všetci žiariacimi hviezdami.{\i0}
Dialogue: 0,0:34:49.83,0:34:57.51,Default,,0000,0000,0000,,Všetkým gratulujem.\N Teraz poviem slová, čo každý chce počuť.
Dialogue: 0,0:35:00.78,0:35:04.05,Default,,0000,0000,0000,,Ceremónie promócií skončili! To je všetko!
Dialogue: 0,0:35:37.50,0:35:39.70,Default,,0000,0000,0000,,-=O 8 rokov neskôr=-
Dialogue: 0,0:35:48.66,0:35:50.13,Default,,0000,0000,0000,,Ešte raz.
Dialogue: 0,0:35:50.29,0:35:54.23,Default,,0000,0000,0000,,Znovu? Vystúpenie za pár hodín začne.
Dialogue: 0,0:35:54.36,0:35:57.17,Default,,0000,0000,0000,,Kedy skončíme nahrávanie?
Dialogue: 0,0:35:57.33,0:35:58.73,Default,,0000,0000,0000,,Tak to dokončime dobre na jeden krát,
Dialogue: 0,0:35:58.90,0:36:00.07,Default,,0000,0000,0000,,spievaj poriadne.
Dialogue: 0,0:36:01.90,0:36:04.17,Default,,0000,0000,0000,,Nechaj by som to tak inokedy,
Dialogue: 0,0:36:04.31,0:36:05.77,Default,,0000,0000,0000,,ale nie dnes.
Dialogue: 0,0:36:05.94,0:36:08.24,Default,,0000,0000,0000,,Dobre vieš, ako je dôležitá táto titulná skladba.
Dialogue: 0,0:36:08.41,0:36:12.21,Default,,0000,0000,0000,,Viem, viem veľmi dobre.
Dialogue: 0,0:36:13.15,0:36:15.52,Default,,0000,0000,0000,,Tak, skús ešte raz.
Dialogue: 0,0:36:18.95,0:36:22.69,Default,,0000,0000,0000,,{\i1}Po ôsmich rokoch, tancujúci umelec, ktorý povedal,\N že tanec je pre neho všetkým,{\i0}
Dialogue: 0,0:36:22.86,0:36:25.83,Default,,0000,0000,0000,,{\i1}už viac netancuje.{\i0}
Dialogue: 0,0:36:34.94,0:36:37.94,Default,,0000,0000,0000,,Náš učiteľ bol predtým Super Idolom.
Dialogue: 0,0:36:38.11,0:36:39.54,Default,,0000,0000,0000,,Som jeho fanúšičkou.
Dialogue: 0,0:36:39.71,0:36:42.44,Default,,0000,0000,0000,,Aj teraz je božský.
Dialogue: 0,0:36:45.01,0:36:51.22,Default,,0000,0000,0000,,Vy vzadu, čo tam furt kecáte?
Dialogue: 0,0:36:52.92,0:36:56.29,Default,,0000,0000,0000,,Používate ústa na tancovanie? Udriem vás!
Dialogue: 0,0:36:56.73,0:37:02.33,Default,,0000,0000,0000,,Dopredu, rýchlo.
Dialogue: 0,0:37:04.77,0:37:07.10,Default,,0000,0000,0000,,Miláčik!
Dialogue: 0,0:37:10.51,0:37:13.11,Default,,0000,0000,0000,,Zlatko, zase ich karháš?
Dialogue: 0,0:37:13.31,0:37:15.34,Default,,0000,0000,0000,,Každý deň dostávame hovory rodičov,
Dialogue: 0,0:37:15.51,0:37:18.28,Default,,0000,0000,0000,,že hodiny v škole sú striktnejšie než v spoločnosti.
Dialogue: 0,0:37:18.45,0:37:22.08,Default,,0000,0000,0000,,Preto naša škola dokáže vybudovať idoly.
Dialogue: 0,0:37:23.95,0:37:25.59,Default,,0000,0000,0000,,Viem...viem...
Dialogue: 0,0:37:25.72,0:37:26.96,Default,,0000,0000,0000,,Čo mi miláčik zakáže,
Dialogue: 0,0:37:27.12,0:37:28.26,Default,,0000,0000,0000,,to neurobím.
Dialogue: 0,0:37:28.46,0:37:30.56,Default,,0000,0000,0000,,{\i1}Super Idol sa rozpadli.{\i0}
Dialogue: 0,0:37:30.76,0:37:35.80,Default,,0000,0000,0000,,{\i1}Tieto hrdličky si našli niečo, čo majú radi.{\i0}
Dialogue: 0,0:37:41.44,0:37:42.94,Default,,0000,0000,0000,,Prosím sadnite si.
Dialogue: 0,0:37:48.34,0:37:50.78,Default,,0000,0000,0000,,Čo sa deje?
Dialogue: 0,0:37:51.58,0:37:57.89,Default,,0000,0000,0000,,Keď hovoríme o postave, mám dobrú postavu. Vzhľad? Tiež dobrý.\N Osobnosť? Aj tú mám dobrú.
Dialogue: 0,0:37:58.15,0:38:01.39,Default,,0000,0000,0000,,V ničom nie som zlý, som perfektný.
Dialogue: 0,0:38:03.33,0:38:05.63,Default,,0000,0000,0000,,Tak prečo?
Dialogue: 0,0:38:05.89,0:38:09.76,Default,,0000,0000,0000,,Je horší ako muži, ktorí sú priemerní.
Dialogue: 0,0:38:09.93,0:38:13.90,Default,,0000,0000,0000,,Priemerní muži...
Dialogue: 0,0:38:14.07,0:38:17.27,Default,,0000,0000,0000,,muži ako Park HongJu?
Dialogue: 0,0:38:22.21,0:38:26.61,Default,,0000,0000,0000,,Žeby...ste mali nadprirodzené schopnosti?
Dialogue: 0,0:38:28.08,0:38:30.95,Default,,0000,0000,0000,,Čo? Čo to?
Dialogue: 0,0:38:31.62,0:38:36.36,Default,,0000,0000,0000,,Stále mrkáš na iné ženy, však?
Dialogue: 0,0:38:36.96,0:38:40.43,Default,,0000,0000,0000,,Nie, to som nikdy neurobil.
Dialogue: 0,0:38:41.53,0:38:44.23,Default,,0000,0000,0000,,Toto nepomôže. Musím to sama vidieť.
Dialogue: 0,0:38:44.40,0:38:45.57,Default,,0000,0000,0000,,Nasleduj ma.
Dialogue: 0,0:38:57.35,0:38:59.05,Default,,0000,0000,0000,,Prečo nejdeš za mnou?
Dialogue: 0,0:38:59.21,0:39:00.75,Default,,0000,0000,0000,,Neveríš mi?
Dialogue: 0,0:39:00.92,0:39:06.86,Default,,0000,0000,0000,,Som expert na lásku a dokážem rozumieť iným na 100%.
Dialogue: 0,0:39:07.02,0:39:07.72,Default,,0000,0000,0000,,Poď so mnou!
Dialogue: 0,0:39:07.89,0:39:10.89,Default,,0000,0000,0000,,{\i1}Je to rovnaké pri malej šamanke, čo nemá nadprirodzené schopnosti.{\i0}
Dialogue: 0,0:39:11.46,0:39:14.66,Default,,0000,0000,0000,,Učiteľka Ahn, je to správne?
Dialogue: 0,0:39:15.16,0:39:17.07,Default,,0000,0000,0000,,Pekné. Učiteľ Yang,
Dialogue: 0,0:39:17.27,0:39:19.03,Default,,0000,0000,0000,,neveríš v môj zmysel pre módu?
Dialogue: 0,0:39:19.23,0:39:21.74,Default,,0000,0000,0000,,Ukáž všetko čo máš, ukáž svoju túžbu!
Dialogue: 0,0:39:21.87,0:39:24.91,Default,,0000,0000,0000,,Niečo, čo prejde cez oblečenie.
Dialogue: 0,0:39:25.41,0:39:27.64,Default,,0000,0000,0000,,Vyzerá to divne.
Dialogue: 0,0:39:27.78,0:39:28.54,Default,,0000,0000,0000,,Učiteľ Yang!
Dialogue: 0,0:39:28.78,0:39:29.44,Default,,0000,0000,0000,,Áno?
Dialogue: 0,0:39:29.44,0:39:30.75,Default,,0000,0000,0000,,Prišla som na dobrú choreografiu.
Dialogue: 0,0:39:30.88,0:39:31.88,Default,,0000,0000,0000,,Chceš aby som tancoval?
Dialogue: 0,0:39:32.05,0:39:33.05,Default,,0000,0000,0000,,Samozrejme.
Dialogue: 0,0:39:33.21,0:39:34.65,Default,,0000,0000,0000,,Skús to.
Dialogue: 0,0:39:36.35,0:39:40.79,Default,,0000,0000,0000,,1. 2. 3. 4. 2. 2. 3. 4.
Dialogue: 0,0:39:40.99,0:39:42.36,Default,,0000,0000,0000,,Aké to je? Je to dobré?
Dialogue: 0,0:39:42.52,0:39:43.33,Default,,0000,0000,0000,,Vyzerá to divne.
Dialogue: 0,0:39:43.49,0:39:44.19,Default,,0000,0000,0000,,Ako divne?
Dialogue: 0,0:39:44.36,0:39:45.99,Default,,0000,0000,0000,,Skús to.
Dialogue: 0,0:39:46.16,0:39:52.23,Default,,0000,0000,0000,,3. 4. 1. 2. 3. 4. 2. 2. 3. 4.
Dialogue: 0,0:39:52.40,0:39:53.84,Default,,0000,0000,0000,,Veľmi dobre, učiteľ Yang!
Dialogue: 0,0:39:54.00,0:39:54.70,Default,,0000,0000,0000,,Naozaj?
Dialogue: 0,0:39:54.90,0:39:56.57,Default,,0000,0000,0000,,Toto oblečenie, plus tento tanec,
Dialogue: 0,0:39:56.74,0:39:57.74,Default,,0000,0000,0000,,to bude kompletný úspech.
Dialogue: 0,0:39:59.31,0:40:02.28,Default,,0000,0000,0000,,Toto je najlepšie, najlepšie! Veľa šťastia!
Dialogue: 0,0:40:02.44,0:40:04.71,Default,,0000,0000,0000,,Naozaj je to, správne?
Dialogue: 0,0:40:04.95,0:40:06.75,Default,,0000,0000,0000,,Okej, veľa šťastia!
Dialogue: 0,0:40:06.95,0:40:08.02,Default,,0000,0000,0000,,Veľa šťastia!
Dialogue: 0,0:40:12.79,0:40:16.56,Default,,0000,0000,0000,,Sústreď sa, je to tvoja posledná možnosť. Veľa šťastia!
Dialogue: 0,0:40:16.73,0:40:19.06,Default,,0000,0000,0000,,Ďalší človek nech príde.
Dialogue: 0,0:40:18.38,0:40:24.49,Default,,0000,0000,0000,,CBC. STAR.
Dialogue: 0,0:40:23.37,0:40:25.50,Default,,0000,0000,0000,,Zdravím.
Dialogue: 0,0:40:36.21,0:40:38.41,Default,,0000,0000,0000,,Hovoríte, že ste učiteľom na Kirin Strednej?
Dialogue: 0,0:40:38.58,0:40:42.28,Default,,0000,0000,0000,,Áno, predtým než vyjde zo mňa oheň,
Dialogue: 0,0:40:42.42,0:40:45.62,Default,,0000,0000,0000,,chcem niečo skúsiť aspoň posledný krát.
Dialogue: 0,0:40:46.66,0:40:50.66,Default,,0000,0000,0000,,Čo máte s oblečením?
Dialogue: 0,0:40:50.93,0:40:55.20,Default,,0000,0000,0000,,Chcem Vám ukázať všetko čo viem.
Dialogue: 0,0:40:56.63,0:40:59.13,Default,,0000,0000,0000,,"Neopúšťaj ma", je vlastná pesnička?
Dialogue: 0,0:41:00.20,0:41:00.77,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:41:00.77,0:41:03.61,Default,,0000,0000,0000,,Dobre. Vypočujme si to.
Dialogue: 0,0:41:17.18,0:41:23.48,Default,,0000,0000,0000,,Párty ľudia!
Dialogue: 0,0:41:38.90,0:41:46.01,Default,,0000,0000,0000,,Neopúšťaj ma. Neznesiem keď vidím ako odchádzaš.
Dialogue: 0,0:41:46.01,0:41:52.51,Default,,0000,0000,0000,,Aj keď odídeš, budem tu stáť, pozerať sa na teba.
Dialogue: 0,0:41:52.51,0:42:00.52,Default,,0000,0000,0000,,Poviem ti to ešte raz, neopúšťaj ma.
Dialogue: 0,0:42:00.52,0:42:07.03,Default,,0000,0000,0000,,Moja láska ti pomôže nájsť cestu ku mne.
Dialogue: 0,0:42:07.03,0:42:10.53,Default,,0000,0000,0000,,Oh bejbe.
Dialogue: 0,0:42:18.25,0:42:21.12,Default,,0000,0000,0000,,Učili ste sa niekde spievať?
Dialogue: 0,0:42:21.28,0:42:22.72,Default,,0000,0000,0000,,Nie.
Dialogue: 0,0:42:22.88,0:42:25.65,Default,,0000,0000,0000,,Nemáte dobré základy.
Dialogue: 0,0:42:25.82,0:42:30.29,Default,,0000,0000,0000,,Musíte poriadne dýchať a spievať kým vydychujete.
Dialogue: 0,0:42:30.46,0:42:31.83,Default,,0000,0000,0000,,Keďže Vaša vokalizácia nie je dobrá,
Dialogue: 0,0:42:31.99,0:42:35.90,Default,,0000,0000,0000,,spievate hlavou na jednu stranu a potom skončí takto.
Dialogue: 0,0:42:38.77,0:42:44.91,Default,,0000,0000,0000,,Ale, Vaša odvaha stojí za pochvalu.
Dialogue: 0,0:42:45.11,0:42:53.21,Default,,0000,0000,0000,,A tiež, Váš štýl sa tu nevidí často.
Dialogue: 0,0:42:58.05,0:43:00.59,Default,,0000,0000,0000,,Učiteľ Yang! Ako to išlo?
Dialogue: 0,0:43:00.76,0:43:02.36,Default,,0000,0000,0000,,Eliminovali ťa?
Dialogue: 0,0:43:02.66,0:43:05.43,Default,,0000,0000,0000,,Ja...spravil som to!
Dialogue: 0,0:43:06.73,0:43:09.50,Default,,0000,0000,0000,,{\i1}Počula som, že učitelia z Kirin Strednej, ktorých sen bolo stať sa spevákom,{\i0}
Dialogue: 0,0:43:09.66,0:43:13.17,Default,,0000,0000,0000,,{\i1}tiež na sebe pracujú.{\i0}
Dialogue: 0,0:43:22.61,0:43:24.55,Default,,0000,0000,0000,,Diva, ktorá skoro stratila svoj hlas,
Dialogue: 0,0:43:24.71,0:43:27.08,Default,,0000,0000,0000,,použila silu lásky na povznesenie sa z krízy.
Dialogue: 0,0:43:27.25,0:43:29.32,Default,,0000,0000,0000,,Aj keď nemohla robiť sólo aktivity ako vždy snívala,
Dialogue: 0,0:43:29.48,0:43:33.46,Default,,0000,0000,0000,,stala sa druhou polovicou populárneho duetového páru.
Dialogue: 0,0:43:40.10,0:43:41.26,Default,,0000,0000,0000,,Neurobil som dnes žiadnu chybu, však?
Dialogue: 0,0:43:41.43,0:43:43.50,Default,,0000,0000,0000,,Presne, žiadnu.
Dialogue: 0,0:43:44.60,0:43:47.20,Default,,0000,0000,0000,,Stále robia duety?
Dialogue: 0,0:43:47.34,0:43:48.44,Default,,0000,0000,0000,,Nemajú žiadne sólo aktivity?
Dialogue: 0,0:43:48.60,0:43:49.54,Default,,0000,0000,0000,,O tom hovorím.
Dialogue: 0,0:43:49.70,0:43:52.64,Default,,0000,0000,0000,,Ten HongJu ma štve.
Dialogue: 0,0:43:57.85,0:43:58.78,Default,,0000,0000,0000,,Ako vyzeráme?
Dialogue: 0,0:43:58.95,0:44:00.02,Default,,0000,0000,0000,,Ako inak?
Dialogue: 0,0:44:00.08,0:44:01.62,Default,,0000,0000,0000,,Ty najhoršie.
Dialogue: 0,0:44:04.69,0:44:06.96,Default,,0000,0000,0000,,Všetko vďaka Nane.
Dialogue: 0,0:44:08.86,0:44:09.76,Default,,0000,0000,0000,,Nemal si fotenie?
Dialogue: 0,0:44:09.92,0:44:10.93,Default,,0000,0000,0000,,Už si skončil?
Dialogue: 0,0:44:11.09,0:44:14.06,Default,,0000,0000,0000,,Zase si vo vojne lásky?
Dialogue: 0,0:44:14.33,0:44:15.63,Default,,0000,0000,0000,,Keď budeš stále takýto,
Dialogue: 0,0:44:15.93,0:44:17.30,Default,,0000,0000,0000,,tvoj vzhľad sa zhorší, budeš známy ako zlý chlapec.
Dialogue: 0,0:44:17.47,0:44:20.20,Default,,0000,0000,0000,,Vôbec.
Dialogue: 0,0:44:20.37,0:44:23.57,Default,,0000,0000,0000,,Som čistý a nevinný muž.
Dialogue: 0,0:44:24.27,0:44:27.78,Default,,0000,0000,0000,,Tak potom, obmedz tie dotyky.
Dialogue: 0,0:44:29.51,0:44:31.41,Default,,0000,0000,0000,,Musíme ísť, meškáme.
Dialogue: 0,0:44:31.58,0:44:33.88,Default,,0000,0000,0000,,Musíme všetko dokončiť pred vystúpením o ôsmej.
Dialogue: 0,0:44:34.02,0:44:35.32,Default,,0000,0000,0000,,O ôsmej?
Dialogue: 0,0:44:35.92,0:44:37.39,Default,,0000,0000,0000,,Aké vystúpenie?
Dialogue: 0,0:44:37.55,0:44:39.59,Default,,0000,0000,0000,,Naše.
Dialogue: 0,0:44:42.76,0:44:43.99,Default,,0000,0000,0000,,Poďme.
Dialogue: 0,0:44:45.09,0:44:49.96,Default,,0000,0000,0000,,{\i1}Ich trojuholníková láska sa stále nevyriešila.{\i0}
Dialogue: 0,0:44:50.13,0:44:52.80,Default,,0000,0000,0000,,Nevyzerá to, že to dokážeš, vzdaj sa.
Dialogue: 0,0:45:01.71,0:45:04.08,Default,,0000,0000,0000,,{\i1}Slobodná duša čo sníva o bytím rock stár,{\i0}
Dialogue: 0,0:45:04.28,0:45:05.88,Default,,0000,0000,0000,,{\i1}čo vedie svet s rockom,{\i0}
Dialogue: 0,0:45:06.05,0:45:07.92,Default,,0000,0000,0000,,{\i1}hovorí, že je ťažké spoľahnúť sa na svoje schopnosti.{\i0}
Dialogue: 0,0:45:08.08,0:45:09.25,Default,,0000,0000,0000,,{\i1}Rock spevákom cez noc,{\i0}
Dialogue: 0,0:45:09.42,0:45:10.95,Default,,0000,0000,0000,,{\i1}a učiteľom hudby cez deň.{\i0}
Dialogue: 0,0:45:11.09,0:45:13.59,Default,,0000,0000,0000,,{\i1}Míňa svoju energiu na študentov,{\i0}
Dialogue: 0,0:45:13.76,0:45:15.39,Default,,0000,0000,0000,,{\i1}nikdy sa nevzdávajúc svojej rockovej duše.{\i0}
Dialogue: 0,0:45:15.56,0:45:18.93,Default,,0000,0000,0000,,{\i1}Je podobný riaditeľovi, však?{\i0}
Dialogue: 0,0:45:20.90,0:45:25.23,Default,,0000,0000,0000,,Hoci dúfam, že každý sa zúčastní
Dialogue: 0,0:45:26.84,0:45:29.34,Default,,0000,0000,0000,,v spoločnom vystúpení medzi bývalými\N absolventmi a terajšími študentmi,
Dialogue: 0,0:45:29.50,0:45:30.97,Default,,0000,0000,0000,,je to nerealistické.
Dialogue: 0,0:45:31.17,0:45:33.78,Default,,0000,0000,0000,,Konkurz bude
Dialogue: 0,0:45:33.94,0:45:36.38,Default,,0000,0000,0000,,vysokého štandartu.
Dialogue: 0,0:45:36.54,0:45:42.72,Default,,0000,0000,0000,,Tak či tak, každý sa musí pripraviť,
Dialogue: 0,0:45:42.88,0:45:45.49,Default,,0000,0000,0000,,a podieľať sa na konkurze.
Dialogue: 0,0:45:47.89,0:45:49.39,Default,,0000,0000,0000,,Žiadna odozva?
Dialogue: 0,0:45:50.63,0:45:51.83,Default,,0000,0000,0000,,Čo ste takí slabí?
Dialogue: 0,0:45:52.03,0:45:54.83,Default,,0000,0000,0000,,Každý musíte zakričať aspoň 100-krát! Teraz!
Dialogue: 0,0:45:55.06,0:45:57.00,Default,,0000,0000,0000,,Začnite!
Dialogue: 0,0:45:59.87,0:46:02.24,Default,,0000,0000,0000,,Učiteľ Jin, poďte na chvíľku.
Dialogue: 0,0:46:02.54,0:46:03.77,Default,,0000,0000,0000,,Som v polovici hodiny.
Dialogue: 0,0:46:03.97,0:46:05.27,Default,,0000,0000,0000,,Deje sa niečo?
Dialogue: 0,0:46:05.77,0:46:07.48,Default,,0000,0000,0000,,Prosím vyjdite na chvíľku,
Dialogue: 0,0:46:07.64,0:46:09.61,Default,,0000,0000,0000,,pohni si ty nespratník.
Dialogue: 0,0:46:11.25,0:46:12.05,Default,,0000,0000,0000,,Nerob bordel okolo toho.
Dialogue: 0,0:46:12.05,0:46:14.12,Default,,0000,0000,0000,,Na chvíľku poďte, učiteľ Jin.
Dialogue: 0,0:46:14.28,0:46:16.22,Default,,0000,0000,0000,,Čo sa deje?
Dialogue: 0,0:46:17.89,0:46:19.99,Default,,0000,0000,0000,,Ty nespratník, prečo zase robíš problémy?
Dialogue: 0,0:46:20.16,0:46:21.82,Default,,0000,0000,0000,,Kedy som urobil problémy?
Dialogue: 0,0:46:22.06,0:46:24.49,Default,,0000,0000,0000,,Vrchný riaditeľ sa po tebe zháňa, poď rýchlo.
Dialogue: 0,0:46:28.96,0:46:30.40,Default,,0000,0000,0000,,Spoločné vystúpenie medzi
Dialogue: 0,0:46:30.60,0:46:33.07,Default,,0000,0000,0000,,bývalými absolventmi a terajšími študentami,
Dialogue: 0,0:46:33.27,0:46:34.94,Default,,0000,0000,0000,,si so mnou ani neprebral,
Dialogue: 0,0:46:35.10,0:46:36.71,Default,,0000,0000,0000,,a už je to v médiách?
Dialogue: 0,0:46:36.84,0:46:39.87,Default,,0000,0000,0000,,Ako ostatní učitelia upracú tvoj neporiadok?
Dialogue: 0,0:46:40.04,0:46:41.94,Default,,0000,0000,0000,,Netreba nič upratovať,
Dialogue: 0,0:46:42.11,0:46:44.21,Default,,0000,0000,0000,,nie je potreba to robiť.
Dialogue: 0,0:46:45.08,0:46:46.98,Default,,0000,0000,0000,,Nie je dobrá možnosť?
Dialogue: 0,0:46:47.12,0:46:49.22,Default,,0000,0000,0000,,Naša Kirin Umelecká Stredná
Dialogue: 0,0:46:49.35,0:46:51.49,Default,,0000,0000,0000,,bude mať 17. výročie.
Dialogue: 0,0:46:51.82,0:46:54.42,Default,,0000,0000,0000,,Cítim, že treba začať novú tradíciu,
Dialogue: 0,0:46:54.59,0:46:58.63,Default,,0000,0000,0000,,a aby to bola extrémne dôležitá udalosť.
Dialogue: 0,0:46:59.73,0:47:01.36,Default,,0000,0000,0000,,Aj kebyže ti dám schválenie,
Dialogue: 0,0:47:01.53,0:47:03.06,Default,,0000,0000,0000,,aj kebyže úmysel je dobrý,
Dialogue: 0,0:47:03.47,0:47:07.00,Default,,0000,0000,0000,,tak čo so scenárom? Čo s riaditeľom?
Dialogue: 0,0:47:07.17,0:47:10.27,Default,,0000,0000,0000,,Kto pozve bývalých absolventov a vystupujúcich?
Dialogue: 0,0:47:11.31,0:47:13.01,Default,,0000,0000,0000,,O to sa netreba báť.
Dialogue: 0,0:47:13.17,0:47:14.38,Default,,0000,0000,0000,,Urobím ako uznám za vhodné.
Dialogue: 0,0:47:14.54,0:47:16.98,Default,,0000,0000,0000,,Ty nespratník, ako uznáš za vhodné?
Dialogue: 0,0:47:18.88,0:47:21.65,Default,,0000,0000,0000,,Učiteľ Jin, čo spravíte ako uznáte za vhodné?
Dialogue: 0,0:47:21.85,0:47:23.49,Default,,0000,0000,0000,,Aký máte plán?
Dialogue: 0,0:47:23.69,0:47:29.99,Default,,0000,0000,0000,,Na teraz, sa pozrite na scenár.
Dialogue: 0,0:47:30.16,0:47:32.33,Default,,0000,0000,0000,,Toto je scenár.
Dialogue: 0,0:47:36.67,0:47:38.40,Default,,0000,0000,0000,,~ {\i1}Dream High{\i0} ~
Dialogue: 0,0:47:39.57,0:47:42.64,Default,,0000,0000,0000,,{\i1}Teraz jedine zostali{\i0}
Dialogue: 0,0:47:42.80,0:47:44.64,Default,,0000,0000,0000,,{\i1}škaredé káčatko, ktoré šikanovali,{\i0}
Dialogue: 0,0:47:44.81,0:47:46.11,Default,,0000,0000,0000,,{\i1}bez hudobného sluchu Shin HaeSung,{\i0}
Dialogue: 0,0:47:46.27,0:47:48.88,Default,,0000,0000,0000,,{\i1}a drzá, zlá herečka Lian.{\i0}
Dialogue: 0,0:47:52.21,0:47:56.62,Default,,0000,0000,0000,,Čo? Koho nazývaš drzou?
Dialogue: 0,0:47:56.92,0:47:59.69,Default,,0000,0000,0000,,Kedy som taká bola?
Dialogue: 0,0:48:01.02,0:48:02.86,Default,,0000,0000,0000,,Zrejme scenár je veľmi dojemný.
Dialogue: 0,0:48:03.02,0:48:06.19,Default,,0000,0000,0000,,Ešte aj plačete.
Dialogue: 0,0:48:07.20,0:48:09.90,Default,,0000,0000,0000,,Pretože som z toho cítila úprimnosť.
Dialogue: 0,0:48:15.14,0:48:17.11,Default,,0000,0000,0000,,Preháňate.
Dialogue: 0,0:48:17.27,0:48:19.71,Default,,0000,0000,0000,,Ja vediem tento scenár.
Dialogue: 0,0:48:21.98,0:48:24.08,Default,,0000,0000,0000,,Hoci v tom bola úprimnosť,
Dialogue: 0,0:48:24.25,0:48:25.08,Default,,0000,0000,0000,,ako to podať...
Dialogue: 0,0:48:25.25,0:48:27.15,Default,,0000,0000,0000,,chýbalo tu niečo nové.
Dialogue: 0,0:48:27.32,0:48:30.05,Default,,0000,0000,0000,,Veľa vecí sa tu deje.
Dialogue: 0,0:48:30.22,0:48:32.02,Default,,0000,0000,0000,,Veľa.
Dialogue: 0,0:48:32.82,0:48:36.12,Default,,0000,0000,0000,,Omo, plakala si?
Dialogue: 0,0:48:36.29,0:48:39.03,Default,,0000,0000,0000,,Kto plakal?
Dialogue: 0,0:48:39.26,0:48:41.80,Default,,0000,0000,0000,,Písmo bolo veľmi malé a slová k sebe natlačené,
Dialogue: 0,0:48:42.00,0:48:43.40,Default,,0000,0000,0000,,takto!
Dialogue: 0,0:48:44.37,0:48:47.64,Default,,0000,0000,0000,,Si taká istá, Lian.
Dialogue: 0,0:48:48.30,0:48:50.51,Default,,0000,0000,0000,,Zaujímalo by ma, kto dostal známu režisérku Shin HaeSung
Dialogue: 0,0:48:50.64,0:48:52.61,Default,,0000,0000,0000,,naspäť domov z Broadway-u?
Dialogue: 0,0:48:53.07,0:48:55.04,Default,,0000,0000,0000,,Ktože to...
Dialogue: 0,0:48:55.21,0:48:57.18,Default,,0000,0000,0000,,Ty tiež, odložila si svoje turné
Dialogue: 0,0:48:57.35,0:48:59.91,Default,,0000,0000,0000,,aby si sa zrazu vrátila domov?
Dialogue: 0,0:49:01.05,0:49:02.88,Default,,0000,0000,0000,,Jin YuJin?
Dialogue: 0,0:49:03.02,0:49:04.19,Default,,0000,0000,0000,,Aj ty?
Dialogue: 0,0:49:08.06,0:49:12.53,Default,,0000,0000,0000,,Bola si v kontakte s JB-m?
Dialogue: 0,0:49:50.17,0:49:51.63,Default,,0000,0000,0000,,Čo to robíš?
Dialogue: 0,0:49:51.80,0:49:55.97,Default,,0000,0000,0000,,Musíš zaplatiť, ak vidíš vystúpenie, mačka unnie.
Dialogue: 0,0:49:56.24,0:49:58.41,Default,,0000,0000,0000,,HaePung!
Dialogue: 0,0:50:24.20,0:50:26.37,Default,,0000,0000,0000,,Muzikál s ktorým teraz vystúpime
Dialogue: 0,0:50:26.53,0:50:29.14,Default,,0000,0000,0000,,je "Dream High".\N (Snívaj vysoko)
Dialogue: 0,0:50:31.54,0:50:33.64,Default,,0000,0000,0000,,Neberte to na ľahkú váhu,
Dialogue: 0,0:50:33.81,0:50:35.98,Default,,0000,0000,0000,,len preto, že je to muzikál pre školu.
Dialogue: 0,0:50:36.14,0:50:37.41,Default,,0000,0000,0000,,Skutoční profesionáli,
Dialogue: 0,0:50:37.58,0:50:39.28,Default,,0000,0000,0000,,dajú do toho všetko,
Dialogue: 0,0:50:39.45,0:50:42.35,Default,,0000,0000,0000,,čo patrí na javisko.
Dialogue: 0,0:50:42.58,0:50:44.79,Default,,0000,0000,0000,,Mimochodom na tento muzikál,
Dialogue: 0,0:50:44.99,0:50:47.76,Default,,0000,0000,0000,,nebudem robiť rozdiely medzi bývalými absolventmi a terajšími študentami.
Dialogue: 0,0:50:47.96,0:50:51.73,Default,,0000,0000,0000,,Hlavné postavy sa vyberú pomocou konkurzu.
Dialogue: 0,0:50:54.00,0:50:55.70,Default,,0000,0000,0000,,Lian, ktorá je tu so mnou,
Dialogue: 0,0:50:55.86,0:50:58.80,Default,,0000,0000,0000,,nebude výnimkou.
Dialogue: 0,0:51:03.94,0:51:05.44,Default,,0000,0000,0000,,Hej, Shin HaeSung!
Dialogue: 0,0:51:05.61,0:51:06.84,Default,,0000,0000,0000,,Už si skončila?
Dialogue: 0,0:51:07.01,0:51:10.15,Default,,0000,0000,0000,,Áno, zásady sú zásady.
Dialogue: 0,0:51:10.65,0:51:13.55,Default,,0000,0000,0000,,Kvôli tomuto som odložila svoje turné
Dialogue: 0,0:51:13.72,0:51:15.95,Default,,0000,0000,0000,,a ty chceš aby som išla na konkurz?
Dialogue: 0,0:51:16.12,0:51:17.42,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:51:17.42,0:51:21.06,Default,,0000,0000,0000,,Nikdy som tú zásadu nebrala do úvahy.
Dialogue: 0,0:51:22.66,0:51:27.50,Default,,0000,0000,0000,,Vystupovala som ešte aj v Ghane,
Dialogue: 0,0:51:27.70,0:51:29.53,Default,,0000,0000,0000,,myslíš si, že pôjdem na konkurz
Dialogue: 0,0:51:29.73,0:51:31.53,Default,,0000,0000,0000,,len aby som bola vo vystúpení?
Dialogue: 0,0:51:31.67,0:51:33.44,Default,,0000,0000,0000,,Ak si veríš,
Dialogue: 0,0:51:33.57,0:51:35.14,Default,,0000,0000,0000,,aký je problém?
Dialogue: 0,0:51:35.27,0:51:39.81,Default,,0000,0000,0000,,Môžeš sa na tom javisku pokojne ukázať.
Dialogue: 0,0:51:41.84,0:51:43.95,Default,,0000,0000,0000,,Niekto by ju mal zastaviť.
Dialogue: 0,0:51:44.08,0:51:46.21,Default,,0000,0000,0000,,Teraz? To nie.
Dialogue: 0,0:51:46.38,0:51:49.55,Default,,0000,0000,0000,,HaeSung je veľmi striktná.
Dialogue: 0,0:51:49.72,0:51:54.06,Default,,0000,0000,0000,,Hej, HaeSung vyzerá teraz naozaj ako režisérka.
Dialogue: 0,0:51:54.62,0:51:57.73,Default,,0000,0000,0000,,Aj Al Pacino, keď nájde rolu čo sa mu páči,
Dialogue: 0,0:51:57.86,0:51:59.96,Default,,0000,0000,0000,,pôjde aj helikoptérou len aby stihol konkurz.
Dialogue: 0,0:52:00.16,0:52:02.30,Default,,0000,0000,0000,,Nevieš to lepšie než kdokoľvek iný?
Dialogue: 0,0:52:02.50,0:52:05.83,Default,,0000,0000,0000,,Všakže? Svetová stár.
Dialogue: 0,0:52:12.57,0:52:15.18,Default,,0000,0000,0000,,Fajn, HaeSung.
Dialogue: 0,0:52:15.58,0:52:17.05,Default,,0000,0000,0000,,Ak dokážeš zájsť tak ďaleko,
Dialogue: 0,0:52:17.18,0:52:19.85,Default,,0000,0000,0000,,budem ti veriť a nasledovať ťa.
Dialogue: 0,0:52:25.19,0:52:28.16,Default,,0000,0000,0000,,Toto nemehlo vyzerá stále rovnako.
Dialogue: 0,0:52:32.86,0:52:35.30,Default,,0000,0000,0000,,Sem tam keď vidím správy, až sa prekvapím.
Dialogue: 0,0:52:35.46,0:52:39.37,Default,,0000,0000,0000,,To nemehlo čo poznám sa stalo slávnym.
Dialogue: 0,0:52:41.14,0:52:43.74,Default,,0000,0000,0000,,Kto hovorí, že je nemehlom?
Dialogue: 0,0:52:48.61,0:52:50.88,Default,,0000,0000,0000,,Prečo si tu až teraz?
Dialogue: 0,0:52:51.05,0:52:53.95,Default,,0000,0000,0000,,Hlavná postava sa má objaviť ako posledná, nie?
Dialogue: 0,0:52:54.22,0:52:56.65,Default,,0000,0000,0000,,Hej, každý tu je hviezdou.
Dialogue: 0,0:52:56.82,0:52:58.65,Default,,0000,0000,0000,,HershE, I:dn, Super Idol.
Dialogue: 0,0:52:58.79,0:53:02.59,Default,,0000,0000,0000,,Je tu aj Hollywoodská herečka.
Dialogue: 0,0:53:04.13,0:53:07.23,Default,,0000,0000,0000,,Tým pádom, som fanúšikom HaeSung číslo 1.
Dialogue: 0,0:53:23.41,0:53:23.61,Default,,0000,0000,0000,,Fajn.
Dialogue: 0,0:53:23.61,0:53:24.71,Default,,0000,0000,0000,,Oci, prišiel si!
Dialogue: 0,0:53:24.88,0:53:25.68,Default,,0000,0000,0000,,Kde je tvoja unnie?
Dialogue: 0,0:53:25.85,0:53:28.38,Default,,0000,0000,0000,,Pravdepodobne je v čakárni a riadi vystúpenie.
Dialogue: 0,0:53:28.52,0:53:29.52,Default,,0000,0000,0000,,Potom sa na ňu pozrime.
Dialogue: 0,0:53:29.72,0:53:31.62,Default,,0000,0000,0000,,Ale nevidel som ju 7 rokov.
Dialogue: 0,0:53:31.79,0:53:33.62,Default,,0000,0000,0000,,Chýba ti, že?
Dialogue: 0,0:53:33.76,0:53:34.96,Default,,0000,0000,0000,,Ani trochu sa nezmenila.
Dialogue: 0,0:53:35.12,0:53:35.76,Default,,0000,0000,0000,,Naozaj?
Dialogue: 0,0:53:35.92,0:53:38.76,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:53:38.76,0:53:40.83,Default,,0000,0000,0000,,Prepáčte.
Dialogue: 0,0:53:41.60,0:53:44.93,Default,,0000,0000,0000,,Nie ste náhodou Lianinou matkou?
Dialogue: 0,0:53:48.40,0:53:49.70,Default,,0000,0000,0000,,Poznáte ma?
Dialogue: 0,0:53:49.87,0:53:52.67,Default,,0000,0000,0000,,Som otec Shin HaeSung.
Dialogue: 0,0:53:52.87,0:53:56.95,Default,,0000,0000,0000,,Otec tej Shin HaeSung, režisérky z Broadwayu?
Dialogue: 0,0:53:57.15,0:53:58.85,Default,,0000,0000,0000,,Rada Vás spoznávam.
Dialogue: 0,0:53:59.05,0:54:00.32,Default,,0000,0000,0000,,Ste moc milá.
Dialogue: 0,0:54:00.48,0:54:02.12,Default,,0000,0000,0000,,Lian je teraz dosť populárna.
Dialogue: 0,0:54:02.28,0:54:03.69,Default,,0000,0000,0000,,To vďaka HaeSung.
Dialogue: 0,0:54:03.85,0:54:05.19,Default,,0000,0000,0000,,Nie, ste moc milá.
Dialogue: 0,0:54:05.35,0:54:07.16,Default,,0000,0000,0000,,Rada Vás spoznávam.
Dialogue: 0,0:54:07.32,0:54:09.72,Default,,0000,0000,0000,,Ja tiež.
Dialogue: 0,0:54:29.08,0:54:30.58,Default,,0000,0000,0000,,Toto nepôjde.
Dialogue: 0,0:54:30.75,0:54:33.35,Default,,0000,0000,0000,,Unnie! Čo to robíš?
Dialogue: 0,0:54:33.58,0:54:35.32,Default,,0000,0000,0000,,Nebudeš hrať časť HaeSung?
Dialogue: 0,0:54:35.48,0:54:37.99,Default,,0000,0000,0000,,Vyzeraj viac obyčajne, čo najviac sa dá.
Dialogue: 0,0:54:38.19,0:54:39.75,Default,,0000,0000,0000,,Obyčajne, obyčajne!
Dialogue: 0,0:54:39.89,0:54:41.82,Default,,0000,0000,0000,,Najdôležitejšou časťou HaeSung
Dialogue: 0,0:54:41.99,0:54:43.06,Default,,0000,0000,0000,,je byť obyčajným.
Dialogue: 0,0:54:43.22,0:54:45.13,Default,,0000,0000,0000,,Rozumieš?
Dialogue: 0,0:54:50.10,0:54:52.57,Default,,0000,0000,0000,,Myslím, že ju treba prezliecť do tohto,
Dialogue: 0,0:54:52.77,0:54:55.44,Default,,0000,0000,0000,,je to v poriadku? Pani režisérka?
Dialogue: 0,0:54:55.64,0:54:57.04,Default,,0000,0000,0000,,V poriadku.
Dialogue: 0,0:55:01.31,0:55:04.55,Default,,0000,0000,0000,,Naposledy sa pozrime na rekvizity.
Dialogue: 0,0:55:04.75,0:55:07.25,Default,,0000,0000,0000,,Toto...alebo toto?
Dialogue: 0,0:55:07.45,0:55:08.82,Default,,0000,0000,0000,,Toto.
Dialogue: 0,0:55:09.88,0:55:11.32,Default,,0000,0000,0000,,Trochu to označ, musí to vyzerať trochu použito.
Dialogue: 0,0:55:11.49,0:55:13.09,Default,,0000,0000,0000,,Okej, rozumiem.
Dialogue: 0,0:55:14.26,0:55:15.66,Default,,0000,0000,0000,,Tieto jablká nebudú dobré!
Dialogue: 0,0:55:15.82,0:55:16.46,Default,,0000,0000,0000,,Prečo?
Dialogue: 0,0:55:16.62,0:55:19.99,Default,,0000,0000,0000,,Jablká musia byť úplne červené a žiarivé.
Dialogue: 0,0:55:20.16,0:55:23.90,Default,,0000,0000,0000,,Len tak ich všetci uvidia aj vzadu.
Dialogue: 0,0:55:24.10,0:55:26.37,Default,,0000,0000,0000,,Prosím vymeň ich.
Dialogue: 0,0:55:26.57,0:55:28.44,Default,,0000,0000,0000,,Tá ženská je zlá,
Dialogue: 0,0:55:28.60,0:55:31.04,Default,,0000,0000,0000,,neznášam Broadway.
Dialogue: 0,0:55:31.21,0:55:36.38,Default,,0000,0000,0000,,Naozaj, zabudla som na to!
Dialogue: 0,0:55:36.58,0:55:37.71,Default,,0000,0000,0000,,Príbor.
Dialogue: 0,0:55:39.38,0:55:42.98,Default,,0000,0000,0000,,Vážne, mám tu paličky.
Dialogue: 0,0:55:43.15,0:55:45.45,Default,,0000,0000,0000,,Iba dones nejakú lyžičku.
Dialogue: 0,0:55:47.36,0:55:48.66,Default,,0000,0000,0000,,Hej, je jedno ako si zabraná prácou,
Dialogue: 0,0:55:48.82,0:55:53.03,Default,,0000,0000,0000,,ale ako si môžeš dať do vrecka paličky?
Dialogue: 0,0:55:53.50,0:55:57.73,Default,,0000,0000,0000,,Oh a ešte to CD ešte nie je nahrané?
Dialogue: 0,0:56:14.38,0:56:16.79,Default,,0000,0000,0000,,Sme B,
Dialogue: 0,0:56:16.95,0:56:19.35,Default,,0000,0000,0000,,nie A, ale B.
Dialogue: 0,0:56:19.55,0:56:21.29,Default,,0000,0000,0000,,Nikdy sa nestaneme A triedou,
Dialogue: 0,0:56:21.49,0:56:23.22,Default,,0000,0000,0000,,sme B triedou.
Dialogue: 0,0:56:23.39,0:56:27.26,Default,,0000,0000,0000,,Sme za B triedu v škole.
Dialogue: 0,0:56:27.43,0:56:30.07,Default,,0000,0000,0000,,Nižšia trieda študentov.
Dialogue: 0,0:56:32.37,0:56:34.84,Default,,0000,0000,0000,,To nie len naša škola robí.
Dialogue: 0,0:56:35.00,0:56:36.17,Default,,0000,0000,0000,,Všetky školy to robia,
Dialogue: 0,0:56:36.34,0:56:37.94,Default,,0000,0000,0000,,delia nás medzi excelentných a chudobných skupín,
Dialogue: 0,0:56:38.11,0:56:39.81,Default,,0000,0000,0000,,alebo do vyšších a nižších tried.
Dialogue: 0,0:56:40.01,0:56:44.08,Default,,0000,0000,0000,,Iba sú rozdielne v menách, A trieda a B trieda stále existujú.
Dialogue: 0,0:56:46.45,0:56:50.82,Default,,0000,0000,0000,,Len pred pár dňami som odišla zo školy.
Dialogue: 0,0:56:51.02,0:56:53.56,Default,,0000,0000,0000,,Cítila som sa zahanbene za môj spev,
Dialogue: 0,0:56:53.72,0:56:56.49,Default,,0000,0000,0000,,tak som utiekla.
Dialogue: 0,0:56:59.73,0:57:02.40,Default,,0000,0000,0000,,Chcem sa každého spýtať otázku.
Dialogue: 0,0:57:02.53,0:57:05.93,Default,,0000,0000,0000,,Patria sny len tým, čo majú talent?
Dialogue: 0,0:57:06.10,0:57:07.64,Default,,0000,0000,0000,,Ak nemáte talent,
Dialogue: 0,0:57:07.80,0:57:11.94,Default,,0000,0000,0000,,nemáte právo snívať?
Dialogue: 0,0:57:15.84,0:57:18.78,Default,,0000,0000,0000,,Ak chcete snívať, musíte najprv zatvoriť oči.
Dialogue: 0,0:57:18.95,0:57:20.08,Default,,0000,0000,0000,,Ak vaše oči sú zatvorené,
Dialogue: 0,0:57:20.22,0:57:22.48,Default,,0000,0000,0000,,tak ako vás vidia iní,
Dialogue: 0,0:57:22.65,0:57:23.89,Default,,0000,0000,0000,,už nie je dôležité.
Dialogue: 0,0:57:24.05,0:57:25.59,Default,,0000,0000,0000,,Len keď si zavriete oči,
Dialogue: 0,0:57:25.75,0:57:30.83,Default,,0000,0000,0000,,dokážete čeliť svojmu skutočnému ja.
Dialogue: 0,0:57:30.96,0:57:33.56,Default,,0000,0000,0000,,Keď si zatvorím oči, hovorievam si,
Dialogue: 0,0:57:33.70,0:57:36.33,Default,,0000,0000,0000,,nikdy sa nevzdávaj svojho sna,
Dialogue: 0,0:57:36.50,0:57:39.70,Default,,0000,0000,0000,,pretože sny nepatria iba tým, čo majú talent,
Dialogue: 0,0:57:39.87,0:57:41.74,Default,,0000,0000,0000,,ale všetkým, ktorí dokážu snívať.
Dialogue: 0,0:57:41.90,0:57:44.01,Default,,0000,0000,0000,,Ver v malému a úbohému dáždniku ktorý máš,
Dialogue: 0,0:57:44.17,0:57:46.64,Default,,0000,0000,0000,,a horsa do ťažkého dažďa.
Dialogue: 0,0:57:46.81,0:57:50.08,Default,,0000,0000,0000,,Plánujeme využiť náš malý talent,
Dialogue: 0,0:57:50.25,0:57:53.75,Default,,0000,0000,0000,,k vysnívaniu toho najväčšieho sna aký dokážeme.
Dialogue: 0,0:57:58.35,0:57:59.82,Default,,0000,0000,0000,,Táto pieseň je pre
Dialogue: 0,0:57:59.99,0:58:01.16,Default,,0000,0000,0000,,tých, čo sú zranení zo svojich snov,
Dialogue: 0,0:58:01.29,0:58:03.06,Default,,0000,0000,0000,,pre tých, čo sú nútení vzdať sa svojich snov,
Dialogue: 0,0:58:03.22,0:58:05.76,Default,,0000,0000,0000,,pre tých nevynikajúcich študentov, menej talentovaných študentov,\N študentov z nižších tried.
Dialogue: 0,0:58:05.93,0:58:09.00,Default,,0000,0000,0000,,Pre každého, čo žije životom B triedy.
Dialogue: 0,0:58:32.91,0:58:36.38,Default,,0000,0000,0000,,~ {\i1}Snívam vysoko, mám sen.{\i0} ~
Dialogue: 0,0:58:36.58,0:58:40.05,Default,,0000,0000,0000,,~ {\i1}Keď sú časy ťažké, zavriem oči.{\i0} ~
Dialogue: 0,0:58:40.22,0:58:43.32,Default,,0000,0000,0000,,~ {\i1}Ten moment keď dosiahnem svoj sen,{\i0} ~
Dialogue: 0,0:58:43.52,0:58:47.39,Default,,0000,0000,0000,,~ {\i1}zobudím sa a začnem znovu.{\i0} ~
Dialogue: 0,0:58:48.16,0:58:53.56,Default,,0000,0000,0000,,~ {\i1}Dnes zase váham keď čelím obávaným\N koncom svojich snov.{\i0} ~
Dialogue: 0,0:58:54.40,0:59:02.27,Default,,0000,0000,0000,,~ {\i1}Ako malé novorodeniatko,\N ktoré nikdy neopúšťa hniezdo zo strachu z pádu.{\i0} ~
Dialogue: 0,0:59:02.87,0:59:08.48,Default,,0000,0000,0000,,~ {\i1}Dokážem to?\N Dosiahnem svojho sna?{\i0} ~
Dialogue: 0,0:59:09.21,0:59:16.79,Default,,0000,0000,0000,,~ {\i1}Jeden krok dopredu a potom ďalší.\N Vždy keď sa pohnem ďalej, cítim strach.{\i0} ~
Dialogue: 0,0:59:17.22,0:59:20.66,Default,,0000,0000,0000,,~ {\i1}Snívam vysoko, mám sen.{\i0} ~
Dialogue: 0,0:59:20.89,0:59:24.36,Default,,0000,0000,0000,,~ {\i1}Keď sú časy ťažké, zavriem oči.{\i0} ~
Dialogue: 0,0:59:24.49,0:59:27.70,Default,,0000,0000,0000,,~ {\i1}Ten moment keď dosiahnem svoj sen,{\i0} ~
Dialogue: 0,0:59:27.90,0:59:31.70,Default,,0000,0000,0000,,~ {\i1}zobudím sa a začnem znovu.{\i0} ~
Dialogue: 0,0:59:31.90,0:59:35.41,Default,,0000,0000,0000,,~ {\i1}Dokážem vysoko lietať, verím si.{\i0} ~
Dialogue: 0,0:59:35.57,0:59:39.11,Default,,0000,0000,0000,,~ {\i1}Jedného dňa vzlietnem do neba.{\i0} ~
Dialogue: 0,0:59:39.28,0:59:42.45,Default,,0000,0000,0000,,~ {\i1}S roztiahnutými krídlami,\N môžem vzlietnuť vyššie než ktokoľvek iný.{\i0} ~
Dialogue: 0,0:59:42.61,0:59:47.12,Default,,0000,0000,0000,,~ {\i1}Budem si voľne lietať, vysoko.{\i0} ~
Dialogue: 0,0:59:47.32,0:59:52.76,Default,,0000,0000,0000,,~ {\i1}Keď padnem,\N potrebujem niekoho kto mi dodá odvahu.{\i0} ~
Dialogue: 0,0:59:53.49,1:00:01.03,Default,,0000,0000,0000,,~ {\i1}Guráž vyhnúť sa prašnému ja,\N a dostať sa naspäť na svoje nohy aby som znovu vzlietol.{\i0} ~
Dialogue: 0,1:00:01.93,1:00:07.54,Default,,0000,0000,0000,,~ {\i1}Ver mi, ver mojmu osudu,{\i0} ~
Dialogue: 0,1:00:08.30,1:00:16.01,Default,,0000,0000,0000,,~ {\i1}aby som prešiel všetky nepredvídateľné udalosti stojace\N v mojej ceste.{\i0} ~
Dialogue: 0,1:00:16.48,1:00:19.72,Default,,0000,0000,0000,,~ {\i1}Snívam vysoko, mám sen.{\i0} ~
Dialogue: 0,1:00:19.92,1:00:23.39,Default,,0000,0000,0000,,~ {\i1}Keď sú časy ťažké, zavriem oči.{\i0} ~
Dialogue: 0,1:00:23.55,1:00:26.76,Default,,0000,0000,0000,,~ {\i1}Ten moment keď dosiahnem svoj sen,{\i0} ~
Dialogue: 0,1:00:26.96,1:00:30.83,Default,,0000,0000,0000,,~ {\i1}zobudím sa a začnem znovu.{\i0} ~
Dialogue: 0,1:00:30.99,1:00:34.53,Default,,0000,0000,0000,,~ {\i1}Dokážem vysoko lietať, verím si.{\i0} ~
Dialogue: 0,1:00:34.70,1:00:38.13,Default,,0000,0000,0000,,~ {\i1}Jedného dňa vzlietnem do neba.{\i0} ~
Dialogue: 0,1:00:38.33,1:00:41.50,Default,,0000,0000,0000,,~ {\i1}S roztiahnutými krídlami,\N môžem vzlietnuť vyššie než ktokoľvek iný.{\i0} ~
Dialogue: 0,1:00:41.70,1:00:46.44,Default,,0000,0000,0000,,~ {\i1}Budem si voľne lietať, vysoko.{\i0} ~
Dialogue: 0,1:00:46.64,1:00:48.21,Default,,0000,0000,0000,,~ {\i1}Snívaj vysoko, maj šancu vysoko vzlietnuť.{\i0} ~
Dialogue: 0,1:00:48.38,1:00:49.91,Default,,0000,0000,0000,,~ {\i1}Všetká bolesť a smútok majte sa.{\i0} ~
Dialogue: 0,1:00:50.08,1:00:53.42,Default,,0000,0000,0000,,~ {\i1}Vzlietnem vysoko ako tie hviezdy na nebi.{\i0} ~
Dialogue: 0,1:00:53.58,1:00:55.42,Default,,0000,0000,0000,,~ {\i1}Nechaj svoje sny ukázať sa, je čas aby si zažiaril.{\i0} ~
Dialogue: 0,1:00:55.62,1:00:57.25,Default,,0000,0000,0000,,~ {\i1}Len teraz sme začali, musia byť moje.{\i0} ~
Dialogue: 0,1:00:57.42,1:00:59.19,Default,,0000,0000,0000,,~ {\i1}Neboj sa budúcnosti, ktorú si musíš vytvoriť.{\i0} ~
Dialogue: 0,1:00:59.39,1:01:00.92,Default,,0000,0000,0000,,~ {\i1}Teraz by si mal ísť, plný sebavedomia.{\i0} ~
Dialogue: 0,1:01:01.16,1:01:04.49,Default,,0000,0000,0000,,~ {\i1}Osud. Je nevyhnutný. Tento nevyhnutný osud,{\i0} ~
Dialogue: 0,1:01:04.66,1:01:06.43,Default,,0000,0000,0000,,~ {\i1}vyšiel skôr ako my.{\i0} ~
Dialogue: 0,1:01:06.60,1:01:08.23,Default,,0000,0000,0000,,~ {\i1}Je to úplne nová fantázia.{\i0} ~
Dialogue: 0,1:01:08.40,1:01:10.43,Default,,0000,0000,0000,,~ {\i1}Takže odteraz, poď a chyť sa mojej ruky.{\i0} ~
Dialogue: 0,1:01:10.60,1:01:12.07,Default,,0000,0000,0000,,~ {\i1}Odteraz máme jeden cieľ.{\i0} ~
Dialogue: 0,1:01:12.27,1:01:13.94,Default,,0000,0000,0000,,~ {\i1}Nikdy sa nevzdáme svojich snov a budúcnosti.{\i0} ~
Dialogue: 0,1:01:14.14,1:01:15.84,Default,,0000,0000,0000,,~ {\i1}Mladosť, túžba. Každý tu bude vysoko snívať.{\i0} ~
Dialogue: 0,1:01:16.01,1:01:18.74,Default,,0000,0000,0000,,~ {\i1}Snívam vysoko, mám sen.{\i0} ~
Dialogue: 0,1:01:18.91,1:01:22.35,Default,,0000,0000,0000,,~ {\i1}Keď sú časy ťažké, zavriem oči.{\i0} ~
Dialogue: 0,1:01:22.55,1:01:25.82,Default,,0000,0000,0000,,~ {\i1}Ten moment keď dosiahnem svoj sen,{\i0} ~
Dialogue: 0,1:01:26.02,1:01:28.18,Default,,0000,0000,0000,,~ {\i1}zobudím sa...{\i0} ~
Dialogue: 0,1:01:30.83,1:01:35.10,Default,,0000,0000,0000,,{\i1}Každý z nás najprv nedosiahol svoj sen.{\i0}
Dialogue: 0,1:01:35.10,1:01:39.51,Default,,0000,0000,0000,,{\i1}Avšak, osoba, ktorá si svoj sen vytvorila už dávno predtým,{\i0}
Dialogue: 0,1:01:39.51,1:01:41.81,Default,,0000,0000,0000,,{\i1}konečne cíti, že je k tomu snu bližšie.{\i0}
Dialogue: 0,1:01:41.81,1:01:44.65,Default,,0000,0000,0000,,{\i1}Predtým než to vieme, príde čas.{\i0}
Dialogue: 0,1:01:44.65,1:01:47.28,Default,,0000,0000,0000,,{\i1}Sny niektorých ľudí boli dosiahnuté.{\i0}
Dialogue: 0,1:01:47.28,1:01:52.25,Default,,0000,0000,0000,,{\i1}Niektorí ľudia môžu iba snívať sny,\N konečne som tomu pochopila.{\i0}
Dialogue: 0,1:01:53.45,1:01:56.32,Default,,0000,0000,0000,,{\i1}Každý je rôzne zafarbený,{\i0}
Dialogue: 0,1:01:56.32,1:01:59.59,Default,,0000,0000,0000,,{\i1}vždy je tu jasné svetlo.{\i0}
Dialogue: 0,1:01:59.59,1:02:01.96,Default,,0000,0000,0000,,{\i1}Moji priatelia čo stoja pri mne...{\i0}
Dialogue: 0,1:02:01.96,1:02:03.97,Default,,0000,0000,0000,,{\i1}niekedy osvetlia moje temné momenty,{\i0}
Dialogue: 0,1:02:03.97,1:02:07.37,Default,,0000,0000,0000,,{\i1}dodávajú mi svetlo ako hviezdy.{\i0}
Dialogue: 0,1:02:07.37,1:02:09.04,Default,,0000,0000,0000,,{\i1}Ja tiež, ako keby niekto žiaril.{\i0}
Dialogue: 0,1:02:09.04,1:02:13.21,Default,,0000,0000,0000,,{\i1}Navždy jediné čo môžem...{\i0}
Dialogue: 0,1:02:13.21,1:02:17.95,Default,,0000,0000,0000,,{\i1}a čo stále robím je...\Nže snívam.{\i0}
