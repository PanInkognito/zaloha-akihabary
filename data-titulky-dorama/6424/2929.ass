Dialogue: 0,0:00:00.00,0:00:02.00,Default,,0000,0000,0000,,SK Titulky: Hyumi - seesska.blog.cz/\N ENG Subs: DramaFever
Dialogue: 0,0:00:02.65,0:00:05.56,Default,,0000,0000,0000,,~ {\i1}Keď sme spolu.{\i0} ~
Dialogue: 0,0:00:05.76,0:00:09.59,Default,,0000,0000,0000,,~ {\i1}Keď sme spolu.{\i0} ~
Dialogue: 0,0:00:09.79,0:00:12.36,Default,,0000,0000,0000,,~ {\i1}Všetko sa zlepší.{\i0} ~
Dialogue: 0,0:00:12.70,0:00:14.93,Default,,0000,0000,0000,,~ {\i1}Bolestné spomienky sa vytratia.{\i0} ~
Dialogue: 0,0:00:15.16,0:00:18.33,Default,,0000,0000,0000,,~ {\i1}Keď sme spolu.{\i0} ~
Dialogue: 0,0:00:18.50,0:00:22.07,Default,,0000,0000,0000,,~ {\i1}Keď sme spolu.{\i0} ~
Dialogue: 0,0:00:22.31,0:00:24.71,Default,,0000,0000,0000,,~ {\i1}Som znovu šťastná.{\i0} ~
Dialogue: 0,0:00:25.27,0:00:27.71,Default,,0000,0000,0000,,~ {\i1}Nevediac prečo, usmievam sa.{\i0} ~
Dialogue: 0,0:00:27.91,0:00:30.98,Default,,0000,0000,0000,,~ {\i1}Keď sme spolu.{\i0} ~
Dialogue: 0,0:00:31.18,0:00:34.78,Default,,0000,0000,0000,,~ {\i1}Keď sme spolu.{\i0} ~
Dialogue: 0,0:00:34.98,0:00:37.65,Default,,0000,0000,0000,,~ {\i1}Všetko sa zlepší.{\i0} ~
Dialogue: 0,0:00:37.92,0:00:40.29,Default,,0000,0000,0000,,~ {\i1}Bolestné spomienky sa vytratia.{\i0} ~
Dialogue: 0,0:00:40.52,0:00:43.46,Default,,0000,0000,0000,,~ {\i1}Keď sme spolu.{\i0} ~
Dialogue: 0,0:00:43.69,0:00:46.60,Default,,0000,0000,0000,,~ {\i1}Keď sme spolu.{\i0} ~
Dialogue: 0,0:00:47.50,0:00:50.10,Default,,0000,0000,0000,,~ {\i1}Som znovu šťastná.{\i0} ~
Dialogue: 0,0:00:50.57,0:00:53.67,Default,,0000,0000,0000,,~ {\i1}Nevediac prečo, usmievam sa.{\i0} ~
Dialogue: 0,0:01:50.05,0:01:51.91,Default,,0000,0000,0000,,Musíš byť veľmi nervózny.
Dialogue: 0,0:01:52.11,0:01:55.45,Default,,0000,0000,0000,,Keďže ideš po JB-ovi.
Dialogue: 0,0:01:55.80,0:01:57.94,Default,,0000,0000,0000,,Moju pesničku...
Dialogue: 0,0:01:57.94,0:02:01.74,Default,,0000,0000,0000,,stačí keď bude počúvať iba jedna osoba.
Dialogue: 0,0:02:01.74,0:02:04.18,Default,,0000,0000,0000,,Je rad na nás.
Dialogue: 0,0:02:06.18,0:02:07.55,Default,,0000,0000,0000,,Poďme.
Dialogue: 0,0:02:13.38,0:02:16.82,Default,,0000,0000,0000,,Netreba držať si ruky.
Dialogue: 0,0:02:28.00,0:02:30.94,Default,,0000,0000,0000,,Dnes si bola skvelá.
Dialogue: 0,0:02:30.94,0:02:33.00,Default,,0000,0000,0000,,Blahoželám.
Dialogue: 0,0:02:33.00,0:02:36.57,Default,,0000,0000,0000,,O tom, keď som povedala, že si naničhodná,
Dialogue: 0,0:02:36.57,0:02:38.55,Default,,0000,0000,0000,,beriem späť.
Dialogue: 0,0:02:40.07,0:02:42.05,Default,,0000,0000,0000,,Je to skvelá pieseň.
Dialogue: 0,0:02:42.05,0:02:44.02,Default,,0000,0000,0000,,Nabudúce keď niečo zložíš,
Dialogue: 0,0:02:44.02,0:02:46.52,Default,,0000,0000,0000,,ju dones mne.
Dialogue: 0,0:02:46.52,0:02:49.05,Default,,0000,0000,0000,,Zaspievam ju.
Dialogue: 0,0:02:49.05,0:02:52.49,Default,,0000,0000,0000,,V tom prípade to bude mojou cťou.
Dialogue: 0,0:02:52.49,0:02:55.76,Default,,0000,0000,0000,,Zase sa správaš tak milo.
Dialogue: 0,0:02:55.76,0:02:58.93,Default,,0000,0000,0000,,Mala by si isť von.
Dialogue: 0,0:02:58.93,0:03:02.93,Default,,0000,0000,0000,,Pôjdeme spolu?
Dialogue: 0,0:03:02.93,0:03:06.00,Default,,0000,0000,0000,,Musíš byť zodpovedná až do konca.
Dialogue: 0,0:03:21.99,0:03:24.19,Default,,0000,0000,0000,,Čo je?
Dialogue: 0,0:03:24.19,0:03:26.56,Default,,0000,0000,0000,,Hneváš sa na mňa?
Dialogue: 0,0:03:26.56,0:03:29.13,Default,,0000,0000,0000,,Prečo?
Dialogue: 0,0:03:32.70,0:03:35.63,Default,,0000,0000,0000,,Nepovedala si, že je to duet?
Dialogue: 0,0:03:35.63,0:03:38.74,Default,,0000,0000,0000,,Prečo si nepovedala jasne,
Dialogue: 0,0:03:38.74,0:03:40.94,Default,,0000,0000,0000,,že to nemalo byť s tebou ale Lian?
Dialogue: 0,0:03:40.94,0:03:44.08,Default,,0000,0000,0000,,Prečo si mi to skôr nepovedala?
Dialogue: 0,0:03:44.08,0:03:46.51,Default,,0000,0000,0000,,Prepáč.
Dialogue: 0,0:03:46.51,0:03:50.25,Default,,0000,0000,0000,,Ak ti je to ľúto, objednaj mi niečo na jedenie.
Dialogue: 0,0:03:50.25,0:03:52.18,Default,,0000,0000,0000,,Čože?
Dialogue: 0,0:03:52.18,0:03:54.62,Default,,0000,0000,0000,,Musíš svoj sľub dodržať.
Dialogue: 0,0:04:00.19,0:04:02.39,Default,,0000,0000,0000,,Zdravím všetkých.
Dialogue: 0,0:04:02.39,0:04:06.13,Default,,0000,0000,0000,,Zložil som túto pieseň,
Dialogue: 0,0:04:06.13,0:04:08.27,Default,,0000,0000,0000,,pre jedinú osobu.
Dialogue: 0,0:04:08.27,0:04:12.84,Default,,0000,0000,0000,,Až doteraz, ako vždy,
Dialogue: 0,0:04:12.84,0:04:17.91,Default,,0000,0000,0000,,a aj v budúcnosti, budem vždy jej jedinou slnečnicou.
Dialogue: 0,0:04:51.88,0:04:58.45,Default,,0000,0000,0000,,~ {\i1}Láska medzi tebou a mnou,{\i0} ~
Dialogue: 0,0:04:58.82,0:05:05.03,Default,,0000,0000,0000,,~ {\i1}kedy prestane pršať?{\i0} ~
Dialogue: 0,0:05:05.03,0:05:10.00,Default,,0000,0000,0000,,~ {\i1}Plávaš v ďalekom nebi.{\i0} ~
Dialogue: 0,0:05:11.53,0:05:16.27,Default,,0000,0000,0000,,~ {\i1}A ja som na najpokornejšom mieste. {\i0} ~
Dialogue: 0,0:05:31.72,0:05:38.43,Default,,0000,0000,0000,,~ {\i1}Aj tak som šťastný.{\i0} ~
Dialogue: 0,0:05:38.43,0:05:45.53,Default,,0000,0000,0000,,~ {\i1}Svietiš na mňa slnečným lúčom. {\i0} ~
Dialogue: 0,0:05:45.53,0:05:51.54,Default,,0000,0000,0000,,~ {\i1}Prijímam ho.{\i0} ~
Dialogue: 0,0:05:51.54,0:05:57.04,Default,,0000,0000,0000,,~ {\i1}Pozerám sa naň.{\i0} ~
Dialogue: 0,0:05:57.04,0:06:04.05,Default,,0000,0000,0000,,~ {\i1}Iba slnko sa díva.{\i0} ~
Dialogue: 0,0:06:04.52,0:06:07.96,Default,,0000,0000,0000,,~ {\i1}Z oblakov vedľa neho,\N stále leje,{\i0} ~
Dialogue: 0,0:06:07.96,0:06:10.86,Default,,0000,0000,0000,,~ {\i1}mokne mi celé telo.{\i0} ~
Dialogue: 0,0:06:10.86,0:06:13.36,Default,,0000,0000,0000,,~ {\i1}Začínam vädnúť{\i0} ~
Dialogue: 0,0:06:13.70,0:06:17.37,Default,,0000,0000,0000,,~ {\i1}keď vidím aká si unavená.{\i0} ~
Dialogue: 0,0:06:17.70,0:06:18.80,Default,,0000,0000,0000,,~ {\i1}Ak ťa chcem vidieť,{\i0} ~
Dialogue: 0,0:06:18.80,0:06:24.37,Default,,0000,0000,0000,,~ {\i1}iba vidieť ťa každý deň,\Npočkám si všetky obdobia.{\i0} ~
Dialogue: 0,0:06:24.37,0:06:30.88,Default,,0000,0000,0000,,~ {\i1}Kvetina, rásti.{\i0} ~
Dialogue: 0,0:06:30.88,0:06:37.68,Default,,0000,0000,0000,,~ {\i1}Kvetina, rásti ďalej.{\i0} ~
Dialogue: 0,0:06:37.68,0:06:43.99,Default,,0000,0000,0000,,~ {\i1}Áno, musíš vyrásť viac.{\i0} ~
Dialogue: 0,0:06:43.99,0:07:03.01,Default,,0000,0000,0000,,~ {\i1}Aby som na teba lepšie videl.{\i0} ~
Dialogue: 0,0:07:42.98,0:07:46.08,Default,,0000,0000,0000,,Ak si chcela utiecť, tak prečo si sem vôbec chodila?
Dialogue: 0,0:07:51.62,0:07:54.56,Default,,0000,0000,0000,,Ja som ten čo uteká.
Dialogue: 0,0:07:54.56,0:07:56.06,Default,,0000,0000,0000,,Ak si ma nedokázala presvedčiť aby som zostal,
Dialogue: 0,0:07:56.06,0:07:57.50,Default,,0000,0000,0000,,tak sa ma drž aspoň teraz.
Dialogue: 0,0:07:57.50,0:08:00.20,Default,,0000,0000,0000,,Aby som zase neutiekol.
Dialogue: 0,0:08:00.20,0:08:03.13,Default,,0000,0000,0000,,Nie si moja matka?
Dialogue: 0,0:08:03.13,0:08:05.74,Default,,0000,0000,0000,,YuJin...
Dialogue: 0,0:08:10.38,0:08:13.75,Default,,0000,0000,0000,,Môžeš odísť po vypočutí mojej pesničky.
Dialogue: 0,0:08:13.75,0:08:15.68,Default,,0000,0000,0000,,Prosím.
Dialogue: 0,0:08:33.33,0:08:36.83,Default,,0000,0000,0000,,Klamal som.
Dialogue: 0,0:08:38.77,0:08:46.21,Default,,0000,0000,0000,,Klamal som keď som povedal,\N že moji rodičia zomreli.
Dialogue: 0,0:09:03.59,0:09:05.96,Default,,0000,0000,0000,,Presne ako vravia klebety,
Dialogue: 0,0:09:05.96,0:09:10.97,Default,,0000,0000,0000,,som nukleárny muž, Jin YuJin.
Dialogue: 0,0:09:10.97,0:09:14.41,Default,,0000,0000,0000,,Veľmi som sa hanbil.
Dialogue: 0,0:09:14.41,0:09:17.08,Default,,0000,0000,0000,,Takže tých 10 rokov po tom,
Dialogue: 0,0:09:17.08,0:09:20.95,Default,,0000,0000,0000,,som žil v popieraní.
Dialogue: 0,0:09:20.95,0:09:24.58,Default,,0000,0000,0000,,Čože? Klebety sú pravdivé?
Dialogue: 0,0:09:24.58,0:09:29.02,Default,,0000,0000,0000,,Ten malý, tajil to aj pred nami.
Dialogue: 0,0:09:29.02,0:09:30.79,Default,,0000,0000,0000,,Trpel samotou, iba on sám.
Dialogue: 0,0:09:30.79,0:09:35.73,Default,,0000,0000,0000,,Muselo to byť veľmi ťažké.
Dialogue: 0,0:09:35.73,0:09:38.10,Default,,0000,0000,0000,,Kvôli tomuto chlapcovi, čo plakal krokodílie slzy,
Dialogue: 0,0:09:38.10,0:09:41.17,Default,,0000,0000,0000,,a tohto nemožného syna,
Dialogue: 0,0:09:41.17,0:09:45.24,Default,,0000,0000,0000,,vám musel spôsobiť veľa ťažkostí, však?
Dialogue: 0,0:09:45.24,0:09:48.97,Default,,0000,0000,0000,,Venujem túto pieseň mojej matke,\N ktorá sem prišla.
Dialogue: 0,0:10:12.01,0:10:20.28,Default,,0000,0000,0000,,~ {\i1}Prepáč.{\i0} ~
Dialogue: 0,0:10:22.08,0:10:31.99,Default,,0000,0000,0000,,~ {\i1}Dni takto znovu utekajú.{\i0} ~
Dialogue: 0,0:10:31.99,0:10:37.20,Default,,0000,0000,0000,,~ {\i1}Nervózny zlozvyk, ktorý sa nedá napraviť,{\i0} ~
Dialogue: 0,0:10:37.20,0:10:42.81,Default,,0000,0000,0000,,~ {\i1}a buchot srdca,\N ktorý sa nestratí.{\i0} ~
Dialogue: 0,0:10:42.81,0:10:53.55,Default,,0000,0000,0000,,~ {\i1}Myseľ a vety, ktoré sa neskrížia.{\i0} ~
Dialogue: 0,0:10:53.55,0:11:00.23,Default,,0000,0000,0000,,~ {\i1}Ja stále váham.{\i0} ~
Dialogue: 0,0:11:01.25,0:11:06.03,Default,,0000,0000,0000,,~ {\i1}Tvoje oči sú stále oslnivé,{\i0} ~
Dialogue: 0,0:11:06.03,0:11:14.04,Default,,0000,0000,0000,,~ {\i1}sú nedosiahnuteľné.{\i0} ~
Dialogue: 0,0:11:14.04,0:11:21.88,Default,,0000,0000,0000,,~ {\i1}Ty, si veľmi bezstarostná.{\i0} ~
Dialogue: 0,0:11:21.88,0:11:26.72,Default,,0000,0000,0000,,~ {\i1}Je ťažké zblížiť sa.{\i0} ~
Dialogue: 0,0:11:26.72,0:11:34.69,Default,,0000,0000,0000,,~ {\i1}Vedel som sa okolo teba iba obšmietať.{\i0} ~
Dialogue: 0,0:11:34.69,0:11:41.60,Default,,0000,0000,0000,,~ {\i1}Možno sa niečo skrýva v srdci.{\i0} ~
Dialogue: 0,0:11:50.31,0:11:58.11,Default,,0000,0000,0000,,~ {\i1}Ja stále váham.{\i0} ~
Dialogue: 0,0:11:58.11,0:12:02.92,Default,,0000,0000,0000,,~ {\i1}Tvoje oči sú stále oslnivé{\i0} ~
Dialogue: 0,0:12:02.92,0:12:10.93,Default,,0000,0000,0000,,~ {\i1}sú nedosiahnuteľné.{\i0} ~
Dialogue: 0,0:12:10.93,0:12:18.67,Default,,0000,0000,0000,,~ {\i1}Ty, si veľmi bezstarostná.{\i0} ~
Dialogue: 0,0:12:18.67,0:12:23.27,Default,,0000,0000,0000,,~ {\i1}Je ťažko zblížiť sa.{\i0} ~
Dialogue: 0,0:12:23.27,0:12:31.48,Default,,0000,0000,0000,,~ {\i1}Vedel som sa okolo teba iba obšmietať.{\i0} ~
Dialogue: 0,0:12:31.48,0:12:40.09,Default,,0000,0000,0000,,~ {\i1}Možno sa niečo skrýva v srdci.{\i0} ~
Dialogue: 0,0:13:28.89,0:13:32.96,Default,,0000,0000,0000,,Jin YuJin, plakal si?
Dialogue: 0,0:13:32.96,0:13:35.07,Default,,0000,0000,0000,,Plačeš?
Dialogue: 0,0:13:35.07,0:13:39.04,Default,,0000,0000,0000,,Plakal si?
Dialogue: 0,0:13:39.04,0:13:40.97,Default,,0000,0000,0000,,Bol si skvelý!
Dialogue: 0,0:13:40.97,0:13:43.37,Default,,0000,0000,0000,,Poslúchol si čo som povedala,
Dialogue: 0,0:13:43.37,0:13:45.44,Default,,0000,0000,0000,,si na to pyšný?
Dialogue: 0,0:13:45.44,0:13:48.35,Default,,0000,0000,0000,,Máš ma rád?
Dialogue: 0,0:13:48.35,0:13:51.08,Default,,0000,0000,0000,,Bol som dobrý?
Dialogue: 0,0:13:52.32,0:13:55.32,Default,,0000,0000,0000,,Vidíš, necítiš sa lepšie,\N keď si bol k mame dobrý?
Dialogue: 0,0:13:55.45,0:13:57.52,Default,,0000,0000,0000,,Naozaj ma dobre poslúchaš.
Dialogue: 0,0:13:57.69,0:14:01.53,Default,,0000,0000,0000,,Krása!
Dialogue: 0,0:14:04.83,0:14:06.20,Default,,0000,0000,0000,,Jedz pomaly.
Dialogue: 0,0:14:06.20,0:14:07.50,Default,,0000,0000,0000,,Zadusíš sa.
Dialogue: 0,0:14:07.50,0:14:16.14,Default,,0000,0000,0000,,Na.
Dialogue: 0,0:14:16.14,0:14:18.21,Default,,0000,0000,0000,,Tiež si daj.
Dialogue: 0,0:14:18.21,0:14:20.38,Default,,0000,0000,0000,,Dobre.
Dialogue: 0,0:14:28.82,0:14:33.19,Default,,0000,0000,0000,,Jedz týmto.
Dialogue: 0,0:14:33.19,0:14:36.69,Default,,0000,0000,0000,,Čo? S tým nedokážeš jesť?
Dialogue: 0,0:14:36.69,0:14:39.63,Default,,0000,0000,0000,,Nemôžeš to použiť?
Dialogue: 0,0:14:39.63,0:14:42.00,Default,,0000,0000,0000,,Ohľadne nás, my-
Dialogue: 0,0:14:42.00,0:14:45.37,Default,,0000,0000,0000,,Ešte sme neskončili.
Dialogue: 0,0:14:46.67,0:14:48.31,Default,,0000,0000,0000,,Nemyslím si, že sme už skončili.
Dialogue: 0,0:14:48.51,0:14:52.54,Default,,0000,0000,0000,,Iba ty to tak cítiš.
Dialogue: 0,0:14:52.54,0:14:54.48,Default,,0000,0000,0000,,Nesľúbili sme si?
Dialogue: 0,0:14:54.48,0:14:55.75,Default,,0000,0000,0000,,Pozveš ma na jedlá,
Dialogue: 0,0:14:55.75,0:14:57.38,Default,,0000,0000,0000,,pošleš mi jablká,
Dialogue: 0,0:14:57.38,0:15:00.89,Default,,0000,0000,0000,,a hocičo čo chcem.
Dialogue: 0,0:15:03.76,0:15:05.76,Default,,0000,0000,0000,,Pozvala som ťa na jedlo,
Dialogue: 0,0:15:05.76,0:15:09.19,Default,,0000,0000,0000,,a jablká ti pošlem.
Dialogue: 0,0:15:09.19,0:15:11.20,Default,,0000,0000,0000,,Sľúb mi to.
Dialogue: 0,0:15:13.13,0:15:15.80,Default,,0000,0000,0000,,S mojou chorobou sa ťažko žije.
Dialogue: 0,0:15:15.80,0:15:18.00,Default,,0000,0000,0000,,Než mi vyzdravie koleno,
Dialogue: 0,0:15:18.00,0:15:20.17,Default,,0000,0000,0000,,buď pri mne.
Dialogue: 0,0:15:23.11,0:15:27.18,Default,,0000,0000,0000,,Beriem to ako súhlas.
Dialogue: 0,0:15:35.12,0:15:37.42,Default,,0000,0000,0000,,Meno človeka, čo teraz vyhlásim,
Dialogue: 0,0:15:37.42,0:15:42.19,Default,,0000,0000,0000,,sa stane posledným členom Super Idol.
Dialogue: 0,0:15:45.06,0:15:47.17,Default,,0000,0000,0000,,A ako navrch, v tomto vystúpení,
Dialogue: 0,0:15:47.17,0:15:51.74,Default,,0000,0000,0000,,ten dotyčný koho meno poviem,
Dialogue: 0,0:15:51.74,0:15:56.24,Default,,0000,0000,0000,,je aj ten, čo získal najviac bodov.
Dialogue: 0,0:16:05.68,0:16:06.98,Default,,0000,0000,0000,,Lian!
Dialogue: 0,0:16:26.24,0:16:29.01,Default,,0000,0000,0000,,JB, ktorý vystupoval s Lian,
Dialogue: 0,0:16:29.01,0:16:32.58,Default,,0000,0000,0000,,dnes tiež získava prvé miesto.
Dialogue: 0,0:16:32.58,0:16:34.88,Default,,0000,0000,0000,,Avšak, Nana a JB,
Dialogue: 0,0:16:34.88,0:16:38.32,Default,,0000,0000,0000,,stále nemôžu pokračovať v aktivitách,
Dialogue: 0,0:16:38.32,0:16:41.25,Default,,0000,0000,0000,,takže im bude daná druhá šanca.
Dialogue: 0,0:16:58.57,0:17:01.07,Default,,0000,0000,0000,,O mesiac neskôr, v tomto okolí,
Dialogue: 0,0:17:01.07,0:17:05.38,Default,,0000,0000,0000,,jeden z nich bude vybraný na otvorenie\N vystúpenia pre EMG Svetové turné,
Dialogue: 0,0:17:05.38,0:17:08.01,Default,,0000,0000,0000,,ako koniec Super Idola.
Dialogue: 0,0:17:08.01,0:17:10.91,Default,,0000,0000,0000,,Šiesti, čo boli vybraní ako Super Idolovia,
Dialogue: 0,0:17:10.91,0:17:15.25,Default,,0000,0000,0000,,spolu s JB-m a Nanou,\N dostanú tú možnosť.
Dialogue: 0,0:17:16.32,0:17:18.56,Default,,0000,0000,0000,,Nebuďte spokojní s dnešným výstupom.
Dialogue: 0,0:17:18.56,0:17:22.49,Default,,0000,0000,0000,,Ale tešte sa prosím na ďalšie vystúpenie o mesiac.
Dialogue: 0,0:17:29.00,0:17:30.57,Default,,0000,0000,0000,,EMG?
Dialogue: 0,0:17:30.57,0:17:32.50,Default,,0000,0000,0000,,Oh! Mám to!
Dialogue: 0,0:17:32.50,0:17:34.61,Default,,0000,0000,0000,,Nebol súčasťou tohto minulý rok Song SamDong
Dialogue: 0,0:17:34.61,0:17:35.51,Default,,0000,0000,0000,,a nevyhral prvé miesto?
Dialogue: 0,0:17:35.51,0:17:39.58,Default,,0000,0000,0000,,Áno a vydal album pod menom Tei\N v tej spoločnosti.
Dialogue: 0,0:17:39.58,0:17:42.68,Default,,0000,0000,0000,,A ešte vyhral aj ocenenie.
Dialogue: 0,0:17:51.96,0:17:53.73,Default,,0000,0000,0000,,Super.
Dialogue: 0,0:17:53.73,0:18:00.67,Default,,0000,0000,0000,,Na.
Dialogue: 0,0:18:00.67,0:18:03.10,Default,,0000,0000,0000,,Teraz keď mám jablko,
Dialogue: 0,0:18:03.10,0:18:04.90,Default,,0000,0000,0000,,už som zostal iba ja s\N vyliečením kolena.
Dialogue: 0,0:18:04.90,0:18:05.84,Default,,0000,0000,0000,,Však?
Dialogue: 0,0:18:05.84,0:18:07.47,Default,,0000,0000,0000,,Presne.
Dialogue: 0,0:18:07.47,0:18:09.61,Default,,0000,0000,0000,,Shin HaeSung?
Dialogue: 0,0:18:09.61,0:18:12.21,Default,,0000,0000,0000,,O niečom sa potrebujeme porozprávať.
Dialogue: 0,0:18:13.58,0:18:16.05,Default,,0000,0000,0000,,Tvoja prvá pieseň "Ahoj Ja",
Dialogue: 0,0:18:16.05,0:18:18.98,Default,,0000,0000,0000,,je už zahrnutá v albume YeEun.
Dialogue: 0,0:18:18.98,0:18:21.05,Default,,0000,0000,0000,,Tvoja druhá pieseň, "Spolu",
Dialogue: 0,0:18:21.05,0:18:24.29,Default,,0000,0000,0000,,vyhrala prvé miesto v hlasovaní súťaže.
Dialogue: 0,0:18:24.29,0:18:27.96,Default,,0000,0000,0000,,Podľa toho usudzujem,\N že získavaš ocenenia rad radom.
Dialogue: 0,0:18:27.96,0:18:29.96,Default,,0000,0000,0000,,Blahoželám.
Dialogue: 0,0:18:29.96,0:18:33.10,Default,,0000,0000,0000,,Ďakujem.
Dialogue: 0,0:18:33.10,0:18:37.67,Default,,0000,0000,0000,,Hae Sung, pesničky čo si vytvorila,\N majú schopnosť dojať ľudí.
Dialogue: 0,0:18:37.67,0:18:40.71,Default,,0000,0000,0000,,Aj keď základy nie sú úplne dobré,
Dialogue: 0,0:18:40.71,0:18:43.54,Default,,0000,0000,0000,,stále je tu ten pocit,\N čo dokáže chytiť každého za srdce.
Dialogue: 0,0:18:43.54,0:18:46.98,Default,,0000,0000,0000,,Takže, čo s tým?
Dialogue: 0,0:18:46.98,0:18:50.85,Default,,0000,0000,0000,,Nikdy si nepremýšľala o odbore v kompozícií?
Dialogue: 0,0:18:50.85,0:18:53.35,Default,,0000,0000,0000,,Hoci som zahltená úctou,
Dialogue: 0,0:18:53.35,0:18:54.85,Default,,0000,0000,0000,,ale keby tu bola možnosť,
Dialogue: 0,0:18:54.85,0:18:57.66,Default,,0000,0000,0000,,skúsila by som to.
Dialogue: 0,0:19:01.79,0:19:05.36,Default,,0000,0000,0000,,Toto je list o prijatí z Berklee Vysokej.
Dialogue: 0,0:19:05.36,0:19:07.23,Default,,0000,0000,0000,,Tatiež som prichystal miesto v kancelárií\N Amerického EMG,
Dialogue: 0,0:19:07.23,0:19:11.74,Default,,0000,0000,0000,,kde môžeš stážistovať na 6 mesiacov.
Dialogue: 0,0:19:15.17,0:19:18.04,Default,,0000,0000,0000,,Ak tam pôjdem, kedy to začne?
Dialogue: 0,0:19:18.04,0:19:20.41,Default,,0000,0000,0000,,Čím skôr, tým lepšie.
Dialogue: 0,0:19:20.41,0:19:22.21,Default,,0000,0000,0000,,Kým váhaš,
Dialogue: 0,0:19:22.21,0:19:25.35,Default,,0000,0000,0000,,možnosť stážistky sa zaplní.
Dialogue: 0,0:19:25.35,0:19:29.25,Default,,0000,0000,0000,,Škola začína novým semestrom budúci mesiac.
Dialogue: 0,0:19:29.25,0:19:31.76,Default,,0000,0000,0000,,Musíš sa rozhodnúť tento mesiac.
Dialogue: 0,0:19:31.76,0:19:35.99,Default,,0000,0000,0000,,Ďakujem, že mi dávate tak skvelú príležitosť, ale-
Dialogue: 0,0:19:35.99,0:19:39.20,Default,,0000,0000,0000,,Počul som, že tvojim snom je byť speváčkou,
Dialogue: 0,0:19:39.20,0:19:42.20,Default,,0000,0000,0000,,hoci spievať nevieš.
Dialogue: 0,0:19:42.20,0:19:44.80,Default,,0000,0000,0000,,Len teraz si objavila svoje schopnosti,
Dialogue: 0,0:19:44.80,0:19:50.11,Default,,0000,0000,0000,,a chceš sa vzdať takejto príležitosti?
Dialogue: 0,0:19:50.11,0:19:53.81,Default,,0000,0000,0000,,Príležitosti nechodia tak často.
Dialogue: 0,0:19:57.02,0:19:58.52,Default,,0000,0000,0000,,Je ťažké uveriť,
Dialogue: 0,0:19:58.52,0:20:00.25,Default,,0000,0000,0000,,že táto súťaž už skončila.
Dialogue: 0,0:20:00.25,0:20:03.19,Default,,0000,0000,0000,,Boli sme so študentmi až do konca.
Dialogue: 0,0:20:03.19,0:20:06.12,Default,,0000,0000,0000,,Učiteľka Ahn, si vážne skvelá.
Dialogue: 0,0:20:06.12,0:20:09.89,Default,,0000,0000,0000,,Starať sa o tie nemehlá si zvládla skvelo.
Dialogue: 0,0:20:09.89,0:20:12.76,Default,,0000,0000,0000,,Učiteľka Hyeon, si tiež skvelá.
Dialogue: 0,0:20:12.76,0:20:14.80,Default,,0000,0000,0000,,Však?
Dialogue: 0,0:20:14.80,0:20:17.30,Default,,0000,0000,0000,,Čo to je?
Dialogue: 0,0:20:17.30,0:20:18.20,Default,,0000,0000,0000,,Prečo?
Dialogue: 0,0:20:18.20,0:20:19.94,Default,,0000,0000,0000,,Čo to je?
Dialogue: 0,0:20:21.71,0:20:24.61,Default,,0000,0000,0000,,Učiteľ Yanf. Chceš sa podieľať na KBC stár?
Dialogue: 0,0:20:24.61,0:20:29.18,Default,,0000,0000,0000,,Nie! Chcel som to odporučiť študentom.
Dialogue: 0,0:20:29.18,0:20:30.31,Default,,0000,0000,0000,,Čo je na tom zlé?
Dialogue: 0,0:20:30.31,0:20:32.82,Default,,0000,0000,0000,,Nie je to zlé, choď a vyskúšaj to.
Dialogue: 0,0:20:32.82,0:20:34.62,Default,,0000,0000,0000,,Počula som, že si spieval na KTV minule,
Dialogue: 0,0:20:34.62,0:20:36.69,Default,,0000,0000,0000,,myslím, že ti to ide.
Dialogue: 0,0:20:36.69,0:20:37.69,Default,,0000,0000,0000,,Vážne?
Dialogue: 0,0:20:37.69,0:20:39.39,Default,,0000,0000,0000,,Hej.
Dialogue: 0,0:20:39.39,0:20:41.76,Default,,0000,0000,0000,,Keď vidím ako sa študenti podieľajú na tej súťaži,
Dialogue: 0,0:20:41.76,0:20:44.26,Default,,0000,0000,0000,,moje srdce poskočilo s nimi.
Dialogue: 0,0:20:44.26,0:20:45.40,Default,,0000,0000,0000,,Ak sa nechytím tejto možnosti,
Dialogue: 0,0:20:45.40,0:20:47.80,Default,,0000,0000,0000,,myslím, že to budem ľutovať pozbytok svojho života.
Dialogue: 0,0:20:47.80,0:20:49.50,Default,,0000,0000,0000,,Čo záleží na veku?
Dialogue: 0,0:20:49.50,0:20:51.30,Default,,0000,0000,0000,,Do toho.
Dialogue: 0,0:20:51.30,0:20:52.94,Default,,0000,0000,0000,,Áno, do toho,
Dialogue: 0,0:20:52.94,0:20:53.94,Default,,0000,0000,0000,,bude to sranda!
Dialogue: 0,0:20:53.94,0:20:55.01,Default,,0000,0000,0000,,Pomôžeme ti.
Dialogue: 0,0:20:55.01,0:20:56.21,Default,,0000,0000,0000,,Naozaj?
Dialogue: 0,0:20:56.21,0:20:57.41,Default,,0000,0000,0000,,Jasné.
Dialogue: 0,0:21:16.89,0:21:19.50,Default,,0000,0000,0000,,Čo, žiadne vysoké ťa nezaujali?
Dialogue: 0,0:21:23.25,0:21:25.24,Default,,0000,0000,0000,,Nechcem na výšku.
Dialogue: 0,0:21:29.74,0:21:31.94,Default,,0000,0000,0000,,Ak nepôjdeš, tak potom čo?
Dialogue: 0,0:21:31.94,0:21:34.05,Default,,0000,0000,0000,,Chcem byť manažérkou.
Dialogue: 0,0:21:35.81,0:21:37.82,Default,,0000,0000,0000,,Hej, nie každý ním môže byť,
Dialogue: 0,0:21:37.82,0:21:39.68,Default,,0000,0000,0000,,dáva to zabrať.
Dialogue: 0,0:21:39.68,0:21:41.99,Default,,0000,0000,0000,,Musíš sa presne orientovať rozvrhom,
Dialogue: 0,0:21:41.99,0:21:43.76,Default,,0000,0000,0000,,ak sa niečo stane, musíš to urovnať.
Dialogue: 0,0:21:43.76,0:21:44.62,Default,,0000,0000,0000,,Musíš obstarávať iných ľudí,
Dialogue: 0,0:21:44.62,0:21:48.19,Default,,0000,0000,0000,,je to únavné ako pre telo, aj pre myseľ.
Dialogue: 0,0:21:48.19,0:21:49.99,Default,,0000,0000,0000,,Nerobil si to aj ty?
Dialogue: 0,0:21:49.99,0:21:52.86,Default,,0000,0000,0000,,Chcem sa učiť od teba.
Dialogue: 0,0:21:52.86,0:21:54.80,Default,,0000,0000,0000,,Mala by si ďalej študovať biznis.
Dialogue: 0,0:21:54.80,0:21:56.17,Default,,0000,0000,0000,,Choď na výšku.
Dialogue: 0,0:21:56.17,0:21:57.80,Default,,0000,0000,0000,,A keď dokončím výšku?
Dialogue: 0,0:21:57.80,0:22:00.81,Default,,0000,0000,0000,,Aj tak by som sa od teba učila.
Dialogue: 0,0:22:00.81,0:22:02.74,Default,,0000,0000,0000,,Aj kebyže nedokončím výšku, budem robiť to isté.
Dialogue: 0,0:22:04.94,0:22:07.81,Default,,0000,0000,0000,,Prečo tam musím ísť?
Dialogue: 0,0:22:07.95,0:22:10.48,Default,,0000,0000,0000,,Len kvôli známkam?
Dialogue: 0,0:22:16.02,0:22:19.59,Default,,0000,0000,0000,,Tak to spravme takto.
Dialogue: 0,0:22:19.89,0:22:22.03,Default,,0000,0000,0000,,Riaď sa rozvrhom Super Idolov,
Dialogue: 0,0:22:22.16,0:22:24.00,Default,,0000,0000,0000,,ochutnaj aké to je.
Dialogue: 0,0:22:24.13,0:22:25.73,Default,,0000,0000,0000,,Potom, použi schopnosti,
Dialogue: 0,0:22:25.90,0:22:29.77,Default,,0000,0000,0000,,a keď dokončíš ten rozvrh, schválim to.
Dialogue: 0,0:22:29.93,0:22:33.74,Default,,0000,0000,0000,,Myslíš to vážne, že? Naozaj?
Dialogue: 0,0:22:40.04,0:22:44.05,Default,,0000,0000,0000,,Budete robiť aktivity ako tím.
Dialogue: 0,0:22:44.05,0:22:48.22,Default,,0000,0000,0000,,Najdôležitejšími vecami pri tímových aktivitách,
Dialogue: 0,0:22:48.22,0:22:52.56,Default,,0000,0000,0000,,sú dôvera, nádej, láska.
Dialogue: 0,0:22:52.72,0:22:55.96,Default,,0000,0000,0000,,Čo sa týka všetkého.
Dialogue: 0,0:23:00.50,0:23:04.14,Default,,0000,0000,0000,,Inak povedané, musíte si navzájom dôverovať,
Dialogue: 0,0:23:04.27,0:23:06.80,Default,,0000,0000,0000,,a mať sa radi.
Dialogue: 0,0:23:06.97,0:23:09.84,Default,,0000,0000,0000,,Aby sa to splnilo, čo je dôležité?
Dialogue: 0,0:23:10.01,0:23:12.98,Default,,0000,0000,0000,,Keď máte spoločné aktivity, budú časy,\N keď si budete liesť na nervy.
Dialogue: 0,0:23:13.11,0:23:15.38,Default,,0000,0000,0000,,Ale počas toho času,\N musíte vziať tie iritujúce pocity,
Dialogue: 0,0:23:15.55,0:23:18.88,Default,,0000,0000,0000,,a zmeniť ich na pozitíva.
Dialogue: 0,0:23:19.05,0:23:22.79,Default,,0000,0000,0000,,Okej potom. UiBong. Čo je na ňom\N najhoršie?
Dialogue: 0,0:23:22.95,0:23:25.16,Default,,0000,0000,0000,,Nič také tu nie je.
Dialogue: 0,0:23:25.32,0:23:30.09,Default,,0000,0000,0000,,Keď je ticho, je chutný...
Dialogue: 0,0:23:30.26,0:23:34.20,Default,,0000,0000,0000,,ale keď si otvorí ústa, je vulgárny.
Dialogue: 0,0:23:34.33,0:23:36.90,Default,,0000,0000,0000,,Presne, jeho akcent.
Dialogue: 0,0:23:37.07,0:23:40.41,Default,,0000,0000,0000,,Tak, sú tu nejaké dobré veci,
Dialogue: 0,0:23:40.41,0:23:42.21,Default,,0000,0000,0000,,okrem jeho akcentu?
Dialogue: 0,0:23:42.37,0:23:45.98,Default,,0000,0000,0000,,Je ľahko dosiahnuteľný, žiadny nátlak alebo\N otravnosť z jeho strany.
Dialogue: 0,0:23:46.11,0:23:49.31,Default,,0000,0000,0000,,Áno, fanúšikovia sa skamarátia skôr s ním.
Dialogue: 0,0:23:49.48,0:23:52.48,Default,,0000,0000,0000,,A môžme získať fanúšikov z Gyeongsang-do tiež.
Dialogue: 0,0:23:52.62,0:23:54.42,Default,,0000,0000,0000,,Ah áno.
Dialogue: 0,0:23:55.29,0:23:57.22,Default,,0000,0000,0000,,Tak, ďalej.
Dialogue: 0,0:23:57.39,0:24:02.39,Default,,0000,0000,0000,,HongJu. Čo máte na ňom najneradšej?
Dialogue: 0,0:24:02.56,0:24:06.73,Default,,0000,0000,0000,,Nevyzerá ako idol,\N skôr ako nejaký ahjussi.
Dialogue: 0,0:24:07.03,0:24:10.23,Default,,0000,0000,0000,,Mal by byť idolom, ale vyzerá skôr ako ahjussi.
Dialogue: 0,0:24:10.40,0:24:13.71,Default,,0000,0000,0000,,Takže, nejaké pozítíva že vyzerá ako ahjussi?
Dialogue: 0,0:24:13.87,0:24:17.98,Default,,0000,0000,0000,,Má schopnosť vytvoriť pieseň na štýl "Život B-triedy".
Dialogue: 0,0:24:18.18,0:24:19.74,Default,,0000,0000,0000,,Áno, skvelé.
Dialogue: 0,0:24:19.88,0:24:22.35,Default,,0000,0000,0000,,A čo potom YuJin, čo na ňom nemáme radi?
Dialogue: 0,0:24:22.48,0:24:25.45,Default,,0000,0000,0000,,- Ja!\N- Ja!
Dialogue: 0,0:24:26.88,0:24:30.12,Default,,0000,0000,0000,,Extrémne tvrdohlavý.
Dialogue: 0,0:24:30.32,0:24:36.43,Default,,0000,0000,0000,,Áno. A ešte tá vec s jeho minulosťou.
Dialogue: 0,0:24:36.86,0:24:39.86,Default,,0000,0000,0000,,Ale, o jeho tvrdohlavosti,
Dialogue: 0,0:24:40.03,0:24:42.70,Default,,0000,0000,0000,,sú na to nejaké pozitíva?
Dialogue: 0,0:24:44.47,0:24:46.44,Default,,0000,0000,0000,,Zázrační sú takí od prírody.
Dialogue: 0,0:24:46.60,0:24:48.94,Default,,0000,0000,0000,,Je to lepšie ako byť pokorným\N a nebyť schopný niečo vytvoriť.
Dialogue: 0,0:24:49.11,0:24:50.24,Default,,0000,0000,0000,,Však?
Dialogue: 0,0:24:50.61,0:24:53.14,Default,,0000,0000,0000,,Áno. Dobre.
Dialogue: 0,0:24:53.31,0:24:56.98,Default,,0000,0000,0000,,A teraz, slabosť Lian?
Dialogue: 0,0:24:57.15,0:24:59.05,Default,,0000,0000,0000,,Nie je tu žiadna, či?
Dialogue: 0,0:24:59.22,0:25:00.99,Default,,0000,0000,0000,,Je jediná, čo si myslí, že jej tvár je kresba.
Dialogue: 0,0:25:01.19,0:25:02.59,Default,,0000,0000,0000,,Lepí si dvojité mihalnice na oči.
Dialogue: 0,0:25:02.75,0:25:05.12,Default,,0000,0000,0000,,Žiadne spôsoby.
Dialogue: 0,0:25:06.26,0:25:09.09,Default,,0000,0000,0000,,Okej, to že nemá spôsoby,
Dialogue: 0,0:25:09.09,0:25:11.43,Default,,0000,0000,0000,,je v tom nejaké pozitívum?
Dialogue: 0,0:25:15.27,0:25:16.50,Default,,0000,0000,0000,,Je možné nejaké mať?
Dialogue: 0,0:25:16.50,0:25:17.37,Default,,0000,0000,0000,,Nie je.
Dialogue: 0,0:25:17.37,0:25:21.21,Default,,0000,0000,0000,,Tak to je zlé.
Dialogue: 0,0:25:23.37,0:25:27.48,Default,,0000,0000,0000,,Decká! Oddnes, som vašou manažérkou.
Dialogue: 0,0:25:27.48,0:25:28.91,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:25:47.80,0:25:49.43,Default,,0000,0000,0000,,Dobre.
Dialogue: 0,0:26:01.85,0:26:06.48,Default,,0000,0000,0000,,186cm, 72kg. To je ideál.
Dialogue: 0,0:26:06.68,0:26:08.79,Default,,0000,0000,0000,,Toto je pýcha Južno Kórejskej republiky.
Dialogue: 0,0:26:08.92,0:26:10.39,Default,,0000,0000,0000,,Tuším si sa o seba staral.
Dialogue: 0,0:26:10.56,0:26:14.43,Default,,0000,0000,0000,,Manažérka, ja som človek, čo má svaly
Dialogue: 0,0:26:14.56,0:26:17.10,Default,,0000,0000,0000,,už len z pitia vody.
Dialogue: 0,0:26:23.17,0:26:26.24,Default,,0000,0000,0000,,Namiesto toho musíme namerať cez brucho.
Dialogue: 0,0:26:29.71,0:26:36.65,Default,,0000,0000,0000,,75.1kg. Musíš zhodiť.
Dialogue: 0,0:26:36.81,0:26:39.92,Default,,0000,0000,0000,,To preto, že som veľa jedol na obed.
Dialogue: 0,0:26:40.08,0:26:42.12,Default,,0000,0000,0000,,Vyzerá to, akoby si zjedol celú kravu.
Dialogue: 0,0:26:42.29,0:26:45.56,Default,,0000,0000,0000,,Tu je krava.
Dialogue: 0,0:26:49.06,0:26:51.93,Default,,0000,0000,0000,,Moment. Nie si stylistkou.
Dialogue: 0,0:26:52.10,0:26:53.90,Default,,0000,0000,0000,,Prečo to robíš?
Dialogue: 0,0:26:55.17,0:26:57.34,Default,,0000,0000,0000,,Musíme ušetriť prachy.
Dialogue: 0,0:26:57.50,0:27:00.24,Default,,0000,0000,0000,,Si zúrivejšia ako tvoj otec.
Dialogue: 0,0:27:02.11,0:27:03.67,Default,,0000,0000,0000,,Ako môže žena...
Dialogue: 0,0:27:03.88,0:27:06.04,Default,,0000,0000,0000,,chytať telo muža ako sa jej zachce?
Dialogue: 0,0:27:06.88,0:27:10.78,Default,,0000,0000,0000,,Kde, kde sa to dotýkaš.
Dialogue: 0,0:27:23.49,0:27:24.90,Default,,0000,0000,0000,,Zdravím.
Dialogue: 0,0:27:25.06,0:27:27.13,Default,,0000,0000,0000,,Ako sa máte?
Dialogue: 0,0:27:27.30,0:27:29.47,Default,,0000,0000,0000,,1. 2. 3. Dobrý deň.
Dialogue: 0,0:27:29.63,0:27:34.21,Default,,0000,0000,0000,,My sme tí, čo preberajú hudbu nukleárnou búrkou,\N Super Idoly.
Dialogue: 0,0:27:35.67,0:27:39.41,Default,,0000,0000,0000,,Táto časť je rozhodne nukleárny výbuch, v poriadku.
Dialogue: 0,0:27:39.58,0:27:42.58,Default,,0000,0000,0000,,Ale táto časť je nukleárnou bombou.
Dialogue: 0,0:27:42.75,0:27:44.32,Default,,0000,0000,0000,,Je toto jeden tím?
Dialogue: 0,0:27:44.52,0:27:46.38,Default,,0000,0000,0000,,Áno, som manažérkou.
Dialogue: 0,0:27:46.55,0:27:48.85,Default,,0000,0000,0000,,Prečo ste ich dali dokopy takto?
Dialogue: 0,0:27:49.02,0:27:51.76,Default,,0000,0000,0000,,Je to iné, ako som videl v televízií.
Dialogue: 0,0:27:51.89,0:27:56.13,Default,,0000,0000,0000,,Nabudúce, urobte si lepšie prácu a dajte ich poriadne dokopy.
Dialogue: 0,0:27:56.29,0:27:58.26,Default,,0000,0000,0000,,Ak chcete dostať tvár do médií,
Dialogue: 0,0:27:58.40,0:28:02.10,Default,,0000,0000,0000,,máte ešte dlhú cestu.
Dialogue: 0,0:28:10.64,0:28:13.11,Default,,0000,0000,0000,,Znovu ste prišli, aké dojemné.
Dialogue: 0,0:28:13.28,0:28:14.11,Default,,0000,0000,0000,,Áno, mali ste sa dobre?
Dialogue: 0,0:28:14.11,0:28:15.91,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:28:21.39,0:28:26.36,Default,,0000,0000,0000,,Svaly boli vážne natrhnuté ešte pred nehodou.
Dialogue: 0,0:28:26.36,0:28:28.49,Default,,0000,0000,0000,,Ak budete tancovať a cvičiť veľmi húževnato,
Dialogue: 0,0:28:28.66,0:28:30.46,Default,,0000,0000,0000,,a získate ďalšie poranenia,
Dialogue: 0,0:28:30.60,0:28:32.16,Default,,0000,0000,0000,,je ťažké predvídať čo sa stane.
Dialogue: 0,0:28:32.33,0:28:36.93,Default,,0000,0000,0000,,Najhorším by bolo, že môže sa Vám potom ťažko chodiť.
Dialogue: 0,0:28:40.44,0:28:44.54,Default,,0000,0000,0000,,Pokračujte vo fyzickej terapií,\N aby ste zlepšili svaly.
Dialogue: 0,0:28:44.71,0:28:47.81,Default,,0000,0000,0000,,To pomôže aspoň trochu.
Dialogue: 0,0:28:53.38,0:28:56.09,Default,,0000,0000,0000,,{\i1}Iba teraz keď si našla svoje schopnosti,{\i0}
Dialogue: 0,0:28:56.25,0:28:59.69,Default,,0000,0000,0000,,{\i1}sa plánuješ vzdať\N takejto príležitosti?{\i0}
Dialogue: 0,0:28:59.86,0:29:03.46,Default,,0000,0000,0000,,{\i1}Príležitosti nechodia veľmi často.{\i0}
Dialogue: 0,0:29:07.37,0:29:10.40,Default,,0000,0000,0000,,Gratulujem, gratulujem, gratulujem.\N Aspoň sádru dali preč.
Dialogue: 0,0:29:10.57,0:29:12.70,Default,,0000,0000,0000,,Si v poriadku?
Dialogue: 0,0:29:13.20,0:29:15.81,Default,,0000,0000,0000,,Prečo? Ak poviem že áno,
Dialogue: 0,0:29:15.94,0:29:18.21,Default,,0000,0000,0000,,znovu utečieš?
Dialogue: 0,0:29:18.38,0:29:23.21,Default,,0000,0000,0000,,Nie som v poriadku.\N Stále potrebujem starostlivosť.
Dialogue: 0,0:29:23.92,0:29:28.52,Default,,0000,0000,0000,,Si naozaj v poriadku, však?
Dialogue: 0,0:29:28.65,0:29:31.32,Default,,0000,0000,0000,,Povedal som, že nie.
Dialogue: 0,0:29:31.49,0:29:34.33,Default,,0000,0000,0000,,Môžem aj zomrieť.
Dialogue: 0,0:29:56.51,0:29:59.62,Default,,0000,0000,0000,,Sú veľmi disciplovaní.
Dialogue: 0,0:29:59.78,0:30:02.02,Default,,0000,0000,0000,,Pripomína mi to štážistické časy.
Dialogue: 0,0:30:02.19,0:30:03.29,Default,,0000,0000,0000,,Ak chcete byť ako my,
Dialogue: 0,0:30:03.45,0:30:04.92,Default,,0000,0000,0000,,čaká vás dlhá cesta.
Dialogue: 0,0:30:05.56,0:30:09.06,Default,,0000,0000,0000,,Máte teraz šancu stať sa sunbae.
Dialogue: 0,0:30:10.03,0:30:12.96,Default,,0000,0000,0000,,Znie to zábavne.
Dialogue: 0,0:30:16.00,0:30:18.84,Default,,0000,0000,0000,,Nielen ja javisku, ale aj keď chodíte,
Dialogue: 0,0:30:19.00,0:30:20.37,Default,,0000,0000,0000,,musíte kontrolovať svoj výraz.
Dialogue: 0,0:30:20.54,0:30:23.44,Default,,0000,0000,0000,,Ak bude čo i len jedna osoba, čo vás spozná,
Dialogue: 0,0:30:23.61,0:30:26.34,Default,,0000,0000,0000,,nemôžete sa pozrieť preč ani na sekundu.
Dialogue: 0,0:30:26.51,0:30:31.42,Default,,0000,0000,0000,,Všade sú kamery, ktoré furt fotia, 360 stupňov okolo vás.
Dialogue: 0,0:30:31.62,0:30:34.62,Default,,0000,0000,0000,,Aj keď dýchate, nesmiete relaxovať.
Dialogue: 0,0:30:34.79,0:30:38.42,Default,,0000,0000,0000,,V momente ako sa pozastavíš...
Dialogue: 0,0:30:40.39,0:30:45.06,Default,,0000,0000,0000,,Presne takto, dostane sa to na internet.
Dialogue: 0,0:30:45.23,0:30:46.50,Default,,0000,0000,0000,,To je super.
Dialogue: 0,0:30:46.50,0:30:47.90,Default,,0000,0000,0000,,Môžem si to nechať?
Dialogue: 0,0:30:47.90,0:30:49.77,Default,,0000,0000,0000,,Aj keď niečo takéto hovoríš,
Dialogue: 0,0:30:49.93,0:30:52.04,Default,,0000,0000,0000,,musíš kontrolovať svoj výraz.
Dialogue: 0,0:30:52.20,0:30:53.71,Default,,0000,0000,0000,,Tvoje postavenie je zlé. Vyrovnaj chrbát,
Dialogue: 0,0:30:53.91,0:30:57.44,Default,,0000,0000,0000,,zníž bradu. Dobre, dobre.
Dialogue: 0,0:30:57.61,0:30:58.24,Default,,0000,0000,0000,,Dobre?
Dialogue: 0,0:30:58.41,0:30:59.08,Default,,0000,0000,0000,,Veľmi dobré.
Dialogue: 0,0:30:59.24,0:31:01.18,Default,,0000,0000,0000,,Naozaj?
Dialogue: 0,0:31:03.15,0:31:06.22,Default,,0000,0000,0000,,Počula som, že I:dn nová pesnička bola plagiarizovaná.
Dialogue: 0,0:31:08.62,0:31:11.39,Default,,0000,0000,0000,,Počula som, že Jeong UiBong, zadný tanečník I:dn,
Dialogue: 0,0:31:11.39,0:31:13.36,Default,,0000,0000,0000,,tancuje lepšie ako Vy. Je to pravda?
Dialogue: 0,0:31:15.96,0:31:17.73,Default,,0000,0000,0000,,Ja?
Dialogue: 0,0:31:18.76,0:31:22.90,Default,,0000,0000,0000,,Je pravda, že to vlastne Vy ste boli poslaný\N k vode Lian?
Dialogue: 0,0:31:23.07,0:31:24.37,Default,,0000,0000,0000,,Ja?
Dialogue: 0,0:31:24.54,0:31:27.37,Default,,0000,0000,0000,,Počula som, že Jin YuJin a Shin HaeSung\N spolu chodia...
Dialogue: 0,0:31:41.15,0:31:44.59,Default,,0000,0000,0000,,Čo to...je to vôbec dobrý príklad?
Dialogue: 0,0:31:44.76,0:31:47.59,Default,,0000,0000,0000,,To bol zlý príklad, zlý príklad.
Dialogue: 0,0:31:48.39,0:31:50.13,Default,,0000,0000,0000,,Nebola som taká, ani keď som začínala.
Dialogue: 0,0:31:50.29,0:31:53.83,Default,,0000,0000,0000,,Je úplne trápny, hmm...
Dialogue: 0,0:31:54.17,0:31:58.64,Default,,0000,0000,0000,,Počul som, že Super Idol Jin YuJin neznáša byť idolom,
Dialogue: 0,0:31:58.80,0:32:00.17,Default,,0000,0000,0000,,je to pravda?
Dialogue: 0,0:32:00.34,0:32:02.01,Default,,0000,0000,0000,,Ja?
Dialogue: 0,0:32:02.21,0:32:05.34,Default,,0000,0000,0000,,Žiadny záujem, entuziasmus pre hudbu,
Dialogue: 0,0:32:05.54,0:32:08.98,Default,,0000,0000,0000,,iba byť známym.
Dialogue: 0,0:32:09.11,0:32:11.32,Default,,0000,0000,0000,,Necítite sa znechutene?
Dialogue: 0,0:32:11.48,0:32:13.82,Default,,0000,0000,0000,,Ja...ja?
Dialogue: 0,0:32:13.99,0:32:18.12,Default,,0000,0000,0000,,Sú klebety, ktoré vravia, že ste sa vyznali Shin HaeSung,\N ale ona Vás odmietla...
Dialogue: 0,0:32:18.79,0:32:25.00,Default,,0000,0000,0000,,Bolo to len nedávno\N a už si sa mu sťažovala?
Dialogue: 0,0:32:26.26,0:32:29.57,Default,,0000,0000,0000,,Čože? Naozaj si sa jej vyznal?
Dialogue: 0,0:32:29.73,0:32:32.24,Default,,0000,0000,0000,,Neviem!
Dialogue: 0,0:32:47.95,0:32:52.49,Default,,0000,0000,0000,,Shin HaeSung. Prijatá.
Dialogue: 0,0:33:01.83,0:33:09.14,Default,,0000,0000,0000,,Čo to je? Berklee?
Dialogue: 0,0:33:09.31,0:33:11.98,Default,,0000,0000,0000,,Wow, naša HaeSung vyrástla.
Dialogue: 0,0:33:12.14,0:33:14.21,Default,,0000,0000,0000,,Myslel som si, že žiadna škola ťa nepríjme.
Dialogue: 0,0:33:14.38,0:33:16.91,Default,,0000,0000,0000,,Byť prijatým na americkú univerzitu!
Dialogue: 0,0:33:17.08,0:33:18.52,Default,,0000,0000,0000,,Skvelá práca! Skvelá práca!
Dialogue: 0,0:33:18.72,0:33:20.59,Default,,0000,0000,0000,,Oppa má teraz dôvod prečo ťa sledovať.
Dialogue: 0,0:33:23.32,0:33:25.69,Default,,0000,0000,0000,,Daj mi to.
Dialogue: 0,0:33:28.46,0:33:33.73,Default,,0000,0000,0000,,Ja...tam aj tak nejdem.
Dialogue: 0,0:33:33.87,0:33:35.27,Default,,0000,0000,0000,,Nejdeš?
Dialogue: 0,0:33:35.43,0:33:37.90,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:33:38.20,0:33:41.71,Default,,0000,0000,0000,,Nemusím študovať v Amerike.
Dialogue: 0,0:33:41.84,0:33:43.78,Default,,0000,0000,0000,,Vytvorím dobre piesne v Kórei,
Dialogue: 0,0:33:43.91,0:33:48.21,Default,,0000,0000,0000,,a ostatné štáty môžu prísť\N a kúpiť si ich.
Dialogue: 0,0:33:48.35,0:33:51.18,Default,,0000,0000,0000,,Vieš, že dávaš zabrať, však?
Dialogue: 0,0:33:51.58,0:33:54.32,Default,,0000,0000,0000,,Ak nejdeš, tak to roztrhnem.
Dialogue: 0,0:34:01.53,0:34:04.10,Default,,0000,0000,0000,,Pozri, klameš že nejdeš.
Dialogue: 0,0:34:04.23,0:34:06.23,Default,,0000,0000,0000,,Čo by si urobila, kebyže to naozaj roztrhnem?
Dialogue: 0,0:34:06.40,0:34:08.20,Default,,0000,0000,0000,,Ah, vystrašil si ma.
Dialogue: 0,0:34:08.37,0:34:10.93,Default,,0000,0000,0000,,Daj....daj....
Dialogue: 0,0:34:14.87,0:34:16.74,Default,,0000,0000,0000,,Choď.
Dialogue: 0,0:34:18.21,0:34:19.51,Default,,0000,0000,0000,,Chcem sa pýšiť tiež,
Dialogue: 0,0:34:19.68,0:34:22.71,Default,,0000,0000,0000,,priateľkou, ktorá je známa skladateľka.
Dialogue: 0,0:34:39.73,0:34:41.83,Default,,0000,0000,0000,,Nesmieš! Nesmieš stratiť zmysli!
Dialogue: 0,0:34:41.97,0:34:43.50,Default,,0000,0000,0000,,Čo mám robiť?
Dialogue: 0,0:34:43.67,0:34:45.47,Default,,0000,0000,0000,,Naozaj sa bojím.
Dialogue: 0,0:34:45.64,0:34:47.71,Default,,0000,0000,0000,,Nedokážem to.
Dialogue: 0,0:34:48.64,0:34:50.68,Default,,0000,0000,0000,,Len nasleduj tohto hyunga. Ver mi.
Dialogue: 0,0:34:50.81,0:34:52.88,Default,,0000,0000,0000,,Len mi ver.
Dialogue: 0,0:34:58.05,0:34:59.88,Default,,0000,0000,0000,,Len si mysli, že ja jediná som v obecenstve.
Dialogue: 0,0:35:00.02,0:35:01.79,Default,,0000,0000,0000,,Som tu iba ja.
Dialogue: 0,0:35:14.17,0:35:15.17,Default,,0000,0000,0000,,Veľa šťastia!
Dialogue: 0,0:35:15.30,0:35:17.57,Default,,0000,0000,0000,,Aj tebe.
Dialogue: 0,0:35:18.27,0:35:19.44,Default,,0000,0000,0000,,Veľa šťastia!
Dialogue: 0,0:35:19.57,0:35:20.41,Default,,0000,0000,0000,,To bolí!
Dialogue: 0,0:35:20.57,0:35:22.01,Default,,0000,0000,0000,,Uprav si slúchadlo.
Dialogue: 0,0:35:22.14,0:35:23.71,Default,,0000,0000,0000,,Keď pôjdeš hore, nezabudni si skontrolovať mikrofón.
Dialogue: 0,0:35:24.74,0:35:28.48,Default,,0000,0000,0000,,Dobre, nestrácaj sa len preto,\N že dievčatá budú kričať.
Dialogue: 0,0:35:28.61,0:35:30.82,Default,,0000,0000,0000,,Ja viem! Tiež niečo viem.
Dialogue: 0,0:35:31.02,0:35:32.52,Default,,0000,0000,0000,,Nemusíš sa báť.
Dialogue: 0,0:35:32.85,0:35:34.69,Default,,0000,0000,0000,,Naozaj?
Dialogue: 0,0:35:39.19,0:35:41.33,Default,,0000,0000,0000,,Blahoželám k debutu.
Dialogue: 0,0:35:43.83,0:35:46.06,Default,,0000,0000,0000,,Čo to? Vy spolu chodíte?
Dialogue: 0,0:35:46.23,0:35:46.83,Default,,0000,0000,0000,,Čože?
Dialogue: 0,0:35:47.00,0:35:53.37,Default,,0000,0000,0000,,Čože? On je iba...dobrý kamoš.
Dialogue: 0,0:35:57.21,0:36:01.11,Default,,0000,0000,0000,,A čo keď sme si blízki?
Dialogue: 0,0:36:01.31,0:36:05.78,Default,,0000,0000,0000,,Iba kamaráti. Čo je na tom zlé? Žiarliš?
Dialogue: 0,0:36:05.95,0:36:08.02,Default,,0000,0000,0000,,Žiarliť na čo?
Dialogue: 0,0:36:08.22,0:36:09.55,Default,,0000,0000,0000,,Postaraj sa o Lian.
Dialogue: 0,0:36:09.72,0:36:12.42,Default,,0000,0000,0000,,Ako isto vieš, je veľmi slabá.
Dialogue: 0,0:36:16.29,0:36:18.30,Default,,0000,0000,0000,,Ah, načasovanie je skvelé.
Dialogue: 0,0:36:18.46,0:36:19.43,Default,,0000,0000,0000,,Aké načasovanie?
Dialogue: 0,0:36:19.60,0:36:22.63,Default,,0000,0000,0000,,Neprehral si proti mne?
Dialogue: 0,0:36:23.43,0:36:27.37,Default,,0000,0000,0000,,Čas kľaknúť si.
Dialogue: 0,0:36:29.67,0:36:31.34,Default,,0000,0000,0000,,Si naozaj nemilosrdný.
Dialogue: 0,0:36:31.48,0:36:34.15,Default,,0000,0000,0000,,Len aby si si nemusel kľaknúť, ublížil si si nohe.
Dialogue: 0,0:36:34.31,0:36:35.88,Default,,0000,0000,0000,,A to si si myslel, že si vážne lastovička.
Dialogue: 0,0:36:36.05,0:36:41.42,Default,,0000,0000,0000,,Pozrime sa. Nie je to hmyz?
Dialogue: 0,0:36:44.89,0:36:46.52,Default,,0000,0000,0000,,Kontrola výrazu.
Dialogue: 0,0:36:53.40,0:36:56.00,Default,,0000,0000,0000,,Super Idoly, na scénu.
Dialogue: 0,0:36:57.24,0:36:58.50,Default,,0000,0000,0000,,Urob to tak ako vždy.
Dialogue: 0,0:36:58.67,0:36:59.97,Default,,0000,0000,0000,,Nebuď nervózny.
Dialogue: 0,0:37:00.14,0:37:02.94,Default,,0000,0000,0000,,Nesnaž sa tváriť ako sunbae.
Dialogue: 0,0:37:03.11,0:37:05.48,Default,,0000,0000,0000,,Je tvoja noha v poriadku?
Dialogue: 0,0:37:05.64,0:37:08.75,Default,,0000,0000,0000,,Samozrejme, vždy bola zdravá.
Dialogue: 0,0:37:08.95,0:37:11.25,Default,,0000,0000,0000,,Čoskoro vyzdravie.
Dialogue: 0,0:37:14.02,0:37:20.32,Default,,0000,0000,0000,,Do toho!
Dialogue: 0,0:37:27.67,0:37:31.40,Default,,0000,0000,0000,,Zdravíme, sme Super Idolovia, ako sa máte.
Dialogue: 0,0:37:31.57,0:37:33.84,Default,,0000,0000,0000,,-Skvelá práca.\N-Odviedli ste skvelú prácu.
Dialogue: 0,0:37:34.01,0:37:38.34,Default,,0000,0000,0000,,Lian, si v tom istom tíme ako oni?
Dialogue: 0,0:37:45.18,0:37:46.78,Default,,0000,0000,0000,,{\i1}To nie je sranda.{\i0}
Dialogue: 0,0:37:51.49,0:37:53.19,Default,,0000,0000,0000,,Super Idolovia, prosím rýchlo sa pripravte.
Dialogue: 0,0:38:47.91,0:38:49.81,Default,,0000,0000,0000,,Nech sa páči.
Dialogue: 0,0:38:56.72,0:39:00.06,Default,,0000,0000,0000,,Vždy ješ strašne rýchlo.
Dialogue: 0,0:39:00.22,0:39:02.29,Default,,0000,0000,0000,,Ako keby ťa niekto naháňal.
Dialogue: 0,0:39:02.46,0:39:04.46,Default,,0000,0000,0000,,Myslím, že už sa to stalo zvykom.
Dialogue: 0,0:39:04.63,0:39:08.77,Default,,0000,0000,0000,,Kým som trénoval, tak vzácnym bol aj čas na jedenie.
Dialogue: 0,0:39:10.80,0:39:15.34,Default,,0000,0000,0000,,Jedz pomaly, jesť rýchlo je zlé na zdravie.
Dialogue: 0,0:39:15.51,0:39:17.44,Default,,0000,0000,0000,,Keď budeš tak rýchlo jesť,
Dialogue: 0,0:39:17.61,0:39:20.81,Default,,0000,0000,0000,,tak si tú lyžicu vezmem späť.
Dialogue: 0,0:39:23.11,0:39:24.42,Default,,0000,0000,0000,,Nakŕm ma.
Dialogue: 0,0:39:24.88,0:39:25.42,Default,,0000,0000,0000,,Čože?
Dialogue: 0,0:39:25.58,0:39:28.39,Default,,0000,0000,0000,,Keď budem jesť sám, budem jesť rýchlo.
Dialogue: 0,0:39:28.59,0:39:30.66,Default,,0000,0000,0000,,A možno sa zadrhnem a je to zlé pre zdravie.
Dialogue: 0,0:39:30.82,0:39:34.33,Default,,0000,0000,0000,,Tak ma nakŕm.
Dialogue: 0,0:39:34.49,0:39:37.33,Default,,0000,0000,0000,,Nechaj ma pomaly žuvať a prehĺtať.
Dialogue: 0,0:39:41.40,0:39:42.67,Default,,0000,0000,0000,,Už ma aj ruka bolí.
Dialogue: 0,0:39:42.80,0:39:45.24,Default,,0000,0000,0000,,Nemôžem držať lyžicu.
Dialogue: 0,0:39:45.57,0:39:47.64,Default,,0000,0000,0000,,Rýchlo.
Dialogue: 0,0:40:02.82,0:40:05.89,Default,,0000,0000,0000,,Kvôli HaeSung ma vyhodia z konkurzu.
Dialogue: 0,0:40:06.32,0:40:08.86,Default,,0000,0000,0000,,Keď mi budeš takéto dobroty kupovať každý deň,
Dialogue: 0,0:40:08.99,0:40:11.13,Default,,0000,0000,0000,,bude zo mňa tučko.
Dialogue: 0,0:40:12.20,0:40:15.83,Default,,0000,0000,0000,,Naozaj pôjdeš na konkurz?
Dialogue: 0,0:40:15.97,0:40:18.10,Default,,0000,0000,0000,,Nezostáva ti veľa času.
Dialogue: 0,0:40:18.40,0:40:20.17,Default,,0000,0000,0000,,Ak chcem čoskoro byť na javisku,
Dialogue: 0,0:40:20.34,0:40:21.37,Default,,0000,0000,0000,,ako si praješ, to sa stane len keď sa rozídeme.
Dialogue: 0,0:40:21.54,0:40:23.27,Default,,0000,0000,0000,,Takže?
Dialogue: 0,0:40:28.35,0:40:32.42,Default,,0000,0000,0000,,Ale, aj keď sa mi noha zlepší,
Dialogue: 0,0:40:32.65,0:40:35.59,Default,,0000,0000,0000,,môžeme zájsť na jedlo takto spolu?
Dialogue: 0,0:40:37.82,0:40:38.96,Default,,0000,0000,0000,,To...
Dialogue: 0,0:40:39.12,0:40:40.69,Default,,0000,0000,0000,,Dobre.
Dialogue: 0,0:40:41.03,0:40:42.59,Default,,0000,0000,0000,,Nehovorme o tom.
Dialogue: 0,0:40:42.73,0:40:44.13,Default,,0000,0000,0000,,Už nebudem tvrdohlavým.
Dialogue: 0,0:40:44.30,0:40:46.03,Default,,0000,0000,0000,,Len ma nakŕm.
Dialogue: 0,0:40:46.20,0:40:47.23,Default,,0000,0000,0000,,Ak chcem skutočne trénovať,
Dialogue: 0,0:40:47.37,0:40:48.80,Default,,0000,0000,0000,,musím jesť viac.
Dialogue: 0,0:40:48.93,0:40:50.23,Default,,0000,0000,0000,,Rýchlejšie.
Dialogue: 0,0:42:02.04,0:42:03.54,Default,,0000,0000,0000,,HaeSung?
Dialogue: 0,0:42:03.67,0:42:05.61,Default,,0000,0000,0000,,Keďže má talent na skladanie,
Dialogue: 0,0:42:05.81,0:42:08.45,Default,,0000,0000,0000,,napísal som doporučenie do Berklee.
Dialogue: 0,0:42:09.11,0:42:11.02,Default,,0000,0000,0000,,Ale hovorí, že nejde.
Dialogue: 0,0:42:11.35,0:42:13.92,Default,,0000,0000,0000,,A chcem sa spýtať, či kvôli tebe.
Dialogue: 0,0:42:14.72,0:42:16.39,Default,,0000,0000,0000,,Presne ako tie konkurzy,
Dialogue: 0,0:42:16.55,0:42:19.02,Default,,0000,0000,0000,,keď si našiel cestu ako sa stať znovu spevákom,
Dialogue: 0,0:42:19.19,0:42:21.96,Default,,0000,0000,0000,,HaeSung tiež potrebuje nájsť svoju cestu.
Dialogue: 0,0:42:22.13,0:42:24.90,Default,,0000,0000,0000,,Vzdala sa tej súťaže.
Dialogue: 0,0:42:25.93,0:42:29.17,Default,,0000,0000,0000,,Je škoda, že sa vzdala príležitosti.
Dialogue: 0,0:42:29.93,0:42:32.94,Default,,0000,0000,0000,,Presvedčím ju.
Dialogue: 0,0:43:01.47,0:43:02.97,Default,,0000,0000,0000,,Čo sa deje?
Dialogue: 0,0:43:03.17,0:43:05.44,Default,,0000,0000,0000,,Si ako niekto, kto sa vzdal svojho života.
Dialogue: 0,0:43:05.64,0:43:08.17,Default,,0000,0000,0000,,To preto, lebo som beznádejná.
Dialogue: 0,0:43:08.34,0:43:12.04,Default,,0000,0000,0000,,UiBong, HongJu, stali sa super Super Idolmi.
Dialogue: 0,0:43:12.24,0:43:16.31,Default,,0000,0000,0000,,Rozmýšľam, čo som doteraz robila.
Dialogue: 0,0:43:16.48,0:43:18.48,Default,,0000,0000,0000,,UiBong dobre tancuje,
Dialogue: 0,0:43:18.62,0:43:20.75,Default,,0000,0000,0000,,HongJu dobre spieva.
Dialogue: 0,0:43:20.92,0:43:24.16,Default,,0000,0000,0000,,Tvoje tancovanie a spievanie, nuž...
Dialogue: 0,0:43:24.36,0:43:27.39,Default,,0000,0000,0000,,Chceš bitku?
Dialogue: 0,0:43:28.46,0:43:31.93,Default,,0000,0000,0000,,Pozrime sa na to, si dosť silná na to, aby si bola nahnevaná.
Dialogue: 0,0:43:32.30,0:43:35.97,Default,,0000,0000,0000,,Presne, musím sa povzbudiť.
Dialogue: 0,0:43:36.90,0:43:39.10,Default,,0000,0000,0000,,Nehovorí sa, že keď žena dusí v sebe závisť,
Dialogue: 0,0:43:39.24,0:43:41.61,Default,,0000,0000,0000,,bude snežiť aj v Máji a Júni?
Dialogue: 0,0:43:41.77,0:43:43.47,Default,,0000,0000,0000,,Je to pravda?
Dialogue: 0,0:43:43.61,0:43:45.38,Default,,0000,0000,0000,,Samozrejme.
Dialogue: 0,0:43:45.58,0:43:47.65,Default,,0000,0000,0000,,Žena, ktorú opustil nejaký muž,\N hojí svoje rany,
Dialogue: 0,0:43:47.81,0:43:51.68,Default,,0000,0000,0000,,a potom zúspešní. Vieš koľko je príbehov na to?
Dialogue: 0,0:43:53.12,0:43:58.02,Default,,0000,0000,0000,,Tak, bude to platiť aj na HaeSung?
Dialogue: 0,0:43:58.29,0:44:00.76,Default,,0000,0000,0000,,Chceš ju poslať k vode?
Dialogue: 0,0:44:01.06,0:44:03.09,Default,,0000,0000,0000,,Odišiel som kvôli tebe.
Dialogue: 0,0:44:03.26,0:44:04.96,Default,,0000,0000,0000,,Nehovor jej také veci.
Dialogue: 0,0:44:04.96,0:44:07.77,Default,,0000,0000,0000,,Ty blbec, nikdy som si nemyslela, že si takýto typ človeka!\N Naozaj!
Dialogue: 0,0:44:07.93,0:44:09.40,Default,,0000,0000,0000,,Kto povedal, že to tak je?
Dialogue: 0,0:44:09.77,0:44:12.70,Default,,0000,0000,0000,,Prečo by som ju opúšťal?
Dialogue: 0,0:44:13.10,0:44:16.07,Default,,0000,0000,0000,,To možno ja budem ten opustený.
Dialogue: 0,0:44:37.50,0:44:38.90,Default,,0000,0000,0000,,Zaspieval som to dobre, však?
Dialogue: 0,0:44:42.20,0:44:45.87,Default,,0000,0000,0000,,Moja noha je už zdravá.
Dialogue: 0,0:44:47.27,0:44:48.97,Default,,0000,0000,0000,,Nie si smutná?
Dialogue: 0,0:44:49.14,0:44:50.41,Default,,0000,0000,0000,,Čože?
Dialogue: 0,0:44:50.61,0:44:52.31,Default,,0000,0000,0000,,Až kým sa mi neuzdraví noha,
Dialogue: 0,0:44:52.48,0:44:54.51,Default,,0000,0000,0000,,nedohodli sme sa, že dovtedy sa budeme stretávať?
Dialogue: 0,0:44:55.15,0:44:59.82,Default,,0000,0000,0000,,Presne.
Dialogue: 0,0:45:00.52,0:45:04.22,Default,,0000,0000,0000,,Ak teraz vyhrám, bude svetové turné.
Dialogue: 0,0:45:04.39,0:45:07.89,Default,,0000,0000,0000,,Neviem kedy sa vrátim naspäť do krajiny.
Dialogue: 0,0:45:08.06,0:45:09.79,Default,,0000,0000,0000,,Áno, pravda.
Dialogue: 0,0:45:09.99,0:45:14.37,Default,,0000,0000,0000,,Ak si to ty, určite to zvládneš.
Dialogue: 0,0:45:14.53,0:45:20.07,Default,,0000,0000,0000,,Povedala si, že ma chceš vidieť spievať a tancovať na javisku.
Dialogue: 0,0:45:20.24,0:45:23.74,Default,,0000,0000,0000,,Určite sa k tomu dostaneš.
Dialogue: 0,0:45:23.88,0:45:26.34,Default,,0000,0000,0000,,Ak teraz odídem,
Dialogue: 0,0:45:26.48,0:45:29.55,Default,,0000,0000,0000,,"počkaj na mňa, často sa ti ozvem".
Dialogue: 0,0:45:29.71,0:45:32.78,Default,,0000,0000,0000,,Nebuď taká.
Dialogue: 0,0:45:32.95,0:45:34.85,Default,,0000,0000,0000,,Nechaj ľudí, čo odchádzajú byť trochu v pokoii,
Dialogue: 0,0:45:35.02,0:45:37.12,Default,,0000,0000,0000,,ako ľudia, ktorých tu nechávaš.
Dialogue: 0,0:45:37.29,0:45:39.86,Default,,0000,0000,0000,,Rozdelme sa.
Dialogue: 0,0:45:39.96,0:45:43.43,Default,,0000,0000,0000,,Prečo? Prečo to hovoríš?
Dialogue: 0,0:45:43.56,0:45:45.83,Default,,0000,0000,0000,,Ak tiež máš dobrú príležitosť, ktorá čaká,
Dialogue: 0,0:45:46.00,0:45:49.80,Default,,0000,0000,0000,,nechám ťa ísť so šťastným srdcom.
Dialogue: 0,0:45:50.74,0:45:56.07,Default,,0000,0000,0000,,Keď som bol malý, často sme púšťali s otcom drakov.
Dialogue: 0,0:45:56.24,0:45:58.61,Default,,0000,0000,0000,,Až kým nevyšli do určitej výšky,
Dialogue: 0,0:45:58.78,0:46:02.98,Default,,0000,0000,0000,,vždy som ho poriadne nakreslil a pripevnil.
Dialogue: 0,0:46:03.15,0:46:05.45,Default,,0000,0000,0000,,Ale keď drak vyšiel moc vysoko,
Dialogue: 0,0:46:05.58,0:46:08.89,Default,,0000,0000,0000,,otec vždy roztrhol šnúru,
Dialogue: 0,0:46:09.02,0:46:14.83,Default,,0000,0000,0000,,aby mohol ísť tak hore, ako si srdce želá.
Dialogue: 0,0:46:15.23,0:46:17.90,Default,,0000,0000,0000,,Aj ty tak môžeš ísť, však?
Dialogue: 0,0:46:18.06,0:46:21.97,Default,,0000,0000,0000,,Môžem ťa nechať tak ísť, však?
Dialogue: 0,0:46:30.11,0:46:34.25,Default,,0000,0000,0000,,To je môj posledná darček pre teba.
Dialogue: 0,0:46:38.95,0:46:40.65,Default,,0000,0000,0000,,Dokončil si to!
Dialogue: 0,0:46:43.92,0:46:46.69,Default,,0000,0000,0000,,Vždy keď mi je zle, chytil som to,
Dialogue: 0,0:46:46.86,0:46:51.53,Default,,0000,0000,0000,,a vyriešil.
Dialogue: 0,0:47:27.03,0:47:28.40,Default,,0000,0000,0000,,Máš pravdu.
Dialogue: 0,0:47:28.57,0:47:29.83,Default,,0000,0000,0000,,Úplne sa zmenila.
Dialogue: 0,0:47:29.83,0:47:30.83,Default,,0000,0000,0000,,Však?
Dialogue: 0,0:47:31.97,0:47:33.67,Default,,0000,0000,0000,,Takže teraz,
Dialogue: 0,0:47:33.87,0:47:36.24,Default,,0000,0000,0000,,je čas pre kozmetiku tváre, pedikúru,
Dialogue: 0,0:47:36.44,0:47:38.78,Default,,0000,0000,0000,,a meranie váhy.
Dialogue: 0,0:47:39.04,0:47:42.71,Default,,0000,0000,0000,,Teraz je ten čas na tú kozmetiku.
Dialogue: 0,0:47:43.38,0:47:45.22,Default,,0000,0000,0000,,Vtedy, keď sme mali mesiac pred výstupom do médií,
Dialogue: 0,0:47:45.38,0:47:47.22,Default,,0000,0000,0000,,ani trochu si sa na nácvik nesnažila.
Dialogue: 0,0:47:47.39,0:47:49.29,Default,,0000,0000,0000,,Vravela si, že ti to zničí figúru.
Dialogue: 0,0:47:49.45,0:47:52.29,Default,,0000,0000,0000,,To bolo keď som bola zodpovedná za vzhľad HershE.
Dialogue: 0,0:47:52.46,0:47:53.66,Default,,0000,0000,0000,,A teraz?
Dialogue: 0,0:47:53.83,0:47:55.83,Default,,0000,0000,0000,,Volajte ma Lian-ce.\N-= Smeruje na Beyonce =-
Dialogue: 0,0:47:55.99,0:47:58.36,Default,,0000,0000,0000,,Lian-ce.
Dialogue: 0,0:47:58.56,0:48:00.67,Default,,0000,0000,0000,,Dostanem tvoju pozíciu.
Dialogue: 0,0:48:00.83,0:48:03.40,Default,,0000,0000,0000,,Idol schopností.
Dialogue: 0,0:48:04.84,0:48:07.37,Default,,0000,0000,0000,,Ak budeš takto postupovať, budeš prvá.
Dialogue: 0,0:48:07.54,0:48:10.41,Default,,0000,0000,0000,,Ja budem prvá.
Dialogue: 0,0:48:12.24,0:48:17.48,Default,,0000,0000,0000,,~ {\i1}Stále...počujem tvoj hlas.{\i0} ~
Dialogue: 0,0:48:17.48,0:48:23.25,Default,,0000,0000,0000,,~ {\i1}Stále...cítim dotyk tvojich rúk.{\i0} ~
Dialogue: 0,0:48:23.25,0:48:31.76,Default,,0000,0000,0000,,~ {\i1}Dnes, žijem bez tvojich stôp.{\i0} ~
Dialogue: 0,0:48:33.70,0:48:39.10,Default,,0000,0000,0000,,~ {\i1}Stále...ťa vidím.{\i0} ~
Dialogue: 0,0:48:39.10,0:48:44.57,Default,,0000,0000,0000,,~ {\i1}Stále...cítim tvoje teplo.{\i0} ~
Dialogue: 0,0:48:44.57,0:48:52.38,Default,,0000,0000,0000,,~ {\i1}Aj dnes, zostanem v tvojich spomienkach.{\i0} ~
Dialogue: 0,0:48:56.02,0:49:01.29,Default,,0000,0000,0000,,~ {\i1}V strede neznámych tvári\N prechádzajúc uličkami.{\i0} ~
Dialogue: 0,0:49:01.29,0:49:06.76,Default,,0000,0000,0000,,~ {\i1}Ešte aj pri padaní listov,\N tancujúce osamotene vo vetre.{\i0} ~
Dialogue: 0,0:49:06.76,0:49:12.30,Default,,0000,0000,0000,,~ {\i1}Aj vo vzduchu,\N ktorý mi škriabe líca v noci.{\i0} ~
Dialogue: 0,0:49:12.30,0:49:18.91,Default,,0000,0000,0000,,~ {\i1}Vo všetkom čo vidím, počujem a cítim, si ty. Presne. {\i0} ~
Dialogue: 0,0:49:18.91,0:49:24.45,Default,,0000,0000,0000,,~ {\i1}Ako ty, ako ty?{\i0} ~
Dialogue: 0,0:49:26.58,0:49:34.12,Default,,0000,0000,0000,,~ {\i1}Cítiš to isté, čo ja? Ako ty?{\i0} ~
Dialogue: 0,0:50:22.44,0:50:25.91,Default,,0000,0000,0000,,Vraciam ti to, tvoj poklad.
Dialogue: 0,0:50:27.41,0:50:33.18,Default,,0000,0000,0000,,Už som povedala, že to nepotrebujem.
Dialogue: 0,0:50:33.35,0:50:37.26,Default,,0000,0000,0000,,Sme priateľky, však?
Dialogue: 0,0:50:37.39,0:50:39.59,Default,,0000,0000,0000,,Shin HaeSung?
Dialogue: 0,0:50:39.76,0:50:42.26,Default,,0000,0000,0000,,Ohľadne hviezd...
Dialogue: 0,0:50:42.43,0:50:44.93,Default,,0000,0000,0000,,keď sú samé sú krásne,
Dialogue: 0,0:50:45.10,0:50:47.67,Default,,0000,0000,0000,,ale tie, čo sa zhlukujú spolu,
Dialogue: 0,0:50:47.80,0:50:51.77,Default,,0000,0000,0000,,tvoria nový príbeh, nové meno.
Dialogue: 0,0:50:51.94,0:50:53.50,Default,,0000,0000,0000,,Ako zoskupenie?
Dialogue: 0,0:50:55.71,0:50:59.28,Default,,0000,0000,0000,,Ak veľká žiarivá hviezda ako ty,
Dialogue: 0,0:50:59.41,0:51:02.91,Default,,0000,0000,0000,,a malá, zanedbateľná
Dialogue: 0,0:51:03.05,0:51:06.15,Default,,0000,0000,0000,,prašná hviezda ako ja sa stanú priateľkami,
Dialogue: 0,0:51:06.32,0:51:11.52,Default,,0000,0000,0000,,nevytvorí to nový príbeh?
Dialogue: 0,0:51:11.72,0:51:18.50,Default,,0000,0000,0000,,Shin HaeSung, si moja priateľka.
Dialogue: 0,0:51:18.66,0:51:20.77,Default,,0000,0000,0000,,Ten nový príbeh,
Dialogue: 0,0:51:20.93,0:51:24.24,Default,,0000,0000,0000,,keď nadíde čas, musíš mi o ňom povedať.
Dialogue: 0,0:51:37.18,0:51:40.15,Default,,0000,0000,0000,,Zaujíma ma jedna vec.
Dialogue: 0,0:51:40.28,0:51:42.85,Default,,0000,0000,0000,,Niečo na mne ťa zaujíma?
Dialogue: 0,0:51:48.09,0:51:51.16,Default,,0000,0000,0000,,Je jedno ako sa dívam, si príliš jednoduchá.
Dialogue: 0,0:51:51.36,0:51:55.67,Default,,0000,0000,0000,,A nie moc pekná.
Dialogue: 0,0:51:55.87,0:52:01.21,Default,,0000,0000,0000,,Ale, čo videl JB na tebe?
Dialogue: 0,0:52:01.37,0:52:06.61,Default,,0000,0000,0000,,Čo videl, čo ja nemám?
Dialogue: 0,0:52:06.78,0:52:08.55,Default,,0000,0000,0000,,Ktovie čo to je...
Dialogue: 0,0:52:08.71,0:52:11.92,Default,,0000,0000,0000,,Niečo na tom bude.
Dialogue: 0,0:52:12.05,0:52:15.25,Default,,0000,0000,0000,,Nepovieš mi to?
Dialogue: 0,0:52:17.42,0:52:18.82,Default,,0000,0000,0000,,Skús to vyriešiť.
Dialogue: 0,0:52:18.99,0:52:21.66,Default,,0000,0000,0000,,Keď to vyriešiš, poviem ti.
Dialogue: 0,0:52:25.40,0:52:29.60,Default,,0000,0000,0000,,Čo? Nie je to už vyriešené?
Dialogue: 0,0:52:29.77,0:52:34.44,Default,,0000,0000,0000,,Si presne ako tá vyriešená rubiková kocka.
Dialogue: 0,0:52:34.57,0:52:36.21,Default,,0000,0000,0000,,Čistá a uprataná.
Dialogue: 0,0:52:36.41,0:52:38.54,Default,,0000,0000,0000,,Dobre definovaná.
Dialogue: 0,0:52:38.68,0:52:41.38,Default,,0000,0000,0000,,Stačí sa na teba pozrieť,
Dialogue: 0,0:52:41.55,0:52:46.42,Default,,0000,0000,0000,,nemôžu ti nijak pomôcť.
Dialogue: 0,0:52:55.73,0:53:00.26,Default,,0000,0000,0000,,Ako toto, nechaj niekoho, nech naplní medzery.
Dialogue: 0,0:53:00.43,0:53:02.33,Default,,0000,0000,0000,,Aj keď sa pár krát zmýliš,
Dialogue: 0,0:53:02.50,0:53:06.07,Default,,0000,0000,0000,,znovu to vyriešiš.
Dialogue: 0,0:53:12.91,0:53:16.35,Default,,0000,0000,0000,,Wow, javisková hala je tak veľká!
Dialogue: 0,0:53:16.48,0:53:18.25,Default,,0000,0000,0000,,Vyzerá to, ako keby sme mali nejaký sľub, však?
Dialogue: 0,0:53:20.52,0:53:22.35,Default,,0000,0000,0000,,Choď za nimi.
Dialogue: 0,0:53:22.52,0:53:25.09,Default,,0000,0000,0000,,Nie si predsa učiteľom angličtiny?
Dialogue: 0,0:53:25.26,0:53:28.96,Default,,0000,0000,0000,,Hovorím getovou angličtinou.
Dialogue: 0,0:53:30.80,0:53:32.70,Default,,0000,0000,0000,,To potom nefunguje?
Dialogue: 0,0:53:40.51,0:53:43.24,Default,,0000,0000,0000,,Moc jasné.
Dialogue: 0,0:53:44.64,0:53:48.01,Default,,0000,0000,0000,,Môj otec rád robí graciózne veci,
Dialogue: 0,0:53:48.18,0:53:49.11,Default,,0000,0000,0000,,to je jeho problém.
Dialogue: 0,0:53:49.28,0:53:51.48,Default,,0000,0000,0000,,Hen, ako je javisková hala vyjasnená.
Dialogue: 0,0:53:51.62,0:53:53.55,Default,,0000,0000,0000,,Minuli sa na to peniaze.
Dialogue: 0,0:53:53.72,0:53:56.75,Default,,0000,0000,0000,,Naša Seul je už skromná?
Dialogue: 0,0:53:57.16,0:53:59.39,Default,,0000,0000,0000,,Si veľmi pozorná.
Dialogue: 0,0:53:59.56,0:54:00.73,Default,,0000,0000,0000,,Bojím sa, že nebudeš poriadne zarábať,
Dialogue: 0,0:54:00.93,0:54:03.66,Default,,0000,0000,0000,,a namiesto toho o peniaze prídeš.
Dialogue: 0,0:54:03.83,0:54:06.63,Default,,0000,0000,0000,,Nedôveruješ oppa?
Dialogue: 0,0:54:06.80,0:54:09.47,Default,,0000,0000,0000,,Ako ti môžem veriť?
Dialogue: 0,0:54:10.37,0:54:11.84,Default,,0000,0000,0000,,Hej, poďme.
Dialogue: 0,0:54:12.00,0:54:13.70,Default,,0000,0000,0000,,Čo sa deje?
Dialogue: 0,0:54:27.89,0:54:29.49,Default,,0000,0000,0000,,Poďme.
Dialogue: 0,0:54:52.41,0:54:54.45,Default,,0000,0000,0000,,Rada to ješ, však? Skús to.
Dialogue: 0,0:54:54.88,0:54:58.32,Default,,0000,0000,0000,,Áno...
Dialogue: 0,0:54:58.32,0:54:59.82,Default,,0000,0000,0000,,Prečo si nepozvala JB-a?
Dialogue: 0,0:54:59.95,0:55:01.29,Default,,0000,0000,0000,,On je veľmi rád.
Dialogue: 0,0:55:01.45,0:55:03.39,Default,,0000,0000,0000,,Nevystupujte sa dnes?
Dialogue: 0,0:55:03.52,0:55:05.32,Default,,0000,0000,0000,,JB sa na vystúpenie musí pripraviť.
Dialogue: 0,0:55:05.49,0:55:07.66,Default,,0000,0000,0000,,Pravda.
Dialogue: 0,0:55:07.83,0:55:09.53,Default,,0000,0000,0000,,Je mu už lepšie?
Dialogue: 0,0:55:09.69,0:55:12.40,Default,,0000,0000,0000,,Je mu fajn.
Dialogue: 0,0:55:12.60,0:55:13.63,Default,,0000,0000,0000,,Najedz sa.
Dialogue: 0,0:55:13.80,0:55:18.54,Default,,0000,0000,0000,,Unnie, keď pôjdeš preč, nezabúdaj na svoje korene.
Dialogue: 0,0:55:18.74,0:55:20.40,Default,,0000,0000,0000,,Kde si sa naučila také divné slová?
Dialogue: 0,0:55:20.61,0:55:24.24,Default,,0000,0000,0000,,Prečo ma nenecháš hovoriť?
Dialogue: 0,0:56:01.41,0:56:02.98,Default,,0000,0000,0000,,Zahrajme si.
Dialogue: 0,0:56:03.15,0:56:05.08,Default,,0000,0000,0000,,Nejdeš na vystúpenie?
Dialogue: 0,0:56:05.22,0:56:06.45,Default,,0000,0000,0000,,Čo tu robíš?
Dialogue: 0,0:56:06.62,0:56:09.52,Default,,0000,0000,0000,,A čo tu robíš ty?
Dialogue: 0,0:56:09.69,0:56:12.82,Default,,0000,0000,0000,,Nie je to tvoja vec.
Dialogue: 0,0:56:13.89,0:56:16.89,Default,,0000,0000,0000,,Vsaďme sa.
Dialogue: 0,0:56:17.03,0:56:18.16,Default,,0000,0000,0000,,Strielajme lopty.
Dialogue: 0,0:56:18.33,0:56:21.77,Default,,0000,0000,0000,,O čo sa teraz vsadíme?
Dialogue: 0,0:56:21.90,0:56:23.23,Default,,0000,0000,0000,,Vyprevadenie HaeSung.
Dialogue: 0,0:56:23.37,0:56:25.54,Default,,0000,0000,0000,,Poriadne.
Dialogue: 0,0:56:25.80,0:56:28.14,Default,,0000,0000,0000,,HaeSung ide na letisko.
Dialogue: 0,0:56:28.71,0:56:30.58,Default,,0000,0000,0000,,Viem.
Dialogue: 0,0:56:30.78,0:56:33.58,Default,,0000,0000,0000,,Vieš a stále si tu?
Dialogue: 0,0:56:33.71,0:56:35.75,Default,,0000,0000,0000,,Hanebne sa jej vyhýbať,
Dialogue: 0,0:56:35.88,0:56:38.88,Default,,0000,0000,0000,,sa nepovažuje za slovo "poriadne".
Dialogue: 0,0:56:39.02,0:56:42.79,Default,,0000,0000,0000,,Nechaj ju ísť s prázdnou mysľou.
Dialogue: 0,0:56:43.65,0:56:46.29,Default,,0000,0000,0000,,Poriadne sa rozlúč.
Dialogue: 0,0:56:46.46,0:56:48.39,Default,,0000,0000,0000,,A čo ty?
Dialogue: 0,0:56:48.59,0:56:51.33,Default,,0000,0000,0000,,Prečo nejdeš na letisko?
Dialogue: 0,0:56:51.50,0:56:55.60,Default,,0000,0000,0000,,Sú kamaráti a milenci to isté?
Dialogue: 0,0:56:55.77,0:56:58.60,Default,,0000,0000,0000,,Prečo by som tam išiel?
Dialogue: 0,0:57:00.87,0:57:02.41,Default,,0000,0000,0000,,Choď!
Dialogue: 0,0:57:02.77,0:57:05.34,Default,,0000,0000,0000,,Keď príde čas, neľutuj to.
Dialogue: 0,0:57:21.46,0:57:23.39,Default,,0000,0000,0000,,Zase široký hod.
Dialogue: 0,0:57:24.60,0:57:26.30,Default,,0000,0000,0000,,Keď príde čas na rozhodnutosť,
Dialogue: 0,0:57:26.46,0:57:28.30,Default,,0000,0000,0000,,vždy som mimo.
Dialogue: 0,0:57:32.54,0:57:35.84,Default,,0000,0000,0000,,Čo si to tam mumleš?
Dialogue: 0,0:57:37.01,0:57:39.94,Default,,0000,0000,0000,,Jin YuJin, si kúl.
Dialogue: 0,0:57:40.08,0:57:41.35,Default,,0000,0000,0000,,Cítiš sa lepšie,
Dialogue: 0,0:57:41.51,0:57:44.32,Default,,0000,0000,0000,,keď sa takým snažíš byť?
Dialogue: 0,0:57:44.48,0:57:46.62,Default,,0000,0000,0000,,Moja nálada je veľmi dobrá.
Dialogue: 0,0:57:46.78,0:57:47.59,Default,,0000,0000,0000,,Áno?
Dialogue: 0,0:57:47.75,0:57:49.55,Default,,0000,0000,0000,,Moja kondícia je tiež najlepšia.
Dialogue: 0,0:57:49.69,0:57:51.72,Default,,0000,0000,0000,,No ako? Zahráte si?
Dialogue: 0,0:57:51.86,0:57:53.66,Default,,0000,0000,0000,,Zabudni na to.
Dialogue: 0,0:57:53.83,0:57:55.39,Default,,0000,0000,0000,,Keď sa na teba pozerám, ako keby si išiel plakať.
Dialogue: 0,0:57:55.56,0:57:58.30,Default,,0000,0000,0000,,Kto? Ja?
Dialogue: 0,0:57:58.46,0:58:03.03,Default,,0000,0000,0000,,Ako dlho ho plánuješ nasledovať?
Dialogue: 0,0:58:04.64,0:58:07.91,Default,,0000,0000,0000,,Chceš byť náhradníkom celý život?
Dialogue: 0,0:58:08.07,0:58:10.78,Default,,0000,0000,0000,,Ak budeš takto pokračovať,
Dialogue: 0,0:58:10.94,0:58:16.45,Default,,0000,0000,0000,,nikdy neuvidíš, čo je pred ním.
Dialogue: 0,0:58:26.89,0:58:31.50,Default,,0000,0000,0000,,Dnes je finálny výber Super Idola,
Dialogue: 0,0:58:31.70,0:58:34.07,Default,,0000,0000,0000,,posledný konkurz.
Dialogue: 0,0:58:34.23,0:58:35.87,Default,,0000,0000,0000,,Finálny víťaz,
Dialogue: 0,0:58:36.03,0:58:38.04,Default,,0000,0000,0000,,môže vydať singel,
Dialogue: 0,0:58:38.20,0:58:41.87,Default,,0000,0000,0000,,a zúčastniť sa EMG svetového turné.
Dialogue: 0,0:58:42.04,0:58:43.67,Default,,0000,0000,0000,,Ako som povedal,
Dialogue: 0,0:58:43.81,0:58:49.81,Default,,0000,0000,0000,,hodnotenie sa skladá z dvoch častí, tancovania a spevu.
Dialogue: 0,0:58:54.62,0:58:56.62,Default,,0000,0000,0000,,Ste pripravení?
Dialogue: 0,0:58:56.79,0:58:58.29,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:58:58.46,0:59:00.22,Default,,0000,0000,0000,,Kde je JB?
Dialogue: 0,0:59:00.39,0:59:02.89,Default,,0000,0000,0000,,YuJin tu tiež nie je.
Dialogue: 0,0:59:03.06,0:59:06.06,Default,,0000,0000,0000,,Videl ich niekto?
Dialogue: 0,0:59:17.98,0:59:23.55,Default,,0000,0000,0000,,Ak budeš mať problémy,\N nedus to tvrdohlavo v sebe, ale zavolaj mi.
Dialogue: 0,0:59:24.25,0:59:26.58,Default,,0000,0000,0000,,Ďakujem oci.
Dialogue: 0,0:59:26.75,0:59:30.99,Default,,0000,0000,0000,,Určite sa stanem vynikajúcou skladateľkou.
Dialogue: 0,0:59:31.26,0:59:34.03,Default,,0000,0000,0000,,Raz som v televízií videl,
Dialogue: 0,0:59:34.23,0:59:36.63,Default,,0000,0000,0000,,že Eskimáci, ktorí žijú na Severnom Póle,
Dialogue: 0,0:59:36.79,0:59:39.53,Default,,0000,0000,0000,,nepoznajú slovo vynikajúci.
Dialogue: 0,0:59:39.73,0:59:41.93,Default,,0000,0000,0000,,Je jedno či si človekom, alebo veľrybou,
Dialogue: 0,0:59:42.10,0:59:44.94,Default,,0000,0000,0000,,môžeš iba viesť život tak ako dokážeš.
Dialogue: 0,0:59:45.10,0:59:47.81,Default,,0000,0000,0000,,To, že tu nie sú vynikajúci Eskimáci,
Dialogue: 0,0:59:47.97,0:59:50.98,Default,,0000,0000,0000,,je, že tu nie sú ani vynikajúce veľryby.
Dialogue: 0,0:59:51.14,0:59:55.95,Default,,0000,0000,0000,,Takže nemusíš sa namáhať a byť vynikajúcou osobou.
Dialogue: 0,0:59:56.11,1:00:00.89,Default,,0000,0000,0000,,Stačí keď budeš mojou dcérou Shin HaeSung,\N aby som bol pokojný.
Dialogue: 0,1:00:01.05,1:00:03.76,Default,,0000,0000,0000,,Keď je to niečo, čo moja dcéra HaeSung robí,
Dialogue: 0,1:00:03.92,1:00:06.72,Default,,0000,0000,0000,,bezpodmienečne to podporím.
Dialogue: 0,1:00:09.26,1:00:11.20,Default,,0000,0000,0000,,Oci.
Dialogue: 0,1:00:11.76,1:00:13.63,Default,,0000,0000,0000,,Unnie, veľa šťastia!
Dialogue: 0,1:00:39.76,1:00:43.46,Default,,0000,0000,0000,,Hej, mysleli sme si, že neprídeš, vystrašil si nás.
Dialogue: 0,1:00:43.63,1:00:46.10,Default,,0000,0000,0000,,Ako môže hlavný kandidát chýbať?
Dialogue: 0,1:00:46.26,1:00:47.43,Default,,0000,0000,0000,,Som jediným hlavným kandidátom?
Dialogue: 0,1:00:47.57,1:00:49.23,Default,,0000,0000,0000,,Všetci ste kandidátmi.
Dialogue: 0,1:00:49.40,1:00:55.01,Default,,0000,0000,0000,,Áno, uvidíme komu sa pošťastí?\N Musíme počkať a dočkať sa.
Dialogue: 0,1:00:56.84,1:01:00.55,Default,,0000,0000,0000,,A čo JB? Neprišli ste spolu?
Dialogue: 0,1:02:39.28,1:02:42.15,Default,,0000,0000,0000,,{\i1}Vtedy, keď si povedal, že ma máš rád,{\i0}
Dialogue: 0,1:02:42.31,1:02:45.82,Default,,0000,0000,0000,,{\i1}spýtala som sa na dôvod, však?{\i0}
Dialogue: 0,1:02:45.95,1:02:48.19,Default,,0000,0000,0000,,{\i1}Vždy som taká bola.{\i0}
Dialogue: 0,1:02:48.35,1:02:50.66,Default,,0000,0000,0000,,{\i1}Ak nemám čistú odpoveď,{\i0}
Dialogue: 0,1:02:50.82,1:02:52.26,Default,,0000,0000,0000,,{\i1}cítim sa nepokojne.{\i0}
Dialogue: 0,1:02:52.42,1:02:53.39,Default,,0000,0000,0000,,{\i1}Ale.{\i0}
Dialogue: 0,1:02:53.56,1:02:57.20,Default,,0000,0000,0000,,Toto je posledný darček odo mňa.
Dialogue: 0,1:02:57.36,1:02:59.40,Default,,0000,0000,0000,,{\i1}Keď si mi dal vyriešenú rubikovú kocku,{\i0}
Dialogue: 0,1:02:59.56,1:03:01.10,Default,,0000,0000,0000,,{\i1}bola som veľmi šťastná.{\i0}
Dialogue: 0,1:03:04.47,1:03:07.91,Default,,0000,0000,0000,,{\i1}Ale, zrazu som pochopila.{\i0}
Dialogue: 0,1:03:08.07,1:03:12.31,Default,,0000,0000,0000,,{\i1}Kým si ťažko pracoval na hľadanie odpovede,{\i0}
Dialogue: 0,1:03:12.48,1:03:15.61,Default,,0000,0000,0000,,{\i1}nikdy som sa ťa nespýtala.{\i0}
Dialogue: 0,1:03:15.75,1:03:17.42,Default,,0000,0000,0000,,{\i1}Aj keď je už príliš neskoro,{\i0}
Dialogue: 0,1:03:17.55,1:03:19.32,Default,,0000,0000,0000,,{\i1}Chcem sa strašne moc spýtať,{\i0}
Dialogue: 0,1:03:19.48,1:03:21.79,Default,,0000,0000,0000,,{\i1}a potom nájsť odpoveď.{\i0}
Dialogue: 0,1:03:21.92,1:03:24.42,Default,,0000,0000,0000,,{\i1}Je to príbeh, ktorý som našla v knihe.{\i0}
Dialogue: 0,1:03:24.59,1:03:26.56,Default,,0000,0000,0000,,{\i1}Boh pošle ľuďom{\i0}
Dialogue: 0,1:03:26.72,1:03:28.89,Default,,0000,0000,0000,,{\i1}nedokonalú rubikovú kocku,{\i0}
Dialogue: 0,1:03:29.03,1:03:32.20,Default,,0000,0000,0000,,{\i1}so zádrhelom, ktorý nikdy nevyriešiš.{\i0}
Dialogue: 0,1:03:32.40,1:03:38.00,Default,,0000,0000,0000,,{\i1}Ak posledným zádrhelom, ktorý nie je dovolený{\i0}
Dialogue: 0,1:03:38.17,1:03:41.51,Default,,0000,0000,0000,,{\i1}si ty,{\i0}
Dialogue: 0,1:03:41.67,1:03:44.24,Default,,0000,0000,0000,,{\i1}keď nadíde čas, čo mám robiť?{\i0}
Dialogue: 0,1:03:54.35,1:03:55.85,Default,,0000,0000,0000,,Shin HaeSung.
Dialogue: 0,1:04:11.84,1:04:14.31,Default,,0000,0000,0000,,Prečo si prišiel?
Dialogue: 0,1:04:14.47,1:04:16.47,Default,,0000,0000,0000,,Nezačal už konkurz?
Dialogue: 0,1:04:16.61,1:04:18.94,Default,,0000,0000,0000,,Nepochop to zle,
Dialogue: 0,1:04:19.11,1:04:23.45,Default,,0000,0000,0000,,neprišiel som ťa zadržať.
Dialogue: 0,1:04:24.58,1:04:30.89,Default,,0000,0000,0000,,Prišiel som, aby som ti povedal, že noha sa úplne vyliečila.
Dialogue: 0,1:04:31.06,1:04:33.99,Default,,0000,0000,0000,,Videla si ma bežať, však?
Dialogue: 0,1:04:34.16,1:04:39.10,Default,,0000,0000,0000,,Tak sa o mňa už nemusíš báť.
Dialogue: 0,1:04:39.56,1:04:42.67,Default,,0000,0000,0000,,Teraz, sa staraj iba o seba.
Dialogue: 0,1:04:42.83,1:04:46.27,Default,,0000,0000,0000,,Mysli na seba.
Dialogue: 0,1:04:46.47,1:04:51.44,Default,,0000,0000,0000,,Nebudem sa o teba báť.
Dialogue: 0,1:04:51.64,1:04:56.35,Default,,0000,0000,0000,,Naozaj? Tak je dobre.
Dialogue: 0,1:04:58.65,1:05:01.65,Default,,0000,0000,0000,,Povedala si, že sa nebudeš o mňa už báť,\N tak prečo plačeš?
Dialogue: 0,1:05:01.82,1:05:06.29,Default,,0000,0000,0000,,Asi mám niečo v oku.
Dialogue: 0,1:05:06.49,1:05:09.79,Default,,0000,0000,0000,,Už som povedal, že si škaredá keď plačeš.
Dialogue: 0,1:05:10.96,1:05:14.43,Default,,0000,0000,0000,,Neplač. Prestaň.
Dialogue: 0,1:05:16.97,1:05:19.34,Default,,0000,0000,0000,,Ak budeš plakať odídem.
Dialogue: 0,1:05:23.98,1:05:27.85,Default,,0000,0000,0000,,To nie.
Dialogue: 0,1:05:28.11,1:05:29.66,Default,,0000,0000,0000,,Už neplačem.
