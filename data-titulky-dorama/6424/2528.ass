Dialogue: 0,0:00:01.00,0:00:08.00,Default,,0000,0000,0000,,SK Titulky: seesska.blog.cz/\NHyumi\N ENG Titulky: DramaFever
Dialogue: 0,0:00:09.60,0:00:11.41,Default,,0000,0000,0000,,Tuším sa z teba dnes stane hviezda.
Dialogue: 0,0:00:11.41,0:00:13.81,Default,,0000,0000,0000,,Čo to za hviezdu?
Dialogue: 0,0:00:13.81,0:00:16.61,Default,,0000,0000,0000,,Nie vďaka tebe skončíme tréning?
Dialogue: 0,0:00:17.61,0:00:20.71,Default,,0000,0000,0000,,Nuž...spravila som niečo dobré.
Dialogue: 0,0:00:20.71,0:00:22.82,Default,,0000,0000,0000,,A zase...
Dialogue: 0,0:00:32.93,0:00:36.63,Default,,0000,0000,0000,,Naozaj vedia, že som našla ostatok krabíc?
Dialogue: 0,0:00:36.63,0:00:37.93,Default,,0000,0000,0000,,Je to možné?
Dialogue: 0,0:00:37.93,0:00:40.63,Default,,0000,0000,0000,,Asi len majú kempový ohňostroj.
Dialogue: 0,0:00:40.63,0:00:42.54,Default,,0000,0000,0000,,Poďme a uvidíme.
Dialogue: 0,0:00:43.44,0:00:45.94,Default,,0000,0000,0000,,V takomto chladnom počasí,
Dialogue: 0,0:00:45.94,0:00:47.64,Default,,0000,0000,0000,,v takýto krásny čas,
Dialogue: 0,0:00:47.64,0:00:49.54,Default,,0000,0000,0000,,každý trpel.
Dialogue: 0,0:00:49.54,0:00:51.84,Default,,0000,0000,0000,,Všetci samôžete vrátiť a oddýchnuť si.
Dialogue: 0,0:00:51.84,0:00:53.25,Default,,0000,0000,0000,,Zobuďte sa o ôsmej ráno
Dialogue: 0,0:00:53.25,0:00:54.75,Default,,0000,0000,0000,,aby ste stihli autobus do školy.
Dialogue: 0,0:00:54.75,0:00:56.25,Default,,0000,0000,0000,,Skvelá práca.
Dialogue: 0,0:01:08.86,0:01:10.26,Default,,0000,0000,0000,,Pane.
Dialogue: 0,0:01:10.26,0:01:12.27,Default,,0000,0000,0000,,Našli sme tie krabice.
Dialogue: 0,0:01:12.27,0:01:14.97,Default,,0000,0000,0000,,Môžme Vám ich teraz dať?
Dialogue: 0,0:01:16.97,0:01:18.97,Default,,0000,0000,0000,,Neviem, kto tie krabice vlastní.
Dialogue: 0,0:01:30.28,0:01:32.59,Default,,0000,0000,0000,,{\i1}Všetci sa prezlečte a von do nočného tréningu,{\i0}
Dialogue: 0,0:01:32.59,0:01:34.69,Default,,0000,0000,0000,,{\i1}a zhromaždite sa.{\i0}
Dialogue: 0,0:01:38.39,0:01:40.19,Default,,0000,0000,0000,,A že vraj sme kamaráti.
Dialogue: 0,0:01:46.30,0:01:48.40,Default,,0000,0000,0000,,Prepáčte.
Dialogue: 0,0:01:49.80,0:01:52.21,Default,,0000,0000,0000,,Naozaj mi je to ľúto.
Dialogue: 0,0:01:53.31,0:01:56.31,Default,,0000,0000,0000,,Dala si veľa energie to ich nájdenia,
Dialogue: 0,0:01:56.31,0:01:59.11,Default,,0000,0000,0000,,a nepočula si ani jediné ďakujem.\N Prečo sa ospravedlňuješ?
Dialogue: 0,0:02:04.62,0:02:07.32,Default,,0000,0000,0000,,Dávam ti poslednú šancu.
Dialogue: 0,0:02:09.02,0:02:12.63,Default,,0000,0000,0000,,Naozaj som nič nespravila s tvojim mikrofónom.
Dialogue: 0,0:02:12.73,0:02:15.43,Default,,0000,0000,0000,,Ty si to spravila, že?
Dialogue: 0,0:02:16.93,0:02:20.73,Default,,0000,0000,0000,,Som nenávidenou osobou, ktorá robí triky!
Dialogue: 0,0:02:20.83,0:02:24.24,Default,,0000,0000,0000,,Je to tak očividné,
Dialogue: 0,0:02:24.24,0:02:26.74,Default,,0000,0000,0000,,a stále to zapieraš?
Dialogue: 0,0:02:28.44,0:02:31.54,Default,,0000,0000,0000,,Ak prezradím čo si spravila,
Dialogue: 0,0:02:31.54,0:02:34.35,Default,,0000,0000,0000,,tiež budeš takáto.
Dialogue: 0,0:02:35.65,0:02:38.25,Default,,0000,0000,0000,,Prepáčte.
Dialogue: 0,0:02:51.76,0:02:54.47,Default,,0000,0000,0000,,Naozaj som dlho trénovala,
Dialogue: 0,0:02:54.47,0:02:58.07,Default,,0000,0000,0000,,ale keď som vystúpila na javisko, dostala som strach.
Dialogue: 0,0:02:58.87,0:03:03.08,Default,,0000,0000,0000,,Tak som z mikrofónu vytiahla batérie.
Dialogue: 0,0:03:04.18,0:03:08.08,Default,,0000,0000,0000,,Kvôli tomuto, priateľka bola neprávom obvinená.
Dialogue: 0,0:03:08.08,0:03:12.19,Default,,0000,0000,0000,,Chcela som povedať pravdu,\N ale nenašla som odvahu.
Dialogue: 0,0:03:13.59,0:03:15.39,Default,,0000,0000,0000,,Prestaň.
Dialogue: 0,0:03:15.39,0:03:19.09,Default,,0000,0000,0000,,Ak poprosím otca o odpustenie,\N bude všetko v poriadku?
Dialogue: 0,0:03:19.09,0:03:23.40,Default,,0000,0000,0000,,Oci, prosím modli sa pre svoju naničhodnú dcéru.
Dialogue: 0,0:03:24.80,0:03:27.00,Default,,0000,0000,0000,,Tento list,
Dialogue: 0,0:03:27.00,0:03:28.80,Default,,0000,0000,0000,,je tvoj?
Dialogue: 0,0:03:30.00,0:03:32.91,Default,,0000,0000,0000,,Dnes si to napsala otcovi.
Dialogue: 0,0:03:43.82,0:03:45.72,Default,,0000,0000,0000,,Ten list...
Dialogue: 0,0:03:47.12,0:03:48.72,Default,,0000,0000,0000,,áno je.
Dialogue: 0,0:03:59.73,0:04:01.83,Default,,0000,0000,0000,,Prepáč.
Dialogue: 0,0:04:12.55,0:04:14.65,Default,,0000,0000,0000,,Už je neskoro.
Dialogue: 0,0:04:19.55,0:04:21.35,Default,,0000,0000,0000,,Vedela si?
Dialogue: 0,0:04:22.66,0:04:26.76,Default,,0000,0000,0000,,Malo by sa hrešiť ľudí, čo spravia chybu,
Dialogue: 0,0:04:26.76,0:04:29.96,Default,,0000,0000,0000,,ale ľudia ako ty, ktorí spravia takéto chyby,
Dialogue: 0,0:04:29.96,0:04:32.77,Default,,0000,0000,0000,,namiesto toho dostávajú sympatie,\N keď si to nezaslúžia.
Dialogue: 0,0:04:34.47,0:04:36.67,Default,,0000,0000,0000,,Chcela si získať sympatie?
Dialogue: 0,0:04:38.87,0:04:42.08,Default,,0000,0000,0000,,Si dobrá.
Dialogue: 0,0:04:42.78,0:04:45.68,Default,,0000,0000,0000,,Keby som to bola ja, radšej by som bola\N keby ma vyhrešili.
Dialogue: 0,0:04:45.68,0:04:47.68,Default,,0000,0000,0000,,To ja som ju zastavil.
Dialogue: 0,0:04:49.18,0:04:52.19,Default,,0000,0000,0000,,Zastavil som ju a nedovolil jej povedať pravdu.
Dialogue: 0,0:04:53.89,0:04:55.99,Default,,0000,0000,0000,,Prečo?
Dialogue: 0,0:04:56.19,0:04:59.69,Default,,0000,0000,0000,,Pretože si tiež neurobila nič dobrého.
Dialogue: 0,0:05:00.79,0:05:04.40,Default,,0000,0000,0000,,Keď Shin HaeSung trénovala,\N trénovala si s ňou vôbec?
Dialogue: 0,0:05:04.40,0:05:06.90,Default,,0000,0000,0000,,Prečo sa do toho zamiešavaš?
Dialogue: 0,0:05:06.90,0:05:11.40,Default,,0000,0000,0000,,Ak nechceš, aby sa do toho iní montovali,\N tak by ste to mali vyriešiť v tichosti.
Dialogue: 0,0:05:12.11,0:05:16.31,Default,,0000,0000,0000,,Budeš sa cítiť dobre, keď ju pred ostatnými strápniš?
Dialogue: 0,0:05:17.71,0:05:23.02,Default,,0000,0000,0000,,Neobviňovali ma všetci?
Dialogue: 0,0:05:23.02,0:05:26.22,Default,,0000,0000,0000,,Tak musím odhaliť pravdu pred všetkými.
Dialogue: 0,0:05:26.22,0:05:28.62,Default,,0000,0000,0000,,Teraz keď si veci vyriešila, tak by stačilo.
Dialogue: 0,0:05:36.13,0:05:38.23,Default,,0000,0000,0000,,Môžeš utiecť.
Dialogue: 0,0:05:39.63,0:05:43.04,Default,,0000,0000,0000,,Neutiekla si aj keď si bola na javisku?
Dialogue: 0,0:05:45.04,0:05:47.94,Default,,0000,0000,0000,,Prečo sa chceš stať speváčkou?
Dialogue: 0,0:05:49.14,0:05:51.54,Default,,0000,0000,0000,,Keby som bola tebou...
Dialogue: 0,0:05:51.54,0:05:55.45,Default,,0000,0000,0000,,na ten sen by som zabudla.
Dialogue: 0,0:06:25.18,0:06:26.98,Default,,0000,0000,0000,,Prečo sa HaeSung tak správala?
Dialogue: 0,0:06:26.98,0:06:29.68,Default,,0000,0000,0000,,Niet jej pomoci.
Dialogue: 0,0:06:30.38,0:06:33.69,Default,,0000,0000,0000,,Lian, vzala obvinenie bez slova,
Dialogue: 0,0:06:33.69,0:06:35.79,Default,,0000,0000,0000,,nie je taká zlá.
Dialogue: 0,0:06:35.79,0:06:39.09,Default,,0000,0000,0000,,Presne, ani sa nehádala, že to nespravila.
Dialogue: 0,0:06:39.09,0:06:40.19,Default,,0000,0000,0000,,Má dobré srdce.
Dialogue: 0,0:06:45.00,0:06:46.60,Default,,0000,0000,0000,,JB?
Dialogue: 0,0:06:47.20,0:06:49.50,Default,,0000,0000,0000,,Povedal riaditeľovi o tvojej situácií,
Dialogue: 0,0:06:49.50,0:06:51.80,Default,,0000,0000,0000,,a povedal, že chce aby si bola vo vyššej triede.
Dialogue: 0,0:06:51.80,0:06:54.31,Default,,0000,0000,0000,,Ako my.
Dialogue: 0,0:06:55.91,0:06:57.91,Default,,0000,0000,0000,,Prečo sa zrazu o mňa tak stará?
Dialogue: 0,0:06:57.91,0:07:00.31,Default,,0000,0000,0000,,Nestaral sa tak o teba JB vždy?
Dialogue: 0,0:07:01.51,0:07:03.52,Default,,0000,0000,0000,,Povedal riaditeľ niečo?
Dialogue: 0,0:07:03.52,0:07:07.32,Default,,0000,0000,0000,,Nájde možnosť, aby si sa tam dostala.
Dialogue: 0,0:07:08.12,0:07:10.32,Default,,0000,0000,0000,,Nájde možnosť?
Dialogue: 0,0:07:25.54,0:07:26.44,Default,,0000,0000,0000,,( Dôvod k vyhodeniu )
Dialogue: 0,0:07:26.44,0:07:28.44,Default,,0000,0000,0000,,( Na skúške koncom mesiaca kvôli vybratiu batérie z mikrofónu\N spôsobilo problémy pri vystupovani )
Dialogue: 0,0:07:28.44,0:07:30.44,Default,,0000,0000,0000,,( Ako trest bude 15 dňové vylúčenie zo školy  )
Dialogue: 0,0:07:41.05,0:07:42.96,Default,,0000,0000,0000,,Suspendovaná na 15 dní.
Dialogue: 0,0:08:06.78,0:08:09.48,Default,,0000,0000,0000,,Toto...
Dialogue: 0,0:08:09.48,0:08:12.89,Default,,0000,0000,0000,,Našla som to na cvičisku.
Dialogue: 0,0:08:13.99,0:08:16.69,Default,,0000,0000,0000,,Je to tvoje, že?
Dialogue: 0,0:08:22.80,0:08:25.70,Default,,0000,0000,0000,,Už to nechcem, tak som to vyhodila.
Dialogue: 0,0:08:27.30,0:08:31.20,Default,,0000,0000,0000,,Keď umývaš dlážku,\N len to zdvihni a zahoď.
Dialogue: 0,0:08:32.91,0:08:37.41,Default,,0000,0000,0000,,Dnes, nevidím tvoj šťastný deň.
Dialogue: 0,0:09:16.85,0:09:18.85,Default,,0000,0000,0000,,Nezotiera sa to dáko ľahko?
Dialogue: 0,0:10:06.60,0:10:08.70,Default,,0000,0000,0000,,To ma nevidíš?
Dialogue: 0,0:10:08.70,0:10:12.91,Default,,0000,0000,0000,,{\i1}Nenechám nikomu vedieť, že si moja dcéra,\N vydrž iba rok. Dobre?{\i0}
Dialogue: 0,0:10:16.81,0:10:18.11,Default,,0000,0000,0000,,Vrchný riaditeľ.
Dialogue: 0,0:10:20.61,0:10:24.22,Default,,0000,0000,0000,,Chcem naozaj učiť žiakov z vyššej triedy\N aspoň raz.
Dialogue: 0,0:10:24.92,0:10:28.52,Default,,0000,0000,0000,,Nie sú žiaci z nižšej triedy rovnako vzácni?
Dialogue: 0,0:10:28.52,0:10:29.82,Default,,0000,0000,0000,,Predtým než sa rozhodnete,
Dialogue: 0,0:10:29.82,0:10:32.53,Default,,0000,0000,0000,,toto sú pesničky, ktoré zložili študenti, ktorých som učila.
Dialogue: 0,0:10:32.53,0:10:34.43,Default,,0000,0000,0000,,Aspoň raz si ich vypočujte.
Dialogue: 0,0:10:35.13,0:10:39.33,Default,,0000,0000,0000,,Žiaci, čo nič nevedeli to dotiahli celkom ďaleko\N kým som ich učila.
Dialogue: 0,0:10:39.33,0:10:41.33,Default,,0000,0000,0000,,Ak budem viesť študentov z vyššej triedy,
Dialogue: 0,0:10:41.33,0:10:44.04,Default,,0000,0000,0000,,verím tomu, že sa zlepšia ešte rýchlejšie.
Dialogue: 0,0:10:44.04,0:10:45.84,Default,,0000,0000,0000,,Vypočujem si ich.
Dialogue: 0,0:10:46.64,0:10:49.64,Default,,0000,0000,0000,,Naozaj tomu verím, vrchný riaditeľ!
Dialogue: 0,0:10:59.35,0:11:00.45,Default,,0000,0000,0000,,Vystrašil si ma!
Dialogue: 0,0:11:01.85,0:11:05.46,Default,,0000,0000,0000,,V dlhej histórií Kirin Umeleckej Strednej,
Dialogue: 0,0:11:05.46,0:11:10.06,Default,,0000,0000,0000,,som jediný, čo prežil.
Dialogue: 0,0:11:10.06,0:11:12.67,Default,,0000,0000,0000,,Chceš počuť tajomstvo k úspechu?
Dialogue: 0,0:11:15.07,0:11:17.47,Default,,0000,0000,0000,,Toto som si myslel.
Dialogue: 0,0:11:17.47,0:11:21.87,Default,,0000,0000,0000,,Je to ako vzduch,
Dialogue: 0,0:11:21.87,0:11:24.68,Default,,0000,0000,0000,,bez zvuku a bez farby.
Dialogue: 0,0:11:26.28,0:11:28.78,Default,,0000,0000,0000,,Skoro neexistujúci.
Dialogue: 0,0:11:28.78,0:11:30.78,Default,,0000,0000,0000,,Iba takto,
Dialogue: 0,0:11:30.78,0:11:34.89,Default,,0000,0000,0000,,nastane dlhý pradlhý výlet.
Dialogue: 0,0:11:36.09,0:11:38.49,Default,,0000,0000,0000,,Pocit neviditeľnosti.
Dialogue: 0,0:11:38.89,0:11:43.40,Default,,0000,0000,0000,,Myslím si, že učiteľ Yang je v najväčšom nebezpečenstve.
Dialogue: 0,0:11:44.60,0:11:49.60,Default,,0000,0000,0000,,Nepočuješ zvuk tvojho dlhého,\N tenkého života, ako sa skracuje?
Dialogue: 0,0:11:49.60,0:11:51.30,Default,,0000,0000,0000,,Pozri sa na riaditeľa.
Dialogue: 0,0:12:04.02,0:12:09.02,Default,,0000,0000,0000,,=Ododneška riaditeľ už nebude chodiť do školy.=
Dialogue: 0,0:12:09.02,0:12:12.13,Default,,0000,0000,0000,,=Predtým než dorazí nový riaditeľ,=
Dialogue: 0,0:12:12.13,0:12:15.03,Default,,0000,0000,0000,,=budem ja vašim dočasným riaditeľom.=
Dialogue: 0,0:12:15.03,0:12:16.33,Default,,0000,0000,0000,,=To je všetko.=
Dialogue: 0,0:12:22.14,0:12:24.04,Default,,0000,0000,0000,,Čo tu robíš namiesto toho, aby si bol v škole?
Dialogue: 0,0:12:24.04,0:12:26.14,Default,,0000,0000,0000,,Učiteľ, ste na tom rovnako.
Dialogue: 0,0:12:27.04,0:12:27.84,Default,,0000,0000,0000,,Čože?
Dialogue: 0,0:12:27.84,0:12:29.14,Default,,0000,0000,0000,,Kam idete?
Dialogue: 0,0:12:37.45,0:12:38.95,Default,,0000,0000,0000,,Prečo ste rezignovali?
Dialogue: 0,0:12:39.05,0:12:41.15,Default,,0000,0000,0000,,Čo?
Dialogue: 0,0:12:41.15,0:12:42.26,Default,,0000,0000,0000,,Peniaze.
Dialogue: 0,0:12:42.26,0:12:43.46,Default,,0000,0000,0000,,Prečo ste mi ich požičali?
Dialogue: 0,0:12:43.46,0:12:44.66,Default,,0000,0000,0000,,Nevedel som kde ich minúť.
Dialogue: 0,0:12:44.66,0:12:47.16,Default,,0000,0000,0000,,Fajn?
Dialogue: 0,0:12:47.16,0:12:49.76,Default,,0000,0000,0000,,Vyzerá to, že naozaj ich nemáte kde minúť.
Dialogue: 0,0:12:49.76,0:12:52.37,Default,,0000,0000,0000,,Darebák, odkedy si sa začal takto vyjadrovať?
Dialogue: 0,0:12:52.37,0:12:55.47,Default,,0000,0000,0000,,Vyhodili Vás, však?
Dialogue: 0,0:12:56.97,0:12:58.97,Default,,0000,0000,0000,,Prečo Vás vyhodili?
Dialogue: 0,0:12:58.97,0:13:01.67,Default,,0000,0000,0000,,Je to všeto čo máte?
Dialogue: 0,0:13:01.67,0:13:04.48,Default,,0000,0000,0000,,YuJin.
Dialogue: 0,0:13:04.48,0:13:06.38,Default,,0000,0000,0000,,Jin YuJin...
Dialogue: 0,0:13:06.38,0:13:08.38,Default,,0000,0000,0000,,Pravdupovediac,
Dialogue: 0,0:13:08.38,0:13:10.78,Default,,0000,0000,0000,,nie je za tým nič.
Dialogue: 0,0:13:10.78,0:13:13.59,Default,,0000,0000,0000,,Už si dosť veľkou príťažou.
Dialogue: 0,0:13:13.59,0:13:15.79,Default,,0000,0000,0000,,Nehodím sa k role učiteľa.
Dialogue: 0,0:13:15.79,0:13:17.29,Default,,0000,0000,0000,,Mimo to,
Dialogue: 0,0:13:17.29,0:13:20.69,Default,,0000,0000,0000,,rezignoval som ja.
Dialogue: 0,0:13:20.69,0:13:25.20,Default,,0000,0000,0000,,Nech je akokoľvek, som stle tvojim učiteľom,\N tak ti dávam poslednú radu.
Dialogue: 0,0:13:27.10,0:13:29.90,Default,,0000,0000,0000,,Chceš byť ako ja?
Dialogue: 0,0:13:29.90,0:13:32.41,Default,,0000,0000,0000,,Huh?
Dialogue: 0,0:13:32.41,0:13:35.41,Default,,0000,0000,0000,,Nechceš, že?
Dialogue: 0,0:13:35.41,0:13:38.81,Default,,0000,0000,0000,,Tak keď sa vrátiš do školy, dávaj si pozor.
Dialogue: 0,0:13:43.22,0:13:48.12,Default,,0000,0000,0000,,Hej...
Dialogue: 0,0:13:53.73,0:13:55.93,Default,,0000,0000,0000,,zober si späť čo je tvoje.
Dialogue: 0,0:13:55.93,0:13:58.23,Default,,0000,0000,0000,,A, už sem nechoď.
Dialogue: 0,0:13:59.93,0:14:02.04,Default,,0000,0000,0000,,Všetkému čo ste povedali,
Dialogue: 0,0:14:02.04,0:14:05.04,Default,,0000,0000,0000,,neverím. Neverím ani slovu.
Dialogue: 0,0:14:06.54,0:14:08.74,Default,,0000,0000,0000,,Už sa sem nevracaj!
Dialogue: 0,0:14:09.84,0:14:12.85,Default,,0000,0000,0000,,Skazený spratek! Chodí sem zavčas rána.
Dialogue: 0,0:14:16.25,0:14:17.45,Default,,0000,0000,0000,,{\i1}Hej všetci.{\i0}
Dialogue: 0,0:14:17.45,0:14:19.95,Default,,0000,0000,0000,,Len vďaka vám,
Dialogue: 0,0:14:19.95,0:14:23.66,Default,,0000,0000,0000,,sme sa rozhodli renovovať ubytovňu!
Dialogue: 0,0:14:37.47,0:14:39.27,Default,,0000,0000,0000,,{\i1}Najprv to boli dvaja ľudia na izbu,{\i0}
Dialogue: 0,0:14:39.27,0:14:42.18,Default,,0000,0000,0000,,{\i1}ale počas renovácií,\N budete musieť zostať ako ste.{\i0}
Dialogue: 0,0:14:42.18,0:14:45.78,Default,,0000,0000,0000,,{\i1}Nová budova pre nových aj prestupných\N študentov,{\i0}
Dialogue: 0,0:14:45.78,0:14:49.88,Default,,0000,0000,0000,,{\i1}bude určená pre 3 alebo 4 osoby na izbu.{\i0}
Dialogue: 0,0:14:49.88,0:14:53.19,Default,,0000,0000,0000,,Ubytovňa nie je vidieckou lóžou.
Dialogue: 0,0:14:53.19,0:14:55.19,Default,,0000,0000,0000,,V jednu chvíľu je to tak a potom zase inak.
Dialogue: 0,0:14:55.19,0:15:00.99,Default,,0000,0000,0000,,Tak potom tiež budeš musieť opustiť svoju izbu.\N Nie si tiež v nižšej triede?
Dialogue: 0,0:15:18.11,0:15:21.71,Default,,0000,0000,0000,,Ktorá je moja posteľ?
Dialogue: 0,0:15:21.71,0:15:25.12,Default,,0000,0000,0000,,Toto je izba, nie taxík.
Dialogue: 0,0:15:25.12,0:15:29.12,Default,,0000,0000,0000,,Ako sa sem môžu zmestiť 4 osoby?
Dialogue: 0,0:15:29.12,0:15:33.23,Default,,0000,0000,0000,,Takže, sú tu iba dve postele, že?
Dialogue: 0,0:15:34.73,0:15:38.23,Default,,0000,0000,0000,,Ty si lúzerkou.
Dialogue: 0,0:15:38.23,0:15:40.93,Default,,0000,0000,0000,,Ty samotárkou.
Dialogue: 0,0:15:43.04,0:15:46.54,Default,,0000,0000,0000,,Ty vyzeráš najnormálnejšie.
Dialogue: 0,0:15:46.54,0:15:49.24,Default,,0000,0000,0000,,Budem s tebou v posteli.
Dialogue: 0,0:15:49.24,0:15:50.94,Default,,0000,0000,0000,,Máš dobrý čuch.
Dialogue: 0,0:15:51.44,0:15:52.25,Default,,0000,0000,0000,,Ty!
Dialogue: 0,0:15:53.05,0:15:55.05,Default,,0000,0000,0000,,Okamžite vypadni.
Dialogue: 0,0:15:55.05,0:15:57.65,Default,,0000,0000,0000,,Choď do inej izby.
Dialogue: 0,0:16:13.47,0:16:14.77,Default,,0000,0000,0000,,Ktorá je tvoja posteľ?
Dialogue: 0,0:16:14.77,0:16:15.77,Default,,0000,0000,0000,,Wow.
Dialogue: 0,0:16:15.77,0:16:20.27,Default,,0000,0000,0000,,Vieš rozdeliť jablko holými rukami.
Dialogue: 0,0:16:20.27,0:16:22.58,Default,,0000,0000,0000,,Povedala som ti aby si vypadla!
Dialogue: 0,0:16:39.79,0:16:42.30,Default,,0000,0000,0000,,Na chvíľu drž hubu.
Dialogue: 0,0:16:55.91,0:16:57.61,Default,,0000,0000,0000,,Aby ste zmenili zlozvyky,
Dialogue: 0,0:16:57.61,0:16:59.01,Default,,0000,0000,0000,,musíme sa naučiť zlozvyky
Dialogue: 0,0:16:59.01,0:17:01.21,Default,,0000,0000,0000,,úspešných modelov.
Dialogue: 0,0:17:01.21,0:17:03.82,Default,,0000,0000,0000,,Jack Canfield.
Dialogue: 0,0:17:05.62,0:17:07.72,Default,,0000,0000,0000,,Hej!
Dialogue: 0,0:17:07.72,0:17:10.12,Default,,0000,0000,0000,,Všetci sa zobuďte!
Dialogue: 0,0:17:14.03,0:17:18.13,Default,,0000,0000,0000,,Hej, zobuď sa.
Dialogue: 0,0:17:18.13,0:17:19.63,Default,,0000,0000,0000,,Čo to znamená?
Dialogue: 0,0:17:19.63,0:17:21.63,Default,,0000,0000,0000,,Aby sme zmenili zlozvyky,
Dialogue: 0,0:17:21.63,0:17:22.94,Default,,0000,0000,0000,,musíme sa naučiť zlozvyky
Dialogue: 0,0:17:22.94,0:17:26.44,Default,,0000,0000,0000,,úspešných modelov.
Dialogue: 0,0:17:26.44,0:17:29.44,Default,,0000,0000,0000,,Ak chceme zmeniť zlozvyky,
Dialogue: 0,0:17:29.44,0:17:32.45,Default,,0000,0000,0000,,musíme si brať príklad z úspešných modelov, aby\N sme sa to naučili.
Dialogue: 0,0:17:40.65,0:17:43.66,Default,,0000,0000,0000,,{\i1}Myslím si, že učiteľ Yang je v najväčšom nebezpečenstve.{\i0}
Dialogue: 0,0:17:43.66,0:17:47.26,Default,,0000,0000,0000,,{\i1}Nepočuješ zvuk tvojho dlhého,\N tenkého života, ako sa skracuje?{\i0}
Dialogue: 0,0:17:47.26,0:17:49.86,Default,,0000,0000,0000,,{\i1}Pozri sa na riaditeľa.{\i0}
Dialogue: 0,0:17:49.86,0:17:53.67,Default,,0000,0000,0000,,Prehĺbte priateľstvo,
Dialogue: 0,0:17:53.67,0:17:57.27,Default,,0000,0000,0000,,a z vašej rodiny spravte
Dialogue: 0,0:17:57.27,0:18:00.67,Default,,0000,0000,0000,,hlavným východiskom
Dialogue: 0,0:18:00.67,0:18:03.18,Default,,0000,0000,0000,,k podpore a...
Dialogue: 0,0:18:03.18,0:18:06.08,Default,,0000,0000,0000,,Prepáčte, že rušíme hodinu.
Dialogue: 0,0:18:06.08,0:18:08.98,Default,,0000,0000,0000,,Ako vidíte, mám hodinu.
Dialogue: 0,0:18:08.98,0:18:10.38,Default,,0000,0000,0000,,Hodina,
Dialogue: 0,0:18:10.38,0:18:15.59,Default,,0000,0000,0000,,hodina musí bežať bez vyrušovania.
Dialogue: 0,0:18:15.59,0:18:16.39,Default,,0000,0000,0000,,Toto je...
Dialogue: 0,0:18:16.39,0:18:19.79,Default,,0000,0000,0000,,Toto je nový učiteľ.
Dialogue: 0,0:18:19.79,0:18:22.70,Default,,0000,0000,0000,,Zdravím, rád Vás spoznávam.
Dialogue: 0,0:18:22.70,0:18:25.30,Default,,0000,0000,0000,,Ja tiež.
Dialogue: 0,0:18:25.30,0:18:27.40,Default,,0000,0000,0000,,Neskôr sa môžete porozprávať.
Dialogue: 0,0:18:27.40,0:18:31.50,Default,,0000,0000,0000,,S pár žiakmi sa potrebujem osobne porozprávať.
Dialogue: 0,0:18:40.81,0:18:42.52,Default,,0000,0000,0000,,Toto sú vaše,
Dialogue: 0,0:18:42.52,0:18:45.42,Default,,0000,0000,0000,,CDčká, ktoré ste mali spraviť\N na jednu z úloh, že?
Dialogue: 0,0:18:45.42,0:18:46.62,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:18:46.62,0:18:49.42,Default,,0000,0000,0000,,Kto ich zkomponoval?
Dialogue: 0,0:18:49.42,0:18:53.43,Default,,0000,0000,0000,,My sami.
Dialogue: 0,0:18:53.43,0:18:56.63,Default,,0000,0000,0000,,Je zrejmé, že boli robené jednou a tou istou osobou,
Dialogue: 0,0:18:56.63,0:19:00.23,Default,,0000,0000,0000,,každý to vie už len z počutia.
Dialogue: 0,0:19:00.23,0:19:03.44,Default,,0000,0000,0000,,Kto je to?
Dialogue: 0,0:19:03.44,0:19:06.04,Default,,0000,0000,0000,,Zavolať do školy vašich rodičov kvôli takejto maličkosti,
Dialogue: 0,0:19:06.04,0:19:10.74,Default,,0000,0000,0000,,môže niekomu ublížiť, všakže?
Dialogue: 0,0:19:10.74,0:19:13.45,Default,,0000,0000,0000,,Povedzte mi. Kto to spravil?
Dialogue: 0,0:19:13.45,0:19:14.85,Default,,0000,0000,0000,,Jin YuJin...
Dialogue: 0,0:19:14.85,0:19:15.65,Default,,0000,0000,0000,,Čo si povedal?
Dialogue: 0,0:19:15.65,0:19:16.75,Default,,0000,0000,0000,,Jin YuJin...
Dialogue: 0,0:19:16.75,0:19:17.45,Default,,0000,0000,0000,,Ktože to?
Dialogue: 0,0:19:17.45,0:19:18.65,Default,,0000,0000,0000,,To Jin YuJin.
Dialogue: 0,0:19:29.66,0:19:31.66,Default,,0000,0000,0000,,Riaditeľ.
Dialogue: 0,0:19:31.66,0:19:33.77,Default,,0000,0000,0000,,Prečo rezignoval?
Dialogue: 0,0:19:33.77,0:19:34.37,Default,,0000,0000,0000,,Odkedy
Dialogue: 0,0:19:34.37,0:19:38.17,Default,,0000,0000,0000,,študenti sa starajú o veci učiteľov?
Dialogue: 0,0:19:38.17,0:19:41.17,Default,,0000,0000,0000,,Pretože príčinou môžem byť aj ja.
Dialogue: 0,0:19:41.17,0:19:42.48,Default,,0000,0000,0000,,Bolo to kvôli nám?
Dialogue: 0,0:19:42.48,0:19:44.98,Default,,0000,0000,0000,,Nemáme dobré talenty?
Dialogue: 0,0:19:44.98,0:19:48.18,Default,,0000,0000,0000,,Možno rezignoval kvôli osobným záležitostiam.
Dialogue: 0,0:19:48.18,0:19:51.18,Default,,0000,0000,0000,,Nie je to preto, že by ste ho k tomu donútili?
Dialogue: 0,0:19:55.49,0:19:58.89,Default,,0000,0000,0000,,Takže si Jin YuJin z tretieho ročníka.
Dialogue: 0,0:19:58.89,0:20:02.60,Default,,0000,0000,0000,,Prečo pomáhaš učiteľovi,\N ktorého zaujímajú len peniaze?
Dialogue: 0,0:20:05.10,0:20:07.40,Default,,0000,0000,0000,,Čo tým myslíte?
Dialogue: 0,0:20:07.40,0:20:13.01,Default,,0000,0000,0000,,Videl som, že je v zlej situácií,\N tak som mu pomohol. Kto mal vedieť že bude rezignovať?
Dialogue: 0,0:20:13.01,0:20:15.11,Default,,0000,0000,0000,,Nemrhaj časom obávaním sa
Dialogue: 0,0:20:15.11,0:20:17.21,Default,,0000,0000,0000,,o nezmyselnú vec.
Dialogue: 0,0:20:17.21,0:20:20.21,Default,,0000,0000,0000,,Ži múdro.
Dialogue: 0,0:20:36.83,0:20:40.03,Default,,0000,0000,0000,,Bolo to kvôli mojej záležitosti?
Dialogue: 0,0:20:40.03,0:20:44.24,Default,,0000,0000,0000,,Bojím sa o iných, keď sám sa idem zblázniť.
Dialogue: 0,0:20:54.05,0:20:55.65,Default,,0000,0000,0000,,Dám ti 300.
Dialogue: 0,0:20:55.65,0:20:56.85,Default,,0000,0000,0000,,Len 300?
Dialogue: 0,0:20:56.85,0:21:00.05,Default,,0000,0000,0000,,Ak chceš vyjednávať, tak to zober preč.
Dialogue: 0,0:21:00.05,0:21:03.86,Default,,0000,0000,0000,,Nemôžete byť trochu prijateľnejší?
Dialogue: 0,0:21:03.86,0:21:06.56,Default,,0000,0000,0000,,Tak, ju nesmiete predať.
Dialogue: 0,0:21:06.56,0:21:08.36,Default,,0000,0000,0000,,Kúpim ju čoskoro späť.
Dialogue: 0,0:21:08.36,0:21:10.16,Default,,0000,0000,0000,,Tak zaplať vklad.
Dialogue: 0,0:21:10.16,0:21:14.07,Default,,0000,0000,0000,,Ak by som mohol zaplatiť vklad,\N prečo by som asi predával gitaru?
Dialogue: 0,0:21:27.88,0:21:30.88,Default,,0000,0000,0000,,SunDong.
Dialogue: 0,0:21:30.88,0:21:32.89,Default,,0000,0000,0000,,Škvŕka mi v bruchu,
Dialogue: 0,0:21:32.89,0:21:34.59,Default,,0000,0000,0000,,poďme sa najesť.
Dialogue: 0,0:21:34.59,0:21:35.49,Default,,0000,0000,0000,,Prepáč.
Dialogue: 0,0:21:35.49,0:21:40.89,Default,,0000,0000,0000,,Už som sľúbila kamarátom.
Dialogue: 0,0:21:40.89,0:21:43.50,Default,,0000,0000,0000,,Tak sa choď najesť a potom mi dones zvyšky,\N ak budú.
Dialogue: 0,0:21:43.50,0:21:46.60,Default,,0000,0000,0000,,Jasne, aj ty sa najedz.
Dialogue: 0,0:21:51.90,0:21:53.81,Default,,0000,0000,0000,,Moja káva.
Dialogue: 0,0:21:53.81,0:21:55.81,Default,,0000,0000,0000,,Mala by si byť viac opatrná.
Dialogue: 0,0:22:49.96,0:22:52.57,Default,,0000,0000,0000,,Chcete ísť hrať na ulicu?
Dialogue: 0,0:22:57.97,0:23:00.37,Default,,0000,0000,0000,,Sme nižšia trieda, dáva to vôbec zmysel?
Dialogue: 0,0:23:00.37,0:23:01.27,Default,,0000,0000,0000,,Dáva?
Dialogue: 0,0:23:01.27,0:23:01.98,Default,,0000,0000,0000,,Nedáva, všakže?
Dialogue: 0,0:23:01.98,0:23:04.98,Default,,0000,0000,0000,,Nedáva to žiadny zmysel.
Dialogue: 0,0:23:04.98,0:23:06.48,Default,,0000,0000,0000,,Zariskoval som, že ma môžu vyhodiť,
Dialogue: 0,0:23:06.48,0:23:08.78,Default,,0000,0000,0000,,a stretol som sa s vrchným riaditeľom.
Dialogue: 0,0:23:08.78,0:23:10.28,Default,,0000,0000,0000,,Naozaj?
Dialogue: 0,0:23:10.28,0:23:11.68,Default,,0000,0000,0000,,A potom?
Dialogue: 0,0:23:11.68,0:23:14.59,Default,,0000,0000,0000,,Vyjasnil som mu, že použijeme vlastné metódy,\N aby mohli prijať aj nás.
Dialogue: 0,0:23:14.59,0:23:17.99,Default,,0000,0000,0000,,Jeho tvár sa začala meniť.
Dialogue: 0,0:23:17.99,0:23:19.69,Default,,0000,0000,0000,,Váhal.
Dialogue: 0,0:23:19.69,0:23:22.20,Default,,0000,0000,0000,,Videl som ako sa tváril,
Dialogue: 0,0:23:22.20,0:23:26.00,Default,,0000,0000,0000,,tak som prišiel s podmienkami.
Dialogue: 0,0:23:26.00,0:23:30.10,Default,,0000,0000,0000,,Ak spravíme nehorázne dobre...
Dialogue: 0,0:23:30.10,0:23:31.70,Default,,0000,0000,0000,,Keď to počul vrchný riaditeľ,
Dialogue: 0,0:23:31.70,0:23:35.01,Default,,0000,0000,0000,,videl moje rozhodnutie.
Dialogue: 0,0:23:35.01,0:23:36.21,Default,,0000,0000,0000,,Naozaj?
Dialogue: 0,0:23:36.21,0:23:38.31,Default,,0000,0000,0000,,Naozaj.
Dialogue: 0,0:23:38.31,0:23:40.91,Default,,0000,0000,0000,,Zase preháňaš.
Dialogue: 0,0:23:43.02,0:23:46.42,Default,,0000,0000,0000,,Zabudni na to, len tak rozprávam.
Dialogue: 0,0:23:46.42,0:23:51.22,Default,,0000,0000,0000,,Nezavolal ťa vrchný riaditeľ kvôli CDčkám?
Dialogue: 0,0:23:51.22,0:23:53.53,Default,,0000,0000,0000,,CD? Akým CDčkám?
Dialogue: 0,0:23:53.53,0:23:56.63,Default,,0000,0000,0000,,Tie CDčká, čo si nám spravil na tú úlohu,
Dialogue: 0,0:23:56.63,0:23:58.43,Default,,0000,0000,0000,,zistil to.
Dialogue: 0,0:23:58.43,0:24:01.33,Default,,0000,0000,0000,,Nespomínal to?
Dialogue: 0,0:24:01.33,0:24:02.64,Default,,0000,0000,0000,,Nie.
Dialogue: 0,0:24:02.64,0:24:05.14,Default,,0000,0000,0000,,Ah...
Dialogue: 0,0:24:05.14,0:24:06.54,Default,,0000,0000,0000,,Vypočul si moje kompozície,
Dialogue: 0,0:24:06.54,0:24:11.95,Default,,0000,0000,0000,,a všimol si môj talent.
Dialogue: 0,0:24:11.95,0:24:14.15,Default,,0000,0000,0000,,Ah.... takže to sa stalo.
Dialogue: 0,0:24:16.85,0:24:20.35,Default,,0000,0000,0000,,Tak čo povedal?
Dialogue: 0,0:24:22.46,0:24:24.96,Default,,0000,0000,0000,,Aby sme dali do toho všetko.
Dialogue: 0,0:24:24.96,0:24:28.76,Default,,0000,0000,0000,,Ak rozpoznajú naše schopnosti,
Dialogue: 0,0:24:28.76,0:24:30.46,Default,,0000,0000,0000,,všetko
Dialogue: 0,0:24:30.46,0:24:34.57,Default,,0000,0000,0000,,bude tak ako predtým.
Dialogue: 0,0:24:38.27,0:24:41.47,Default,,0000,0000,0000,,Je to pravda?
Dialogue: 0,0:24:41.47,0:24:44.08,Default,,0000,0000,0000,,A taktiež si zarobíme.
Dialogue: 0,0:24:44.88,0:24:47.58,Default,,0000,0000,0000,,Na čo potrebujeme peniaze?
Dialogue: 0,0:24:47.98,0:24:53.09,Default,,0000,0000,0000,,Pomáhajú ľuďom.
Dialogue: 0,0:24:53.09,0:24:58.69,Default,,0000,0000,0000,,Dajme do toho našu reputáciu a skúsme to.
Dialogue: 0,0:25:05.70,0:25:08.00,Default,,0000,0000,0000,,Pomôžem vám!
Dialogue: 0,0:25:08.90,0:25:11.70,Default,,0000,0000,0000,,Rátajte aj so mnou!
Dialogue: 0,0:25:14.71,0:25:19.31,Default,,0000,0000,0000,,To nepôjde.
Dialogue: 0,0:25:22.22,0:25:25.02,Default,,0000,0000,0000,,Učiteľ, ste učiteľ angličtiny.
Dialogue: 0,0:25:29.72,0:25:32.93,Default,,0000,0000,0000,,Máte kde skúšať?
Dialogue: 0,0:25:48.34,0:25:53.45,Default,,0000,0000,0000,,Skúsme to, nie je to ťažké.
Dialogue: 0,0:25:59.55,0:26:02.06,Default,,0000,0000,0000,,Presiahli ste limit.
Dialogue: 0,0:26:12.67,0:26:18.97,Default,,0000,0000,0000,,No tak, musíte si pamätať len jednu vec.
Dialogue: 0,0:26:18.97,0:26:21.77,Default,,0000,0000,0000,,Divokú dušu.
Dialogue: 0,0:26:21.77,0:26:22.98,Default,,0000,0000,0000,,Musíte prežiť.
Dialogue: 0,0:26:22.98,0:26:25.28,Default,,0000,0000,0000,,Tento svet je džunglou.
Dialogue: 0,0:26:25.28,0:26:30.18,Default,,0000,0000,0000,,Ak si myslíte, že malý potôčik bude navždy\N prúdiť, tak ste na omyle.
Dialogue: 0,0:26:30.18,0:26:33.09,Default,,0000,0000,0000,,Dokonca ani ja.
Dialogue: 0,0:26:33.09,0:26:36.89,Default,,0000,0000,0000,,Ak nezjete iných, vy budete zjedení.\N Sú tu len dve riešenia.
Dialogue: 0,0:26:36.89,0:26:38.59,Default,,0000,0000,0000,,Musíme zjesť ostatných.
Dialogue: 0,0:26:38.59,0:26:42.40,Default,,0000,0000,0000,,Učiteľ, prečo chcete vždy zjesť ostatných ľudí?
Dialogue: 0,0:26:42.40,0:26:46.30,Default,,0000,0000,0000,,Inak ten učiteľ z cudziny zje mňa...
Dialogue: 0,0:26:46.30,0:26:51.30,Default,,0000,0000,0000,,Nie, čo mám na mysli je, že musím prežiť.
Dialogue: 0,0:27:15.71,0:27:18.08,Default,,0000,0000,0000,,~ {\i1}To je v poriadku, všetko bude dobré.{\i0} ~
Dialogue: 0,0:27:18.83,0:27:23.24,Default,,0000,0000,0000,,Nie, nie, nie!
Dialogue: 0,0:27:26.94,0:27:28.24,Default,,0000,0000,0000,,Vyzerá to, že to bude zlé.
Dialogue: 0,0:27:28.24,0:27:30.84,Default,,0000,0000,0000,,Čo dobré tu je?
Dialogue: 0,0:27:38.35,0:27:39.85,Default,,0000,0000,0000,,Nevieš ako čítať noty?
Dialogue: 0,0:27:39.85,0:27:42.76,Default,,0000,0000,0000,,Nepoznám moc túto pesničku, takže...
Dialogue: 0,0:27:42.76,0:27:45.76,Default,,0000,0000,0000,,Ako sa dá spievať pesničku len keď sa niekto pozrie na noty?
Dialogue: 0,0:27:45.76,0:27:48.56,Default,,0000,0000,0000,,Nie je to náhodou základ?
Dialogue: 0,0:27:48.56,0:27:50.26,Default,,0000,0000,0000,,Skúsim to ešte raz.
Dialogue: 0,0:27:50.26,0:27:52.37,Default,,0000,0000,0000,,Skús to sama.
Dialogue: 0,0:27:54.47,0:27:59.37,Default,,0000,0000,0000,,Dobre, kde mám začať?
Dialogue: 0,0:27:59.37,0:28:01.37,Default,,0000,0000,0000,,Od začiatku.
Dialogue: 0,0:28:08.53,0:28:14.77,Default,,0000,0000,0000,,~ {\i1}V minulosti, bez žiadnych plánov, ty...{\i0} ~
Dialogue: 0,0:28:19.39,0:28:21.59,Default,,0000,0000,0000,,Nemôžeš použiť kolísanie frekvencie a amplitúdy?
Dialogue: 0,0:28:21.59,0:28:24.60,Default,,0000,0000,0000,,Ani sa nevieš kontrolovať.
Dialogue: 0,0:28:24.60,0:28:28.50,Default,,0000,0000,0000,,Spievaj čistejšie.
Dialogue: 0,0:28:28.50,0:28:29.50,Default,,0000,0000,0000,,V pohode.
Dialogue: 0,0:28:29.50,0:28:31.50,Default,,0000,0000,0000,,A spievaj hlasnejšie.
Dialogue: 0,0:28:31.50,0:28:35.31,Default,,0000,0000,0000,,Mala by si viesť doprovod,\N ale ty si tým doprovodom zakrytá.
Dialogue: 0,0:28:35.31,0:28:37.81,Default,,0000,0000,0000,,Mmm.
Dialogue: 0,0:28:37.81,0:28:41.31,Default,,0000,0000,0000,,Čo? Si nešťastná?
Dialogue: 0,0:28:41.31,0:28:49.52,Default,,0000,0000,0000,,Nie. Asi musím byť vážne zlá.
Dialogue: 0,0:28:49.52,0:28:53.43,Default,,0000,0000,0000,,Ak spravím niečo zle, len mi to povedz.
Dialogue: 0,0:28:53.43,0:28:56.23,Default,,0000,0000,0000,,Skúsim ešte raz.
Dialogue: 0,0:28:59.44,0:29:07.95,Default,,0000,0000,0000,,~ {\i1}V minulosti, bez žiadnych plánov,{\i0} ~\N~ {\i1}si prišiel sem do Seoulu.{\i0} ~
Dialogue: 0,0:29:14.55,0:29:19.65,Default,,0000,0000,0000,,Prepáčte, musím už odísť.
Dialogue: 0,0:29:19.65,0:29:22.06,Default,,0000,0000,0000,,Po večeri sa musíme vrátiť a ešte nacvičovať.
Dialogue: 0,0:29:22.06,0:29:26.06,Default,,0000,0000,0000,,Prepáčte, ešte som neupratala všetky izby.
Dialogue: 0,0:29:26.06,0:29:28.76,Default,,0000,0000,0000,,Práve teraz, je upratovanie dôležitejšie ako prežitie?
Dialogue: 0,0:29:29.86,0:29:32.67,Default,,0000,0000,0000,,Ako môže skupina nacvičovať bez jednej osoby?
Dialogue: 0,0:29:32.67,0:29:35.47,Default,,0000,0000,0000,,Bude to takto aj v budúcnosti?
Dialogue: 0,0:29:35.47,0:29:40.07,Default,,0000,0000,0000,,Sľubujem, že budem cvičiť predtým než prídem.
Dialogue: 0,0:29:40.07,0:29:42.38,Default,,0000,0000,0000,,Nemôžeš to spraviť.
Dialogue: 0,0:29:42.38,0:29:46.68,Default,,0000,0000,0000,,Tímová práca je všetko, ak sa rozdelíme, niet šanci.
Dialogue: 0,0:29:46.68,0:29:48.98,Default,,0000,0000,0000,,Budeme zjedení.
Dialogue: 0,0:29:48.98,0:29:52.79,Default,,0000,0000,0000,,Prepáčte.
Dialogue: 0,0:29:52.79,0:29:54.09,Default,,0000,0000,0000,,Okej, okej. Rýchlo poďme.
Dialogue: 0,0:29:54.09,0:29:55.89,Default,,0000,0000,0000,,Rýchlo sa najeme a vrátime na nácvik.
Dialogue: 0,0:29:55.89,0:29:59.59,Default,,0000,0000,0000,,Rýchlo, rýchlo, najedzme sa.
Dialogue: 0,0:30:01.49,0:30:03.50,Default,,0000,0000,0000,,Prečo si zrazu takáto?
Dialogue: 0,0:30:04.10,0:30:05.30,Default,,0000,0000,0000,,Čo som spravila?
Dialogue: 0,0:30:05.30,0:30:09.00,Default,,0000,0000,0000,,Prečo si zrazu poslušná?
Dialogue: 0,0:30:14.81,0:30:17.31,Default,,0000,0000,0000,,Nahraj to a vypočuj si to.
Dialogue: 0,0:30:18.41,0:30:21.21,Default,,0000,0000,0000,,Aspoň budeš vedieť aké nepríjemné je
Dialogue: 0,0:30:21.21,0:30:24.32,Default,,0000,0000,0000,,pre ostaných keď spievaš.
Dialogue: 0,0:30:38.33,0:30:42.84,Default,,0000,0000,0000,,Kto to spravil?
Dialogue: 0,0:30:44.14,0:30:51.04,Default,,0000,0000,0000,,Tá nová spolubývajúca povedala, že máš veľa vecí\N a je tu málo priestoru.
Dialogue: 0,0:30:51.04,0:30:53.15,Default,,0000,0000,0000,,To je všetko.
Dialogue: 0,0:30:53.75,0:30:56.95,Default,,0000,0000,0000,,Odkiaľ vôbec prišla?
Dialogue: 0,0:30:56.95,0:31:01.55,Default,,0000,0000,0000,,Musím to povedať vedúcemu.
Dialogue: 0,0:31:01.55,0:31:08.26,Default,,0000,0000,0000,,Pomstím sa. Skutočne!
Dialogue: 0,0:31:50.20,0:31:52.81,Default,,0000,0000,0000,,Musím to tu umyť.
Dialogue: 0,0:31:54.51,0:31:56.01,Default,,0000,0000,0000,,Čože?
Dialogue: 0,0:31:56.01,0:31:58.51,Default,,0000,0000,0000,,Nič.
Dialogue: 0,0:31:58.51,0:32:00.21,Default,,0000,0000,0000,,Nehovorila si, že tu musíš umyť?
Dialogue: 0,0:32:00.21,0:32:04.32,Default,,0000,0000,0000,,Už môžeš. Ja som skončil.
Dialogue: 0,0:32:05.82,0:32:09.62,Default,,0000,0000,0000,,Jasne si ma počul a predstieraš, že nie.
Dialogue: 0,0:32:13.13,0:32:15.93,Default,,0000,0000,0000,,Už sa zase vynosili.
Dialogue: 0,0:32:27.54,0:32:30.74,Default,,0000,0000,0000,,Môžem to poskladať behom minútky.
Dialogue: 0,0:32:30.74,0:32:34.95,Default,,0000,0000,0000,,Ty to dokážeš za minútu? Toto?
Dialogue: 0,0:32:34.95,0:32:37.95,Default,,0000,0000,0000,,Čo? A čo tak za 30 sekúnd?
Dialogue: 0,0:32:37.95,0:32:41.35,Default,,0000,0000,0000,,Nikdy som neskúšala na 30 sekúnd.
Dialogue: 0,0:32:41.35,0:32:44.16,Default,,0000,0000,0000,,Naozaj to dáš za minútu?
Dialogue: 0,0:32:44.16,0:32:47.56,Default,,0000,0000,0000,,Ak mi neveríš, tak si to odstopuj stopkami.
Dialogue: 0,0:33:02.88,0:33:04.78,Default,,0000,0000,0000,,Ako si to spravila?
Dialogue: 0,0:33:04.78,0:33:06.88,Default,,0000,0000,0000,,Mne sa to nepodarilo ani raz.
Dialogue: 0,0:33:06.88,0:33:08.78,Default,,0000,0000,0000,,Čože?
Dialogue: 0,0:33:08.78,0:33:12.89,Default,,0000,0000,0000,,Ani raz si neuspel za tie dva roky?
Dialogue: 0,0:33:12.89,0:33:17.39,Default,,0000,0000,0000,,Odkiaľ vieš, že to mám dva roky?
Dialogue: 0,0:33:20.59,0:33:23.10,Default,,0000,0000,0000,,Som tvoja fanúšička!
Dialogue: 0,0:33:23.10,0:33:29.00,Default,,0000,0000,0000,,Spomedzi tvojich fanúšikov, kto nevie, že to máš?
Dialogue: 0,0:33:30.50,0:33:34.61,Default,,0000,0000,0000,,Zrejme ti to nejde, pretože to moc nechceš vyriešiť.
Dialogue: 0,0:33:34.61,0:33:36.91,Default,,0000,0000,0000,,Je ti jedno, či to spravíš alebo nie,
Dialogue: 0,0:33:36.91,0:33:41.91,Default,,0000,0000,0000,,iba to prehadzuješ bez rozmýšľania,\N preto si nikdy neuspel.
Dialogue: 0,0:33:52.74,0:33:57.21,Default,,0000,0000,0000,,~ {\i1}To je v poriadku, všetko bude dobré.{\i0} ~
Dialogue: 0,0:33:57.37,0:34:01.31,Default,,0000,0000,0000,,~ {\i1}Ako tvoja briliantná budúcnosť.{\i0} ~
Dialogue: 0,0:34:01.45,0:34:05.25,Default,,0000,0000,0000,,~ {\i1}To je v poriadku, všetko bude--{\i0} ~
Dialogue: 0,0:34:08.14,0:34:13.85,Default,,0000,0000,0000,,HaeSung, koľkokrát si spravila tú istú chybu?
Dialogue: 0,0:34:13.85,0:34:15.45,Default,,0000,0000,0000,,Prepáčte.
Dialogue: 0,0:34:15.45,0:34:19.95,Default,,0000,0000,0000,,Nemôžeš byť vždy taká,\N čo budeš robiť?
Dialogue: 0,0:34:19.95,0:34:23.96,Default,,0000,0000,0000,,Som naozaj taká zlá?
Dialogue: 0,0:34:23.96,0:34:27.36,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:34:27.36,0:34:29.86,Default,,0000,0000,0000,,Trénovala si?
Dialogue: 0,0:34:29.86,0:34:30.96,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:34:30.96,0:34:34.97,Default,,0000,0000,0000,,Trénovala si, ale stále si sa nezlepšila.
Dialogue: 0,0:34:34.97,0:34:38.07,Default,,0000,0000,0000,,Skúšala si ten prehrávač čo som ti dal?
Dialogue: 0,0:34:38.07,0:34:39.27,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:34:39.27,0:34:41.77,Default,,0000,0000,0000,,Nič si necítila, keď si to počúvala?
Dialogue: 0,0:34:41.77,0:34:44.58,Default,,0000,0000,0000,,Z nás piatich by aspoň jeden sa mohol zlepšiť.
Dialogue: 0,0:34:44.58,0:34:47.08,Default,,0000,0000,0000,,Kvôli tebe, všetci trpia.
Dialogue: 0,0:34:48.38,0:34:50.08,Default,,0000,0000,0000,,Hudbu si treba užívať.
Dialogue: 0,0:34:50.08,0:34:52.39,Default,,0000,0000,0000,,Nemusíš to tak hovoriť.
Dialogue: 0,0:34:52.39,0:34:54.39,Default,,0000,0000,0000,,Ani nečakám, že to bude extra perfektné.
Dialogue: 0,0:34:54.69,0:34:58.09,Default,,0000,0000,0000,,Aspoň to sprav tak,\N aby sa necítili ľudia zle, keď ťa budú počúvať.
Dialogue: 0,0:35:00.19,0:35:02.10,Default,,0000,0000,0000,,Teraz, všetci.
Dialogue: 0,0:35:02.40,0:35:06.40,Default,,0000,0000,0000,,Poďme na ulicu túto nedeľu.
Dialogue: 0,0:35:07.50,0:35:08.70,Default,,0000,0000,0000,,Eh?
Dialogue: 0,0:35:09.20,0:35:11.80,Default,,0000,0000,0000,,Vyskúmať reakcie prechádzajúcich.
Dialogue: 0,0:35:12.71,0:35:16.01,Default,,0000,0000,0000,,Je to najlepšia metóda ako získať skúsenosti.
Dialogue: 0,0:35:16.21,0:35:17.91,Default,,0000,0000,0000,,Čože? Ako tam môžeme ísť v takejto kondícií?
Dialogue: 0,0:35:18.11,0:35:19.51,Default,,0000,0000,0000,,Nezostáva nám náhodou málo času? Darebák.
Dialogue: 0,0:35:19.61,0:35:23.72,Default,,0000,0000,0000,,Musíme si pohnúť.
Dialogue: 0,0:35:26.52,0:35:28.22,Default,,0000,0000,0000,,Ahojte.
Dialogue: 0,0:35:28.72,0:35:30.32,Default,,0000,0000,0000,,Ahojte.
Dialogue: 0,0:35:32.03,0:35:33.33,Default,,0000,0000,0000,,To...
Dialogue: 0,0:35:35.13,0:35:39.43,Default,,0000,0000,0000,,nechápem, prečo sa ku mne takto správaš.
Dialogue: 0,0:35:39.63,0:35:41.13,Default,,0000,0000,0000,,Ak nechceš aby sa na teba zhora pozerali,
Dialogue: 0,0:35:42.04,0:35:44.44,Default,,0000,0000,0000,,tak sa snaž nebyť takou osobou.
Dialogue: 0,0:35:45.64,0:35:47.44,Default,,0000,0000,0000,,To bude stačiť.
Dialogue: 0,0:35:57.05,0:35:59.35,Default,,0000,0000,0000,,Máš na dnes nejaké plány?
Dialogue: 0,0:36:00.25,0:36:01.85,Default,,0000,0000,0000,,Plánujem ísť domov.
Dialogue: 0,0:36:04.86,0:36:06.36,Default,,0000,0000,0000,,To je dobré.
Dialogue: 0,0:36:06.56,0:36:09.86,Default,,0000,0000,0000,,Bála som sa, že vystrojíš nejakú párty,\Nalebo niečo také.
Dialogue: 0,0:36:10.86,0:36:13.37,Default,,0000,0000,0000,,Tiež som sa už dohodla že budem jesť s rodičmi.
Dialogue: 0,0:36:13.57,0:36:14.97,Default,,0000,0000,0000,,Párty?
Dialogue: 0,0:36:15.67,0:36:17.37,Default,,0000,0000,0000,,Aký je dnes deň?
Dialogue: 0,0:36:20.57,0:36:22.38,Default,,0000,0000,0000,,Zabudni na to.
Dialogue: 0,0:36:27.48,0:36:31.08,Default,,0000,0000,0000,,Naozaj strávi svoje narodeniny s rodinou?
Dialogue: 0,0:36:31.99,0:36:34.79,Default,,0000,0000,0000,,Tak detinské, že oslava.
Dialogue: 0,0:36:35.19,0:36:36.69,Default,,0000,0000,0000,,Chytí sa Lian?
Dialogue: 0,0:36:36.99,0:36:39.09,Default,,0000,0000,0000,,Ju sa dá ľahko prelstiť.
Dialogue: 0,0:36:39.39,0:36:40.39,Default,,0000,0000,0000,,Prídeš, však?
Dialogue: 0,0:36:40.59,0:36:41.89,Default,,0000,0000,0000,,Nie.
Dialogue: 0,0:36:42.20,0:36:43.60,Default,,0000,0000,0000,,Spravte to sami.
Dialogue: 0,0:36:45.70,0:36:48.80,Default,,0000,0000,0000,,Prečo? JB súhlasil.
Dialogue: 0,0:36:51.40,0:36:53.21,Default,,0000,0000,0000,,Práve preto nejdem.
Dialogue: 0,0:37:02.42,0:37:05.62,Default,,0000,0000,0000,,Prečo? Ešte sa nezačali riešiť keci o rozpade skupiny.
Dialogue: 0,0:37:06.12,0:37:09.62,Default,,0000,0000,0000,,Ak budeš stále takým samotárskym,\N tak už ťa viac nebudem otravovať.
Dialogue: 0,0:37:17.93,0:37:19.23,Default,,0000,0000,0000,,Zbláznila si sa?
Dialogue: 0,0:37:26.14,0:37:28.44,Default,,0000,0000,0000,,Prídeš alebo nie?
Dialogue: 0,0:37:28.94,0:37:30.54,Default,,0000,0000,0000,,Zakričím, aby prišli ľudia.
Dialogue: 0,0:37:32.25,0:37:32.55,Default,,0000,0000,0000,,Daj mi to.
Dialogue: 0,0:37:35.45,0:37:36.15,Default,,0000,0000,0000,,1...
Dialogue: 0,0:37:37.45,0:37:39.25,Default,,0000,0000,0000,,2...
Dialogue: 0,0:37:39.85,0:37:41.15,Default,,0000,0000,0000,,Siwoo má dole oblečenie!
Dialogue: 0,0:37:41.35,0:37:43.26,Default,,0000,0000,0000,,Nie, pôjdem, dobre?
Dialogue: 0,0:37:58.97,0:38:01.27,Default,,0000,0000,0000,,Na čo sú teraz dobré?
Dialogue: 0,0:38:05.98,0:38:08.08,Default,,0000,0000,0000,,Aha, aké sú?
Dialogue: 0,0:38:08.28,0:38:11.48,Default,,0000,0000,0000,,Ako môžu mať tvoji fanúšici tak dobrý štýl?
Dialogue: 0,0:38:11.68,0:38:13.19,Default,,0000,0000,0000,,Znesiem to veľmi rada.
Dialogue: 0,0:38:14.99,0:38:16.29,Default,,0000,0000,0000,,Ideš niekam?
Dialogue: 0,0:38:16.69,0:38:18.39,Default,,0000,0000,0000,,Ideme vystupovať na ulicu.
Dialogue: 0,0:38:18.49,0:38:22.10,Default,,0000,0000,0000,,Ak máš čas, tak prídi,\N zaspievam ti špeciálnu narodeninovú pesničku.
Dialogue: 0,0:38:23.30,0:38:25.80,Default,,0000,0000,0000,,Narodeniny...
Dialogue: 0,0:38:31.20,0:38:33.21,Default,,0000,0000,0000,,Prekvapenie!
Dialogue: 0,0:38:37.91,0:38:38.61,Default,,0000,0000,0000,,Kde je Lian?
Dialogue: 0,0:38:38.91,0:38:40.51,Default,,0000,0000,0000,,Nebola na izbe.
Dialogue: 0,0:38:40.81,0:38:42.42,Default,,0000,0000,0000,,Tuším je naozaj naštvaná.
Dialogue: 0,0:38:42.72,0:38:44.52,Default,,0000,0000,0000,,A nemáme tu so sebou telefóny.
Dialogue: 0,0:38:48.22,0:38:49.52,Default,,0000,0000,0000,,Skús ju nájsť.
Dialogue: 0,0:38:51.32,0:38:53.33,Default,,0000,0000,0000,,To som aj plánoval.
Dialogue: 0,0:39:03.94,0:39:06.64,Default,,0000,0000,0000,,HongJu, nehraj príliš jemne, dobre?
Dialogue: 0,0:39:07.14,0:39:08.24,Default,,0000,0000,0000,,Toto je rock vystúpenie.
Dialogue: 0,0:39:08.44,0:39:09.64,Default,,0000,0000,0000,,Musíš vedieť ako zaujať chodcov.
Dialogue: 0,0:39:12.45,0:39:15.85,Default,,0000,0000,0000,,Rozumieš, však?\N Nebuď nervózny!
Dialogue: 0,0:39:16.25,0:39:19.35,Default,,0000,0000,0000,,HaeSung, nebuď nervózna, musíš byť ako speváčka.
Dialogue: 0,0:39:19.65,0:39:20.15,Default,,0000,0000,0000,,Dobre.
Dialogue: 0,0:39:20.35,0:39:21.96,Default,,0000,0000,0000,,Zrejme si trochu nervózna.
Dialogue: 0,0:39:22.96,0:39:23.56,Default,,0000,0000,0000,,Veľa šťastia!
Dialogue: 0,0:39:23.56,0:39:24.76,Default,,0000,0000,0000,,Nebuď nervózny.
Dialogue: 0,0:39:25.56,0:39:26.46,Default,,0000,0000,0000,,YuJin, dobre?
Dialogue: 0,0:39:28.36,0:39:30.05,Default,,0000,0000,0000,,Musíš mať na to kľúčový bod.
Dialogue: 0,0:39:30.06,0:39:31.36,Default,,0000,0000,0000,,Áno, áno.
Dialogue: 0,0:39:31.36,0:39:32.97,Default,,0000,0000,0000,,Chápeš? Nie si nervózny, že nie?
Dialogue: 0,0:39:33.57,0:39:34.57,Default,,0000,0000,0000,,UiBong.
Dialogue: 0,0:39:37.17,0:39:39.17,Default,,0000,0000,0000,,Dnes si pekne oblečená.
Dialogue: 0,0:39:39.37,0:39:41.77,Default,,0000,0000,0000,,Naozaj? Lian mi to dala.
Dialogue: 0,0:39:42.08,0:39:44.68,Default,,0000,0000,0000,,Je milšia než som si myslela, je aj veľmi štedrá.
Dialogue: 0,0:39:44.98,0:39:46.08,Default,,0000,0000,0000,,Nie je to pekné?
Dialogue: 0,0:39:46.48,0:39:48.18,Default,,0000,0000,0000,,Tiež nevyzeráš zle.
Dialogue: 0,0:39:53.59,0:39:55.39,Default,,0000,0000,0000,,Čo teraz?
Dialogue: 0,0:40:08.20,0:40:09.50,Default,,0000,0000,0000,,Nevystupujete?
Dialogue: 0,0:40:10.90,0:40:13.11,Default,,0000,0000,0000,,Iba som vás zazrela jak som sem prišla.
Dialogue: 0,0:40:16.41,0:40:19.01,Default,,0000,0000,0000,,Niečo predveďte, vypočujem si to a odídem.
Dialogue: 0,0:40:21.41,0:40:22.52,Default,,0000,0000,0000,,Oh! Našiel som ju.
Dialogue: 0,0:40:25.52,0:40:27.12,Default,,0000,0000,0000,,Na Hongdae.
Dialogue: 0,0:40:27.32,0:40:29.02,Default,,0000,0000,0000,,Čo je tam vystúpenie?
Dialogue: 0,0:40:29.22,0:40:30.52,Default,,0000,0000,0000,,Čo?
Dialogue: 0,0:40:36.13,0:40:37.33,Default,,0000,0000,0000,,Poďme.
Dialogue: 0,0:40:37.63,0:40:39.23,Default,,0000,0000,0000,,Presne. Poďme.
Dialogue: 0,0:40:39.53,0:40:40.13,Default,,0000,0000,0000,,Ideme.
Dialogue: 0,0:40:40.33,0:40:42.04,Default,,0000,0000,0000,,Vy choďte.
Dialogue: 0,0:40:58.05,0:41:00.45,Default,,0000,0000,0000,,Vďaka tebe sem prišlo kopec ľudí.
Dialogue: 0,0:41:01.35,0:41:02.76,Default,,0000,0000,0000,,Sila idola.
Dialogue: 0,0:41:04.26,0:41:06.16,Default,,0000,0000,0000,,Lian nebude spievať?
Dialogue: 0,0:41:06.36,0:41:07.66,Default,,0000,0000,0000,,Lian nebude spievať?
Dialogue: 0,0:41:07.86,0:41:10.16,Default,,0000,0000,0000,,Doteraz som čakal len preto, lebo som si myslel,\N že bude Lian vystupovať.
Dialogue: 0,0:41:10.36,0:41:10.96,Default,,0000,0000,0000,,Aj ja.
Dialogue: 0,0:41:11.16,0:41:11.97,Default,,0000,0000,0000,,Nevystupujete?
Dialogue: 0,0:41:12.27,0:41:14.87,Default,,0000,0000,0000,,Zaspievaj.
Dialogue: 0,0:41:15.17,0:41:28.28,Default,,0000,0000,0000,,Zaspievaj.
Dialogue: 0,0:41:28.78,0:41:29.98,Default,,0000,0000,0000,,Chceš zaspievať?
Dialogue: 0,0:41:30.48,0:41:31.99,Default,,0000,0000,0000,,Tak dobre.
Dialogue: 0,0:41:46.40,0:41:48.20,Default,,0000,0000,0000,,Je vystupovanie nejakou hrou?
Dialogue: 0,0:41:50.00,0:41:52.01,Default,,0000,0000,0000,,Je to ina nácvik.
Dialogue: 0,0:41:53.61,0:41:54.91,Default,,0000,0000,0000,,Chce to publikum.
Dialogue: 0,0:41:55.21,0:41:56.11,Default,,0000,0000,0000,,Hej!
Dialogue: 0,0:41:56.41,0:42:00.11,Default,,0000,0000,0000,,Ak teraz budem spievať, tak budú lietať kamene.
Dialogue: 0,0:42:23.54,0:42:31.74,Default,,0000,0000,0000,,Lian, Lian!
Dialogue: 0,0:42:49.24,0:42:59.05,Default,,0000,0000,0000,,~ {\i1}V minulosti, bež žiadnych plánov,{\i0} ~\N~ {\i1}si prišiel sem do Seoulu.{\i0} ~
Dialogue: 0,0:42:59.05,0:43:06.79,Default,,0000,0000,0000,,~ {\i1}Tvoj zmätený výraz,{\i0} ~\N~ {\i1}vyzeral ako kedysi môj.{\i0} ~
Dialogue: 0,0:43:06.79,0:43:16.87,Default,,0000,0000,0000,,~ {\i1}Tvoj dôvod k žitiu,{\i0} ~\N~ {\i1}je jedno aký je.{\i0} ~
Dialogue: 0,0:43:16.87,0:43:24.11,Default,,0000,0000,0000,,~ {\i1}Ak to znamená žiť bez ľutovania{\i0} ~\N~ {\i1}tak si superstár.{\i0} ~
Dialogue: 0,0:43:24.11,0:43:28.45,Default,,0000,0000,0000,,~ {\i1}To je v poriadku, všetko bude dobré.{\i0} ~
Dialogue: 0,0:43:28.45,0:43:32.72,Default,,0000,0000,0000,,~ {\i1}Ako tvoja briliantná budúcnosť.{\i0} ~
Dialogue: 0,0:43:32.72,0:43:37.19,Default,,0000,0000,0000,,~ {\i1}To je v poriadku, všetko bude dobré.{\i0} ~
Dialogue: 0,0:43:37.19,0:43:42.29,Default,,0000,0000,0000,,~ {\i1}Nemali by sme nikdy pochybovať.{\i0} ~
Dialogue: 0,0:43:43.19,0:43:52.70,Default,,0000,0000,0000,,~ {\i1}Keď veci sú pre teba ťažkými a rozplačú ťa{\i0} ~\N~ {\i1}za všetok potu a sĺz,{\i0} ~
Dialogue: 0,0:43:52.70,0:43:57.04,Default,,0000,0000,0000,,~ {\i1}pripime si.{\i0} ~
Dialogue: 0,0:43:59.08,0:44:03.45,Default,,0000,0000,0000,,~ {\i1}To je v poriadku, všetko bude dobré.{\i0} ~
Dialogue: 0,0:44:03.45,0:44:07.69,Default,,0000,0000,0000,,~ {\i1}Ako tvoja briliantná budúcnosť.{\i0} ~
Dialogue: 0,0:44:07.69,0:44:11.92,Default,,0000,0000,0000,,~ {\i1}To je v poriadku, všetko bude dobré.{\i0} ~
Dialogue: 0,0:44:11.92,0:44:16.23,Default,,0000,0000,0000,,~ {\i1}Nemali by sme mať žiadne pochybnosti.{\i0} ~
Dialogue: 0,0:44:34.18,0:44:38.32,Default,,0000,0000,0000,,~ {\i1}To je to čo znamená byť ako superstár.{\i0} ~
Dialogue: 0,0:45:07.80,0:45:08.90,Default,,0000,0000,0000,,( Daj si s priateľmi. Otec. )
Dialogue: 0,0:45:33.63,0:45:35.83,Default,,0000,0000,0000,,Lian, nevrátila sa?
Dialogue: 0,0:45:36.93,0:45:37.83,Default,,0000,0000,0000,,Mmm...
Dialogue: 0,0:45:40.43,0:45:42.54,Default,,0000,0000,0000,,Ak sa vráti, povedz jej, nech ide do skúšobne.
Dialogue: 0,0:45:42.54,0:45:44.94,Default,,0000,0000,0000,,Totiž...
Dialogue: 0,0:45:56.15,0:45:57.55,Default,,0000,0000,0000,,Nie je tu?
Dialogue: 0,0:45:57.55,0:45:58.95,Default,,0000,0000,0000,,Kam zase išla?
Dialogue: 0,0:45:58.95,0:46:01.85,Default,,0000,0000,0000,,Vážne, hľadáme ju celý deň.
Dialogue: 0,0:46:02.16,0:46:05.76,Default,,0000,0000,0000,,Nepovedal som, aby si nechystala oslavu?
Dialogue: 0,0:46:11.66,0:46:14.47,Default,,0000,0000,0000,,Je to zadarmo, tak si dajte koľko chcete.
Dialogue: 0,0:46:17.57,0:46:20.37,Default,,0000,0000,0000,,Áno. Najedzte sa. Ochladzuje sa.
Dialogue: 0,0:46:20.37,0:46:22.58,Default,,0000,0000,0000,,Dám si.
Dialogue: 0,0:46:31.78,0:46:33.79,Default,,0000,0000,0000,,Nebudeš jesť?
Dialogue: 0,0:46:34.49,0:46:35.99,Default,,0000,0000,0000,,Tu.
Dialogue: 0,0:46:39.89,0:46:40.99,Default,,0000,0000,0000,,Daj si ešte.
Dialogue: 0,0:46:42.30,0:46:44.20,Default,,0000,0000,0000,,Počula som len o anti-fanúšikov,
Dialogue: 0,0:46:44.20,0:46:45.50,Default,,0000,0000,0000,,ale sú vážne hrozní.
Dialogue: 0,0:46:45.50,0:46:47.80,Default,,0000,0000,0000,,Akí fanúšikovia? Sú to len anti.
Dialogue: 0,0:46:47.80,0:46:51.10,Default,,0000,0000,0000,,Predtým som dostala aj mŕtve potkany.
Dialogue: 0,0:46:52.31,0:46:54.61,Default,,0000,0000,0000,,A holuba, ktorého zrazilo auto.
Dialogue: 0,0:46:55.41,0:46:57.31,Default,,0000,0000,0000,,10 švábov.
Dialogue: 0,0:46:58.41,0:46:59.41,Default,,0000,0000,0000,,Stačí.
Dialogue: 0,0:46:59.71,0:47:03.62,Default,,0000,0000,0000,,Ale je to lepšie, ako nemať žiadnu pozornosť.
Dialogue: 0,0:47:03.62,0:47:06.62,Default,,0000,0000,0000,,Takí ľudia by mali byť potrestaní.
Dialogue: 0,0:47:10.42,0:47:12.23,Default,,0000,0000,0000,,Papaj. Daj si.
Dialogue: 0,0:47:19.93,0:47:25.34,Default,,0000,0000,0000,,Viem, že si bol na mňa hnusný,
Dialogue: 0,0:47:27.24,0:47:31.74,Default,,0000,0000,0000,,aby si ma naučil, že?
Dialogue: 0,0:47:34.25,0:47:35.95,Default,,0000,0000,0000,,Myslel si si, že to poviem,
Dialogue: 0,0:47:35.95,0:47:38.65,Default,,0000,0000,0000,,ty otravný idiot!
Dialogue: 0,0:47:38.65,0:47:41.96,Default,,0000,0000,0000,,Miliónkrát otravný.
Dialogue: 0,0:47:42.76,0:47:46.56,Default,,0000,0000,0000,,Myslel si si, že ja...
Dialogue: 0,0:47:46.56,0:47:50.36,Default,,0000,0000,0000,,Myslel si si, že mi bude dobré byť ignorovaná?
Dialogue: 0,0:47:51.46,0:47:54.17,Default,,0000,0000,0000,,Talentovaná osoba ako si ty,
Dialogue: 0,0:47:54.17,0:47:56.47,Default,,0000,0000,0000,,oproti mne.
Dialogue: 0,0:48:28.60,0:48:28.80,Default,,0000,0000,0000,,1, 2, 3, 4.
Dialogue: 0,0:48:28.80,0:48:35.41,Default,,0000,0000,0000,,~ {\i1}Jedného dňa, keď sa znovu stretneme,{\i0} ~
Dialogue: 0,0:48:36.61,0:48:45.32,Default,,0000,0000,0000,,~ {\i1}tak sa môžeme na seba usmievať.{\i0} ~
Dialogue: 0,0:48:45.32,0:48:49.42,Default,,0000,0000,0000,,~ {\i1}Spoločne sme rozhodnutí,{\i0} ~
Dialogue: 0,0:48:49.42,0:48:52.23,Default,,0000,0000,0000,,~ {\i1}že dáme dnešnú krásu,{\i0} ~
Dialogue: 0,0:48:52.23,0:48:53.53,Default,,0000,0000,0000,,Komu?
Dialogue: 0,0:48:53.53,0:48:59.63,Default,,0000,0000,0000,,~ {\i1}táto pieseň je len pre teba.{\i0} ~
Dialogue: 0,0:49:30.56,0:49:32.77,Default,,0000,0000,0000,,Nie je to vôbec smiešne.
Dialogue: 0,0:49:50.88,0:49:52.09,Default,,0000,0000,0000,,Čo?
Dialogue: 0,0:49:52.69,0:49:54.79,Default,,0000,0000,0000,,To nie je môj štýl.
Dialogue: 0,0:49:54.79,0:49:56.19,Default,,0000,0000,0000,,Nič.
Dialogue: 0,0:49:58.79,0:49:59.79,Default,,0000,0000,0000,,Nesmej sa!
Dialogue: 0,0:50:03.30,0:50:06.10,Default,,0000,0000,0000,,Nesmej sa. Nesmej sa keď sa nemáš.
Dialogue: 0,0:50:08.40,0:50:10.20,Default,,0000,0000,0000,,Sfúkni sviečky.
Dialogue: 0,0:50:10.20,0:50:12.11,Default,,0000,0000,0000,,Keď nesfúkneš, spravím to ja.
Dialogue: 0,0:50:52.75,0:50:54.95,Default,,0000,0000,0000,,Všetko najlepšie, Lian.
Dialogue: 0,0:50:57.95,0:50:59.65,Default,,0000,0000,0000,,Nech sa páči, všetko najlepšie!
Dialogue: 0,0:50:59.65,0:51:01.05,Default,,0000,0000,0000,,Dám vám kolu.
Dialogue: 0,0:51:01.05,0:51:02.76,Default,,0000,0000,0000,,Ďakujeme!
Dialogue: 0,0:51:02.76,0:51:04.66,Default,,0000,0000,0000,,Prečo nám dávate iba toto?
Dialogue: 0,0:51:04.66,0:51:06.36,Default,,0000,0000,0000,,Čože?
Dialogue: 0,0:51:06.36,0:51:08.76,Default,,0000,0000,0000,,Nepovedali ste, že máme piť ako dospeláci?
Dialogue: 0,0:51:09.46,0:51:11.86,Default,,0000,0000,0000,,Chceš aby ťa dospelák udrel?
Dialogue: 0,0:51:11.86,0:51:13.37,Default,,0000,0000,0000,,Prepáčte.
Dialogue: 0,0:51:13.37,0:51:14.17,Default,,0000,0000,0000,,Len to vypite.
Dialogue: 0,0:51:14.17,0:51:16.57,Default,,0000,0000,0000,,-Dobre. - Pite. Pite.
Dialogue: 0,0:51:17.07,0:51:21.98,Default,,0000,0000,0000,,Napime sa. Dajme si.
Dialogue: 0,0:51:33.29,0:51:34.59,Default,,0000,0000,0000,,Kam ideš?
Dialogue: 0,0:51:42.30,0:51:43.50,Default,,0000,0000,0000,,Do ubytovne.
Dialogue: 0,0:51:44.70,0:51:47.10,Default,,0000,0000,0000,,Bojíš sa o Shin HaeSung?
Dialogue: 0,0:51:49.10,0:51:52.01,Default,,0000,0000,0000,,Nie, idem sa prezliecť.
Dialogue: 0,0:52:05.02,0:52:10.22,Default,,0000,0000,0000,,Za to čo sa predtým stalo, ti ďakujem.
Dialogue: 0,0:52:35.45,0:52:36.25,Default,,0000,0000,0000,,~ {\i1}Budeš si pamätať môj sen?{\i0} ~
Dialogue: 0,0:52:36.25,0:52:41.75,Default,,0000,0000,0000,,~ {\i1}Budeš si pamätať môj sen?{\i0} ~
Dialogue: 0,0:52:41.75,0:52:45.16,Default,,0000,0000,0000,,~ {\i1}Usmej sa pre mňa.{\i0} ~
Dialogue: 0,0:52:45.16,0:52:47.86,Default,,0000,0000,0000,,~ {\i1}Znovu povstaň.{\i0} ~
Dialogue: 0,0:52:47.86,0:52:54.87,Default,,0000,0000,0000,,~ {\i1}Prosím povedz mi, že to dokážeš.{\i0} ~
Dialogue: 0,0:53:00.67,0:53:02.68,Default,,0000,0000,0000,,Takže najprv tu.
Dialogue: 0,0:53:04.48,0:53:06.48,Default,,0000,0000,0000,,Táto pesnička...
Dialogue: 0,0:53:11.78,0:53:14.09,Default,,0000,0000,0000,,je tá, s ktorou si chcela, aby som ti pomohol.
Dialogue: 0,0:53:14.09,0:53:15.69,Default,,0000,0000,0000,,Mám pravdu?
Dialogue: 0,0:53:15.69,0:53:19.19,Default,,0000,0000,0000,,Ako dlho si tam stál?
Dialogue: 0,0:53:20.09,0:53:22.00,Default,,0000,0000,0000,,Počul si všetko?
Dialogue: 0,0:53:24.70,0:53:26.40,Default,,0000,0000,0000,,Pokračuj.
Dialogue: 0,0:53:26.40,0:53:28.00,Default,,0000,0000,0000,,Som veľmi zvedavý.
Dialogue: 0,0:53:40.61,0:53:44.32,Default,,0000,0000,0000,,Nechcela si ísť naspäť do ubytovne?
Dialogue: 0,0:53:44.32,0:53:46.72,Default,,0000,0000,0000,,Idem so skúšobne.
Dialogue: 0,0:53:46.72,0:53:48.52,Default,,0000,0000,0000,,Prečo?
Dialogue: 0,0:53:50.32,0:53:53.33,Default,,0000,0000,0000,,Tak ty si potom prečo prišiel do školy? Je neskoro.
Dialogue: 0,0:53:54.43,0:53:55.93,Default,,0000,0000,0000,,Ja...
Dialogue: 0,0:53:56.93,0:53:59.03,Default,,0000,0000,0000,,niekoho hľadám.
Dialogue: 0,0:54:06.44,0:54:07.84,Default,,0000,0000,0000,,~ {\i1}Ako sa máš?{\i0} ~
Dialogue: 0,0:54:07.84,0:54:12.45,Default,,0000,0000,0000,,~ {\i1}Ako sa ti darí so snami?{\i0} ~
Dialogue: 0,0:54:12.45,0:54:17.55,Default,,0000,0000,0000,,~ {\i1}Nebolí ťa to, aj keď sa ti ubližuje?{\i0} ~
Dialogue: 0,0:54:18.45,0:54:24.86,Default,,0000,0000,0000,,~ {\i1}Možno si zvyknutý na život aký teraz máš.{\i0} ~
Dialogue: 0,0:54:24.86,0:54:28.56,Default,,0000,0000,0000,,~ {\i1}Necítiš sa niekedy unudene?{\i0} ~
Dialogue: 0,0:54:29.36,0:54:35.57,Default,,0000,0000,0000,,~ {\i1}Cítiš sa niekedy osamelo a veľmi obnosene?{\i0} ~
Dialogue: 0,0:54:35.57,0:54:40.77,Default,,0000,0000,0000,,~ {\i1}Budeš si pamätať môj sen?{\i0} ~
Dialogue: 0,0:54:41.67,0:54:44.88,Default,,0000,0000,0000,,~ {\i1}Usmej sa pre mňa.{\i0} ~
Dialogue: 0,0:54:44.88,0:54:47.68,Default,,0000,0000,0000,,~ {\i1}Znovu povstaň.{\i0} ~
Dialogue: 0,0:54:47.68,0:54:52.99,Default,,0000,0000,0000,,~ {\i1}Nechaj byť tvoje srdce šťastným čo najviac.{\i0} ~
Dialogue: 0,0:54:55.39,0:55:01.49,Default,,0000,0000,0000,,~ {\i1}Ahoj. Je 11.mesiac roku 2011.{\i0} ~
Dialogue: 0,0:55:01.49,0:55:04.90,Default,,0000,0000,0000,,~ {\i1}Je veľmi chladné počasie.{\i0} ~
Dialogue: 0,0:55:30.32,0:55:31.82,Default,,0000,0000,0000,,Čo to teraz robíte?
Dialogue: 0,0:55:44.24,0:55:46.54,Default,,0000,0000,0000,,Nedivím sa, že si nebol s nami.
Dialogue: 0,0:56:03.36,0:56:04.56,Default,,0000,0000,0000,,Povedal som, porozprávajme sa.
Dialogue: 0,0:56:04.56,0:56:07.06,Default,,0000,0000,0000,,Čo chceš zrazu povedať?
Dialogue: 0,0:56:08.26,0:56:10.16,Default,,0000,0000,0000,,Viem, že dnes máš narodeniny.
Dialogue: 0,0:56:11.16,0:56:13.27,Default,,0000,0000,0000,,Čo je tak skvelé na narodeninách?
Dialogue: 0,0:56:13.27,0:56:16.17,Default,,0000,0000,0000,,Nestarám sa, či si vedel, že dnes mám narodeniny alebo nie.
Dialogue: 0,0:56:16.17,0:56:18.17,Default,,0000,0000,0000,,Ale,
Dialogue: 0,0:56:18.17,0:56:20.27,Default,,0000,0000,0000,,tvoj zrak sa veľmi zhoršil,
Dialogue: 0,0:56:21.07,0:56:23.68,Default,,0000,0000,0000,,veď si tam sedel a počúval ako spieva.
Dialogue: 0,0:56:23.68,0:56:24.78,Default,,0000,0000,0000,,A čo potom ty?
Dialogue: 0,0:56:25.78,0:56:27.38,Default,,0000,0000,0000,,Počul som, že si vystupovala na ulici.
Dialogue: 0,0:56:28.08,0:56:30.28,Default,,0000,0000,0000,,Nie je vzhľad tvojho manažmentu trochu chatrný?
Dialogue: 0,0:56:30.28,0:56:31.58,Default,,0000,0000,0000,,Bola to zábava.
Dialogue: 0,0:56:32.29,0:56:35.29,Default,,0000,0000,0000,,Môj hlas celkom sa hodí s tomu Jin YuJin-ovmu.
Dialogue: 0,0:56:35.59,0:56:39.19,Default,,0000,0000,0000,,Asi už viem, prečo náš vedúci je dojatý jeho umeleckým talentom.
Dialogue: 0,0:56:42.50,0:56:45.10,Default,,0000,0000,0000,,Stále to nevieš?
Dialogue: 0,0:56:45.50,0:56:49.10,Default,,0000,0000,0000,,Náš vedúci si chce vybrať Jin YuJin-a.
Dialogue: 0,0:56:49.90,0:56:52.01,Default,,0000,0000,0000,,Prebuď sa.
Dialogue: 0,0:56:52.01,0:56:55.11,Default,,0000,0000,0000,,Teraz nie je vhodný čas, aby si nám niečo hovoril.
Dialogue: 0,0:56:56.11,0:56:57.21,Default,,0000,0000,0000,,Už si skončila?
Dialogue: 0,0:56:57.21,0:56:58.21,Default,,0000,0000,0000,,Nie.
Dialogue: 0,0:56:59.21,0:57:02.92,Default,,0000,0000,0000,,Ak budeš mať čas, príď sa pozrieť na našu show.
Dialogue: 0,0:57:03.42,0:57:04.42,Default,,0000,0000,0000,,Vaše vystúpenie?
Dialogue: 0,0:57:04.42,0:57:05.62,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:57:06.32,0:57:08.92,Default,,0000,0000,0000,,Chcela som vystúpiť len raz.
Dialogue: 0,0:57:10.42,0:57:13.03,Default,,0000,0000,0000,,Ale vďaka niekomu chcem v tom pokračovať.
Dialogue: 0,0:57:15.63,0:57:17.43,Default,,0000,0000,0000,,Dokedy budeš takáto?
Dialogue: 0,0:57:21.83,0:57:23.54,Default,,0000,0000,0000,,Ty si začal.
Dialogue: 0,0:57:34.55,0:57:36.95,Default,,0000,0000,0000,,Dala si mikrofón Lian...
Dialogue: 0,0:57:36.95,0:57:38.75,Default,,0000,0000,0000,,a potom si si išla sadnúť aby si sama plakala?
Dialogue: 0,0:57:40.65,0:57:44.36,Default,,0000,0000,0000,,Predsa len, je to jediná vec, v ktorej si dobrá.
Dialogue: 0,0:57:45.16,0:57:47.56,Default,,0000,0000,0000,,Nechcem sa s tebou hádať.
Dialogue: 0,0:57:48.16,0:57:50.66,Default,,0000,0000,0000,,Kedy prestaneš utekať?
Dialogue: 0,0:57:52.07,0:57:56.57,Default,,0000,0000,0000,,Ty, prestaň sa tváriť, že ti na mne záleží.
Dialogue: 0,0:57:56.57,0:57:59.57,Default,,0000,0000,0000,,Niekedy keď niečo zle spravím,
Dialogue: 0,0:57:59.57,0:58:01.37,Default,,0000,0000,0000,,tiež mnou pohŕdaš, pravda?
Dialogue: 0,0:58:02.78,0:58:03.88,Default,,0000,0000,0000,,Pohŕdač mnou,
Dialogue: 0,0:58:03.88,0:58:06.38,Default,,0000,0000,0000,,a preto mi rozkazuješ.
Dialogue: 0,0:58:09.38,0:58:14.29,Default,,0000,0000,0000,,Ty a Lian ste šťastne spolu vystupovali.
Dialogue: 0,0:58:15.39,0:58:17.09,Default,,0000,0000,0000,,Odviedli ste skvelú prácu.
Dialogue: 0,0:58:17.09,0:58:19.89,Default,,0000,0000,0000,,Čo som spravila ja, je iba mojou vecou.
Dialogue: 0,0:58:20.39,0:58:23.20,Default,,0000,0000,0000,,Žiarliš teraz, že Lian ti ukradla miesto?
Dialogue: 0,0:58:25.00,0:58:27.20,Default,,0000,0000,0000,,Pred ňou nedokážeš vydať ani hlások,
Dialogue: 0,0:58:27.20,0:58:30.60,Default,,0000,0000,0000,,a za jej chrbtom si naštvaná a zahanbujúca.
Dialogue: 0,0:58:30.60,0:58:32.81,Default,,0000,0000,0000,,Takto žiješ?
Dialogue: 0,0:58:36.61,0:58:40.41,Default,,0000,0000,0000,,Tak, chceš ešte vystupovať, alebo nie?
Dialogue: 0,0:58:42.02,0:58:43.82,Default,,0000,0000,0000,,Zober Lian.
Dialogue: 0,0:58:46.42,0:58:47.62,Default,,0000,0000,0000,,Fajn.
Dialogue: 0,0:58:48.22,0:58:49.52,Default,,0000,0000,0000,,Rozumiem.
Dialogue: 0,0:58:51.72,0:58:53.43,Default,,0000,0000,0000,,To sa Shin HaeSung podobá.
Dialogue: 0,0:59:20.15,0:59:22.06,Default,,0000,0000,0000,,Nemôžeš to vyhodiť?
Dialogue: 0,0:59:23.66,0:59:26.36,Default,,0000,0000,0000,,Nepovedala si, že už nie si JB-ho fanúšička?
Dialogue: 0,0:59:26.86,0:59:29.36,Default,,0000,0000,0000,,Prečo, otravujte ťa to?
Dialogue: 0,0:59:29.36,0:59:31.76,Default,,0000,0000,0000,,Nevidíš, že spím na dlážke kvôli tebe?
Dialogue: 0,0:59:31.76,0:59:34.67,Default,,0000,0000,0000,,Prečo si tak hlasná, že neviem zaspať?
Dialogue: 0,0:59:38.17,0:59:39.57,Default,,0000,0000,0000,,Hej, samotárka!
Dialogue: 0,0:59:40.37,0:59:42.08,Default,,0000,0000,0000,,Ak to nevydržíš, tak vypadni!
Dialogue: 0,0:59:42.08,0:59:43.78,Default,,0000,0000,0000,,Dávaj si pozor čo hovoríš.
Dialogue: 0,0:59:46.58,0:59:47.98,Default,,0000,0000,0000,,Ah, naozaj!
Dialogue: 0,0:59:52.19,0:59:54.79,Default,,0000,0000,0000,,Hej, nech je to čo chce, musíš to vydržať.
Dialogue: 0,0:59:54.79,0:59:56.59,Default,,0000,0000,0000,,A prečo by som musela?
Dialogue: 0,0:59:56.59,0:59:57.89,Default,,0000,0000,0000,,Pusti mi ruku.
Dialogue: 0,1:00:10.80,1:00:11.80,Default,,0000,0000,0000,,Proste odíď.
Dialogue: 0,1:00:11.80,1:00:14.31,Default,,0000,0000,0000,,Vzdaj sa svojho ega, dokážeš niekoho udrieť tak ľahko.
Dialogue: 0,1:00:14.31,1:00:16.41,Default,,0000,0000,0000,,Čo sa deje?\N Vráťte sa.
Dialogue: 0,1:00:19.41,1:00:20.61,Default,,0000,0000,0000,,Čo sa stalo?
Dialogue: 0,1:00:37.53,1:00:38.53,Default,,0000,0000,0000,,To čo?
Dialogue: 0,1:00:39.03,1:00:40.23,Default,,0000,0000,0000,,Kto to sem dal...
Dialogue: 0,1:00:40.23,1:00:41.23,Default,,0000,0000,0000,,Neviem.
Dialogue: 0,1:00:43.24,1:00:44.14,Default,,0000,0000,0000,,Aj ja mám jedno.
Dialogue: 0,1:00:44.14,1:00:45.84,Default,,0000,0000,0000,,- Oh naozaj?\N- Aj ty máš?
Dialogue: 0,1:00:49.64,1:00:51.64,Default,,0000,0000,0000,,Lian tu nie je?
Dialogue: 0,1:00:51.64,1:00:52.65,Default,,0000,0000,0000,,Mmm.
Dialogue: 0,1:00:56.05,1:00:59.45,Default,,0000,0000,0000,,Zrejme ich sem dala Shin HaeSung.
Dialogue: 0,1:00:59.45,1:01:00.65,Default,,0000,0000,0000,,Shin HaeSung? Prečo?
Dialogue: 0,1:01:01.05,1:01:02.46,Default,,0000,0000,0000,,Odkiaľ to vieš?
Dialogue: 0,1:01:04.46,1:01:06.86,Default,,0000,0000,0000,,Odkiaľ vieš, že ich tam dala Shin HaeSung?
Dialogue: 0,1:01:26.48,1:01:28.68,Default,,0000,0000,0000,,Snaží sa takto každému ospravedlniť?
Dialogue: 0,1:01:28.68,1:01:29.88,Default,,0000,0000,0000,,Ktovie.
Dialogue: 0,1:01:35.39,1:01:39.29,Default,,0000,0000,0000,,Vaša dcéra sa nesprávala dobre počas suspendovanej periódy.
Dialogue: 0,1:01:39.29,1:01:43.00,Default,,0000,0000,0000,,Dokonca aj použila násilie na jej spolužiačku.
Dialogue: 0,1:01:43.70,1:01:46.00,Default,,0000,0000,0000,,To bola nehoda.
Dialogue: 0,1:01:46.00,1:01:48.70,Default,,0000,0000,0000,,Aj ona to povedala.
Dialogue: 0,1:01:48.70,1:01:50.20,Default,,0000,0000,0000,,Buď ticho.
Dialogue: 0,1:01:53.01,1:01:57.41,Default,,0000,0000,0000,,Teraz či chce Shin HaeSung debutovať\N alebo ísť na umeleckú univerzitu,
Dialogue: 0,1:01:57.41,1:01:58.91,Default,,0000,0000,0000,,obe možnosti budú ťažké.
Dialogue: 0,1:02:00.01,1:02:01.81,Default,,0000,0000,0000,,Dokonca aj na našom vystúpení,
Dialogue: 0,1:02:01.81,1:02:03.62,Default,,0000,0000,0000,,spravila problémy.
Dialogue: 0,1:02:03.62,1:02:06.92,Default,,0000,0000,0000,,V učení je stále dobrá...
Dialogue: 0,1:02:08.42,1:02:11.42,Default,,0000,0000,0000,,To ale moc nepomáha na našej škole.
Dialogue: 0,1:02:11.42,1:02:15.83,Default,,0000,0000,0000,,Samozrejme, lepšie je byť dobrá v učení než zlá.
Dialogue: 0,1:02:15.83,1:02:17.93,Default,,0000,0000,0000,,Neignorujete jej silu,
Dialogue: 0,1:02:17.93,1:02:20.83,Default,,0000,0000,0000,,a všímate si iba jej slabosti?
Dialogue: 0,1:02:20.83,1:02:22.04,Default,,0000,0000,0000,,Huh?
Dialogue: 0,1:02:22.44,1:02:24.74,Default,,0000,0000,0000,,Keby som bol učiteľom,
Dialogue: 0,1:02:24.74,1:02:27.34,Default,,0000,0000,0000,,zo súcitu k žiakom kvôli ich neostej budúcnosti,
Dialogue: 0,1:02:27.34,1:02:29.34,Default,,0000,0000,0000,,by som mal zlomené srdce.
Dialogue: 0,1:02:29.74,1:02:31.84,Default,,0000,0000,0000,,Tušil som, že táto škola má takýchto učiteľov.
Dialogue: 0,1:02:31.84,1:02:33.75,Default,,0000,0000,0000,,Ani som nepriviedol svoje dieťa domov,
Dialogue: 0,1:02:33.75,1:02:34.85,Default,,0000,0000,0000,,a plne ju dal škole na zodpovednosť.
Dialogue: 0,1:02:34.85,1:02:35.95,Default,,0000,0000,0000,,Oci...
Dialogue: 0,1:02:35.95,1:02:38.05,Default,,0000,0000,0000,,Len buď ticho.
Dialogue: 0,1:02:41.15,1:02:42.96,Default,,0000,0000,0000,,Chcem ju dať preč na inú školu.
Dialogue: 0,1:02:44.06,1:02:45.16,Default,,0000,0000,0000,,Prosím?
Dialogue: 0,1:02:45.16,1:02:46.26,Default,,0000,0000,0000,,Oci.
Dialogue: 0,1:02:46.96,1:02:51.97,Default,,0000,0000,0000,,Nemôžem nechať svoje dieťa takejto škole.
Dialogue: 0,1:02:54.27,1:02:55.07,Default,,0000,0000,0000,,Poďme.
Dialogue: 0,1:02:55.07,1:02:57.07,Default,,0000,0000,0000,,Oci. Otec!
Dialogue: 0,1:02:57.57,1:03:00.17,Default,,0000,0000,0000,,Oci!
Dialogue: 0,1:03:04.68,1:03:06.18,Default,,0000,0000,0000,,Nejdem.
Dialogue: 0,1:03:06.18,1:03:08.48,Default,,0000,0000,0000,,Kam ideš?
Dialogue: 0,1:03:09.48,1:03:10.98,Default,,0000,0000,0000,,Prestaň hovoriť nezmysli!
Dialogue: 0,1:03:10.98,1:03:12.89,Default,,0000,0000,0000,,Pripravím pre teba papier na prestup do inej školy.
Dialogue: 0,1:03:12.89,1:03:13.79,Default,,0000,0000,0000,,Najprv choď domov.
Dialogue: 0,1:03:13.79,1:03:16.69,Default,,0000,0000,0000,,Som veľmi unavená kvôli otcovi.
Dialogue: 0,1:03:16.69,1:03:18.59,Default,,0000,0000,0000,,Preto som ti povedal, aby si to prestala robiť.
Dialogue: 0,1:03:19.09,1:03:20.89,Default,,0000,0000,0000,,Ak stále nebudeš so mnou súhlasiť,
Dialogue: 0,1:03:20.89,1:03:22.60,Default,,0000,0000,0000,,tak potom v budúcnosti sa postaraj sama o seba.
Dialogue: 0,1:03:22.60,1:03:24.90,Default,,0000,0000,0000,,Ja už sa do toho potom miešať nebudem.
Dialogue: 0,1:03:24.90,1:03:27.50,Default,,0000,0000,0000,,Ej. Už neviem čo viac povedať.
Dialogue: 0,1:05:02.70,1:05:05.10,Default,,0000,0000,0000,,Em. Tieto jablká chutia dobre.
Dialogue: 0,1:05:08.20,1:05:09.70,Default,,0000,0000,0000,,Ak ich chceš zahodiť,
Dialogue: 0,1:05:09.70,1:05:11.20,Default,,0000,0000,0000,,môžem ich mať ja?
Dialogue: 0,1:05:11.80,1:05:13.51,Default,,0000,0000,0000,,Naozaj mám rád jablká.
Dialogue: 0,1:05:26.22,1:05:31.02,Default,,0000,0000,0000,,Ja...chcem utiecť.
Dialogue: 0,1:05:31.02,1:05:32.93,Default,,0000,0000,0000,,Chcem utiecť.
Dialogue: 0,1:05:32.93,1:05:34.43,Default,,0000,0000,0000,,Akoby to nestačilo.
Dialogue: 0,1:05:35.23,1:05:37.83,Default,,0000,0000,0000,,Chcem odísť.
Dialogue: 0,1:05:49.04,1:05:52.35,Default,,0000,0000,0000,,Normálne dievčatá vyzerajú pekne keď plačú.
Dialogue: 0,1:05:54.65,1:05:55.85,Default,,0000,0000,0000,,Naozaj sa ti nehodí plač.
Dialogue: 0,1:05:57.35,1:05:58.25,Default,,0000,0000,0000,,Už viac neplač.
Dialogue: 0,1:05:59.05,1:06:00.25,Default,,0000,0000,0000,,Prestaň!
