Dialogue: 0,0:00:00.00,0:00:04.20,Default,,0000,0000,0000,,SK Titulky: Hyumi - seesska.blog.cz\N ENG Subs: DramaFever
Dialogue: 0,0:00:04.50,0:00:08.30,Default,,0000,0000,0000,,Teraz tu stoja dve posledné súťažiace bez partnerov.
Dialogue: 0,0:00:08.50,0:00:12.50,Default,,0000,0000,0000,,JB a Jin YuJin neprišli.
Dialogue: 0,0:00:12.60,0:00:16.04,Default,,0000,0000,0000,,Lian a Shin HaeSung,
Dialogue: 0,0:00:16.17,0:00:20.54,Default,,0000,0000,0000,,chcete byť spolu?
Dialogue: 0,0:01:12.60,0:01:17.60,Default,,0000,0000,0000,,Je naozaj pre mňa ťažké povedať to,
Dialogue: 0,0:01:17.80,0:01:20.54,Default,,0000,0000,0000,,ale už som sa rozhodol.
Dialogue: 0,0:01:20.67,0:01:27.14,Default,,0000,0000,0000,,HaeSung, mám ťa rád.
Dialogue: 0,0:01:27.41,0:01:30.41,Default,,0000,0000,0000,,Prečo?
Dialogue: 0,0:01:30.61,0:01:33.15,Default,,0000,0000,0000,,Nie je dôvod.
Dialogue: 0,0:01:33.28,0:01:34.99,Default,,0000,0000,0000,,Je jedno ako premýšľam,
Dialogue: 0,0:01:35.15,0:01:39.92,Default,,0000,0000,0000,,stále tu nie je dôvod.
Dialogue: 0,0:01:40.29,0:01:42.66,Default,,0000,0000,0000,,Prečo mám rád hudbu?
Dialogue: 0,0:01:42.79,0:01:45.36,Default,,0000,0000,0000,,Prečo chcem byť spevákom?
Dialogue: 0,0:01:45.53,0:01:50.10,Default,,0000,0000,0000,,Takisto ani k tomu nie je dôvod.
Dialogue: 0,0:01:50.23,0:01:55.47,Default,,0000,0000,0000,,Proste ťa mám rád.
Dialogue: 0,0:01:55.61,0:01:59.48,Default,,0000,0000,0000,,Chcem s tebou spievať,
Dialogue: 0,0:01:59.48,0:02:02.61,Default,,0000,0000,0000,,chcem stáť s tebou na javisku,
Dialogue: 0,0:02:02.61,0:02:05.62,Default,,0000,0000,0000,,a proste s tebou spievať.
Dialogue: 0,0:02:10.18,0:02:15.35,Default,,0000,0000,0000,,~ {\i1}Budem sa ťa pevne držať.{\i0} ~
Dialogue: 0,0:02:16.79,0:02:22.63,Default,,0000,0000,0000,,~ {\i1}Vždy pri tebe pozerať sa na mňa.{\i0} ~
Dialogue: 0,0:02:23.23,0:02:29.31,Default,,0000,0000,0000,,Shin HaeSung, prijímaš vyznanie JB-a?
Dialogue: 0,0:02:29.60,0:02:35.47,Default,,0000,0000,0000,,~ {\i1}Znovu si ma nechal v pozadí podľahnúť.{\i0} ~
Dialogue: 0,0:02:36.07,0:02:41.11,Default,,0000,0000,0000,,~ {\i1}V blikajúcich lúčov svetla.{\i0} ~
Dialogue: 0,0:02:41.75,0:02:44.82,Default,,0000,0000,0000,,{\i1}~ Ty si hviezda. ~{\i0}
Dialogue: 0,0:02:44.82,0:02:48.42,Default,,0000,0000,0000,,{\i1}~ Najjasnejšia hviezda sveta. ~{\i0}
Dialogue: 0,0:02:48.89,0:02:53.76,Default,,0000,0000,0000,,{\i1}~ Tie svetlá rozsvietili moje tmavé srdce. ~{\i0}
Dialogue: 0,0:02:54.26,0:02:56.99,Default,,0000,0000,0000,,{\i1}~ Ty si hviezda. ~{\i0}
Dialogue: 0,0:02:57.53,0:03:01.10,Default,,0000,0000,0000,,{\i1}~ Najjasnejšia hviezda sveta ~{\i0}
Dialogue: 0,0:03:01.60,0:03:08.01,Default,,0000,0000,0000,,{\i1}~ Ktorá roztápa moje chladné srdce ~{\i0}
Dialogue: 0,0:03:19.02,0:03:20.89,Default,,0000,0000,0000,,Študentka HaeSung a JB,
Dialogue: 0,0:03:20.89,0:03:22.39,Default,,0000,0000,0000,,sa stali partnermi.
Dialogue: 0,0:03:22.39,0:03:23.69,Default,,0000,0000,0000,,Teraz tí čo zostávajú sú
Dialogue: 0,0:03:23.69,0:03:28.27,Default,,0000,0000,0000,,Lian a Jin YuJin.
Dialogue: 0,0:03:58.27,0:04:00.06,Default,,0000,0000,0000,,Spojením sa Lian a Jin YuJinom,
Dialogue: 0,0:04:00.06,0:04:01.70,Default,,0000,0000,0000,,sú páry rozhodnuté.
Dialogue: 0,0:04:02.53,0:04:03.93,Default,,0000,0000,0000,,Až do dňa vystúpenia,
Dialogue: 0,0:04:03.93,0:04:06.30,Default,,0000,0000,0000,,aby ste dobre vystúpili,
Dialogue: 0,0:04:06.30,0:04:08.31,Default,,0000,0000,0000,,želám všetkým veľa šťastia.
Dialogue: 0,0:04:41.44,0:04:43.77,Default,,0000,0000,0000,,To čo som povedala, o kamarátkach,
Dialogue: 0,0:04:43.77,0:04:45.48,Default,,0000,0000,0000,,beriem späť.
Dialogue: 0,0:04:48.95,0:04:51.22,Default,,0000,0000,0000,,Gratulujem.
Dialogue: 0,0:04:52.08,0:04:54.02,Default,,0000,0000,0000,,Ísť proti hlavnému riaditeľovi,
Dialogue: 0,0:04:54.02,0:04:55.82,Default,,0000,0000,0000,,robíš prvýkrát, že?
Dialogue: 0,0:04:58.16,0:05:00.16,Default,,0000,0000,0000,,Lian...
Dialogue: 0,0:05:00.16,0:05:02.49,Default,,0000,0000,0000,,Ďakujem.
Dialogue: 0,0:05:02.49,0:05:03.86,Default,,0000,0000,0000,,Vďaka tebe som si uvedomila,
Dialogue: 0,0:05:03.86,0:05:08.23,Default,,0000,0000,0000,,že som sa celý čas mýlila.
Dialogue: 0,0:05:09.83,0:05:12.24,Default,,0000,0000,0000,,Vďaka tebe a Shin HaeSung,
Dialogue: 0,0:05:12.24,0:05:17.04,Default,,0000,0000,0000,,budem oveľa vážnejšia.
Dialogue: 0,0:05:17.04,0:05:20.91,Default,,0000,0000,0000,,Chybu čo si spravil,
Dialogue: 0,0:05:20.91,0:05:22.85,Default,,0000,0000,0000,,si čoskoro uvedomíš.
Dialogue: 0,0:05:28.85,0:05:31.22,Default,,0000,0000,0000,,Ďakujem.
Dialogue: 0,0:05:31.22,0:05:32.66,Default,,0000,0000,0000,,Úprimne.
Dialogue: 0,0:05:45.54,0:05:47.30,Default,,0000,0000,0000,,Pomôžem ti,
Dialogue: 0,0:05:47.30,0:05:49.74,Default,,0000,0000,0000,,vyhrať to.
Dialogue: 0,0:05:50.98,0:05:54.18,Default,,0000,0000,0000,,Vyhráme to.
Dialogue: 0,0:06:17.13,0:06:22.27,Default,,0000,0000,0000,,Takže chystáš sa znovu súťažiť?
Dialogue: 0,0:06:22.47,0:06:25.21,Default,,0000,0000,0000,,Mami, neveríš mi?
Dialogue: 0,0:06:25.34,0:06:28.05,Default,,0000,0000,0000,,Budem prvá.
Dialogue: 0,0:06:28.21,0:06:29.51,Default,,0000,0000,0000,,Nerozpadla sa HershE?
Dialogue: 0,0:06:29.71,0:06:31.78,Default,,0000,0000,0000,,Prečo musíš zostať v tej spoločnosti?
Dialogue: 0,0:06:31.92,0:06:34.22,Default,,0000,0000,0000,,Môžeš ísť do inej a znovu hrať.
Dialogue: 0,0:06:34.35,0:06:36.65,Default,,0000,0000,0000,,Ak chceš tak veľmi spievať,
Dialogue: 0,0:06:36.85,0:06:39.09,Default,,0000,0000,0000,,tak sa pripoj k dievčenskej skupine v inej firme.
Dialogue: 0,0:06:39.29,0:06:42.29,Default,,0000,0000,0000,,Chceš aby som bola zase tvárou skupiny?
Dialogue: 0,0:06:42.46,0:06:44.23,Default,,0000,0000,0000,,Čo je na tom zlé?
Dialogue: 0,0:06:44.40,0:06:47.03,Default,,0000,0000,0000,,Nie je to preto, že si veľmi krásna?
Dialogue: 0,0:06:47.23,0:06:52.17,Default,,0000,0000,0000,,Mami, už som z tých slov chorá.
Dialogue: 0,0:06:57.54,0:07:00.34,Default,,0000,0000,0000,,Len počkaj.
Dialogue: 0,0:07:00.48,0:07:02.65,Default,,0000,0000,0000,,Budem najlepšia.
Dialogue: 0,0:07:03.28,0:07:06.78,Default,,0000,0000,0000,,Využijem svoje schopnosti, aby som to každému dokázala.
Dialogue: 0,0:07:07.22,0:07:10.02,Default,,0000,0000,0000,,Ahjumma, doneste mi prosím ďalšiu misku ryže.
Dialogue: 0,0:07:27.91,0:07:30.11,Default,,0000,0000,0000,,Nevyzerá to, akoby sa nič vážne nestalo?
Dialogue: 0,0:07:30.27,0:07:31.68,Default,,0000,0000,0000,,Včera bolo včera.
Dialogue: 0,0:07:31.78,0:07:34.35,Default,,0000,0000,0000,,Podľa jej výrazu, to vyzerá akoby na všetko zabudla.
Dialogue: 0,0:07:35.35,0:07:37.98,Default,,0000,0000,0000,,Ale nie je nič medzi nimi, že?
Dialogue: 0,0:07:38.12,0:07:39.92,Default,,0000,0000,0000,,Včera najvyhľadávanejšia téma bola,
Dialogue: 0,0:07:40.08,0:07:41.79,Default,,0000,0000,0000,,že JB má dve priateľky.
Dialogue: 0,0:07:47.02,0:07:49.86,Default,,0000,0000,0000,,Už ste sa s tým stretli raz, pri skúškach na konci mesiaca.
Dialogue: 0,0:07:50.03,0:07:52.73,Default,,0000,0000,0000,,Základom v duete je rozumieť si.
Dialogue: 0,0:07:52.90,0:07:55.53,Default,,0000,0000,0000,,Len keď viete o silných a slabých stránkach\N toho druhého,
Dialogue: 0,0:07:55.67,0:07:58.47,Default,,0000,0000,0000,,môžete spolupracovať.
Dialogue: 0,0:07:58.60,0:08:01.04,Default,,0000,0000,0000,,Spolupracovať na základe druhého silnej stránky,
Dialogue: 0,0:08:01.17,0:08:03.04,Default,,0000,0000,0000,,a skryť svoje nedostatky.
Dialogue: 0,0:08:03.21,0:08:04.68,Default,,0000,0000,0000,,Rozumením si a spoluprácou,
Dialogue: 0,0:08:04.84,0:08:08.65,Default,,0000,0000,0000,,budete v perfektnej harmónií.
Dialogue: 0,0:08:08.85,0:08:12.42,Default,,0000,0000,0000,,Poďme, pýtajte sa jeden druhého.
Dialogue: 0,0:08:12.55,0:08:14.22,Default,,0000,0000,0000,,Spýtajte sa na všetko, čo vás zaujíma,
Dialogue: 0,0:08:14.35,0:08:17.72,Default,,0000,0000,0000,,a odpovedzte čo najúprimnejšie.
Dialogue: 0,0:08:21.09,0:08:26.86,Default,,0000,0000,0000,,Číslo telefónu. Meranie.
Dialogue: 0,0:08:27.80,0:08:31.30,Default,,0000,0000,0000,,Aký je tvoj ideálny typ?
Dialogue: 0,0:08:48.55,0:08:51.06,Default,,0000,0000,0000,,Vystrašila si ma.
Dialogue: 0,0:08:52.06,0:08:55.09,Default,,0000,0000,0000,,Je najlepšie byť aspoň trochu úprimný.
Dialogue: 0,0:08:55.29,0:08:57.49,Default,,0000,0000,0000,,Máš rád HaeSung.
Dialogue: 0,0:08:57.66,0:08:59.46,Default,,0000,0000,0000,,Vidím to svojimi očami.
Dialogue: 0,0:08:59.66,0:09:02.83,Default,,0000,0000,0000,,Hej, to je ti otázka? To je nútenie.
Dialogue: 0,0:09:03.30,0:09:05.40,Default,,0000,0000,0000,,Čo máš rád?
Dialogue: 0,0:09:05.54,0:09:06.90,Default,,0000,0000,0000,,Koho máš rád?
Dialogue: 0,0:09:07.10,0:09:09.61,Default,,0000,0000,0000,,Ako veľmi ju máš rád?
Dialogue: 0,0:09:09.84,0:09:12.34,Default,,0000,0000,0000,,Nepovedala, že si musíme rozumieť?
Dialogue: 0,0:09:12.48,0:09:13.98,Default,,0000,0000,0000,,Len tak môžeme byť prví.
Dialogue: 0,0:09:14.11,0:09:16.28,Default,,0000,0000,0000,,Stačí!
Dialogue: 0,0:09:17.21,0:09:21.79,Default,,0000,0000,0000,,Si nahnevaný. Červenáš sa.
Dialogue: 0,0:09:21.99,0:09:24.09,Default,,0000,0000,0000,,Čo? Čo?
Dialogue: 0,0:09:24.22,0:09:26.16,Default,,0000,0000,0000,,Nemusím nič viac vedieť.
Dialogue: 0,0:09:26.36,0:09:29.29,Default,,0000,0000,0000,,Jin YuJin má rád Shin HaeSung.
Dialogue: 0,0:09:29.43,0:09:32.03,Default,,0000,0000,0000,,Koniec chápania Jin YuJina.
Dialogue: 0,0:09:32.23,0:09:33.10,Default,,0000,0000,0000,,Prosím?
Dialogue: 0,0:09:33.23,0:09:34.97,Default,,0000,0000,0000,,Koniec.
Dialogue: 0,0:09:48.55,0:09:50.68,Default,,0000,0000,0000,,Na čo to je?
Dialogue: 0,0:09:50.78,0:09:52.28,Default,,0000,0000,0000,,Fotíme dnes niečo?
Dialogue: 0,0:09:52.48,0:09:53.48,Default,,0000,0000,0000,,Fotiť čo?
Dialogue: 0,0:09:53.58,0:09:54.79,Default,,0000,0000,0000,,Dedinské decká.
Dialogue: 0,0:09:54.92,0:09:56.72,Default,,0000,0000,0000,,Nepovedali, že dnes niečo nahrávame?
Dialogue: 0,0:09:56.92,0:09:58.72,Default,,0000,0000,0000,,Čo že to bolo?
Dialogue: 0,0:09:58.92,0:10:00.29,Default,,0000,0000,0000,,To...
Dialogue: 0,0:10:06.53,0:10:08.23,Default,,0000,0000,0000,,Nemusíte sa tváriť prekvapene.
Dialogue: 0,0:10:08.43,0:10:11.37,Default,,0000,0000,0000,,Dnešná špeciálna hodina je veľmi jednoduchá.
Dialogue: 0,0:10:11.54,0:10:14.10,Default,,0000,0000,0000,,Každý z partnerov toho svojho oblečie do čoho chce.
Dialogue: 0,0:10:14.24,0:10:16.87,Default,,0000,0000,0000,,Potom, začneme fotiť.
Dialogue: 0,0:10:17.61,0:10:19.68,Default,,0000,0000,0000,,Obliecť ako chceme?
Dialogue: 0,0:10:19.84,0:10:23.55,Default,,0000,0000,0000,,Podľa toho ako každý chce?
Dialogue: 0,0:10:51.18,0:10:53.18,Default,,0000,0000,0000,,Nie je playboyom?
Dialogue: 0,0:10:53.28,0:10:54.91,Default,,0000,0000,0000,,Ak má naozaj rád Nanu,
Dialogue: 0,0:10:55.11,0:10:56.85,Default,,0000,0000,0000,,nemal by mať rád iba ju?
Dialogue: 0,0:10:57.05,0:10:58.62,Default,,0000,0000,0000,,Takže, je oblečený
Dialogue: 0,0:10:58.82,0:11:01.85,Default,,0000,0000,0000,,v štýle úprimne naladeného muža,\N ktorý miluje iba jednu ženu.
Dialogue: 0,0:11:05.49,0:11:07.86,Default,,0000,0000,0000,,Nie je naozaj dobrý v spievani?
Dialogue: 0,0:11:08.06,0:11:11.43,Default,,0000,0000,0000,,Má všetko čo ženy majú rady.
Dialogue: 0,0:11:11.60,0:11:14.37,Default,,0000,0000,0000,,Ak je tu jedna vec, čo mu chýba,
Dialogue: 0,0:11:14.50,0:11:16.47,Default,,0000,0000,0000,,tak je to sebavedomie.
Dialogue: 0,0:11:16.60,0:11:20.30,Default,,0000,0000,0000,,V jeho srdci, je možno playboyom.
Dialogue: 0,0:11:20.47,0:11:23.81,Default,,0000,0000,0000,,Hudba je láska.
Dialogue: 0,0:11:28.18,0:11:31.05,Default,,0000,0000,0000,,Prepáčte, niekto Vás nahlásil za hlasnú hudbu.
Dialogue: 0,0:11:31.22,0:11:33.28,Default,,0000,0000,0000,,Som tu, aby som Vás zatkol.
Dialogue: 0,0:11:42.59,0:11:47.36,Default,,0000,0000,0000,,Je to trochu divné.
Dialogue: 0,0:11:48.23,0:11:50.67,Default,,0000,0000,0000,,Naozaj ti to sedí.
Dialogue: 0,0:11:50.87,0:11:52.17,Default,,0000,0000,0000,,Shin HaeSung, ktorú poznám,
Dialogue: 0,0:11:52.37,0:11:54.37,Default,,0000,0000,0000,,bola vždy labuťou.
Dialogue: 0,0:11:54.54,0:11:55.81,Default,,0000,0000,0000,,Myslela si, že je škaredým káčatkom,
Dialogue: 0,0:11:55.97,0:11:59.31,Default,,0000,0000,0000,,tak nemohla rozstrieť poriadne svoje krídla a vzlietnuť.
Dialogue: 0,0:12:01.11,0:12:05.78,Default,,0000,0000,0000,,Chcem ti ukázať, ako ďaleko môžeš lietať.
Dialogue: 0,0:12:12.92,0:12:17.56,Default,,0000,0000,0000,,Najradšej mám terajšieho JB-a.
Dialogue: 0,0:12:17.73,0:12:22.60,Default,,0000,0000,0000,,Ako ja, obyčajný stredoškolák JB.
Dialogue: 0,0:12:39.35,0:12:42.25,Default,,0000,0000,0000,,Aké je to, je to pekné?
Dialogue: 0,0:12:42.42,0:12:43.75,Default,,0000,0000,0000,,Pretože som princezná.
Dialogue: 0,0:12:43.85,0:12:46.29,Default,,0000,0000,0000,,Už prestaň s tvojou chorobou princeznej.
Dialogue: 0,0:12:46.92,0:12:49.49,Default,,0000,0000,0000,,Nebuď vždy taká pyšná.
Dialogue: 0,0:12:49.66,0:12:52.66,Default,,0000,0000,0000,,Odstráň tie mihalnice.
Dialogue: 0,0:12:52.80,0:12:55.37,Default,,0000,0000,0000,,Neťaží ťa to ani trochu?
Dialogue: 0,0:12:55.57,0:12:59.44,Default,,0000,0000,0000,,Máš ma rád?
Dialogue: 0,0:12:59.80,0:13:03.11,Default,,0000,0000,0000,,Ak si nedáš make-up, porozmýšľam o tom.
Dialogue: 0,0:13:05.54,0:13:09.48,Default,,0000,0000,0000,,Fajn, fajn, fajn.
Dialogue: 0,0:13:24.66,0:13:26.66,Default,,0000,0000,0000,,Jin YuJin, ja to viem...
Dialogue: 0,0:13:26.86,0:13:28.97,Default,,0000,0000,0000,,určite nie si v triede B.
Dialogue: 0,0:13:29.10,0:13:31.04,Default,,0000,0000,0000,,Si v triede A.
Dialogue: 0,0:13:31.24,0:13:35.11,Default,,0000,0000,0000,,A+ triede.
Dialogue: 0,0:13:35.21,0:13:37.71,Default,,0000,0000,0000,,Som nejaká kórejská hovädzina?
Dialogue: 0,0:13:44.65,0:13:48.85,Default,,0000,0000,0000,,1, 2, 3, G nota.
Dialogue: 0,0:14:17.92,0:14:20.85,Default,,0000,0000,0000,,Zmizni z cesty.
Dialogue: 0,0:14:21.51,0:14:28.22,Default,,0000,0000,0000,,{\i1}~ Až teraz, som konečne našiel moju druhú polovicu. ~ {\i0}
Dialogue: 0,0:14:28.42,0:14:33.52,Default,,0000,0000,0000,,{\i1}~ Srdce čo tak tlkoce, ~{\i0}
Dialogue: 0,0:14:33.69,0:14:36.09,Default,,0000,0000,0000,,{\i1}~ Našiel som ju, moju lásku. ~{\i0}
Dialogue: 0,0:14:36.29,0:14:38.20,Default,,0000,0000,0000,,{\i1}~ Tú ktorú hľadám, ~{\i0}
Dialogue: 0,0:14:38.40,0:14:42.27,Default,,0000,0000,0000,,{\i1}~chcem ti dať bezpečné objatie.~{\i0}
Dialogue: 0,0:15:40.60,0:15:44.60,Default,,0000,0000,0000,,Ja...toto spravím s Ailee.
Dialogue: 0,0:15:44.80,0:15:46.90,Default,,0000,0000,0000,,Je to v pohode?
Dialogue: 0,0:15:51.41,0:15:54.48,Default,,0000,0000,0000,,Seul, čo je s tebou?
Dialogue: 0,0:16:05.29,0:16:08.53,Default,,0000,0000,0000,,Keď cvičíte, komunikujte s emóciami druhými.
Dialogue: 0,0:16:08.66,0:16:11.30,Default,,0000,0000,0000,,Efektívne je tango.
Dialogue: 0,0:16:11.50,0:16:13.53,Default,,0000,0000,0000,,Učiteľ Yang!
Dialogue: 0,0:16:18.24,0:16:20.90,Default,,0000,0000,0000,,Vaše oči sa musia stretnúť.
Dialogue: 0,0:16:37.05,0:16:40.26,Default,,0000,0000,0000,,Ahn TaeYeon. Prečo tu stojíš ako blbec?
Dialogue: 0,0:16:40.46,0:16:41.76,Default,,0000,0000,0000,,Ako len môžeš?
Dialogue: 0,0:16:41.93,0:16:42.93,Default,,0000,0000,0000,,Pozri.
Dialogue: 0,0:16:43.06,0:16:44.13,Default,,0000,0000,0000,,Jeden pohľad a vieš,
Dialogue: 0,0:16:44.33,0:16:45.56,Default,,0000,0000,0000,,že uriteľ Yang nie je tvoj typ.
Dialogue: 0,0:16:45.76,0:16:48.87,Default,,0000,0000,0000,,Prečo si ním taká posadnutá?
Dialogue: 0,0:16:49.00,0:16:53.07,Default,,0000,0000,0000,,Si bláznivá, bláznivá.
Dialogue: 0,0:17:09.85,0:17:11.36,Default,,0000,0000,0000,,Nie je to zlé.
Dialogue: 0,0:17:11.45,0:17:13.52,Default,,0000,0000,0000,,Netancuješ zle.
Dialogue: 0,0:17:13.72,0:17:16.16,Default,,0000,0000,0000,,Študenti...viete dobre všetci tancovať, však?
Dialogue: 0,0:17:19.16,0:17:21.77,Default,,0000,0000,0000,,Doľava, vľavo.
Dialogue: 0,0:17:44.79,0:17:48.09,Default,,0000,0000,0000,,Vy dvaja naozaj...robte to poriadne.
Dialogue: 0,0:17:48.29,0:17:49.79,Default,,0000,0000,0000,,Poďte bližšie...
Dialogue: 0,0:17:49.96,0:17:52.73,Default,,0000,0000,0000,,Základom tanca pre dvoch je očný kontakt.
Dialogue: 0,0:17:52.90,0:17:55.03,Default,,0000,0000,0000,,Pozrite sa na seba s očami plnými lásky.
Dialogue: 0,0:17:55.23,0:17:56.10,Default,,0000,0000,0000,,Rýchlo!
Dialogue: 0,0:17:56.27,0:17:57.67,Default,,0000,0000,0000,,Chyťte si ruky!
Dialogue: 0,0:17:57.84,0:17:59.77,Default,,0000,0000,0000,,Držte sa.
Dialogue: 0,0:18:01.67,0:18:04.04,Default,,0000,0000,0000,,Rýchlejšie!
Dialogue: 0,0:18:44.78,0:18:46.58,Default,,0000,0000,0000,,Zase som to pokazila, že?
Dialogue: 0,0:18:48.29,0:18:51.16,Default,,0000,0000,0000,,Nie, tango sa neučí ľahko.
Dialogue: 0,0:18:51.36,0:18:53.42,Default,,0000,0000,0000,,Už teraz si veľmi dobrá.
Dialogue: 0,0:18:53.59,0:18:55.43,Default,,0000,0000,0000,,Skúsime to ešte?
Dialogue: 0,0:19:10.71,0:19:13.28,Default,,0000,0000,0000,,Základom tanga je sebaovládanie.
Dialogue: 0,0:19:13.41,0:19:16.11,Default,,0000,0000,0000,,Aj keď vaše chovanie dokazuje, ako moc sa chcete,
Dialogue: 0,0:19:16.28,0:19:20.85,Default,,0000,0000,0000,,nemôžete stratiť dohľad nad svojimi pocitmi.
Dialogue: 0,0:19:34.67,0:19:36.60,Default,,0000,0000,0000,,Skúsme to znovu.
Dialogue: 0,0:19:36.80,0:19:39.34,Default,,0000,0000,0000,,Hej, mali by ste sa na teba pozerať, keď tancujete.
Dialogue: 0,0:19:39.54,0:19:41.34,Default,,0000,0000,0000,,Ak sa na seba nepozeráte, je to úplne zlé.
Dialogue: 0,0:19:41.47,0:19:42.61,Default,,0000,0000,0000,,Poriadne sledujte.
Dialogue: 0,0:19:42.77,0:19:43.84,Default,,0000,0000,0000,,Tu, otoč sa.
Dialogue: 0,0:19:44.04,0:19:48.15,Default,,0000,0000,0000,,Takto. Sledujte sa keď tancujete, počas tanca sa pozerajte.
Dialogue: 0,0:19:48.15,0:19:49.11,Default,,0000,0000,0000,,Rozumieš?
Dialogue: 0,0:19:49.11,0:19:49.71,Default,,0000,0000,0000,,Áno.
Dialogue: 0,0:19:49.71,0:19:50.65,Default,,0000,0000,0000,,Už chápeš úplne tomu pocitu?
Dialogue: 0,0:19:50.65,0:19:53.22,Default,,0000,0000,0000,,Áno, už áno.
Dialogue: 0,0:20:10.57,0:20:13.07,Default,,0000,0000,0000,,Žije vďaka reputácií Super Idola.
Dialogue: 0,0:20:13.20,0:20:16.37,Default,,0000,0000,0000,,YuJin je sľubný.
Dialogue: 0,0:20:16.57,0:20:18.21,Default,,0000,0000,0000,,Keď je v tomto leveli,
Dialogue: 0,0:20:18.38,0:20:20.94,Default,,0000,0000,0000,,tak sa bude môcť rovnať JB-ovi.
Dialogue: 0,0:20:21.15,0:20:26.32,Default,,0000,0000,0000,,Tak výhra a prehra bude na Lian a HaeSung.
Dialogue: 0,0:20:26.52,0:20:28.39,Default,,0000,0000,0000,,HaeSung sa tiež zlepšila,
Dialogue: 0,0:20:28.59,0:20:30.39,Default,,0000,0000,0000,,a tiež nerobí toľko chýb.
Dialogue: 0,0:20:30.65,0:20:34.39,Default,,0000,0000,0000,,Áno, nie je prepadákom.
Dialogue: 0,0:20:34.63,0:20:38.13,Default,,0000,0000,0000,,Musí to byť sila lásky, že?
Dialogue: 0,0:20:50.07,0:20:51.51,Default,,0000,0000,0000,,Začnime.
Dialogue: 0,0:20:51.71,0:21:04.76,Default,,0000,0000,0000,,1, 2, 3, 4...
Dialogue: 0,0:21:11.33,0:21:17.03,Default,,0000,0000,0000,,Základom tanca pre dvoch je pozeranie si do očí.
Dialogue: 0,0:21:17.94,0:21:21.71,Default,,0000,0000,0000,,- Najprv, skús sa otáčať a urobiť kroky.\N- Okej.
Dialogue: 0,0:21:38.39,0:21:40.19,Default,,0000,0000,0000,,Už som ťa varoval.
Dialogue: 0,0:21:40.36,0:21:42.49,Default,,0000,0000,0000,,A aj tak robíš problémy.
Dialogue: 0,0:21:42.63,0:21:44.90,Default,,0000,0000,0000,,Dokážem vám,
Dialogue: 0,0:21:45.06,0:21:47.36,Default,,0000,0000,0000,,že mám právo stať sa Super Idolom.
Dialogue: 0,0:21:47.50,0:21:50.80,Default,,0000,0000,0000,,Dokážem vám to pomocou duetovej misie.
Dialogue: 0,0:21:51.00,0:21:53.80,Default,,0000,0000,0000,,Je jedno ako veľmi upadnem,\N stále sa budem vedieť vyšplhať hore.
Dialogue: 0,0:21:54.00,0:21:57.01,Default,,0000,0000,0000,,Dokážem vám to!
Dialogue: 0,0:21:57.17,0:21:59.04,Default,,0000,0000,0000,,Aj keď tvojou partnerkou je Shin HaeSung,
Dialogue: 0,0:21:59.18,0:22:01.15,Default,,0000,0000,0000,,si si istý?
Dialogue: 0,0:22:01.31,0:22:03.11,Default,,0000,0000,0000,,Som.
Dialogue: 0,0:22:03.31,0:22:06.32,Default,,0000,0000,0000,,HaeSung a mne sa to podarí.
Dialogue: 0,0:22:06.48,0:22:08.75,Default,,0000,0000,0000,,Budeš zodpovedný za to, čo si teraz povedal?
Dialogue: 0,0:22:08.95,0:22:12.92,Default,,0000,0000,0000,,Áno. Budem.
Dialogue: 0,0:22:13.06,0:22:15.89,Default,,0000,0000,0000,,Musíš si premyslieť niečo predtým, než to povieš.
Dialogue: 0,0:22:16.06,0:22:17.90,Default,,0000,0000,0000,,Len mi verte.
Dialogue: 0,0:22:18.00,0:22:23.63,Default,,0000,0000,0000,,Je to preto, že už si na samom konci,\Na preto nemôžeš padnúť ďalej?
Dialogue: 0,0:22:34.18,0:22:36.58,Default,,0000,0000,0000,,O čom premýšľaš?
Dialogue: 0,0:22:38.45,0:22:45.06,Default,,0000,0000,0000,,HaeSung, vynechajme hodinu.
Dialogue: 0,0:22:45.19,0:22:46.32,Default,,0000,0000,0000,,Čože?
Dialogue: 0,0:24:14.28,0:24:16.28,Default,,0000,0000,0000,,Oddýchnime si pred ďalším nácvikom.
Dialogue: 0,0:24:16.48,0:24:18.38,Default,,0000,0000,0000,,Teraz nemáme čas.
Dialogue: 0,0:24:18.52,0:24:20.88,Default,,0000,0000,0000,,Iba na chvíľku.
Dialogue: 0,0:24:21.09,0:24:23.25,Default,,0000,0000,0000,,Nie sme ani stroje.
Dialogue: 0,0:24:23.45,0:24:25.19,Default,,0000,0000,0000,,Je dobré byť vážnym,
Dialogue: 0,0:24:25.39,0:24:28.26,Default,,0000,0000,0000,,ale čo ak sa niečo stane keď budeme unavení.
Dialogue: 0,0:24:28.39,0:24:33.13,Default,,0000,0000,0000,,Ak to tak neurobíme, neprevalíme ich.
Dialogue: 0,0:24:33.40,0:24:35.33,Default,,0000,0000,0000,,Asi si zabudla,
Dialogue: 0,0:24:35.50,0:24:37.94,Default,,0000,0000,0000,,že som Super Idolom.
Dialogue: 0,0:24:38.14,0:24:41.51,Default,,0000,0000,0000,,Nič nerobím vážne,\N pre to, že by som chcel niekoho preraziť.
Dialogue: 0,0:24:45.58,0:24:50.95,Default,,0000,0000,0000,,Prečo ťa vybrali za Super Idola?
Dialogue: 0,0:24:51.45,0:24:56.45,Default,,0000,0000,0000,,Pretože som na to kvalifikovaný.
Dialogue: 0,0:24:57.55,0:25:03.39,Default,,0000,0000,0000,,Prečo od začiatku vybrali Jin YuJina,
Dialogue: 0,0:25:03.49,0:25:06.33,Default,,0000,0000,0000,,a nie JB-a?
Dialogue: 0,0:25:12.70,0:25:15.14,Default,,0000,0000,0000,,Kam pôjdeme?
Dialogue: 0,0:25:15.64,0:25:17.57,Default,,0000,0000,0000,,Oh, počkať.
Dialogue: 0,0:25:20.44,0:25:22.75,Default,,0000,0000,0000,,Nemám peniaze.
Dialogue: 0,0:25:28.32,0:25:30.95,Default,,0000,0000,0000,,To nebude stačiť.
Dialogue: 0,0:25:31.56,0:25:34.09,Default,,0000,0000,0000,,Bude.
Dialogue: 0,0:25:34.09,0:25:34.56,Default,,0000,0000,0000,,Toto bude stačiť.
Dialogue: 0,0:25:46.30,0:25:50.01,Default,,0000,0000,0000,,Aké je to? Hodí sa mi to?
Dialogue: 0,0:25:50.21,0:25:53.38,Default,,0000,0000,0000,,S týmto vyzeráš obyčajne.
Dialogue: 0,0:25:55.88,0:25:57.65,Default,,0000,0000,0000,,Ideme!
Dialogue: 0,0:25:59.52,0:26:01.45,Default,,0000,0000,0000,,Čo tým myslíš?
Dialogue: 0,0:26:01.59,0:26:04.76,Default,,0000,0000,0000,,Výsledky súťaže už sú hotové.
Dialogue: 0,0:26:04.96,0:26:07.19,Default,,0000,0000,0000,,Netreba byť prekvapený.
Dialogue: 0,0:26:07.32,0:26:12.13,Default,,0000,0000,0000,,Náš riaditeľ je veľmi silná osobnosť.
Dialogue: 0,0:26:12.76,0:26:17.27,Default,,0000,0000,0000,,Môže vytvoriť škandály, ktoré sú lepšie ako obyčajné texty.
Dialogue: 0,0:26:17.40,0:26:20.77,Default,,0000,0000,0000,,Spraviť vyhodnotenie je pre neho ničím.
Dialogue: 0,0:26:20.94,0:26:22.97,Default,,0000,0000,0000,,Povedala si, že je to JB?
Dialogue: 0,0:26:23.54,0:26:27.14,Default,,0000,0000,0000,,A čo my? A čo ja?
Dialogue: 0,0:26:28.58,0:26:37.15,Default,,0000,0000,0000,,Ja som JB-ho družička. Ty si JB-ho náhradník.
Dialogue: 0,0:26:37.15,0:26:39.76,Default,,0000,0000,0000,,Náhradník?
Dialogue: 0,0:26:40.46,0:26:43.09,Default,,0000,0000,0000,,Počul si o náhradníkovi?
Dialogue: 0,0:26:43.26,0:26:49.63,Default,,0000,0000,0000,,Ak si náhradníkom, čo máš v popise práce?
Dialogue: 0,0:27:02.81,0:27:05.15,Default,,0000,0000,0000,,Prepáč, Jin YuJin.
Dialogue: 0,0:27:05.85,0:27:10.35,Default,,0000,0000,0000,,Nemôžeš byť z toho vyvedený z miery.
Dialogue: 0,0:27:10.72,0:27:12.46,Default,,0000,0000,0000,,Musíme sa snažiť.
Dialogue: 0,0:27:14.26,0:27:19.40,Default,,0000,0000,0000,,Určite to musíme vyhrať.
Dialogue: 0,0:27:24.60,0:27:27.84,Default,,0000,0000,0000,,Šwindler, ktorý je dobrý v klamaní!
Dialogue: 0,0:27:29.54,0:27:35.78,Default,,0000,0000,0000,,Hej, Jin YuJin. Keď hovoríš bez premýšľania, kto by ti rozumel?
Dialogue: 0,0:27:35.98,0:27:39.48,Default,,0000,0000,0000,,Rozprávaj jasne.
Dialogue: 0,0:27:39.68,0:27:42.12,Default,,0000,0000,0000,,Výsledky súťaže už boli rozhodnuté.
Dialogue: 0,0:27:42.29,0:27:44.56,Default,,0000,0000,0000,,Aby JB vyhral.
Dialogue: 0,0:27:44.66,0:27:47.16,Default,,0000,0000,0000,,Kto povedal, že chcem byť náhradníkom?
Dialogue: 0,0:27:48.83,0:27:51.43,Default,,0000,0000,0000,,Slová tohto decká sú nejaké výstižné.
Dialogue: 0,0:27:51.56,0:27:53.86,Default,,0000,0000,0000,,Odpovedzte mi!
Dialogue: 0,0:27:54.30,0:27:56.43,Default,,0000,0000,0000,,Výsledky súťaže sú už rozhodnuté.
Dialogue: 0,0:27:56.57,0:27:58.00,Default,,0000,0000,0000,,Je to pravda?
Dialogue: 0,0:27:58.14,0:28:00.10,Default,,0000,0000,0000,,Nie je to rozhodnuté.
Dialogue: 0,0:28:00.30,0:28:02.61,Default,,0000,0000,0000,,Ako vám môžem veriť?
Dialogue: 0,0:28:02.81,0:28:06.58,Default,,0000,0000,0000,,Ale to že si náhradníkom, je pravda.
Dialogue: 0,0:28:06.74,0:28:11.55,Default,,0000,0000,0000,,Až doteraz si vol skvelým, to je tiež pravda.
Dialogue: 0,0:28:12.95,0:28:16.19,Default,,0000,0000,0000,,Či už si náhradníkom alebo výhercom,
Dialogue: 0,0:28:16.39,0:28:21.19,Default,,0000,0000,0000,,je na tebe.
Dialogue: 0,0:28:27.63,0:28:30.50,Default,,0000,0000,0000,,Čo to je?
Dialogue: 0,0:28:31.84,0:28:34.94,Default,,0000,0000,0000,,Je to formulár o kontrakte s OZ Spoločnosťou.
Dialogue: 0,0:28:35.14,0:28:37.84,Default,,0000,0000,0000,,Aj s písomným súhlasom tvojich rodičov.
Dialogue: 0,0:28:38.04,0:28:41.11,Default,,0000,0000,0000,,Nebudeme sa ťa pýtať na aktivity.
Dialogue: 0,0:29:10.04,0:29:11.11,Default,,0000,0000,0000,,To je skvelé.
Dialogue: 0,0:29:25.92,0:29:31.60,Default,,0000,0000,0000,,Vyber si, vyber. Kúpme toto.
Dialogue: 0,0:29:47.61,0:29:48.91,Default,,0000,0000,0000,,Úplne rovnaké.
Dialogue: 0,0:29:49.05,0:29:51.42,Default,,0000,0000,0000,,Ako to môže byť rovnaké?
Dialogue: 0,0:30:24.28,0:30:26.12,Default,,0000,0000,0000,,Moc chutné.
Dialogue: 0,0:30:31.06,0:30:32.72,Default,,0000,0000,0000,,Pozri.
Dialogue: 0,0:30:35.49,0:30:37.29,Default,,0000,0000,0000,,Wow, úžasné.
Dialogue: 0,0:30:49.37,0:30:50.91,Default,,0000,0000,0000,,Nech sa páči.
Dialogue: 0,0:30:51.11,0:30:51.88,Default,,0000,0000,0000,,Ďakujem.
Dialogue: 0,0:30:51.98,0:30:52.90,Default,,0000,0000,0000,,Možno budete mať viac na predaj!
Dialogue: 0,0:30:52.98,0:30:54.41,Default,,0000,0000,0000,,Ďakujem.
Dialogue: 0,0:30:54.98,0:30:57.78,Default,,0000,0000,0000,,Tu, pre teba.
Dialogue: 0,0:30:59.72,0:31:01.55,Default,,0000,0000,0000,,Prečo mi dávaľ lyžicu a paličky?
Dialogue: 0,0:31:01.72,0:31:03.42,Default,,0000,0000,0000,,Darček.
Dialogue: 0,0:31:04.79,0:31:07.06,Default,,0000,0000,0000,,Už som dostal veľa darčekov,
Dialogue: 0,0:31:07.22,0:31:10.29,Default,,0000,0000,0000,,bale toto je prvýkrát, čo som dostal lyžičku a paličky.
Dialogue: 0,0:31:10.49,0:31:14.16,Default,,0000,0000,0000,,Vtedy, keď si ma nakreslil ako lyžicu
Dialogue: 0,0:31:14.37,0:31:17.67,Default,,0000,0000,0000,,a dal ma k paličkám, som bola naozaj vďačná.
Dialogue: 0,0:31:18.10,0:31:20.30,Default,,0000,0000,0000,,Ah...to...
Dialogue: 0,0:31:20.40,0:31:22.81,Default,,0000,0000,0000,,Žartujem.
Dialogue: 0,0:31:22.91,0:31:25.91,Default,,0000,0000,0000,,Rád ješ. Nechcem aby si bol hladný.
Dialogue: 0,0:31:26.11,0:31:30.31,Default,,0000,0000,0000,,Preto som ti to dala.
Dialogue: 0,0:31:30.68,0:31:34.99,Default,,0000,0000,0000,,Tak ti tiež dávam darček.
Dialogue: 0,0:31:37.12,0:31:41.36,Default,,0000,0000,0000,,Keď sa ide jesť, nie je lyžička s paličkami vždy spolu?
Dialogue: 0,0:31:41.56,0:31:43.66,Default,,0000,0000,0000,,Presne ako niť a ihla,
Dialogue: 0,0:31:43.86,0:31:49.53,Default,,0000,0000,0000,,nechcem aby sme sa rozdelili.
Dialogue: 0,0:31:49.80,0:31:53.24,Default,,0000,0000,0000,,Je to divné?
Dialogue: 0,0:32:07.75,0:32:13.66,Default,,0000,0000,0000,,Ja lietam. Ja lietam.
Dialogue: 0,0:32:57.47,0:33:00.84,Default,,0000,0000,0000,,Toto nie je niečo, čo ľudia robia.
Dialogue: 0,0:33:01.54,0:33:04.21,Default,,0000,0000,0000,,Mali by sme to zmeniť.
Dialogue: 0,0:33:04.41,0:33:06.98,Default,,0000,0000,0000,,Zmeniť? Zmeniť čo?
Dialogue: 0,0:33:07.78,0:33:11.52,Default,,0000,0000,0000,,Nana, nemôžem nacvičovať s tebou?
Dialogue: 0,0:33:11.62,0:33:16.99,Default,,0000,0000,0000,,Budem naozaj dobrý.
Dialogue: 0,0:33:17.19,0:33:20.86,Default,,0000,0000,0000,,Nemôžem mu opätovať svoje city.
Dialogue: 0,0:33:21.06,0:33:24.23,Default,,0000,0000,0000,,Tak čo keby som použil viac ženský hlas na spev?
Dialogue: 0,0:33:24.40,0:33:27.30,Default,,0000,0000,0000,,Bude to lepšie?
Dialogue: 0,0:33:27.46,0:33:28.90,Default,,0000,0000,0000,,Je to najlepšia voľba.
Dialogue: 0,0:33:29.10,0:33:31.34,Default,,0000,0000,0000,,Len mi dôveruj, áno?
Dialogue: 0,0:33:34.54,0:33:38.18,Default,,0000,0000,0000,,Pripravení...akcia!
Dialogue: 0,0:33:40.98,0:33:45.98,Default,,0000,0000,0000,,Ja lietam. Ja lietam.
Dialogue: 0,0:34:00.56,0:34:05.93,Default,,0000,0000,0000,,~ {\i1}Musel som sa zamilovať.{\i0} ~
Dialogue: 0,0:34:06.36,0:34:10.50,Default,,0000,0000,0000,,~ {\i1}Keď sa pozrieš na mňa, moje srdce búši o závod.{\i0} ~
Dialogue: 0,0:34:12.17,0:34:17.54,Default,,0000,0000,0000,,~ {\i1}Odkedy bolo moje srdce tak nadšené.{\i0} ~
Dialogue: 0,0:34:18.24,0:34:22.55,Default,,0000,0000,0000,,~ {\i1}Teraz pomaly, bolesť začína.{\i0} ~
Dialogue: 0,0:34:23.01,0:34:25.45,Default,,0000,0000,0000,,~ {\i1}Zamilovávam sa.{\i0} ~
Dialogue: 0,0:34:25.72,0:34:28.59,Default,,0000,0000,0000,,~ {\i1}Ako čas ubieha.{\i0} ~
Dialogue: 0,0:34:29.02,0:34:31.39,Default,,0000,0000,0000,,~ {\i1}Zamilovávam sa.{\i0} ~
Dialogue: 0,0:34:31.76,0:34:34.59,Default,,0000,0000,0000,,~ {\i1}Moje city sa zväčšujú.{\i0} ~
Dialogue: 0,0:34:35.03,0:34:41.73,Default,,0000,0000,0000,,~ {\i1}Aj bez teba, myslím na teba celý deň.{\i0} ~
Dialogue: 0,0:34:41.97,0:34:49.91,Default,,0000,0000,0000,,~ {\i1}Tu som. Čo by som mal urobiť?{\i0} ~
Dialogue: 0,0:35:15.41,0:35:19.04,Default,,0000,0000,0000,,{\i1}Čo povedali moji rodičia??{\i0}
Dialogue: 0,0:35:19.84,0:35:22.95,Default,,0000,0000,0000,,{\i1}Len tak to podpísali?{\i0}
Dialogue: 0,0:35:23.15,0:35:25.85,Default,,0000,0000,0000,,{\i1}Povedali, že je to jediní vec,\N čo môžu pre teba urobiť.{\i0}
Dialogue: 0,0:35:26.02,0:35:29.09,Default,,0000,0000,0000,,{\i1}Tiež súhlasili, že sa nebudú do teba starať.{\i0}
Dialogue: 0,0:35:29.29,0:35:31.09,Default,,0000,0000,0000,,{\i1}Chcú, aby si bol úspešný.{\i0}
Dialogue: 0,0:36:42.03,0:36:43.09,Default,,0000,0000,0000,,Jin YuJin.
Dialogue: 0,0:36:51.34,0:36:53.34,Default,,0000,0000,0000,,Deje sa niečo?
Dialogue: 0,0:36:53.54,0:36:56.64,Default,,0000,0000,0000,,Nemám náladu na teba.
Dialogue: 0,0:37:00.61,0:37:02.91,Default,,0000,0000,0000,,Zahráš si hru?
Dialogue: 0,0:37:09.09,0:37:11.39,Default,,0000,0000,0000,,Chceš sa vsadiť?
Dialogue: 0,0:37:11.52,0:37:12.79,Default,,0000,0000,0000,,Vsádzka?
Dialogue: 0,0:37:12.96,0:37:16.16,Default,,0000,0000,0000,,Len hoď a skóruj odtiaľto.
Dialogue: 0,0:37:16.59,0:37:18.00,Default,,0000,0000,0000,,O čo sa chceš vsadiť?
Dialogue: 0,0:37:18.16,0:37:19.40,Default,,0000,0000,0000,,Ak vyhráš,
Dialogue: 0,0:37:19.53,0:37:23.10,Default,,0000,0000,0000,,dám ti všetko čo chceš.
Dialogue: 0,0:37:23.27,0:37:25.34,Default,,0000,0000,0000,,Oh...naozaj?
Dialogue: 0,0:37:26.67,0:37:29.27,Default,,0000,0000,0000,,Ale ak prehráš,
Dialogue: 0,0:37:29.41,0:37:31.48,Default,,0000,0000,0000,,Shin HaeSung bude patriť mne.
Dialogue: 0,0:37:39.02,0:37:41.35,Default,,0000,0000,0000,,O čom to hovoríš?
Dialogue: 0,0:37:51.80,0:37:53.53,Default,,0000,0000,0000,,To si taký skvelý?
Dialogue: 0,0:37:53.73,0:37:56.97,Default,,0000,0000,0000,,Jang WooJae, myslíš si, že si taký úžasný?
Dialogue: 0,0:37:57.17,0:37:59.90,Default,,0000,0000,0000,,Jin YuJin!
Dialogue: 0,0:38:00.70,0:38:05.48,Default,,0000,0000,0000,,Kto si myslíš, že si?\N Kto si, že ma využívaš?
Dialogue: 0,0:38:05.68,0:38:11.42,Default,,0000,0000,0000,,Náhradník? Prečo som náhradníkom?
Dialogue: 0,0:38:11.92,0:38:15.42,Default,,0000,0000,0000,,Vedel si to, nie?
Dialogue: 0,0:38:15.59,0:38:18.22,Default,,0000,0000,0000,,Odpovedz mi!
Dialogue: 0,0:38:19.86,0:38:24.29,Default,,0000,0000,0000,,Vedel.
Dialogue: 0,0:38:30.90,0:38:34.41,Default,,0000,0000,0000,,Prestaň. Stačí!
Dialogue: 0,0:38:34.84,0:38:36.77,Default,,0000,0000,0000,,Mal by si sa uhýbať, keď ťa udierajú!
Dialogue: 0,0:38:36.97,0:38:38.71,Default,,0000,0000,0000,,Prečo ty? Kto si myslíš, že si?
Dialogue: 0,0:38:38.84,0:38:40.28,Default,,0000,0000,0000,,Prečo nič nerobíš?
Dialogue: 0,0:38:40.41,0:38:44.05,Default,,0000,0000,0000,,Aj tak viem, že nemôžem utiecť.
Dialogue: 0,0:38:46.78,0:38:49.85,Default,,0000,0000,0000,,Viem ako sa cítiš.
Dialogue: 0,0:38:50.09,0:38:54.93,Default,,0000,0000,0000,,Ale neutekaj.
Dialogue: 0,0:38:55.09,0:39:00.96,Default,,0000,0000,0000,,Aj keď musíš prejsť bitkou, musíš byť vytrvalý.
Dialogue: 0,0:39:10.21,0:39:11.51,Default,,0000,0000,0000,,Si v poriadku?
Dialogue: 0,0:39:54.59,0:39:58.66,Default,,0000,0000,0000,,Mám niečo pre teba.
Dialogue: 0,0:39:58.86,0:40:00.39,Default,,0000,0000,0000,,Pre mňa?
Dialogue: 0,0:40:13.50,0:40:16.97,Default,,0000,0000,0000,,Díky.
Dialogue: 0,0:40:18.91,0:40:21.28,Default,,0000,0000,0000,,Nespýtaš sa ma,
Dialogue: 0,0:40:21.45,0:40:25.15,Default,,0000,0000,0000,,prečo mi to YuJin urobil?
Dialogue: 0,0:40:26.28,0:40:29.72,Default,,0000,0000,0000,,Som veľmi zvedavá,
Dialogue: 0,0:40:29.89,0:40:35.83,Default,,0000,0000,0000,,ale verím ti.
Dialogue: 0,0:40:38.76,0:40:41.70,Default,,0000,0000,0000,,Je zima. Nejdeme dnu?
Dialogue: 0,0:40:41.90,0:40:44.60,Default,,0000,0000,0000,,Buďme tu ešte chvíľu.
Dialogue: 0,0:40:46.07,0:40:50.84,Default,,0000,0000,0000,,Dnes nevidno hviezdy.
Dialogue: 0,0:40:54.01,0:41:00.15,Default,,0000,0000,0000,,Ty....vedel si?
Dialogue: 0,0:41:00.35,0:41:10.76,Default,,0000,0000,0000,,Keď som s tebou, je to ako sen.
Dialogue: 0,0:41:11.46,0:41:18.27,Default,,0000,0000,0000,,Den, ktorý už viac nemôžem snívať.
Dialogue: 0,0:41:22.67,0:41:27.77,Default,,0000,0000,0000,,~ {\i1}V tichu blikajúcich lúčov{\i0} ~
Dialogue: 0,0:41:28.31,0:41:31.28,Default,,0000,0000,0000,,{\i1}~ si ty hviezdou. ~{\i0}
Dialogue: 0,0:41:31.51,0:41:35.28,Default,,0000,0000,0000,,{\i1}~ Najjasnejšou hviezdou sveta. ~{\i0}
Dialogue: 0,0:41:35.48,0:41:40.48,Default,,0000,0000,0000,,{\i1}~ Tie lúče rozsvietili moje tmavé srdce. ~{\i0}
Dialogue: 0,0:41:41.08,0:41:43.72,Default,,0000,0000,0000,,{\i1}~ Ty si hviezda. ~{\i0}
Dialogue: 0,0:41:44.29,0:41:47.82,Default,,0000,0000,0000,,{\i1}~ Najjasnejšia hviezda sveta. ~{\i0}
Dialogue: 0,0:41:49.10,0:41:51.84,Default,,0000,0000,0000,,Už to nie je sen.
Dialogue: 0,0:41:59.14,0:42:00.34,Default,,0000,0000,0000,,Hviezda!
Dialogue: 0,0:42:00.58,0:42:01.78,Default,,0000,0000,0000,,Kde?
Dialogue: 0,0:42:02.65,0:42:09.02,Default,,0000,0000,0000,,Nie ta....m, ale tu.
Dialogue: 0,0:42:09.88,0:42:13.72,Default,,0000,0000,0000,,{\i1}~ Najjasnejšia hviezda sveta. ~{\i0}
Dialogue: 0,0:42:13.98,0:42:19.86,Default,,0000,0000,0000,,{\i1}~ Ktorá zohrieva moje chladné srdce. ~{\i0}
Dialogue: 0,0:43:35.77,0:43:39.88,Default,,0000,0000,0000,,Počula som o tvojom otcovi.
Dialogue: 0,0:43:41.38,0:43:47.89,Default,,0000,0000,0000,,Bolo by pekné, keby mohol vidieť tvoje debut vystúpenie.
Dialogue: 0,0:43:49.52,0:43:52.32,Default,,0000,0000,0000,,Nemohol sa toho dočkať.
Dialogue: 0,0:44:02.73,0:44:07.20,Default,,0000,0000,0000,,Rozíďme sa.
Dialogue: 0,0:44:13.34,0:44:17.18,Default,,0000,0000,0000,,Sľúbil som otcovi,
Dialogue: 0,0:44:17.35,0:44:21.05,Default,,0000,0000,0000,,že budem najlepší.
Dialogue: 0,0:44:21.25,0:44:23.99,Default,,0000,0000,0000,,Kvôli debut vystúpenie,
Dialogue: 0,0:44:24.15,0:44:27.29,Default,,0000,0000,0000,,som mu nemohol skoliť poslednú úctu.
Dialogue: 0,0:44:31.86,0:44:36.57,Default,,0000,0000,0000,,Ten sľub, byť najlepším,
Dialogue: 0,0:44:36.57,0:44:39.87,Default,,0000,0000,0000,,chcem splniť.
Dialogue: 0,0:44:54.89,0:44:59.62,Default,,0000,0000,0000,,Ty si ten, čo prerušil sľub.
Dialogue: 0,0:44:59.62,0:45:03.13,Default,,0000,0000,0000,,Pôjdem v pohode.
Dialogue: 0,0:45:03.13,0:45:06.86,Default,,0000,0000,0000,,Keď nadíde ten čas, spýtam sa ťa.
Dialogue: 0,0:45:06.86,0:45:11.04,Default,,0000,0000,0000,,Prečo ja nie,
Dialogue: 0,0:45:11.04,0:45:13.64,Default,,0000,0000,0000,,ale Shin HaeSung áno?
Dialogue: 0,0:45:24.37,0:45:27.31,Default,,0000,0000,0000,,~ {\i1}Viem, že budeš superstár.{\i0} ~
Dialogue: 0,0:45:29.62,0:45:31.76,Default,,0000,0000,0000,,Áno! Sme piaty!
Dialogue: 0,0:45:31.88,0:45:34.89,Default,,0000,0000,0000,,~ {\i1}Jedného dňa ňou budeš.{\i0} ~
Dialogue: 0,0:45:35.15,0:45:39.19,Default,,0000,0000,0000,,~ {\i1}Sme výhercovia.{\i0} ~
Dialogue: 0,0:45:39.36,0:45:42.29,Default,,0000,0000,0000,,~ {\i1}Viem, že budeš superstár.{\i0} ~
Dialogue: 0,0:45:43.03,0:45:44.84,Default,,0000,0000,0000,,Sme štvrtí!
Dialogue: 0,0:45:45.04,0:45:46.00,Default,,0000,0000,0000,,To je milé.
Dialogue: 0,0:45:46.00,0:45:46.96,Default,,0000,0000,0000,,Hmmm...
Dialogue: 0,0:45:46.96,0:45:49.93,Default,,0000,0000,0000,,~ {\i1}Jedného dňa ňou budeš.{\i0} ~
Dialogue: 0,0:45:51.48,0:45:54.78,Default,,0000,0000,0000,,Toto číslo nie je ani dobré ani zlé,
Dialogue: 0,0:45:54.95,0:45:56.91,Default,,0000,0000,0000,,čo si máme myslieť o čísle 2?
Dialogue: 0,0:45:58.02,0:46:00.88,Default,,0000,0000,0000,,Netreba sa ani pozerať, je tu trojka.
Dialogue: 0,0:46:06.06,0:46:07.93,Default,,0000,0000,0000,,Čo sa deje?
Dialogue: 0,0:46:08.09,0:46:10.79,Default,,0000,0000,0000,,Vymeňme si čísla.
Dialogue: 0,0:46:10.99,0:46:13.90,Default,,0000,0000,0000,,Aké máte?
Dialogue: 0,0:46:14.33,0:46:15.43,Default,,0000,0000,0000,,1.
Dialogue: 0,0:46:15.73,0:46:16.97,Default,,0000,0000,0000,,V žiadnom prípade.
Dialogue: 0,0:46:17.40,0:46:19.04,Default,,0000,0000,0000,,Byť prvým je najhoršie.
Dialogue: 0,0:46:19.20,0:46:20.90,Default,,0000,0000,0000,,Nie je vača pieseň živá?
Dialogue: 0,0:46:21.04,0:46:23.64,Default,,0000,0000,0000,,To hneď zaujme pozornosť.
Dialogue: 0,0:46:23.81,0:46:26.68,Default,,0000,0000,0000,,Naše vystúpenie musí ísť neskôr.
Dialogue: 0,0:46:26.88,0:46:29.01,Default,,0000,0000,0000,,Len raz.
Dialogue: 0,0:46:29.18,0:46:30.91,Default,,0000,0000,0000,,Aspoň raz pomôžte.
Dialogue: 0,0:46:31.08,0:46:31.95,Default,,0000,0000,0000,,Iba teraz....
Dialogue: 0,0:46:31.95,0:46:33.78,Default,,0000,0000,0000,,UiBong.
Dialogue: 0,0:46:41.19,0:46:41.96,Default,,0000,0000,0000,,Na.
Dialogue: 0,0:46:41.96,0:46:43.23,Default,,0000,0000,0000,,Veľa šťastia!
Dialogue: 0,0:46:43.39,0:46:45.40,Default,,0000,0000,0000,,Ďakujem.
Dialogue: 0,0:46:45.76,0:46:47.53,Default,,0000,0000,0000,,Ďakujem.
Dialogue: 0,0:46:49.63,0:46:51.37,Default,,0000,0000,0000,,Päťka.
Dialogue: 0,0:46:57.34,0:47:00.64,Default,,0000,0000,0000,,Aké vyznania by ste chceli dostať?
Dialogue: 0,0:47:00.81,0:47:02.88,Default,,0000,0000,0000,,Obsah a forma je rozdielna,
Dialogue: 0,0:47:03.05,0:47:06.52,Default,,0000,0000,0000,,ale ukázať svoju úprimnosť,
Dialogue: 0,0:47:06.68,0:47:09.69,Default,,0000,0000,0000,,nemusí byť taká sladká a úžasná ako toto.
Dialogue: 0,0:47:09.85,0:47:13.46,Default,,0000,0000,0000,,Avšak, nie všetky vyznania budú úspešné.
Dialogue: 0,0:47:13.66,0:47:17.13,Default,,0000,0000,0000,,Kto bude vodcom všetkých splašených sŕdc?
Dialogue: 0,0:47:17.26,0:47:19.33,Default,,0000,0000,0000,,Kto to bude?
Dialogue: 0,0:47:28.67,0:47:32.24,Default,,0000,0000,0000,,~ {\i1}Letná láska, ktorou som vybuchla.{\i0} ~
Dialogue: 0,0:47:32.64,0:47:36.11,Default,,0000,0000,0000,,~ {\i1}Letná láska, prišla tak rýchlo.{\i0} ~
Dialogue: 0,0:47:36.34,0:47:40.11,Default,,0000,0000,0000,,~ {\i1}Stretol som dievča, čo po mne bláznilo.{\i0} ~
Dialogue: 0,0:47:40.38,0:47:43.81,Default,,0000,0000,0000,,~ {\i1}Stretla som chutného chlapca.{\i0} ~
Dialogue: 0,0:47:44.08,0:47:47.12,Default,,0000,0000,0000,,~ {\i1}Letné dni odchádzajú preč{\i0} ~
Dialogue: 0,0:47:47.18,0:47:50.15,Default,,0000,0000,0000,,~ {\i1}tie, oh, oh, tie letné noci.{\i0} ~
Dialogue: 0,0:47:51.86,0:47:53.72,Default,,0000,0000,0000,,~ {\i1}Povedz mi viac, povedz mi viac,{\i0} ~
Dialogue: 0,0:47:53.89,0:47:55.56,Default,,0000,0000,0000,,~ {\i1}dostala si sa veľmi ďaleko?{\i0} ~
Dialogue: 0,0:47:55.79,0:47:57.59,Default,,0000,0000,0000,,~ {\i1}Povedz mi viac, povedz mi viac,{\i0} ~
Dialogue: 0,0:47:57.79,0:47:59.60,Default,,0000,0000,0000,,~ {\i1}ako keby mal nejaké auto?{\i0} ~
Dialogue: 0,0:48:03.43,0:48:06.84,Default,,0000,0000,0000,,~ {\i1}Priplávala ku mne, dostala kŕč.{\i0} ~
Dialogue: 0,0:48:07.30,0:48:10.71,Default,,0000,0000,0000,,~ {\i1}Utekal ku mne a zmokryl mi kabát.{\i0} ~
Dialogue: 0,0:48:10.97,0:48:14.55,Default,,0000,0000,0000,,~ {\i1}Zachránil som jej život, skoro sa utopila.{\i0} ~
Dialogue: 0,0:48:14.98,0:48:18.42,Default,,0000,0000,0000,,~ {\i1}Predviedol sa, striekajúc na každú stranu.{\i0} ~
Dialogue: 0,0:48:18.85,0:48:21.79,Default,,0000,0000,0000,,~ {\i1}Letná stretávka, nič neznamená,{\i0} ~
Dialogue: 0,0:48:21.99,0:48:26.12,Default,,0000,0000,0000,,~ {\i1}ale, oh, oh, tie letné noci.{\i0} ~
Dialogue: 0,0:48:26.69,0:48:28.69,Default,,0000,0000,0000,,~ {\i1}Povedz mi viac, povedz mi viac,{\i0} ~
Dialogue: 0,0:48:29.33,0:48:30.47,Default,,0000,0000,0000,,Mám ťa rád.
Dialogue: 0,0:48:30.53,0:48:32.53,Default,,0000,0000,0000,,~ {\i1}Povedz mi viac, povedz mi viac.{\i0} ~
Dialogue: 0,0:48:32.67,0:48:34.51,Default,,0000,0000,0000,,T-tiež ťa mám rada.
Dialogue: 0,0:48:37.33,0:48:42.31,Default,,0000,0000,0000,,~ {\i1}Nakoniec je to horšie, keď to končí.{\i0} ~
Dialogue: 0,0:48:43.01,0:48:47.18,Default,,0000,0000,0000,,~ {\i1}Tak som jej povedal, že budeme stále priatelia.{\i0} ~
Dialogue: 0,0:48:47.81,0:48:53.72,Default,,0000,0000,0000,,~ {\i1}Potom sme si prisahali lásku.{\i0} ~
Dialogue: 0,0:48:54.32,0:49:01.12,Default,,0000,0000,0000,,~ {\i1}Čo tak asi robí?{\i0} ~
Dialogue: 0,0:49:01.83,0:49:10.60,Default,,0000,0000,0000,,~ {\i1}Letné sny, všité do látky,{\i0} ~
Dialogue: 0,0:49:11.74,0:49:31.49,Default,,0000,0000,0000,,~ {\i1}ale, oh, tie letné noci.{\i0} ~
Dialogue: 0,0:49:56.15,0:49:59.12,Default,,0000,0000,0000,,Dala si to JB-ovi, však?
Dialogue: 0,0:50:01.89,0:50:04.56,Default,,0000,0000,0000,,Aj keď to má svoje odôvodnenie,
Dialogue: 0,0:50:04.76,0:50:06.76,Default,,0000,0000,0000,,ale máš pravdu.
Dialogue: 0,0:50:08.00,0:50:10.33,Default,,0000,0000,0000,,Oh, vyzeráš byť silná.
Dialogue: 0,0:50:10.53,0:50:13.50,Default,,0000,0000,0000,,Dávať niečo čo som vyhodila niekomu inému.
Dialogue: 0,0:50:13.67,0:50:17.17,Default,,0000,0000,0000,,Čo sa snažíš povedať?
Dialogue: 0,0:50:17.34,0:50:20.24,Default,,0000,0000,0000,,Ako tento náhrdelník,
Dialogue: 0,0:50:20.51,0:50:24.11,Default,,0000,0000,0000,,maj si aj JB-a.
Dialogue: 0,0:50:25.72,0:50:29.55,Default,,0000,0000,0000,,Už som ho úplne nechala tak.
Dialogue: 0,0:50:30.22,0:50:34.22,Default,,0000,0000,0000,,Takže, nechaj si ho.
Dialogue: 0,0:50:35.26,0:50:38.23,Default,,0000,0000,0000,,Nehovoríš pravdu, však?
Dialogue: 0,0:50:41.50,0:50:43.57,Default,,0000,0000,0000,,Porovnajúc ostatných,
Dialogue: 0,0:50:43.57,0:50:46.64,Default,,0000,0000,0000,,nemyslíš na neho najviac?
Dialogue: 0,0:50:46.64,0:50:48.97,Default,,0000,0000,0000,,Len nechceš, aby sa zranil.
Dialogue: 0,0:50:48.97,0:50:50.57,Default,,0000,0000,0000,,Nevarovala si ma minule?
Dialogue: 0,0:50:50.57,0:50:54.61,Default,,0000,0000,0000,,Že by som ho nemala prijať, aj keď si ma vyberie.
Dialogue: 0,0:50:54.61,0:50:58.15,Default,,0000,0000,0000,,Už je to vec minulosti.
Dialogue: 0,0:50:58.15,0:51:01.75,Default,,0000,0000,0000,,Či chceš alebo nie,
Dialogue: 0,0:51:01.75,0:51:07.49,Default,,0000,0000,0000,,na konci, JB sa poraní kvôli tebe.
Dialogue: 0,0:51:07.49,0:51:12.20,Default,,0000,0000,0000,,Prečo to hovoríš?
Dialogue: 0,0:51:12.20,0:51:16.93,Default,,0000,0000,0000,,Ako keby si chcela, aby sa zranil.
Dialogue: 0,0:51:16.93,0:51:20.67,Default,,0000,0000,0000,,Budem ho ochraňovať,
Dialogue: 0,0:51:20.67,0:51:23.47,Default,,0000,0000,0000,,aby sa mu nič nestalo.
Dialogue: 0,0:51:28.21,0:51:31.48,Default,,0000,0000,0000,,Dúfam, že hovoríš pravdu.
Dialogue: 0,0:54:38.04,0:54:40.80,Default,,0000,0000,0000,,Nevedela som, že HaeSung má toto v sebe.
Dialogue: 0,0:54:40.97,0:54:42.54,Default,,0000,0000,0000,,Naozaj krásna.
Dialogue: 0,0:54:42.71,0:54:44.04,Default,,0000,0000,0000,,Tak moc žiari,
Dialogue: 0,0:54:44.21,0:54:46.21,Default,,0000,0000,0000,,ako skutočná hviezda.
Dialogue: 0,0:54:47.01,0:54:49.21,Default,,0000,0000,0000,,HaeSung a JB spolu chodia, že?
Dialogue: 0,0:54:49.41,0:54:51.15,Default,,0000,0000,0000,,Vyzerajú spolu tak dobre.
Dialogue: 0,0:54:51.32,0:54:52.78,Default,,0000,0000,0000,,Ale, čo sa stalo Lian?
Dialogue: 0,0:54:52.95,0:54:54.52,Default,,0000,0000,0000,,Nechodila s ním ona?
Dialogue: 0,0:54:54.69,0:54:56.55,Default,,0000,0000,0000,,Neviem, neviem!
Dialogue: 0,0:55:01.95,0:55:04.66,Default,,0000,0000,0000,,{\i1}~ Ako keby si sa na mňa pozerala. ~{\i0}
Dialogue: 0,0:55:04.86,0:55:07.46,Default,,0000,0000,0000,,{\i1}~ Vo vôni čo si ma nechal. ~{\i0}
Dialogue: 0,0:55:07.66,0:55:11.33,Default,,0000,0000,0000,,{\i1}~ Všetko ostatné má ten istý pocit. ~{\i0}
Dialogue: 0,0:55:12.33,0:55:15.83,Default,,0000,0000,0000,,{\i1}~ Vidieť všetko, hneď ako otvorím oči. ~{\i0}
Dialogue: 0,0:55:16.03,0:55:18.37,Default,,0000,0000,0000,,{\i1}~ Po celý deň, ~{\i0}
Dialogue: 0,0:55:18.57,0:55:23.74,Default,,0000,0000,0000,,{\i1}~ Aj bezo mňa, to bude rovnaké, že?? ~{\i0}
Dialogue: 0,0:55:24.27,0:55:27.01,Default,,0000,0000,0000,,{\i1}~ Rozplakať ma. Opustiť ťa. ~{\i0}
Dialogue: 0,0:55:27.21,0:55:30.15,Default,,0000,0000,0000,,{\i1}~ Prestaň hovoriť tie slová. ~{\i0}
Dialogue: 0,0:55:30.38,0:55:33.28,Default,,0000,0000,0000,,{\i1}~ Aby sme oboch nechali na našej krásne ceste, ~{\i0}
Dialogue: 0,0:55:33.48,0:55:38.16,Default,,0000,0000,0000,,{\i1}~ na tomto svete, v mojom dni, v tvojej noci, ~{\i0}
Dialogue: 0,0:55:38.39,0:55:41.26,Default,,0000,0000,0000,,{\i1}~ čakajme ten istý koniec. ~{\i0}
Dialogue: 0,0:55:41.53,0:55:45.40,Default,,0000,0000,0000,,{\i1}~ Moja láska, vždy si nechaj zatvorené oči. ~{\i0}
Dialogue: 0,0:55:46.30,0:55:47.60,Default,,0000,0000,0000,,~ {\i1}Vzdaj sa, vzdaj sa.{\i0} ~
Dialogue: 0,0:55:47.80,0:55:49.07,Default,,0000,0000,0000,,~ {\i1}Vidíš to svetlo.{\i0} ~
Dialogue: 0,0:55:49.23,0:55:50.43,Default,,0000,0000,0000,,~ {\i1}Vzdaj sa, vzdaj sa.{\i0} ~
Dialogue: 0,0:55:50.63,0:55:51.80,Default,,0000,0000,0000,,~ {\i1}Nikto tu nie je.{\i0} ~
Dialogue: 0,0:55:52.00,0:55:53.24,Default,,0000,0000,0000,,~ {\i1}Vzdaj sa, vzdaj sa.{\i0} ~
Dialogue: 0,0:55:53.44,0:55:54.64,Default,,0000,0000,0000,,~ {\i1}Je to naša jediná cesta.{\i0} ~
Dialogue: 0,0:55:55.07,0:55:57.34,Default,,0000,0000,0000,,~ {\i1}Nevidíš vo mne to svetlo?{\i0} ~
Dialogue: 0,0:55:57.57,0:55:58.78,Default,,0000,0000,0000,,~ {\i1}Vzdaj sa, vzdaj sa.{\i0} ~
Dialogue: 0,0:55:59.01,0:56:00.31,Default,,0000,0000,0000,,~ {\i1}Neboj sa, moja láska.{\i0} ~
Dialogue: 0,0:56:00.51,0:56:01.61,Default,,0000,0000,0000,,~ {\i1}Vzdaj sa, vzdaj sa.{\i0} ~
Dialogue: 0,0:56:01.85,0:56:03.01,Default,,0000,0000,0000,,~ {\i1}Je tu pre teba pieseň.{\i0} ~
Dialogue: 0,0:56:03.21,0:56:04.35,Default,,0000,0000,0000,,~ {\i1}Vzdaj sa, vzdaj sa.{\i0} ~
Dialogue: 0,0:56:04.55,0:56:06.02,Default,,0000,0000,0000,,~ {\i1}Nechaj kvety radovať sa. {\i0} ~
Dialogue: 0,0:56:06.28,0:56:09.75,Default,,0000,0000,0000,,~ {\i1}Nevidíš vo mne to svetlo?{\i0} ~
Dialogue: 0,0:56:10.95,0:56:14.36,Default,,0000,0000,0000,,{\i1}~ Hoci moje srdce sa strácalo. ~{\i0}
Dialogue: 0,0:56:14.56,0:56:17.09,Default,,0000,0000,0000,,{\i1}~ Len keď som v tvojom objatí, ~{\i0}
Dialogue: 0,0:56:17.36,0:56:21.20,Default,,0000,0000,0000,,{\i1}~ budem môcť nájsť svoju cestu. ~{\i0}
Dialogue: 0,0:56:22.20,0:56:25.54,Default,,0000,0000,0000,,{\i1}~ Aj na ďalší deň, budeme stále spolu. ~{\i0}
Dialogue: 0,0:56:25.77,0:56:28.11,Default,,0000,0000,0000,,{\i1}~ Aby sme objali celý svet ~{\i0}
Dialogue: 0,0:56:28.34,0:56:32.94,Default,,0000,0000,0000,,{\i1}~ a priniesli každému radosť. ~{\i0}
Dialogue: 0,0:56:34.11,0:56:36.81,Default,,0000,0000,0000,,{\i1}~ Rozplakať ma. Opustiť ťa. ~{\i0}
Dialogue: 0,0:56:39.79,0:56:42.09,Default,,0000,0000,0000,,Lian plače?
Dialogue: 0,0:56:43.93,0:56:45.36,Default,,0000,0000,0000,,Zrejme áno.
Dialogue: 0,0:56:45.53,0:56:47.70,Default,,0000,0000,0000,,Vyzerá to, akoby ju JB nechal.
Dialogue: 0,0:56:48.07,0:56:50.53,Default,,0000,0000,0000,,Lian je chúďatko.
Dialogue: 0,0:56:50.73,0:56:53.20,Default,,0000,0000,0000,,Nemyslíš si, že Lian schválne
Dialogue: 0,0:56:53.20,0:56:55.21,Default,,0000,0000,0000,,si zamenila s nami číslo?
Dialogue: 0,0:56:55.21,0:56:57.41,Default,,0000,0000,0000,,O čom to hovoríš?
Dialogue: 0,0:56:57.41,0:57:00.34,Default,,0000,0000,0000,,To neurobili preto, aby boli poslední.
Dialogue: 0,0:57:00.51,0:57:02.71,Default,,0000,0000,0000,,Ale preto, lebo chceli byť
Dialogue: 0,0:57:02.91,0:57:05.92,Default,,0000,0000,0000,,hneď po JB-m a HaeSung.
Dialogue: 0,0:57:09.25,0:57:12.56,Default,,0000,0000,0000,,Teraz ide z toho skutočný pocit.
Dialogue: 0,0:57:12.72,0:57:16.76,Default,,0000,0000,0000,,Ako keby to bolo naplánované.
Dialogue: 0,0:57:57.63,0:58:00.37,Default,,0000,0000,0000,,Premýšľala si nad dobrou pesničkou?
Dialogue: 0,0:58:01.41,0:58:05.38,Default,,0000,0000,0000,,Vyznanie. Romantika.
Dialogue: 0,0:58:06.11,0:58:10.95,Default,,0000,0000,0000,,Láska nemusí byť vždy sladká.
Dialogue: 0,0:58:11.12,0:58:13.62,Default,,0000,0000,0000,,Čo tým myslíš?
Dialogue: 0,0:58:16.39,0:58:18.69,Default,,0000,0000,0000,,Len počkaj.
Dialogue: 0,0:58:19.12,0:58:22.13,Default,,0000,0000,0000,,Všetci budú stáť na mojej strane.
Dialogue: 0,0:58:37.98,0:58:43.35,Default,,0000,0000,0000,,Dojali vás ich vyznania?
Dialogue: 0,0:58:43.51,0:58:48.92,Default,,0000,0000,0000,,Teraz, držím v ruke výsledky vášho hlasovania.
Dialogue: 0,0:58:49.09,0:58:53.82,Default,,0000,0000,0000,,Pred oznámením výhercu,
Dialogue: 0,0:58:54.02,0:58:56.83,Default,,0000,0000,0000,,oznámim najhoršieho súťažiaceho s najmenším počtom hlasov.
Dialogue: 0,0:58:56.99,0:59:00.30,Default,,0000,0000,0000,,Inak povedané, najhorší hráč.
Dialogue: 0,0:59:03.03,0:59:08.37,Default,,0000,0000,0000,,V dnešnej súťaži Super Idola, bude najhorší hráč,
Dialogue: 0,0:59:08.37,0:59:10.21,Default,,0000,0000,0000,,eliminovaný.
Dialogue: 0,0:59:14.55,0:59:17.48,Default,,0000,0000,0000,,Prečo sa zrazu bude vyraďovať?
Dialogue: 0,0:59:17.61,0:59:20.85,Default,,0000,0000,0000,,Ale, všetkých čo tu majú sú členky HershE a I:dn.
Dialogue: 0,0:59:21.05,0:59:22.15,Default,,0000,0000,0000,,Nedáva to zmysel!
Dialogue: 0,0:59:22.32,0:59:23.59,Default,,0000,0000,0000,,Nikdy o eliminácií nehovorili.
Dialogue: 0,0:59:23.75,0:59:25.49,Default,,0000,0000,0000,,Prečo tak zrazu?
Dialogue: 0,0:59:25.69,0:59:29.36,Default,,0000,0000,0000,,Dnešná misia bola o vyznaní sa.
Dialogue: 0,0:59:29.53,0:59:30.69,Default,,0000,0000,0000,,Už som to hovoril, však?
Dialogue: 0,0:59:30.89,0:59:32.20,Default,,0000,0000,0000,,Každého pieseň,
Dialogue: 0,0:59:32.40,0:59:37.37,Default,,0000,0000,0000,,dokonca aj ľúbostný dopis, je spôsob vyznania sa.
Dialogue: 0,0:59:37.53,0:59:39.34,Default,,0000,0000,0000,,Avšak, obsaj ľúbostného listu,
Dialogue: 0,0:59:39.50,0:59:41.04,Default,,0000,0000,0000,,je jedno ako komplikovane,
Dialogue: 0,0:59:41.21,0:59:43.87,Default,,0000,0000,0000,,je jedno ako krásne je napísaný,
Dialogue: 0,0:59:44.04,0:59:46.88,Default,,0000,0000,0000,,ale prijať odmietnutie od toho druhého,
Dialogue: 0,0:59:47.04,0:59:52.62,Default,,0000,0000,0000,,sa považuje jedine za písanie poznámok bez premýšľania.
Dialogue: 0,0:59:52.78,0:59:56.52,Default,,0000,0000,0000,,Táto misia, nemá žiadny skryté znamenie.
Dialogue: 0,0:59:56.52,0:59:59.32,Default,,0000,0000,0000,,Presne ako hovorí, vyznanie.
Dialogue: 0,0:59:59.32,1:00:01.59,Default,,0000,0000,0000,,Ak tvoje vyznanie je odmietnuté,
Dialogue: 0,1:00:01.59,1:00:05.30,Default,,0000,0000,0000,,tak už sa viac spolu neuvidíte.
Dialogue: 0,1:00:07.33,1:00:09.43,Default,,0000,0000,0000,,Ako som už povedal.
Dialogue: 0,1:00:09.63,1:00:11.34,Default,,0000,0000,0000,,Pre jedného z nich,
Dialogue: 0,1:00:11.50,1:00:15.81,Default,,0000,0000,0000,,je dnes toto posledné vystúpenie.
Dialogue: 0,1:00:16.01,1:00:22.25,Default,,0000,0000,0000,,A potom, okamžite im dojde, čoho sa kedysi tak vrúcne držali.
Dialogue: 0,1:00:22.71,1:00:24.85,Default,,0000,0000,0000,,Teraz...
Dialogue: 0,1:00:25.02,1:00:28.29,Default,,0000,0000,0000,,oznámim meno toho, kto vypadáva.
Dialogue: 0,1:00:38.70,1:00:42.33,Default,,0000,0000,0000,,Dnes, vypadáva...
Dialogue: 0,1:00:48.24,1:00:52.18,Default,,0000,0000,0000,,Eliminovaným je...
Dialogue: 0,1:00:52.18,1:00:53.11,Default,,0000,0000,0000,,JB.
Dialogue: 0,1:01:37.98,1:01:40.98,Default,,0000,0000,0000,,~ {\i1}Viem, že budeš superstár.{\i0} ~
Dialogue: 0,1:01:41.59,1:01:44.79,Default,,0000,0000,0000,,~ {\i1}Tak sa neboj o to kde si.{\i0} ~
Dialogue: 0,1:01:45.22,1:01:48.26,Default,,0000,0000,0000,,~ {\i1}Hlasno zakrič svoje meno,{\i0} ~
Dialogue: 0,1:01:48.69,1:01:52.00,Default,,0000,0000,0000,,~ {\i1}my všetci vieme a milujeme.{\i0} ~
Dialogue: 0,1:01:52.56,1:01:55.43,Default,,0000,0000,0000,,~ {\i1}Viem, že budeš superstár.{\i0} ~
