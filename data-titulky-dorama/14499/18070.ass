Dialogue: 0,0:00:00.00,0:09:04.75,AsianDrama,,0,0,0,,Škaredé Káčatko
Dialogue: 0,0:00:10.20,0:00:12.40,Default,,0,0,0,,Trpaslík je krátkozraký.{\pos(642.667,680)}
Dialogue: 0,0:00:14.80,0:00:16.20,Default,,0,0,0,,No a?{\pos(642.667,680)}
Dialogue: 0,0:00:17.20,0:00:18.90,Default,,0,0,0,,Dopekla…{\pos(642.667,680)}
Dialogue: 0,0:00:18.90,0:00:20.90,Default,,0,0,0,,Pripomína mi…..{\pos(642.667,680)}
Dialogue: 0,0:00:21.10,0:00:22.90,Default,,0,0,0,,toho chlapca.{\pos(642.667,680)}
Dialogue: 0,0:00:24.50,0:00:26.30,Default,,0,0,0,,Nevieš o ňom niečo?{\pos(642.667,680)}
Dialogue: 0,0:00:26.60,0:00:28.30,Default,,0,0,0,,Nevieš na akú strednú školu Wayo chodil?{\pos(642.667,680)}
Dialogue: 0,0:00:30.60,0:00:31.96,Default,,0,0,0,,Choď do pekla.{\pos(642.667,680)}
Dialogue: 0,0:00:31.96,0:00:33.50,Default,,0,0,0,,Ako to mám akože vedieť?{\pos(642.667,680)}
Dialogue: 0,0:00:34.00,0:00:35.90,Default,,0,0,0,,Však ja nie som Google.{\pos(642.667,680)}
Dialogue: 0,0:00:38.70,0:00:39.90,Default,,0,0,0,,Ale…{\pos(642.667,680)}
Dialogue: 0,0:00:41.00,0:00:41.90,Default,,0,0,0,,Bože!{\pos(642.667,680)}
Dialogue: 0,0:00:44.00,0:00:45.60,Default,,0,0,0,,Veľmi sa na seba podobajú.{\pos(642.667,680)}
Dialogue: 0,0:00:47.10,0:00:49.40,Default,,0,0,0,,A taktiež je krátkozraký.{\pos(642.667,680)}
Dialogue: 0,0:00:50.00,0:00:51.40,Default,,0,0,0,,Ujasnime si to.{\pos(642.667,680)}
Dialogue: 0,0:00:52.40,0:00:55.20,Default,,0,0,0,,Povedzme, že je to skutočne ten chlapec z tvojej minulosti.{\pos(642.667,680)}
Dialogue: 0,0:00:55.70,0:00:57.50,Default,,0,0,0,,Čo by si s tou informáciou robil?{\pos(642.667,680)}
Dialogue: 0,0:01:00.50,0:01:02.30,Default,,0,0,0,,Ja neviem.{\pos(642.667,680)}
Dialogue: 0,0:01:03.10,0:01:04.30,Default,,0,0,0,,Ale...{\pos(642.667,680)}
Dialogue: 0,0:01:04.70,0:01:06.30,Default,,0,0,0,,Akoby to bolo možné?{\pos(642.667,680)}
Dialogue: 0,0:01:09.20,0:01:10.80,Default,,0,0,0,,Trpaslík má snehobielu pokožku.{\pos(642.667,680)}
Dialogue: 0,0:01:11.40,0:01:13.80,Default,,0,0,0,,Vyzerá podstatne lepšie, ako ten chlapec\Nz mojej minulosti.{\pos(642.667,680)}
Dialogue: 0,0:01:16.30,0:01:18.00,Default,,0,0,0,,Choď do pekla!{\pos(642.667,680)}
Dialogue: 0,0:01:18.10,0:01:19.10,Default,,0,0,0,,Prečo?{\pos(642.667,680)}
Dialogue: 0,0:01:19.80,0:01:20.80,Default,,0,0,0,,Ale nič…{\pos(642.667,680)}
Dialogue: 0,0:01:20.80,0:01:23.50,Default,,0,0,0,,Bože, je fakt neskoro a chcem spať.{\pos(642.667,680)}
Dialogue: 0,0:01:23.60,0:01:26.00,Default,,0,0,0,,Dobre, tak sa uvidíme zajtra.{\pos(642.667,680)}
Dialogue: 0,0:01:31.95,0:01:32.75,Default,,0,0,0,,Pha!{\pos(642.667,680)}
Dialogue: 0,0:01:32.75,0:01:35.40,Default,,0,0,0,,Ty si neskutočný hlupák!{\pos(642.667,680)}
Dialogue: 0,0:01:54.00,0:01:55.80,Default,,0,0,0,,Hej, Pha!{\pos(642.667,680)}
Dialogue: 0,0:01:56.60,0:01:57.80,Default,,0,0,0,,Čo je?{\pos(642.667,680)}
Dialogue: 0,0:01:59.00,0:02:00.40,Default,,0,0,0,,Stalo sa niečo šokujúce.{\pos(642.667,680)}
Dialogue: 0,0:02:02.40,0:02:04.20,Default,,0,0,0,,Priprav sa.{\pos(642.667,680)}
Dialogue: 0,0:02:04.40,0:02:06.10,Default,,0,0,0,,O čom to tu hovoríte?{\pos(642.667,680)}
Dialogue: 0,0:02:13.10,0:02:14.50,Default,,0,0,0,,Myslím si…{\pos(642.667,680)}
Dialogue: 0,0:02:14.50,0:02:18.00,Default,,0,0,0,,že by sa mal na to pán doktor pozrieť sám.{\pos(642.667,680)}
Dialogue: 0,0:04:33.20,0:04:34.20,Default,,0,0,0,,Hej!{\pos(642.667,680)}
Dialogue: 0,0:04:35.60,0:04:37.20,Default,,0,0,0,,Mám ti poskytnúť prvú pomoc, kamarát?{\pos(642.667,680)}
Dialogue: 0,0:04:38.70,0:04:40.50,Default,,0,0,0,,Prečo som…{\pos(642.667,680)}
Dialogue: 0,0:04:41.00,0:04:42.50,Default,,0,0,0,,Prečo som bol taký hlúpy?{\pos(642.667,680)}
Dialogue: 0,0:04:46.50,0:04:48.40,Default,,0,0,0,,Myslím si, že by si sa mal trochu spamätať.{\pos(642.667,680)}
Dialogue: 0,0:04:48.40,0:04:50.10,Default,,0,0,0,,Bol si neskutočne hlúpy!{\pos(642.667,680)}
Dialogue: 0,0:04:50.10,0:04:51.70,Default,,0,0,0,,Bol si taký hlúpy, až ma z toho bolí hlava!{\pos(642.667,680)}
Dialogue: 0,0:04:51.70,0:04:53.70,Default,,0,0,0,,Bol si taký hlúpy, že sa tvoja hlúposť nedá vyjadriť slovami.{\pos(642.667,680)}
Dialogue: 0,0:04:53.70,0:04:55.30,Default,,0,0,0,,Čo je to s tebou?{\pos(642.667,680)}
Dialogue: 0,0:04:55.30,0:04:58.80,Default,,0,0,0,,Pri každej skúške máš najvyšší počet bodov,\Nno nedokázal si spoznať obyčajného chalana!{\pos(642.667,680)}
Dialogue: 0,0:05:00.80,0:05:03.60,Default,,0,0,0,,Myslel som si, že sa na seba iba podobajú.{\pos(642.667,680)}
Dialogue: 0,0:05:03.60,0:05:04.80,Default,,0,0,0,,Dopekla.{\pos(642.667,680)}
Dialogue: 0,0:05:07.00,0:05:08.60,Default,,0,0,0,,Takže...{\pos(642.667,680)}
Dialogue: 0,0:05:08.60,0:05:10.60,Default,,0,0,0,,Už vieš, kto to v skutočnosti je.{\pos(642.667,680)}
Dialogue: 0,0:05:10.60,0:05:11.90,Default,,0,0,0,,Aký je to pocit?{\pos(642.667,680)}
Dialogue: 0,0:05:14.90,0:05:16.40,Default,,0,0,0,,Cítim nostalgiu.{\pos(642.667,680)}
Dialogue: 0,0:05:19.00,0:05:20.80,Default,,0,0,0,,A čo máš teda v pláne?{\pos(642.667,680)}
Dialogue: 0,0:05:22.00,0:05:24.20,Default,,0,0,0,,Veľmi som ho podpichoval.{\pos(642.667,680)}
Dialogue: 0,0:05:25.10,0:05:26.20,Default,,0,0,0,,Vôbec neviem…{\pos(642.667,680)}
Dialogue: 0,0:05:27.00,0:05:29.90,Default,,0,0,0,,- Neviem, čo mám robiť.\N- No tak, to snáď nemyslíš vážne.{\pos(642.667,680)}
Dialogue: 0,0:05:30.60,0:05:32.20,Default,,0,0,0,,Musíš predsa niečo urobiť.{\pos(642.667,680)}
Dialogue: 0,0:05:33.40,0:05:34.40,Default,,0,0,0,,Inak…{\pos(642.667,680)}
Dialogue: 0,0:05:34.40,0:05:37.10,Default,,0,0,0,,Inak ti ho preberie niekto úplne iný.{\pos(642.667,680)}
Dialogue: 0,0:05:40.00,0:05:41.80,Default,,0,0,0,,Kit má v tomto pravdu.{\pos(642.667,680)}
Dialogue: 0,0:05:41.90,0:05:44.20,Default,,0,0,0,,V ten deň\Nsom ho videl s Forth-om.
Dialogue: 0,0:05:45.00,0:05:47.60,Default,,0,0,0,,Veľmi zvláštne sa pozeral na Waya.{\pos(642.667,680)}
Dialogue: 0,0:05:52.20,0:05:53.60,Default,,0,0,0,,No tak!{\pos(642.667,680)}
Dialogue: 0,0:05:54.00,0:05:55.40,Default,,0,0,0,,Vzchop sa.{\pos(642.667,680)}
Dialogue: 0,0:05:56.40,0:05:58.90,Default,,0,0,0,,Získaj si ho predtým, ako to urobí niekto iný.{\pos(642.667,680)}
Dialogue: 0,0:05:59.00,0:06:00.40,Default,,0,0,0,,Tentokrát…{\pos(642.667,680)}
Dialogue: 0,0:06:00.40,0:06:02.40,Default,,0,0,0,,Tentokrát mu musíš vyznať svoje city.{\pos(642.667,680)}
Dialogue: 0,0:06:02.40,0:06:04.40,Default,,0,0,0,,Nenechaj ho opäť odísť.{\pos(642.667,680)}
Dialogue: 0,0:06:07.00,0:06:09.00,Default,,0,0,0,,- Pha, kam to ideš?\N- Hneď sa vrátim.{\pos(642.667,680)}
Dialogue: 0,0:06:26.40,0:06:28.50,Default,,0,0,0,,Super!{\pos(642.667,680)}
Dialogue: 0,0:06:30.00,0:06:33.70,Default,,0,0,0,,No, s tými okuliarmi\Nvyzeráš fakt zvláštne.{\pos(642.667,680)}
Dialogue: 0,0:06:33.80,0:06:36.80,Default,,0,0,0,,Dobre zvláštne, alebo zle zvláštne?{\pos(642.667,680)}
Dialogue: 0,0:06:38.60,0:06:41.60,Default,,0,0,0,,To neviem,\Nale určite v nich nevyzeráš zle.{\pos(642.667,680)}
Dialogue: 0,0:06:43.30,0:06:45.20,Default,,0,0,0,,Takto som ťa ešte nikdy predtým nevidel.{\pos(642.667,680)}
Dialogue: 0,0:06:47.50,0:06:49.64,Default,,0,0,0,,- Chceš sa ísť niekam najesť?\N- Čože?{\pos(642.667,680)}
Dialogue: 0,0:06:49.64,0:06:51.64,Default,,0,0,0,,Nemôžem, pretože ešte čakám na Ming-a.{\pos(642.667,680)}
Dialogue: 0,0:06:51.64,0:06:54.90,Default,,0,0,0,,- Ahá, dobre.\N- Dobre, maj sa.{\pos(642.667,680)}
Dialogue: 0,0:06:59.00,0:07:01.30,Default,,0,0,0,,- Hej, ako sa máš?\N- Hej, ahoj.{\pos(642.667,680)}
Dialogue: 0,0:07:01.30,0:07:02.20,Default,,0,0,0,,Ahoj.{\pos(642.667,680)}
Dialogue: 0,0:07:02.60,0:07:04.20,Default,,0,0,0,,Som trochu unavený.{\pos(642.667,680)}
Dialogue: 0,0:07:05.00,0:07:06.70,Default,,0,0,0,,Priniesol som ti obed.{\pos(642.667,680)}
Dialogue: 0,0:07:07.00,0:07:08.20,Default,,0,0,0,,Ďakujem.{\pos(642.667,680)}
Dialogue: 0,0:07:13.10,0:07:15.10,Default,,0,0,0,,Toto je taktiež pre teba.{\pos(642.667,680)}
Dialogue: 0,0:07:15.80,0:07:16.70,Default,,0,0,0,,Beam.{\pos(642.667,680)}
Dialogue: 0,0:07:16.70,0:07:19.50,Default,,0,0,0,,Odkiaľ vieš, že milujem jahodové mlieko?{\pos(642.667,680)}
Dialogue: 0,0:07:21.80,0:07:25.80,Default,,0,0,0,,Popravde, kúpil ti ho Phana,\Nno hanbil sa ti ho sem priniesť osobne.{\pos(642.667,680)}
Dialogue: 0,0:07:27.00,0:07:28.80,Default,,0,0,0,,Hej, kamoš.{\pos(642.667,680)}
Dialogue: 0,0:07:30.00,0:07:31.60,Default,,0,0,0,,Nechaj ho, nech sa naobeduje.{\pos(642.667,680)}
Dialogue: 0,0:07:32.30,0:07:33.60,Default,,0,0,0,,Nemá veľa času.{\pos(642.667,680)}
Dialogue: 0,0:07:35.30,0:07:38.00,Default,,0,0,0,,- Už pôjdeme. \N- Dobre.{\pos(642.667,680)}
Dialogue: 0,0:07:38.00,0:07:39.60,Default,,0,0,0,,- Maj sa.\N- Uvidíme sa neskôr.{\pos(642.667,680)}
Dialogue: 0,0:07:42.80,0:07:44.80,Default,,0,0,0,,Hej, Phana!{\pos(642.667,680)}
Dialogue: 0,0:07:49.20,0:07:50.50,Default,,0,0,0,,Ďakujem.{\pos(642.667,680)}
Dialogue: 0,0:07:54.80,0:07:57.30,Default,,0,0,0,,- Pozrime sa.\N- Bože, vystrašil si ma!{\pos(642.667,680)} 
Dialogue: 0,0:08:01.30,0:08:02.90,Default,,0,0,0,,Myslím si, že si trochu chorý.{\pos(642.667,680)}
Dialogue: 0,0:08:03.20,0:08:04.60,Default,,0,0,0,,Ako to myslíš?{\pos(642.667,680)}
Dialogue: 0,0:08:05.20,0:08:08.00,Default,,0,0,0,,No, si nakazený vášnivou láskou!{\pos(642.667,680)}
Dialogue: 0,0:08:08.60,0:08:10.80,Default,,0,0,0,,Jedz, prosím ťa. Už som hladný.{\pos(642.667,680)}
Dialogue: 0,0:08:16.40,0:08:17.60,Default,,0,0,0,,Tu máš.{\pos(642.667,680)}
Dialogue: 0,0:08:18.50,0:08:20.50,Default,,0,0,0,,Prepáč, ale už mám obed.{\pos(642.667,680)}
Dialogue: 0,0:08:21.70,0:08:23.30,Default,,0,0,0,,No, tak sa najedz z oboch.{\pos(642.667,680)}
Dialogue: 0,0:08:23.30,0:08:25.10,Default,,0,0,0,,Si príšerne chudý.{\pos(642.667,680)}
Dialogue: 0,0:08:29.30,0:08:30.80,Default,,0,0,0,,Ďakujem.{\pos(642.667,680)}
Dialogue: 0,0:08:31.60,0:08:33.40,Default,,0,0,0,,Prečo si priniesol obed iba jemu?{\pos(642.667,680)}
Dialogue: 0,0:08:33.40,0:08:35.30,Default,,0,0,0,,A kde je môj obed?{\pos(642.667,680)}
Dialogue: 0,0:08:36.70,0:08:39.30,Default,,0,0,0,,Máš vlastné ruky a nohy.\NChoď si pre obed sám.{\pos(642.667,680)}
Dialogue: 0,0:08:43.80,0:08:46.00,Default,,0,0,0,,Ja som princom tvojho odboru!{\pos(642.667,680)}
Dialogue: 0,0:08:46.00,0:08:48.00,Default,,0,0,0,,
